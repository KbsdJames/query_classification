300a \u5c0f \u654f \u5bb6 \u300b \u3002 \u6bcf \u5929 \u66f4 \u65b0 \u4e24 \u96c6 \uff0c \u592a \u6162 \u4e86 \u3002 \u8feb \u4e0d \u53ca \u5f85 \u60f3 \u77e5 \u9053 \u4ed6 \u4e24 \u6700 \u540e \u597d \u4e86 \u6ca1 \u3002 \u8fd8 \u6709 \u4ed6 \u4eec \u7684 \u5b69 \u5b50 \u3002 \u68c0 \u7d22 \u771f \u7684 \u554a \uff0c \u592a \u597d \u4e86 \u3002 \u60f3 \u53bb \u8bfb \u4e00 \u8bfb \u539f \u8457 \u3002 \u68c0 \u7d22 \u4f60 \u518d \u7ed9 \u6211 \u5267 \u900f \u4e00 \u4e0b \u5927 \u59d0 \u5927 \u674e \u840d \u5457 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u300a \u5c0f \u654f \u5bb6 \u300b \u6d77 \u5357 \u7701 \u4e09 \u6c99 \u5e02 \u6211 \u8fd9 \u4e24 \u5929 \u6b63 \u5728 \u770b \u9ec4 \u78ca \u548c \u5468 \u8fc5 \u6f14 \u7684 \u90a3 \u4e2a \u7535 \u89c6 \u5267 \u5462 \u3002 \u68c0 \u7d22 \u6211 \u770b \u7684 \u662f \u6700 \u8fd1 \u70ed \u64ad \u7684 \u300a \u5c0f \u654f \u5bb6 \u300b \u3002 \u6bcf \u5929 \u66f4 \u65b0 \u4e24 \u96c6 \uff0c \u592a \u6162 \u4e86 \u3002 \u8feb \u4e0d \u53ca \u5f85 \u60f3 \u77e5 \u9053 \u4ed6 \u4e24 \u6700 \u540e \u597d \u4e86 \u6ca1 \u3002 \u8fd8 \u6709 \u4ed6 \u4eec \u7684 \u5b69 \u5b50 \u3002 \u68c0 \u7d22 \u771f \u7684 \u554a \uff0c \u592a \u597d \u4e86 \u3002 \u60f3 \u53bb \u8bfb \u4e00 \u8bfb \u539f \u8457 \u3002 \u68c0 \u7d22 \u4f60 \u518d \u7ed9 \u6211 \u5267 \u900f \u4e00 \u4e0b \u5927 \u59d0 \u5927 \u674e \u840d \u5457 \u3002 \u4e0d \u68c0 \u7d22 \u5c31 \u77e5 \u9053 \u4ed6 \u4eec \u957f \u4e0d \u4e86 \uff0c \u8fd8 \u5f97 \u662f \u6211 \u4eec \u9648 \u5353 \u597d \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u4e2d \u897f \u533a \u4f60 \u5728 \u770b \u5c0f \u8bf4 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u4e2d \u897f \u533a \u4f60 \u5728 \u770b \u5c0f \u8bf4 \u5417 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u5f88 \u5c11 \u770b \u7f51 \u6587 \u5c0f \u8bf4 \uff0c \u4e0d \u8fc7 \u8fd9 \u4e2a \u4eba \u5f88 \u6709 \u540d \uff0c \u6211 \u4e5f \u77e5 \u9053 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u4e2d \u897f \u533a \u4f60 \u5728 \u770b \u5c0f \u8bf4 \u5417 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u5f88 \u5c11 \u770b \u7f51 \u6587 \u5c0f \u8bf4 \uff0c \u4e0d \u8fc7 \u8fd9 \u4e2a \u4eba \u5f88 \u6709 \u540d \uff0c \u6211 \u4e5f \u77e5 \u9053 \u3002 \u4e0d \u68c0 \u7d22 \u770b \u3002 \u4e0d \u8fc7 \u770b \u7ecf \u5178 \u5c0f \u8bf4 \u6bd4 \u8f83 \u591a \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u4e2d \u897f \u533a \u4f60 \u5728 \u770b \u5c0f \u8bf4 \u5417 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u5f88 \u5c11 \u770b \u7f51 \u6587 \u5c0f \u8bf4 \uff0c \u4e0d \u8fc7 \u8fd9 \u4e2a \u4eba \u5f88 \u6709 \u540d \uff0c \u6211 \u4e5f \u77e5 \u9053 \u3002 \u4e0d \u68c0 \u7d22 \u770b \u3002 \u4e0d \u8fc7 \u770b \u7ecf \u5178 \u5c0f \u8bf4 \u6bd4 \u8f83 \u591a \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u633a \u559c \u6b22 \u957f \u7bc7 \u4fc4 \u7f57 \u65af \u6587 \u5b66 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u4e2d \u897f \u533a \u4f60 \u5728 \u770b \u5c0f \u8bf4 \u5417 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u5f88 \u5c11 \u770b \u7f51 \u6587 \u5c0f \u8bf4 \uff0c \u4e0d \u8fc7 \u8fd9 \u4e2a \u4eba \u5f88 \u6709 \u540d \uff0c \u6211 \u4e5f \u77e5 \u9053 \u3002 \u4e0d \u68c0 \u7d22 \u770b \u3002 \u4e0d \u8fc7 \u770b \u7ecf \u5178 \u5c0f \u8bf4 \u6bd4 \u8f83 \u591a \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u633a \u559c \u6b22 \u957f \u7bc7 \u4fc4 \u7f57 \u65af \u6587 \u5b66 \u3002 \u68c0 \u7d22 \u770b \u6765 \u4f60 \u4e0d \u4ec5 \u770b \u7f51 \u6587 \u5c0f \u8bf4 \uff0c \u5bf9 \u540d \u8457 \u4e5f \u633a \u4e86 \u89e3 \u554a \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u7518 \u8083 \u7701 \u4e34 \u590f \u56de \u65cf \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u597d \u50cf \u7279 \u522b \u6d41 \u884c \u60e0 \u7075 \u987f \u725b \u6392 \uff0c \u4f60 \u6709 \u5403 \u8fc7 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u7518 \u8083 \u7701 \u4e34 \u590f \u56de \u65cf \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u597d \u50cf \u7279 \u522b \u6d41 \u884c \u60e0 \u7075 \u987f \u725b \u6392 \uff0c \u4f60 \u6709 \u5403 \u8fc7 \u5417 \uff1f \u68c0 \u7d22 \u771f \u7684 \u5417 \uff1f \u4f60 \u80fd \u6559 \u6559 \u6211 \u600e \u4e48 \u505a \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u7518 \u8083 \u7701 \u4e34 \u590f \u56de \u65cf \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u597d \u50cf \u7279 \u522b \u6d41 \u884c \u60e0 \u7075 \u987f \u725b \u6392 \uff0c \u4f60 \u6709 \u5403 \u8fc7 \u5417 \uff1f \u68c0 \u7d22 \u771f \u7684 \u5417 \uff1f \u4f60 \u80fd \u6559 \u6559 \u6211 \u600e \u4e48 \u505a \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9700 \u8981 \u8fd9 \u4e48 \u591a \u4e1c \u897f \u5440 \u3002 \u90a3 \u8fd8 \u9700 \u8981 \u4ec0 \u4e48 \u5668 \u6750 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u7518 \u8083 \u7701 \u4e34 \u590f \u56de \u65cf \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u597d \u50cf \u7279 \u522b \u6d41 \u884c \u60e0 \u7075 \u987f \u725b \u6392 \uff0c \u4f60 \u6709 \u5403 \u8fc7 \u5417 \uff1f \u68c0 \u7d22 \u771f \u7684 \u5417 \uff1f \u4f60 \u80fd \u6559 \u6559 \u6211 \u600e \u4e48 \u505a \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9700 \u8981 \u8fd9 \u4e48 \u591a \u4e1c \u897f \u5440 \u3002 \u90a3 \u8fd8 \u9700 \u8981 \u4ec0 \u4e48 \u5668 \u6750 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u554a \uff0c \u90a3 \u6211 \u53ef \u80fd \u5c31 \u505a \u4e0d \u6210 \u4e86 \uff0c \u6211 \u4eec \u5bb6 \u6ca1 \u6709 \u70e4 \u7bb1 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u7518 \u8083 \u7701 \u4e34 \u590f \u56de \u65cf \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u597d \u50cf \u7279 \u522b \u6d41 \u884c \u60e0 \u7075 \u987f \u725b \u6392 \uff0c \u4f60 \u6709 \u5403 \u8fc7 \u5417 \uff1f \u68c0 \u7d22 \u771f \u7684 \u5417 \uff1f \u4f60 \u80fd \u6559 \u6559 \u6211 \u600e \u4e48 \u505a \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9700 \u8981 \u8fd9 \u4e48 \u591a \u4e1c \u897f \u5440 \u3002 \u90a3 \u8fd8 \u9700 \u8981 \u4ec0 \u4e48 \u5668 \u6750 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u554a \uff0c \u90a3 \u6211 \u53ef \u80fd \u5c31 \u505a \u4e0d \u6210 \u4e86 \uff0c \u6211 \u4eec \u5bb6 \u6ca1 \u6709 \u70e4 \u7bb1 \u3002 \u4e0d \u68c0 \u7d22 \u90a3 \u592a \u597d \u4e86 \uff0c \u4eca \u5929 \u665a \u4e0a \u6211 \u80fd \u6765 \u4f60 \u5bb6 \u8e6d \u996d \u561b \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5357 \u533a \u6211 \u4eec \u7b49 \u4e0b \u60f3 \u5728 \u7f51 \u4e0a \u4e70 \u70b9 \u96f6 \u98df \uff0c \u4f60 \u6709 \u60f3 \u5403 \u7684 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5357 \u533a \u6211 \u4eec \u7b49 \u4e0b \u60f3 \u5728 \u7f51 \u4e0a \u4e70 \u70b9 \u96f6 \u98df \uff0c \u4f60 \u6709 \u60f3 \u5403 \u7684 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4eec \u60f3 \u4e70 \u70b9 \u9ebb \u85af \u3001 \u9762 \u5305 \u548c \u7cd6 \u5403 \uff0c \u8981 \u4e0d \u8981 \u7ed9 \u4f60 \u5e26 \u4e00 \u70b9 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5357 \u533a \u6211 \u4eec \u7b49 \u4e0b \u60f3 \u5728 \u7f51 \u4e0a \u4e70 \u70b9 \u96f6 \u98df \uff0c \u4f60 \u6709 \u60f3 \u5403 \u7684 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4eec \u60f3 \u4e70 \u70b9 \u9ebb \u85af \u3001 \u9762 \u5305 \u548c \u7cd6 \u5403 \uff0c \u8981 \u4e0d \u8981 \u7ed9 \u4f60 \u5e26 \u4e00 \u70b9 \u4e0d \u68c0 \u7d22 \u90a3 \u4e5f \u53ef \u4ee5 \u554a \uff0c \u6211 \u4e5f \u542c \u8bf4 \u5c71 \u59c6 \u7684 \u70e7 \u9e21 \u5f88 \u597d \u5403 \uff0c \u4f46 \u662f \u542c \u8bf4 \u5c71 \u59c6 \u53ef \u80fd \u8981 \u4f1a \u5458 \u624d \u80fd \u8fdb \u8bf6 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5357 \u533a \u6211 \u4eec \u7b49 \u4e0b \u60f3 \u5728 \u7f51 \u4e0a \u4e70 \u70b9 \u96f6 \u98df \uff0c \u4f60 \u6709 \u60f3 \u5403 \u7684 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4eec \u60f3 \u4e70 \u70b9 \u9ebb \u85af \u3001 \u9762 \u5305 \u548c \u7cd6 \u5403 \uff0c \u8981 \u4e0d \u8981 \u7ed9 \u4f60 \u5e26 \u4e00 \u70b9 \u4e0d \u68c0 \u7d22 \u90a3 \u4e5f \u53ef \u4ee5 \u554a \uff0c \u6211 \u4e5f \u542c \u8bf4 \u5c71 \u59c6 \u7684 \u70e7 \u9e21 \u5f88 \u597d \u5403 \uff0c \u4f46 \u662f \u542c \u8bf4 \u5c71 \u59c6 \u53ef \u80fd \u8981 \u4f1a \u5458 \u624d \u80fd \u8fdb \u8bf6 \u68c0 \u7d22 \u90a3 \u771f \u7684 \u662f \u592a \u597d \u4e86 \uff0c \u6211 \u4eec \u51e0 \u70b9 \u53bb \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5357 \u533a \u6211 \u4eec \u7b49 \u4e0b \u60f3 \u5728 \u7f51 \u4e0a \u4e70 \u70b9 \u96f6 \u98df \uff0c \u4f60 \u6709 \u60f3 \u5403 \u7684 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4eec \u60f3 \u4e70 \u70b9 \u9ebb \u85af \u3001 \u9762 \u5305 \u548c \u7cd6 \u5403 \uff0c \u8981 \u4e0d \u8981 \u7ed9 \u4f60 \u5e26 \u4e00 \u70b9 \u4e0d \u68c0 \u7d22 \u90a3 \u4e5f \u53ef \u4ee5 \u554a \uff0c \u6211 \u4e5f \u542c \u8bf4 \u5c71 \u59c6 \u7684 \u70e7 \u9e21 \u5f88 \u597d \u5403 \uff0c \u4f46 \u662f \u542c \u8bf4 \u5c71 \u59c6 \u53ef \u80fd \u8981 \u4f1a \u5458 \u624d \u80fd \u8fdb \u8bf6 \u68c0 \u7d22 \u90a3 \u771f \u7684 \u662f \u592a \u597d \u4e86 \uff0c \u6211 \u4eec \u51e0 \u70b9 \u53bb \uff1f \u68c0 \u7d22 \u90a3 \u8981 \u5e26 \u4f1e \u5417 \uff1f \u5c0f \u96e8 \u7684 \u8bdd \u6211 \u4e0d \u60f3 \u62ff \u4f1e \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u8349 \u8393 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u73b0 \u5728 \u8349 \u8393 \u597d \u50cf \u6709 \u70b9 \u8d35 \u8bf6 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u8349 \u8393 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u73b0 \u5728 \u8349 \u8393 \u597d \u50cf \u6709 \u70b9 \u8d35 \u8bf6 \u3002 \u68c0 \u7d22 \u662f \u7684 \uff0c \u8d85 \u5e02 \u91cc \u7684 \u8349 \u8393 \u90fd \u5305 \u88c5 \u5f97 \u5f88 \u7cbe \u81f4 \u3002 \u611f \u89c9 \u5473 \u9053 \u5dee \u4e00 \u70b9 \u3002 \u662f \u4e0d \u662f \u56e0 \u4e3a \u8349 \u8393 \u8fd8 \u6ca1 \u6210 \u719f \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u8349 \u8393 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u73b0 \u5728 \u8349 \u8393 \u597d \u50cf \u6709 \u70b9 \u8d35 \u8bf6 \u3002 \u68c0 \u7d22 \u662f \u7684 \uff0c \u8d85 \u5e02 \u91cc \u7684 \u8349 \u8393 \u90fd \u5305 \u88c5 \u5f97 \u5f88 \u7cbe \u81f4 \u3002 \u611f \u89c9 \u5473 \u9053 \u5dee \u4e00 \u70b9 \u3002 \u662f \u4e0d \u662f \u56e0 \u4e3a \u8349 \u8393 \u8fd8 \u6ca1 \u6210 \u719f \u554a \uff1f \u68c0 \u7d22 4 ~ 6 \u6708 \u4efd \u90a3 \u662f \u4e0d \u662f \u90fd \u5feb \u5230 \u4e94 \u4e00 \u4e86 \uff1f \u55ef \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u8349 \u8393 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u73b0 \u5728 \u8349 \u8393 \u597d \u50cf \u6709 \u70b9 \u8d35 \u8bf6 \u3002 \u68c0 \u7d22 \u662f \u7684 \uff0c \u8d85 \u5e02 \u91cc \u7684 \u8349 \u8393 \u90fd \u5305 \u88c5 \u5f97 \u5f88 \u7cbe \u81f4 \u3002 \u611f \u89c9 \u5473 \u9053 \u5dee \u4e00 \u70b9 \u3002 \u662f \u4e0d \u662f \u56e0 \u4e3a \u8349 \u8393 \u8fd8 \u6ca1 \u6210 \u719f \u554a \uff1f \u68c0 \u7d22 4 ~ 6 \u6708 \u4efd \u90a3 \u662f \u4e0d \u662f \u90fd \u5feb \u5230 \u4e94 \u4e00 \u4e86 \uff1f \u55ef \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u8bb0 \u5f97 \u53bb \u5e74 \u6211 \u8fd8 \u53bb \u8349 \u8393 \u5927 \u68da \u91cc \u6458 \u8349 \u8393 \u5403 \u6765 \u7740 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u8349 \u8393 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u73b0 \u5728 \u8349 \u8393 \u597d \u50cf \u6709 \u70b9 \u8d35 \u8bf6 \u3002 \u68c0 \u7d22 \u662f \u7684 \uff0c \u8d85 \u5e02 \u91cc \u7684 \u8349 \u8393 \u90fd \u5305 \u88c5 \u5f97 \u5f88 \u7cbe \u81f4 \u3002 \u611f \u89c9 \u5473 \u9053 \u5dee \u4e00 \u70b9 \u3002 \u662f \u4e0d \u662f \u56e0 \u4e3a \u8349 \u8393 \u8fd8 \u6ca1 \u6210 \u719f \u554a \uff1f \u68c0 \u7d22 4 ~ 6 \u6708 \u4efd \u90a3 \u662f \u4e0d \u662f \u90fd \u5feb \u5230 \u4e94 \u4e00 \u4e86 \uff1f \u55ef \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u8bb0 \u5f97 \u53bb \u5e74 \u6211 \u8fd8 \u53bb \u8349 \u8393 \u5927 \u68da \u91cc \u6458 \u8349 \u8393 \u5403 \u6765 \u7740 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u90a3 \u5929 \u5403 \u597d \u591a \u8349 \u8393 \u3002 \u809a \u5b50 \u90fd \u5feb \u5403 \u6491 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5e7f \u4e1c \u7701 \u6cb3 \u6e90 \u5e02 \u6211 \u6700 \u8fd1 \u7279 \u522b \u60f3 \u5b66 \u6444 \u5f71 \uff0c \u8fd9 \u6837 \u5c31 \u53ef \u4ee5 \u62cd \u597d \u770b \u7684 \u7167 \u7247 \u5566 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5e7f \u4e1c \u7701 \u6cb3 \u6e90 \u5e02 \u6211 \u6700 \u8fd1 \u7279 \u522b \u60f3 \u5b66 \u6444 \u5f71 \uff0c \u8fd9 \u6837 \u5c31 \u53ef \u4ee5 \u62cd \u597d \u770b \u7684 \u7167 \u7247 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u53ef \u662f \uff0c \u6211 \u4e0d \u77e5 \u9053 \u5e94 \u8be5 \u600e \u4e48 \u5f00 \u59cb \u5b66 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5e7f \u4e1c \u7701 \u6cb3 \u6e90 \u5e02 \u6211 \u6700 \u8fd1 \u7279 \u522b \u60f3 \u5b66 \u6444 \u5f71 \uff0c \u8fd9 \u6837 \u5c31 \u53ef \u4ee5 \u62cd \u597d \u770b \u7684 \u7167 \u7247 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u53ef \u662f \uff0c \u6211 \u4e0d \u77e5 \u9053 \u5e94 \u8be5 \u600e \u4e48 \u5f00 \u59cb \u5b66 \u3002 \u68c0 \u7d22 \u5514 \uff0c \u90a3 \u6211 \u662f \u4e0d \u662f \u5f97 \u4e70 \u4e00 \u4e2a \u76f8 \u673a \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5e7f \u4e1c \u7701 \u6cb3 \u6e90 \u5e02 \u6211 \u6700 \u8fd1 \u7279 \u522b \u60f3 \u5b66 \u6444 \u5f71 \uff0c \u8fd9 \u6837 \u5c31 \u53ef \u4ee5 \u62cd \u597d \u770b \u7684 \u7167 \u7247 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u53ef \u662f \uff0c \u6211 \u4e0d \u77e5 \u9053 \u5e94 \u8be5 \u600e \u4e48 \u5f00 \u59cb \u5b66 \u3002 \u68c0 \u7d22 \u5514 \uff0c \u90a3 \u6211 \u662f \u4e0d \u662f \u5f97 \u4e70 \u4e00 \u4e2a \u76f8 \u673a \u5440 \uff1f \u68c0 \u7d22 \u597d \u7684 \uff0c \u90a3 \u6211 \u4eca \u5929 \u5c31 \u53bb \u5546 \u573a \u901b \u4e00 \u901b \uff0c \u770b \u770b \u80fd \u4e0d \u80fd \u4e70 \u5230 \u9002 \u5408 \u7684 \u76f8 \u673a \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5e7f \u4e1c \u7701 \u6cb3 \u6e90 \u5e02 \u6211 \u6700 \u8fd1 \u7279 \u522b \u60f3 \u5b66 \u6444 \u5f71 \uff0c \u8fd9 \u6837 \u5c31 \u53ef \u4ee5 \u62cd \u597d \u770b \u7684 \u7167 \u7247 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u53ef \u662f \uff0c \u6211 \u4e0d \u77e5 \u9053 \u5e94 \u8be5 \u600e \u4e48 \u5f00 \u59cb \u5b66 \u3002 \u68c0 \u7d22 \u5514 \uff0c \u90a3 \u6211 \u662f \u4e0d \u662f \u5f97 \u4e70 \u4e00 \u4e2a \u76f8 \u673a \u5440 \uff1f \u68c0 \u7d22 \u597d \u7684 \uff0c \u90a3 \u6211 \u4eca \u5929 \u5c31 \u53bb \u5546 \u573a \u901b \u4e00 \u901b \uff0c \u770b \u770b \u80fd \u4e0d \u80fd \u4e70 \u5230 \u9002 \u5408 \u7684 \u76f8 \u673a \u3002 \u68c0 \u7d22 \u55ef \uff0c \u8c22 \u8c22 \u63d0 \u9192 \uff0c \u6211 \u51fa \u95e8 \u5566 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6cb3 \u5317 \u7701 \u627f \u5fb7 \u5e02 \u4eca \u5929 \u5728 \u624b \u673a \u4e0a \u770b \u5230 \u4e00 \u53ea \u597d \u53ef \u7231 \u7684 \u732b \u732b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6cb3 \u5317 \u7701 \u627f \u5fb7 \u5e02 \u4eca \u5929 \u5728 \u624b \u673a \u4e0a \u770b \u5230 \u4e00 \u53ea \u597d \u53ef \u7231 \u7684 \u732b \u732b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e2a \u4eba \u6bd4 \u8f83 \u559c \u6b22 \u66b9 \u7f57 \u732b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6cb3 \u5317 \u7701 \u627f \u5fb7 \u5e02 \u4eca \u5929 \u5728 \u624b \u673a \u4e0a \u770b \u5230 \u4e00 \u53ea \u597d \u53ef \u7231 \u7684 \u732b \u732b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e2a \u4eba \u6bd4 \u8f83 \u559c \u6b22 \u66b9 \u7f57 \u732b \u3002 \u68c0 \u7d22 \u662f \u5417 \uff0c \u592a \u68d2 \u4e86 \uff0c \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u5440 \uff0c \u5c31 \u662f \u4e0d \u77e5 \u9053 \u8fd9 \u79cd \u732b \u8d35 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6cb3 \u5317 \u7701 \u627f \u5fb7 \u5e02 \u4eca \u5929 \u5728 \u624b \u673a \u4e0a \u770b \u5230 \u4e00 \u53ea \u597d \u53ef \u7231 \u7684 \u732b \u732b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e2a \u4eba \u6bd4 \u8f83 \u559c \u6b22 \u66b9 \u7f57 \u732b \u3002 \u68c0 \u7d22 \u662f \u5417 \uff0c \u592a \u68d2 \u4e86 \uff0c \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u5440 \uff0c \u5c31 \u662f \u4e0d \u77e5 \u9053 \u8fd9 \u79cd \u732b \u8d35 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u90a3 \u4f60 \u77e5 \u9053 \u9644 \u8fd1 \u6709 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \u7684 \u5ba0 \u7269 \u5e97 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6cb3 \u5317 \u7701 \u627f \u5fb7 \u5e02 \u4eca \u5929 \u5728 \u624b \u673a \u4e0a \u770b \u5230 \u4e00 \u53ea \u597d \u53ef \u7231 \u7684 \u732b \u732b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e2a \u4eba \u6bd4 \u8f83 \u559c \u6b22 \u66b9 \u7f57 \u732b \u3002 \u68c0 \u7d22 \u662f \u5417 \uff0c \u592a \u68d2 \u4e86 \uff0c \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u5440 \uff0c \u5c31 \u662f \u4e0d \u77e5 \u9053 \u8fd9 \u79cd \u732b \u8d35 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u90a3 \u4f60 \u77e5 \u9053 \u9644 \u8fd1 \u6709 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \u7684 \u5ba0 \u7269 \u5e97 \u5417 \uff1f \u68c0 \u7d22 \u592a \u597d \u4e86 \uff0c \u6211 \u6b63 \u597d \u660e \u5929 \u8981 \u53bb \u5e02 \u91cc \uff0c \u53ef \u4ee5 \u987a \u4fbf \u53bb \u770b \u770b \uff0c \u4ef7 \u683c \u5408 \u9002 \u7684 \u8bdd \u6211 \u5c31 \u4e70 \u4e00 \u76f4 \u56de \u6765 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6cb3 \u5317 \u7701 \u627f \u5fb7 \u5e02 \u4eca \u5929 \u5728 \u624b \u673a \u4e0a \u770b \u5230 \u4e00 \u53ea \u597d \u53ef \u7231 \u7684 \u732b \u732b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e2a \u4eba \u6bd4 \u8f83 \u559c \u6b22 \u66b9 \u7f57 \u732b \u3002 \u68c0 \u7d22 \u662f \u5417 \uff0c \u592a \u68d2 \u4e86 \uff0c \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u5440 \uff0c \u5c31 \u662f \u4e0d \u77e5 \u9053 \u8fd9 \u79cd \u732b \u8d35 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u90a3 \u4f60 \u77e5 \u9053 \u9644 \u8fd1 \u6709 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \u7684 \u5ba0 \u7269 \u5e97 \u5417 \uff1f \u68c0 \u7d22 \u592a \u597d \u4e86 \uff0c \u6211 \u6b63 \u597d \u660e \u5929 \u8981 \u53bb \u5e02 \u91cc \uff0c \u53ef \u4ee5 \u987a \u4fbf \u53bb \u770b \u770b \uff0c \u4ef7 \u683c \u5408 \u9002 \u7684 \u8bdd \u6211 \u5c31 \u4e70 \u4e00 \u76f4 \u56de \u6765 \u3002 \u4e0d \u68c0 \u7d22 \u597d \u54d2 \uff0c \u62dc \u62dc \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u82cf \u7fca \u9e23 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u5b66 \u5355 \u677f \u6ed1 \u96ea \uff0c \u5f88 \u523a \u6fc0 \u554a \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u82cf \u7fca \u9e23 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u5b66 \u5355 \u677f \u6ed1 \u96ea \uff0c \u5f88 \u523a \u6fc0 \u554a \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u540d \u5b57 \u6709 \u70b9 \u964c \u751f \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u82cf \u7fca \u9e23 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u5b66 \u5355 \u677f \u6ed1 \u96ea \uff0c \u5f88 \u523a \u6fc0 \u554a \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u540d \u5b57 \u6709 \u70b9 \u964c \u751f \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u4e48 \u5389 \u5bb3 \uff0c \u6211 \u90fd \u6ca1 \u600e \u4e48 \u770b \u51ac \u5965 \u4f1a \u7684 \u6bd4 \u8d5b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u82cf \u7fca \u9e23 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u5b66 \u5355 \u677f \u6ed1 \u96ea \uff0c \u5f88 \u523a \u6fc0 \u554a \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u540d \u5b57 \u6709 \u70b9 \u964c \u751f \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u4e48 \u5389 \u5bb3 \uff0c \u6211 \u90fd \u6ca1 \u600e \u4e48 \u770b \u51ac \u5965 \u4f1a \u7684 \u6bd4 \u8d5b \u3002 \u4e0d \u68c0 \u7d22 \u5355 \u677f \u6ed1 \u96ea \u53ea \u6709 \u8fd9 \u4e24 \u9879 \u6bd4 \u8d5b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u82cf \u7fca \u9e23 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u5b66 \u5355 \u677f \u6ed1 \u96ea \uff0c \u5f88 \u523a \u6fc0 \u554a \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u540d \u5b57 \u6709 \u70b9 \u964c \u751f \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u4e48 \u5389 \u5bb3 \uff0c \u6211 \u90fd \u6ca1 \u600e \u4e48 \u770b \u51ac \u5965 \u4f1a \u7684 \u6bd4 \u8d5b \u3002 \u4e0d \u68c0 \u7d22 \u5355 \u677f \u6ed1 \u96ea \u53ea \u6709 \u8fd9 \u4e24 \u9879 \u6bd4 \u8d5b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u80fd \u53c2 \u52a0 \u4e24 \u4e2a \u5df2 \u7ecf \u5f88 \u5389 \u5bb3 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u4f60 \u6700 \u559c \u6b22 \u4e2d \u56fd \u5386 \u53f2 \u4e0a \u7684 \u54ea \u4e2a \u671d \u4ee3 \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u4f60 \u6700 \u559c \u6b22 \u4e2d \u56fd \u5386 \u53f2 \u4e0a \u7684 \u54ea \u4e2a \u671d \u4ee3 \u5462 \uff1f \u68c0 \u7d22 \u786e \u5b9e \uff0c \u90a3 \u4e2a \u65f6 \u4ee3 \u51fa \u4e86 \u4e0d \u5c11 \u5927 \u8bd7 \u4eba \u5462 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u4f60 \u6700 \u559c \u6b22 \u4e2d \u56fd \u5386 \u53f2 \u4e0a \u7684 \u54ea \u4e2a \u671d \u4ee3 \u5462 \uff1f \u68c0 \u7d22 \u786e \u5b9e \uff0c \u90a3 \u4e2a \u65f6 \u4ee3 \u51fa \u4e86 \u4e0d \u5c11 \u5927 \u8bd7 \u4eba \u5462 \u3002 \u68c0 \u7d22 \u6211 \u559c \u6b22 \u8bfb \u674e \u767d \u7684 \u8bd7 \uff0c \u60f3 \u8c61 \u7470 \u4e3d \uff0c \u767d \u53d1 \u4e09 \u5343 \u4e08 \uff0c \u7f18 \u6101 \u4f3c \u4e2a \u957f \uff0c \u592a \u6d6a \u6f2b \u4e86 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u4f60 \u6700 \u559c \u6b22 \u4e2d \u56fd \u5386 \u53f2 \u4e0a \u7684 \u54ea \u4e2a \u671d \u4ee3 \u5462 \uff1f \u68c0 \u7d22 \u786e \u5b9e \uff0c \u90a3 \u4e2a \u65f6 \u4ee3 \u51fa \u4e86 \u4e0d \u5c11 \u5927 \u8bd7 \u4eba \u5462 \u3002 \u68c0 \u7d22 \u6211 \u559c \u6b22 \u8bfb \u674e \u767d \u7684 \u8bd7 \uff0c \u60f3 \u8c61 \u7470 \u4e3d \uff0c \u767d \u53d1 \u4e09 \u5343 \u4e08 \uff0c \u7f18 \u6101 \u4f3c \u4e2a \u957f \uff0c \u592a \u6d6a \u6f2b \u4e86 \u5427 \u3002 \u68c0 \u7d22 \u5509 \uff0c \u6b64 \u540e \u5c31 \u5f88 \u5c11 \u6709 \u5982 \u6b64 \u5929 \u624d \u7684 \u6587 \u5b66 \u5bb6 \u4e86 \uff0c \u771f \u662f \u53ef \u60dc \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u4f60 \u6700 \u559c \u6b22 \u4e2d \u56fd \u5386 \u53f2 \u4e0a \u7684 \u54ea \u4e2a \u671d \u4ee3 \u5462 \uff1f \u68c0 \u7d22 \u786e \u5b9e \uff0c \u90a3 \u4e2a \u65f6 \u4ee3 \u51fa \u4e86 \u4e0d \u5c11 \u5927 \u8bd7 \u4eba \u5462 \u3002 \u68c0 \u7d22 \u6211 \u559c \u6b22 \u8bfb \u674e \u767d \u7684 \u8bd7 \uff0c \u60f3 \u8c61 \u7470 \u4e3d \uff0c \u767d \u53d1 \u4e09 \u5343 \u4e08 \uff0c \u7f18 \u6101 \u4f3c \u4e2a \u957f \uff0c \u592a \u6d6a \u6f2b \u4e86 \u5427 \u3002 \u68c0 \u7d22 \u5509 \uff0c \u6b64 \u540e \u5c31 \u5f88 \u5c11 \u6709 \u5982 \u6b64 \u5929 \u624d \u7684 \u6587 \u5b66 \u5bb6 \u4e86 \uff0c \u771f \u662f \u53ef \u60dc \u3002 \u4e0d \u68c0 \u7d22 \u5510 \u671d \u4e0d \u4ec5 \u6709 \u5927 \u8bd7 \u4eba \uff0c \u8fd8 \u6709 \u7f8e \u4eba \uff0c \u6768 \u7389 \u73af \u548c \u5510 \u7384 \u5b97 \u7684 \u6545 \u4e8b \u771f \u662f \u52a8 \u4eba \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u7bee \u7403 \u56db \u5ddd \u7701 \u5fb7 \u9633 \u5e02 \u6700 \u8fd1 \u653e \u5047 \uff0c \u6bcf \u5929 \u8eba \u7740 \u90fd \u4e0d \u60f3 \u52a8 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u7bee \u7403 \u56db \u5ddd \u7701 \u5fb7 \u9633 \u5e02 \u6700 \u8fd1 \u653e \u5047 \uff0c \u6bcf \u5929 \u8eba \u7740 \u90fd \u4e0d \u60f3 \u52a8 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u662f \u5f97 \u8fd0 \u52a8 \u953b \u70bc \u4e86 \uff0c \u8981 \u4e0d \u7136 \u5c31 \u5e9f \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u7bee \u7403 \u56db \u5ddd \u7701 \u5fb7 \u9633 \u5e02 \u6700 \u8fd1 \u653e \u5047 \uff0c \u6bcf \u5929 \u8eba \u7740 \u90fd \u4e0d \u60f3 \u52a8 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u662f \u5f97 \u8fd0 \u52a8 \u953b \u70bc \u4e86 \uff0c \u8981 \u4e0d \u7136 \u5c31 \u5e9f \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u81ea \u5df1 \u6bd4 \u8f83 \u559c \u6b22 \u7403 \u7c7b \u8fd0 \u52a8 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u7bee \u7403 \u56db \u5ddd \u7701 \u5fb7 \u9633 \u5e02 \u6700 \u8fd1 \u653e \u5047 \uff0c \u6bcf \u5929 \u8eba \u7740 \u90fd \u4e0d \u60f3 \u52a8 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u662f \u5f97 \u8fd0 \u52a8 \u953b \u70bc \u4e86 \uff0c \u8981 \u4e0d \u7136 \u5c31 \u5e9f \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u81ea \u5df1 \u6bd4 \u8f83 \u559c \u6b22 \u7403 \u7c7b \u8fd0 \u52a8 \u3002 \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u7bee \u7403 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u7bee \u7403 \u56db \u5ddd \u7701 \u5fb7 \u9633 \u5e02 \u6700 \u8fd1 \u653e \u5047 \uff0c \u6bcf \u5929 \u8eba \u7740 \u90fd \u4e0d \u60f3 \u52a8 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u662f \u5f97 \u8fd0 \u52a8 \u953b \u70bc \u4e86 \uff0c \u8981 \u4e0d \u7136 \u5c31 \u5e9f \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u81ea \u5df1 \u6bd4 \u8f83 \u559c \u6b22 \u7403 \u7c7b \u8fd0 \u52a8 \u3002 \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u7bee \u7403 \u4e86 \u3002 \u68c0 \u7d22 \u8981 \u662f \u627e \u4e0d \u5230 \u90a3 \u4e48 \u591a \u4eba \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u7259 \u5237 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u6211 \u4e00 \u5237 \u7259 \u7259 \u9f88 \u5c31 \u51fa \u8840 \uff0c \u8001 \u6298 \u78e8 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u7259 \u5237 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u6211 \u4e00 \u5237 \u7259 \u7259 \u9f88 \u5c31 \u51fa \u8840 \uff0c \u8001 \u6298 \u78e8 \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u5443 \uff0c \u633a \u4e45 \u4e86 \u5427 \uff0c \u4f60 \u63d0 \u9192 \u6211 \u4e86 \u3002 \u770b \u6765 \u6211 \u5e94 \u8be5 \u628a \u7259 \u5237 \u7ed9 \u6362 \u4e00 \u4e0b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u7259 \u5237 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u6211 \u4e00 \u5237 \u7259 \u7259 \u9f88 \u5c31 \u51fa \u8840 \uff0c \u8001 \u6298 \u78e8 \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u5443 \uff0c \u633a \u4e45 \u4e86 \u5427 \uff0c \u4f60 \u63d0 \u9192 \u6211 \u4e86 \u3002 \u770b \u6765 \u6211 \u5e94 \u8be5 \u628a \u7259 \u5237 \u7ed9 \u6362 \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u6211 \u89c9 \u5f97 \u4e5f \u662f \uff0c \u8fd9 \u5237 \u6bdb \u90fd \u786c \u4e86 \u3002 \u4e00 \u4f1a \u513f \u6211 \u5c31 \u53bb \u4e70 \u4e00 \u4e9b \u7259 \u5237 \uff0c \u8bf6 \u4ec0 \u4e48 \u7259 \u5237 \u6bd4 \u8f83 \u597d \u554a \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u7259 \u5237 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u6211 \u4e00 \u5237 \u7259 \u7259 \u9f88 \u5c31 \u51fa \u8840 \uff0c \u8001 \u6298 \u78e8 \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u5443 \uff0c \u633a \u4e45 \u4e86 \u5427 \uff0c \u4f60 \u63d0 \u9192 \u6211 \u4e86 \u3002 \u770b \u6765 \u6211 \u5e94 \u8be5 \u628a \u7259 \u5237 \u7ed9 \u6362 \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u6211 \u89c9 \u5f97 \u4e5f \u662f \uff0c \u8fd9 \u5237 \u6bdb \u90fd \u786c \u4e86 \u3002 \u4e00 \u4f1a \u513f \u6211 \u5c31 \u53bb \u4e70 \u4e00 \u4e9b \u7259 \u5237 \uff0c \u8bf6 \u4ec0 \u4e48 \u7259 \u5237 \u6bd4 \u8f83 \u597d \u554a \u68c0 \u7d22 \u55ef \u6211 \u5230 \u65f6 \u5019 \u53bb \u8d85 \u5e02 \u770b \u770b \u3002 \u5982 \u679c \u603b \u662f \u51fa \u8840 \u7684 \u8bdd \u662f \u4e0d \u662f \u8981 \u6302 \u53e3 \u8154 \u79d1 \u770b \u770b \u554a \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u7259 \u5237 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u6211 \u4e00 \u5237 \u7259 \u7259 \u9f88 \u5c31 \u51fa \u8840 \uff0c \u8001 \u6298 \u78e8 \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u5443 \uff0c \u633a \u4e45 \u4e86 \u5427 \uff0c \u4f60 \u63d0 \u9192 \u6211 \u4e86 \u3002 \u770b \u6765 \u6211 \u5e94 \u8be5 \u628a \u7259 \u5237 \u7ed9 \u6362 \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u6211 \u89c9 \u5f97 \u4e5f \u662f \uff0c \u8fd9 \u5237 \u6bdb \u90fd \u786c \u4e86 \u3002 \u4e00 \u4f1a \u513f \u6211 \u5c31 \u53bb \u4e70 \u4e00 \u4e9b \u7259 \u5237 \uff0c \u8bf6 \u4ec0 \u4e48 \u7259 \u5237 \u6bd4 \u8f83 \u597d \u554a \u68c0 \u7d22 \u55ef \u6211 \u5230 \u65f6 \u5019 \u53bb \u8d85 \u5e02 \u770b \u770b \u3002 \u5982 \u679c \u603b \u662f \u51fa \u8840 \u7684 \u8bdd \u662f \u4e0d \u662f \u8981 \u6302 \u53e3 \u8154 \u79d1 \u770b \u770b \u554a \u68c0 \u7d22 \u597d \u7684 \uff0c \u6211 \u53bb \u591a \u4e70 \u4e00 \u4e9b \u770b \u7740 \u9002 \u5408 \u6211 \u7684 \u7259 \u5237 \uff0c \u7136 \u540e \u4ee5 \u540e \u6ce8 \u610f \u66f4 \u6362 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u8fbd \u5b81 \u7701 \u94c1 \u5cad \u5e02 \u4f60 \u77e5 \u9053 \u8840 \u538b \u591a \u5c11 \u7b97 \u9ad8 \u8840 \u538b \u5417 \uff0c \u611f \u89c9 \u6700 \u8fd1 \u6709 \u70b9 \u5934 \u6655 \uff0c \u662f \u4e0d \u662f \u9ad8 \u8840 \u538b \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u8fbd \u5b81 \u7701 \u94c1 \u5cad \u5e02 \u4f60 \u77e5 \u9053 \u8840 \u538b \u591a \u5c11 \u7b97 \u9ad8 \u8840 \u538b \u5417 \uff0c \u611f \u89c9 \u6700 \u8fd1 \u6709 \u70b9 \u5934 \u6655 \uff0c \u662f \u4e0d \u662f \u9ad8 \u8840 \u538b \u4e86 \u4e0d \u68c0 \u7d22 \u6211 \u624d 20 \u591a \u5c81 \uff0c \u6700 \u8fd1 \u91cf \u7684 \u9ad8 \u538b \u662f 130mmhg \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u8fbd \u5b81 \u7701 \u94c1 \u5cad \u5e02 \u4f60 \u77e5 \u9053 \u8840 \u538b \u591a \u5c11 \u7b97 \u9ad8 \u8840 \u538b \u5417 \uff0c \u611f \u89c9 \u6700 \u8fd1 \u6709 \u70b9 \u5934 \u6655 \uff0c \u662f \u4e0d \u662f \u9ad8 \u8840 \u538b \u4e86 \u4e0d \u68c0 \u7d22 \u6211 \u624d 20 \u591a \u5c81 \uff0c \u6700 \u8fd1 \u91cf \u7684 \u9ad8 \u538b \u662f 130mmhg \u4e0d \u68c0 \u7d22 \u6211 \u5e94 \u8be5 \u6ce8 \u610f \u70b9 \u5565 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u8fbd \u5b81 \u7701 \u94c1 \u5cad \u5e02 \u4f60 \u77e5 \u9053 \u8840 \u538b \u591a \u5c11 \u7b97 \u9ad8 \u8840 \u538b \u5417 \uff0c \u611f \u89c9 \u6700 \u8fd1 \u6709 \u70b9 \u5934 \u6655 \uff0c \u662f \u4e0d \u662f \u9ad8 \u8840 \u538b \u4e86 \u4e0d \u68c0 \u7d22 \u6211 \u624d 20 \u591a \u5c81 \uff0c \u6700 \u8fd1 \u91cf \u7684 \u9ad8 \u538b \u662f 130mmhg \u4e0d \u68c0 \u7d22 \u6211 \u5e94 \u8be5 \u6ce8 \u610f \u70b9 \u5565 \u5440 \u4e0d \u68c0 \u7d22 \u6211 \u5e73 \u65f6 \u5403 \u7684 \u4e5f \u4e0d \u592a \u54b8 \uff0c \u4f4e \u76d0 \u662f \u4e2a \u5565 \u6807 \u51c6 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u8fbd \u5b81 \u7701 \u94c1 \u5cad \u5e02 \u4f60 \u77e5 \u9053 \u8840 \u538b \u591a \u5c11 \u7b97 \u9ad8 \u8840 \u538b \u5417 \uff0c \u611f \u89c9 \u6700 \u8fd1 \u6709 \u70b9 \u5934 \u6655 \uff0c \u662f \u4e0d \u662f \u9ad8 \u8840 \u538b \u4e86 \u4e0d \u68c0 \u7d22 \u6211 \u624d 20 \u591a \u5c81 \uff0c \u6700 \u8fd1 \u91cf \u7684 \u9ad8 \u538b \u662f 130mmhg \u4e0d \u68c0 \u7d22 \u6211 \u5e94 \u8be5 \u6ce8 \u610f \u70b9 \u5565 \u5440 \u4e0d \u68c0 \u7d22 \u6211 \u5e73 \u65f6 \u5403 \u7684 \u4e5f \u4e0d \u592a \u54b8 \uff0c \u4f4e \u76d0 \u662f \u4e2a \u5565 \u6807 \u51c6 \u5440 \u4e0d \u68c0 \u7d22 \u6211 \u5403 \u996d \u4e5f \u4e0d \u592a \u54b8 \uff0c \u4e3b \u8981 \u662f \u814c \u5236 \u98df \u54c1 \u66ff \u4ee3 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u5b66 \u6821 \u5c71 \u4e1c \u7701 \u83b1 \u829c \u5e02 \u9ad8 \u4e2d \u5feb \u6bd5 \u4e1a \u4e86 \uff0c \u662f \u4e0a \u54ea \u4e2a \u5927 \u5b66 \u597d \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u5b66 \u6821 \u5c71 \u4e1c \u7701 \u83b1 \u829c \u5e02 \u9ad8 \u4e2d \u5feb \u6bd5 \u4e1a \u4e86 \uff0c \u662f \u4e0a \u54ea \u4e2a \u5927 \u5b66 \u597d \u5462 \uff1f \u4e0d \u68c0 \u7d22 \u8fd8 \u53ef \u4ee5 \u5427 \uff0c \u4f60 \u77e5 \u9053 \u6cb3 \u5317 \u6709 \u54ea \u4e9b \u672c \u79d1 \u5b66 \u6821 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u5b66 \u6821 \u5c71 \u4e1c \u7701 \u83b1 \u829c \u5e02 \u9ad8 \u4e2d \u5feb \u6bd5 \u4e1a \u4e86 \uff0c \u662f \u4e0a \u54ea \u4e2a \u5927 \u5b66 \u597d \u5462 \uff1f \u4e0d \u68c0 \u7d22 \u8fd8 \u53ef \u4ee5 \u5427 \uff0c \u4f60 \u77e5 \u9053 \u6cb3 \u5317 \u6709 \u54ea \u4e9b \u672c \u79d1 \u5b66 \u6821 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u4f60 \u77e5 \u9053 \u71d5 \u5c71 \u5927 \u5b66 \u5f55 \u53d6 \u5206 \u6570 \u7ebf \u591a \u5c11 \u5206 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u5b66 \u6821 \u5c71 \u4e1c \u7701 \u83b1 \u829c \u5e02 \u9ad8 \u4e2d \u5feb \u6bd5 \u4e1a \u4e86 \uff0c \u662f \u4e0a \u54ea \u4e2a \u5927 \u5b66 \u597d \u5462 \uff1f \u4e0d \u68c0 \u7d22 \u8fd8 \u53ef \u4ee5 \u5427 \uff0c \u4f60 \u77e5 \u9053 \u6cb3 \u5317 \u6709 \u54ea \u4e9b \u672c \u79d1 \u5b66 \u6821 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u4f60 \u77e5 \u9053 \u71d5 \u5c71 \u5927 \u5b66 \u5f55 \u53d6 \u5206 \u6570 \u7ebf \u591a \u5c11 \u5206 \u5417 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u90a3 \u6211 \u4e00 \u5b9a \u8981 \u52aa \u529b \u5b66 \u4e60 \u8003 \u4e0a \u8fd9 \u4e2a \u5927 \u5b66 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u5b66 \u6821 \u5c71 \u4e1c \u7701 \u83b1 \u829c \u5e02 \u9ad8 \u4e2d \u5feb \u6bd5 \u4e1a \u4e86 \uff0c \u662f \u4e0a \u54ea \u4e2a \u5927 \u5b66 \u597d \u5462 \uff1f \u4e0d \u68c0 \u7d22 \u8fd8 \u53ef \u4ee5 \u5427 \uff0c \u4f60 \u77e5 \u9053 \u6cb3 \u5317 \u6709 \u54ea \u4e9b \u672c \u79d1 \u5b66 \u6821 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u4f60 \u77e5 \u9053 \u71d5 \u5c71 \u5927 \u5b66 \u5f55 \u53d6 \u5206 \u6570 \u7ebf \u591a \u5c11 \u5206 \u5417 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u90a3 \u6211 \u4e00 \u5b9a \u8981 \u52aa \u529b \u5b66 \u4e60 \u8003 \u4e0a \u8fd9 \u4e2a \u5927 \u5b66 \u5440 \u4e0d \u68c0 \u7d22 \u55ef \u55ef \uff0c \u90a3 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u5b66 \u4e60 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u5143 \u5b87 \u5b99 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u6700 \u8fd1 \u5143 \u5b87 \u5b99 \u8fd9 \u6982 \u5ff5 \u597d \u706b \uff0c \u542c \u51e0 \u4e2a \u670b \u53cb \u63d0 \u5230 \u8fc7 \uff0c \u6211 \u5c31 \u4e00 \u76f4 \u6ca1 \u5f04 \u61c2 \u8fd9 \u662f \u4e2a \u5565 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u5143 \u5b87 \u5b99 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u6700 \u8fd1 \u5143 \u5b87 \u5b99 \u8fd9 \u6982 \u5ff5 \u597d \u706b \uff0c \u542c \u51e0 \u4e2a \u670b \u53cb \u63d0 \u5230 \u8fc7 \uff0c \u6211 \u5c31 \u4e00 \u76f4 \u6ca1 \u5f04 \u61c2 \u8fd9 \u662f \u4e2a \u5565 \u68c0 \u7d22 \u55ef \uff0c \u542c \u7740 \u5f88 \u7b80 \u5355 \uff0c \u4f46 \u8fd8 \u662f \u6ca1 \u5565 \u5b9e \u611f \uff0c \u4e5f \u4e0d \u77e5 \u9053 \u80fd \u5e72 \u5565 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u5143 \u5b87 \u5b99 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u6700 \u8fd1 \u5143 \u5b87 \u5b99 \u8fd9 \u6982 \u5ff5 \u597d \u706b \uff0c \u542c \u51e0 \u4e2a \u670b \u53cb \u63d0 \u5230 \u8fc7 \uff0c \u6211 \u5c31 \u4e00 \u76f4 \u6ca1 \u5f04 \u61c2 \u8fd9 \u662f \u4e2a \u5565 \u68c0 \u7d22 \u55ef \uff0c \u542c \u7740 \u5f88 \u7b80 \u5355 \uff0c \u4f46 \u8fd8 \u662f \u6ca1 \u5565 \u5b9e \u611f \uff0c \u4e5f \u4e0d \u77e5 \u9053 \u80fd \u5e72 \u5565 \u68c0 \u7d22 \u5662 \u5662 \uff0c \u90a3 \u73b0 \u5728 \u5728 \u54ea \u4e9b \u9886 \u57df \u8fd0 \u7528 \u5730 \u6bd4 \u8f83 \u591a \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u5143 \u5b87 \u5b99 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u6700 \u8fd1 \u5143 \u5b87 \u5b99 \u8fd9 \u6982 \u5ff5 \u597d \u706b \uff0c \u542c \u51e0 \u4e2a \u670b \u53cb \u63d0 \u5230 \u8fc7 \uff0c \u6211 \u5c31 \u4e00 \u76f4 \u6ca1 \u5f04 \u61c2 \u8fd9 \u662f \u4e2a \u5565 \u68c0 \u7d22 \u55ef \uff0c \u542c \u7740 \u5f88 \u7b80 \u5355 \uff0c \u4f46 \u8fd8 \u662f \u6ca1 \u5565 \u5b9e \u611f \uff0c \u4e5f \u4e0d \u77e5 \u9053 \u80fd \u5e72 \u5565 \u68c0 \u7d22 \u5662 \u5662 \uff0c \u90a3 \u73b0 \u5728 \u5728 \u54ea \u4e9b \u9886 \u57df \u8fd0 \u7528 \u5730 \u6bd4 \u8f83 \u591a \u5440 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u8fd9 \u7b97 \u5f97 \u4e0a \u662f \u7b2c \u56db \u6b21 \u6280 \u672f \u9769 \u547d \u4e86 \u5427 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u5143 \u5b87 \u5b99 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u6700 \u8fd1 \u5143 \u5b87 \u5b99 \u8fd9 \u6982 \u5ff5 \u597d \u706b \uff0c \u542c \u51e0 \u4e2a \u670b \u53cb \u63d0 \u5230 \u8fc7 \uff0c \u6211 \u5c31 \u4e00 \u76f4 \u6ca1 \u5f04 \u61c2 \u8fd9 \u662f \u4e2a \u5565 \u68c0 \u7d22 \u55ef \uff0c \u542c \u7740 \u5f88 \u7b80 \u5355 \uff0c \u4f46 \u8fd8 \u662f \u6ca1 \u5565 \u5b9e \u611f \uff0c \u4e5f \u4e0d \u77e5 \u9053 \u80fd \u5e72 \u5565 \u68c0 \u7d22 \u5662 \u5662 \uff0c \u90a3 \u73b0 \u5728 \u5728 \u54ea \u4e9b \u9886 \u57df \u8fd0 \u7528 \u5730 \u6bd4 \u8f83 \u591a \u5440 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u8fd9 \u7b97 \u5f97 \u4e0a \u662f \u7b2c \u56db \u6b21 \u6280 \u672f \u9769 \u547d \u4e86 \u5427 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u6280 \u672f \u9769 \u547d \u4e4b \u540e \u7684 \u516c \u5e73 \u95ee \u9898 \uff0c \u73b0 \u5728 \u8d8a \u6765 \u8d8a \u4e25 \u5cfb \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u5143 \u5b87 \u5b99 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u6700 \u8fd1 \u5143 \u5b87 \u5b99 \u8fd9 \u6982 \u5ff5 \u597d \u706b \uff0c \u542c \u51e0 \u4e2a \u670b \u53cb \u63d0 \u5230 \u8fc7 \uff0c \u6211 \u5c31 \u4e00 \u76f4 \u6ca1 \u5f04 \u61c2 \u8fd9 \u662f \u4e2a \u5565 \u68c0 \u7d22 \u55ef \uff0c \u542c \u7740 \u5f88 \u7b80 \u5355 \uff0c \u4f46 \u8fd8 \u662f \u6ca1 \u5565 \u5b9e \u611f \uff0c \u4e5f \u4e0d \u77e5 \u9053 \u80fd \u5e72 \u5565 \u68c0 \u7d22 \u5662 \u5662 \uff0c \u90a3 \u73b0 \u5728 \u5728 \u54ea \u4e9b \u9886 \u57df \u8fd0 \u7528 \u5730 \u6bd4 \u8f83 \u591a \u5440 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u8fd9 \u7b97 \u5f97 \u4e0a \u662f \u7b2c \u56db \u6b21 \u6280 \u672f \u9769 \u547d \u4e86 \u5427 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u6280 \u672f \u9769 \u547d \u4e4b \u540e \u7684 \u516c \u5e73 \u95ee \u9898 \uff0c \u73b0 \u5728 \u8d8a \u6765 \u8d8a \u4e25 \u5cfb \u4e86 \u68c0 \u7d22 \u662f \u7684 \uff0c \u671f \u5f85 \u653f \u5e9c \u7684 \u8c03 \u63a7 \u80fd \u529b \u5427 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u6d77 \u5357 \u7701 \u6d77 \u53e3 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7684 \u7535 \u89c6 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u6d77 \u5357 \u7701 \u6d77 \u53e3 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7684 \u7535 \u89c6 \u68c0 \u7d22 \u300a \u51c9 \u751f \uff0c \u6211 \u4eec \u53ef \u4e0d \u53ef \u4ee5 \u4e0d \u5fe7 \u4f24 \u300b \u8fd9 \u4e2a \u540d \u5b57 \u542c \u8d77 \u6765 \u5f88 \u4e0d \u9519 \uff0c \u4e0d \u77e5 \u9053 \u662f \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u6d77 \u5357 \u7701 \u6d77 \u53e3 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7684 \u7535 \u89c6 \u68c0 \u7d22 \u300a \u51c9 \u751f \uff0c \u6211 \u4eec \u53ef \u4e0d \u53ef \u4ee5 \u4e0d \u5fe7 \u4f24 \u300b \u8fd9 \u4e2a \u540d \u5b57 \u542c \u8d77 \u6765 \u5f88 \u4e0d \u9519 \uff0c \u4e0d \u77e5 \u9053 \u662f \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u68c0 \u7d22 \u9057 \u61be \uff0c \u6211 \u4e0d \u559c \u6b22 \u592a \u60b2 \u4f24 \u7684 \u7535 \u89c6 \u5267 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u6d77 \u5357 \u7701 \u6d77 \u53e3 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7684 \u7535 \u89c6 \u68c0 \u7d22 \u300a \u51c9 \u751f \uff0c \u6211 \u4eec \u53ef \u4e0d \u53ef \u4ee5 \u4e0d \u5fe7 \u4f24 \u300b \u8fd9 \u4e2a \u540d \u5b57 \u542c \u8d77 \u6765 \u5f88 \u4e0d \u9519 \uff0c \u4e0d \u77e5 \u9053 \u662f \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u68c0 \u7d22 \u9057 \u61be \uff0c \u6211 \u4e0d \u559c \u6b22 \u592a \u60b2 \u4f24 \u7684 \u7535 \u89c6 \u5267 \u68c0 \u7d22 \u300a \u533b \u5983 \u96be \u56da \u300b \u6211 \u5df2 \u7ecf \u770b \u8fc7 \u5f88 \u591a \u904d \u4e86 \uff0c \u73b0 \u5728 \u5df2 \u7ecf \u51fa \u7b2c \u4e8c \u5b63 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u6d77 \u5357 \u7701 \u6d77 \u53e3 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7684 \u7535 \u89c6 \u68c0 \u7d22 \u300a \u51c9 \u751f \uff0c \u6211 \u4eec \u53ef \u4e0d \u53ef \u4ee5 \u4e0d \u5fe7 \u4f24 \u300b \u8fd9 \u4e2a \u540d \u5b57 \u542c \u8d77 \u6765 \u5f88 \u4e0d \u9519 \uff0c \u4e0d \u77e5 \u9053 \u662f \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u68c0 \u7d22 \u9057 \u61be \uff0c \u6211 \u4e0d \u559c \u6b22 \u592a \u60b2 \u4f24 \u7684 \u7535 \u89c6 \u5267 \u68c0 \u7d22 \u300a \u533b \u5983 \u96be \u56da \u300b \u6211 \u5df2 \u7ecf \u770b \u8fc7 \u5f88 \u591a \u904d \u4e86 \uff0c \u73b0 \u5728 \u5df2 \u7ecf \u51fa \u7b2c \u4e8c \u5b63 \u4e86 \u4e0d \u68c0 \u7d22 \u56e0 \u4e3a \u6211 \u5f88 \u559c \u6b22 \u7f51 \u7edc \u5c0f \u751c \u5267 \uff0c \u5f88 \u89e3 \u538b \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u53bb \u9493 \u9c7c \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u53bb \u9493 \u9c7c \u5417 \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \u6211 \u8fd8 \u6ca1 \u9493 \u8fc7 \u9c7c \u54c8 \u54c8 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u53bb \u9493 \u9c7c \u5417 \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \u6211 \u8fd8 \u6ca1 \u9493 \u8fc7 \u9c7c \u54c8 \u54c8 \u68c0 \u7d22 \u662f \u554a \uff0c \u8bdd \u8bf4 \u662f \u4e0d \u662f \u6709 \u4e00 \u4e2a \u53eb \u9493 \u9c7c \u5c9b \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u53bb \u9493 \u9c7c \u5417 \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \u6211 \u8fd8 \u6ca1 \u9493 \u8fc7 \u9c7c \u54c8 \u54c8 \u68c0 \u7d22 \u662f \u554a \uff0c \u8bdd \u8bf4 \u662f \u4e0d \u662f \u6709 \u4e00 \u4e2a \u53eb \u9493 \u9c7c \u5c9b \u4e0d \u68c0 \u7d22 \u771f \u7684 \u662f \u6c14 \u6b7b \u4eba \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u53bb \u9493 \u9c7c \u5417 \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \u6211 \u8fd8 \u6ca1 \u9493 \u8fc7 \u9c7c \u54c8 \u54c8 \u68c0 \u7d22 \u662f \u554a \uff0c \u8bdd \u8bf4 \u662f \u4e0d \u662f \u6709 \u4e00 \u4e2a \u53eb \u9493 \u9c7c \u5c9b \u4e0d \u68c0 \u7d22 \u771f \u7684 \u662f \u6c14 \u6b7b \u4eba \u4e86 \u68c0 \u7d22 \u5509 \u4e2d \u56fd \u8d8a \u6765 \u8d8a \u5f3a \u5927 \u76f8 \u4fe1 \u4f1a \u6536 \u56de \u7684 \uff5e \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u56db \u5ddd \u7701 \u6210 \u90fd \u5e02 \u4e3a \u4e86 \u4f53 \u6d4b \uff0c \u6211 \u75af \u72c2 \u7ec3 \u4e60 \u5750 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u5e0c \u671b \u80fd \u591f \u83b7 \u5f97 \u6ee1 \u5206 \uff0c \u53ef \u60dc \u6700 \u540e \u628a \u6211 \u7684 \u8170 \u626d \u4e86 \uff0c \u597d \u4f24 \u5fc3 \u554a ~ \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u56db \u5ddd \u7701 \u6210 \u90fd \u5e02 \u4e3a \u4e86 \u4f53 \u6d4b \uff0c \u6211 \u75af \u72c2 \u7ec3 \u4e60 \u5750 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u5e0c \u671b \u80fd \u591f \u83b7 \u5f97 \u6ee1 \u5206 \uff0c \u53ef \u60dc \u6700 \u540e \u628a \u6211 \u7684 \u8170 \u626d \u4e86 \uff0c \u597d \u4f24 \u5fc3 \u554a ~ \u68c0 \u7d22 \u90a3 \u6211 \u591a \u4e45 \u624d \u80fd \u4f53 \u6d4b \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u56db \u5ddd \u7701 \u6210 \u90fd \u5e02 \u4e3a \u4e86 \u4f53 \u6d4b \uff0c \u6211 \u75af \u72c2 \u7ec3 \u4e60 \u5750 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u5e0c \u671b \u80fd \u591f \u83b7 \u5f97 \u6ee1 \u5206 \uff0c \u53ef \u60dc \u6700 \u540e \u628a \u6211 \u7684 \u8170 \u626d \u4e86 \uff0c \u597d \u4f24 \u5fc3 \u554a ~ \u68c0 \u7d22 \u90a3 \u6211 \u591a \u4e45 \u624d \u80fd \u4f53 \u6d4b \u554a \uff1f \u68c0 \u7d22 \u597d \u5427 \uff0c \u90a3 \u5c31 \u7b97 \u6211 \u597d \u4e86 \uff0c \u6211 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u4f30 \u8ba1 \u6210 \u7ee9 \u4f1a \u5f88 \u96be \u770b \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u56db \u5ddd \u7701 \u6210 \u90fd \u5e02 \u4e3a \u4e86 \u4f53 \u6d4b \uff0c \u6211 \u75af \u72c2 \u7ec3 \u4e60 \u5750 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u5e0c \u671b \u80fd \u591f \u83b7 \u5f97 \u6ee1 \u5206 \uff0c \u53ef \u60dc \u6700 \u540e \u628a \u6211 \u7684 \u8170 \u626d \u4e86 \uff0c \u597d \u4f24 \u5fc3 \u554a ~ \u68c0 \u7d22 \u90a3 \u6211 \u591a \u4e45 \u624d \u80fd \u4f53 \u6d4b \u554a \uff1f \u68c0 \u7d22 \u597d \u5427 \uff0c \u90a3 \u5c31 \u7b97 \u6211 \u597d \u4e86 \uff0c \u6211 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u4f30 \u8ba1 \u6210 \u7ee9 \u4f1a \u5f88 \u96be \u770b \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u77e5 \u9053 \u7ad9 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u540e \u9762 \u4e24 \u4e2a \u6b63 \u538b \u817f \uff0c \u76d8 \u817f \u4f53 \u524d \u5c48 \u662f \u4ec0 \u4e48 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u56db \u5ddd \u7701 \u6210 \u90fd \u5e02 \u4e3a \u4e86 \u4f53 \u6d4b \uff0c \u6211 \u75af \u72c2 \u7ec3 \u4e60 \u5750 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u5e0c \u671b \u80fd \u591f \u83b7 \u5f97 \u6ee1 \u5206 \uff0c \u53ef \u60dc \u6700 \u540e \u628a \u6211 \u7684 \u8170 \u626d \u4e86 \uff0c \u597d \u4f24 \u5fc3 \u554a ~ \u68c0 \u7d22 \u90a3 \u6211 \u591a \u4e45 \u624d \u80fd \u4f53 \u6d4b \u554a \uff1f \u68c0 \u7d22 \u597d \u5427 \uff0c \u90a3 \u5c31 \u7b97 \u6211 \u597d \u4e86 \uff0c \u6211 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u4f30 \u8ba1 \u6210 \u7ee9 \u4f1a \u5f88 \u96be \u770b \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u77e5 \u9053 \u7ad9 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u540e \u9762 \u4e24 \u4e2a \u6b63 \u538b \u817f \uff0c \u76d8 \u817f \u4f53 \u524d \u5c48 \u662f \u4ec0 \u4e48 \u5440 \uff1f \u68c0 \u7d22 \u6700 \u8fd1 \u7684 \u6211 \u505a \u4e0d \u5230 \u8fd9 \u4e9b \u52a8 \u4f5c \uff0c \u8fd8 \u662f \u7f13 \u7f13 \u5427 \uff0c \u8fc7 \u4e00 \u6bb5 \u65f6 \u95f4 \u518d \u8bf4 ~ \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u56db \u5ddd \u7701 \u6210 \u90fd \u5e02 \u4e3a \u4e86 \u4f53 \u6d4b \uff0c \u6211 \u75af \u72c2 \u7ec3 \u4e60 \u5750 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u5e0c \u671b \u80fd \u591f \u83b7 \u5f97 \u6ee1 \u5206 \uff0c \u53ef \u60dc \u6700 \u540e \u628a \u6211 \u7684 \u8170 \u626d \u4e86 \uff0c \u597d \u4f24 \u5fc3 \u554a ~ \u68c0 \u7d22 \u90a3 \u6211 \u591a \u4e45 \u624d \u80fd \u4f53 \u6d4b \u554a \uff1f \u68c0 \u7d22 \u597d \u5427 \uff0c \u90a3 \u5c31 \u7b97 \u6211 \u597d \u4e86 \uff0c \u6211 \u5750 \u4f4d \u4f53 \u524d \u5c48 \u4f30 \u8ba1 \u6210 \u7ee9 \u4f1a \u5f88 \u96be \u770b \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u77e5 \u9053 \u7ad9 \u4f4d \u4f53 \u524d \u5c48 \uff0c \u540e \u9762 \u4e24 \u4e2a \u6b63 \u538b \u817f \uff0c \u76d8 \u817f \u4f53 \u524d \u5c48 \u662f \u4ec0 \u4e48 \u5440 \uff1f \u68c0 \u7d22 \u6700 \u8fd1 \u7684 \u6211 \u505a \u4e0d \u5230 \u8fd9 \u4e9b \u52a8 \u4f5c \uff0c \u8fd8 \u662f \u7f13 \u7f13 \u5427 \uff0c \u8fc7 \u4e00 \u6bb5 \u65f6 \u95f4 \u518d \u8bf4 ~ \u68c0 \u7d22 \u55ef \u55ef \u597d \u6ef4 \uff0c \u90a3 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u533b \u9662 \u770b \u770b \uff0c \u8ba9 \u533b \u751f \u914d \u70b9 \u836f \u818f \u5565 \u7684 ~ \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5b89 \u5fbd \u7701 \u6dee \u5317 \u5e02 \u5509 \uff0c \u6211 \u548c \u6211 \u540c \u684c \u5c31 \u662f \u5904 \u4e0d \u6765 \uff0c \u4e0d \u77e5 \u9053 \u4e3a \u4ec0 \u4e48 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5b89 \u5fbd \u7701 \u6dee \u5317 \u5e02 \u5509 \uff0c \u6211 \u548c \u6211 \u540c \u684c \u5c31 \u662f \u5904 \u4e0d \u6765 \uff0c \u4e0d \u77e5 \u9053 \u4e3a \u4ec0 \u4e48 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u662f \u5904 \u5973 \u5ea7 \uff0c \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u5979 \u662f \u4ec0 \u4e48 \u661f \u5ea7 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5b89 \u5fbd \u7701 \u6dee \u5317 \u5e02 \u5509 \uff0c \u6211 \u548c \u6211 \u540c \u684c \u5c31 \u662f \u5904 \u4e0d \u6765 \uff0c \u4e0d \u77e5 \u9053 \u4e3a \u4ec0 \u4e48 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u662f \u5904 \u5973 \u5ea7 \uff0c \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u5979 \u662f \u4ec0 \u4e48 \u661f \u5ea7 \u3002 \u68c0 \u7d22 \u54e6 \uff0c \u4e3a \u4ec0 \u4e48 \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5b89 \u5fbd \u7701 \u6dee \u5317 \u5e02 \u5509 \uff0c \u6211 \u548c \u6211 \u540c \u684c \u5c31 \u662f \u5904 \u4e0d \u6765 \uff0c \u4e0d \u77e5 \u9053 \u4e3a \u4ec0 \u4e48 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u662f \u5904 \u5973 \u5ea7 \uff0c \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u5979 \u662f \u4ec0 \u4e48 \u661f \u5ea7 \u3002 \u68c0 \u7d22 \u54e6 \uff0c \u4e3a \u4ec0 \u4e48 \u5462 \uff1f \u68c0 \u7d22 \u786e \u5b9e \uff0c \u6211 \u4eec \u505a \u4e8b \u7684 \u98ce \u683c \u5c31 \u5f88 \u4e0d \u4e00 \u6837 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5b89 \u5fbd \u7701 \u6dee \u5317 \u5e02 \u5509 \uff0c \u6211 \u548c \u6211 \u540c \u684c \u5c31 \u662f \u5904 \u4e0d \u6765 \uff0c \u4e0d \u77e5 \u9053 \u4e3a \u4ec0 \u4e48 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u662f \u5904 \u5973 \u5ea7 \uff0c \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u5979 \u662f \u4ec0 \u4e48 \u661f \u5ea7 \u3002 \u68c0 \u7d22 \u54e6 \uff0c \u4e3a \u4ec0 \u4e48 \u5462 \uff1f \u68c0 \u7d22 \u786e \u5b9e \uff0c \u6211 \u4eec \u505a \u4e8b \u7684 \u98ce \u683c \u5c31 \u5f88 \u4e0d \u4e00 \u6837 \u3002 \u68c0 \u7d22 \u5514 \uff0c \u90a3 \u6211 \u8bd5 \u8bd5 \uff0c \u5e0c \u671b \u80fd \u591f \u6539 \u5584 \u6211 \u4eec \u7684 \u5173 \u7cfb \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u7231 \u60c5 \u6d77 \u5357 \u7701 \u4e94 \u6307 \u5c71 \u5e02 \u4f60 \u4f1a \u770b \u4e00 \u4e9b \u7231 \u60c5 \u4e3b \u5bfc \u7684 \u7535 \u5f71 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u7231 \u60c5 \u6d77 \u5357 \u7701 \u4e94 \u6307 \u5c71 \u5e02 \u4f60 \u4f1a \u770b \u4e00 \u4e9b \u7231 \u60c5 \u4e3b \u5bfc \u7684 \u7535 \u5f71 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \uff0c \u8fd9 \u4e2a \u7535 \u5f71 \u8bb2 \u7684 \u4ec0 \u4e48 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u7231 \u60c5 \u6d77 \u5357 \u7701 \u4e94 \u6307 \u5c71 \u5e02 \u4f60 \u4f1a \u770b \u4e00 \u4e9b \u7231 \u60c5 \u4e3b \u5bfc \u7684 \u7535 \u5f71 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \uff0c \u8fd9 \u4e2a \u7535 \u5f71 \u8bb2 \u7684 \u4ec0 \u4e48 \uff1f \u68c0 \u7d22 \u6709 \u70b9 \u5438 \u5f15 \u6211 \uff0c \u60f3 \u770b \u4e00 \u4e0b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u7231 \u60c5 \u6d77 \u5357 \u7701 \u4e94 \u6307 \u5c71 \u5e02 \u4f60 \u4f1a \u770b \u4e00 \u4e9b \u7231 \u60c5 \u4e3b \u5bfc \u7684 \u7535 \u5f71 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \uff0c \u8fd9 \u4e2a \u7535 \u5f71 \u8bb2 \u7684 \u4ec0 \u4e48 \uff1f \u68c0 \u7d22 \u6709 \u70b9 \u5438 \u5f15 \u6211 \uff0c \u60f3 \u770b \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u7231 \u662f \u6210 \u5168 \uff0c \u4e0d \u662f \u81ea \u79c1 \uff0c \u5c0a \u91cd \u5bf9 \u65b9 \u6240 \u505a \u7684 \u51b3 \u5b9a \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u7231 \u60c5 \u6d77 \u5357 \u7701 \u4e94 \u6307 \u5c71 \u5e02 \u4f60 \u4f1a \u770b \u4e00 \u4e9b \u7231 \u60c5 \u4e3b \u5bfc \u7684 \u7535 \u5f71 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \uff0c \u8fd9 \u4e2a \u7535 \u5f71 \u8bb2 \u7684 \u4ec0 \u4e48 \uff1f \u68c0 \u7d22 \u6709 \u70b9 \u5438 \u5f15 \u6211 \uff0c \u60f3 \u770b \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u7231 \u662f \u6210 \u5168 \uff0c \u4e0d \u662f \u81ea \u79c1 \uff0c \u5c0a \u91cd \u5bf9 \u65b9 \u6240 \u505a \u7684 \u51b3 \u5b9a \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u8fd8 \u770b \u8fc7 \u4ec0 \u4e48 \u7231 \u60c5 \u7535 \u5f71 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u7231 \u60c5 \u6d77 \u5357 \u7701 \u4e94 \u6307 \u5c71 \u5e02 \u4f60 \u4f1a \u770b \u4e00 \u4e9b \u7231 \u60c5 \u4e3b \u5bfc \u7684 \u7535 \u5f71 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \uff0c \u8fd9 \u4e2a \u7535 \u5f71 \u8bb2 \u7684 \u4ec0 \u4e48 \uff1f \u68c0 \u7d22 \u6709 \u70b9 \u5438 \u5f15 \u6211 \uff0c \u60f3 \u770b \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u7231 \u662f \u6210 \u5168 \uff0c \u4e0d \u662f \u81ea \u79c1 \uff0c \u5c0a \u91cd \u5bf9 \u65b9 \u6240 \u505a \u7684 \u51b3 \u5b9a \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u8fd8 \u770b \u8fc7 \u4ec0 \u4e48 \u7231 \u60c5 \u7535 \u5f71 \uff1f \u68c0 \u7d22 \u6211 \u77e5 \u9053 \u8fd9 \u4e2a \uff0c \u6211 \u5728 \u6296 \u97f3 \u4e0a \u770b \u8fc7 \u7247 \u6bb5 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u652f \u4ed8 \u624b \u6bb5 \u4fe1 \u7528 \u5361 \u5185 \u8499 \u53e4 \u81ea \u6cbb \u533a \u547c \u4f26 \u8d1d \u5c14 \u5e02 \u6211 \u8fd8 \u6ca1 \u529e \u8fc7 \u4fe1 \u7528 \u5361 \uff0c \u770b \u5230 \u5f88 \u591a \u7f51 \u8d2d \u5e73 \u53f0 \u90fd \u6709 \u4fe1 \u7528 \u5361 \u4f18 \u60e0 \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u4e0d \u8be5 \u529e \u4e00 \u4e2a \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u652f \u4ed8 \u624b \u6bb5 \u4fe1 \u7528 \u5361 \u5185 \u8499 \u53e4 \u81ea \u6cbb \u533a \u547c \u4f26 \u8d1d \u5c14 \u5e02 \u6211 \u8fd8 \u6ca1 \u529e \u8fc7 \u4fe1 \u7528 \u5361 \uff0c \u770b \u5230 \u5f88 \u591a \u7f51 \u8d2d \u5e73 \u53f0 \u90fd \u6709 \u4fe1 \u7528 \u5361 \u4f18 \u60e0 \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u4e0d \u8be5 \u529e \u4e00 \u4e2a \u68c0 \u7d22 \u5443 \uff0c \u6211 \u6210 \u5e74 \u4f46 \u6ca1 \u6709 \u5de5 \u4f5c \u5440 \u3002 \u8fd8 \u5fc5 \u987b \u6709 \u56fa \u5b9a \u6536 \u5165 \u624d \u884c \u4e48 \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u652f \u4ed8 \u624b \u6bb5 \u4fe1 \u7528 \u5361 \u5185 \u8499 \u53e4 \u81ea \u6cbb \u533a \u547c \u4f26 \u8d1d \u5c14 \u5e02 \u6211 \u8fd8 \u6ca1 \u529e \u8fc7 \u4fe1 \u7528 \u5361 \uff0c \u770b \u5230 \u5f88 \u591a \u7f51 \u8d2d \u5e73 \u53f0 \u90fd \u6709 \u4fe1 \u7528 \u5361 \u4f18 \u60e0 \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u4e0d \u8be5 \u529e \u4e00 \u4e2a \u68c0 \u7d22 \u5443 \uff0c \u6211 \u6210 \u5e74 \u4f46 \u6ca1 \u6709 \u5de5 \u4f5c \u5440 \u3002 \u8fd8 \u5fc5 \u987b \u6709 \u56fa \u5b9a \u6536 \u5165 \u624d \u884c \u4e48 \u68c0 \u7d22 \u54e6 \u90a3 \u6211 \u6015 \u662f \u4e0d \u884c \u4e86 \uff0c \u6211 \u540d \u4e0b \u6ca1 \u6709 \u8d44 \u4ea7 \u4e5f \u6ca1 \u6709 \u56fa \u5b9a \u6536 \u5165 \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u652f \u4ed8 \u624b \u6bb5 \u4fe1 \u7528 \u5361 \u5185 \u8499 \u53e4 \u81ea \u6cbb \u533a \u547c \u4f26 \u8d1d \u5c14 \u5e02 \u6211 \u8fd8 \u6ca1 \u529e \u8fc7 \u4fe1 \u7528 \u5361 \uff0c \u770b \u5230 \u5f88 \u591a \u7f51 \u8d2d \u5e73 \u53f0 \u90fd \u6709 \u4fe1 \u7528 \u5361 \u4f18 \u60e0 \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u4e0d \u8be5 \u529e \u4e00 \u4e2a \u68c0 \u7d22 \u5443 \uff0c \u6211 \u6210 \u5e74 \u4f46 \u6ca1 \u6709 \u5de5 \u4f5c \u5440 \u3002 \u8fd8 \u5fc5 \u987b \u6709 \u56fa \u5b9a \u6536 \u5165 \u624d \u884c \u4e48 \u68c0 \u7d22 \u54e6 \u90a3 \u6211 \u6015 \u662f \u4e0d \u884c \u4e86 \uff0c \u6211 \u540d \u4e0b \u6ca1 \u6709 \u8d44 \u4ea7 \u4e5f \u6ca1 \u6709 \u56fa \u5b9a \u6536 \u5165 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u6211 \u770b \u8fd8 \u7ecf \u5e38 \u6709 \u4eba \u88ab \u9a97 \u3002 \u867d \u7136 \u6211 \u611f \u89c9 \u81ea \u5df1 \u4e0d \u662f \u90a3 \u4e48 \u5bb9 \u6613 \u53d7 \u9a97 \uff0c \u4f46 \uff0c \u8c01 \u77e5 \u9053 \u5462 \u54c8 \u54c8 \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u652f \u4ed8 \u624b \u6bb5 \u4fe1 \u7528 \u5361 \u5185 \u8499 \u53e4 \u81ea \u6cbb \u533a \u547c \u4f26 \u8d1d \u5c14 \u5e02 \u6211 \u8fd8 \u6ca1 \u529e \u8fc7 \u4fe1 \u7528 \u5361 \uff0c \u770b \u5230 \u5f88 \u591a \u7f51 \u8d2d \u5e73 \u53f0 \u90fd \u6709 \u4fe1 \u7528 \u5361 \u4f18 \u60e0 \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u4e0d \u8be5 \u529e \u4e00 \u4e2a \u68c0 \u7d22 \u5443 \uff0c \u6211 \u6210 \u5e74 \u4f46 \u6ca1 \u6709 \u5de5 \u4f5c \u5440 \u3002 \u8fd8 \u5fc5 \u987b \u6709 \u56fa \u5b9a \u6536 \u5165 \u624d \u884c \u4e48 \u68c0 \u7d22 \u54e6 \u90a3 \u6211 \u6015 \u662f \u4e0d \u884c \u4e86 \uff0c \u6211 \u540d \u4e0b \u6ca1 \u6709 \u8d44 \u4ea7 \u4e5f \u6ca1 \u6709 \u56fa \u5b9a \u6536 \u5165 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u6211 \u770b \u8fd8 \u7ecf \u5e38 \u6709 \u4eba \u88ab \u9a97 \u3002 \u867d \u7136 \u6211 \u611f \u89c9 \u81ea \u5df1 \u4e0d \u662f \u90a3 \u4e48 \u5bb9 \u6613 \u53d7 \u9a97 \uff0c \u4f46 \uff0c \u8c01 \u77e5 \u9053 \u5462 \u54c8 \u54c8 \u4e0d \u68c0 \u7d22 \u90a3 \u53ef \u592a \u60e8 \u4e86 \u3002 \u4fe1 \u7528 \u5361 \u8fd9 \u73a9 \u610f \u5e94 \u8be5 \u8fd8 \u662f \u6bd4 \u8f83 \u9002 \u5408 \u767d \u9886 \u7cbe \u82f1 \u53bb \u7528 \u5427 \uff0c \u5e73 \u65f6 \u6211 \u4eec \u6b63 \u5e38 \u751f \u6d3b \u7528 \u5904 \u4e0d \u5927 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5355 \u53cd \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u6211 \u6700 \u8fd1 \u5f00 \u59cb \u7ec3 \u4e60 \u6444 \u5f71 \u4e86 \uff0c \u627e \u5230 \u4e86 \u5bb6 \u91cc \u5f88 \u65e9 \u4ee5 \u524d \u4e70 \u7684 \u5355 \u53cd \u3002 \u4f46 \u53ea \u4f1a \u7528 \u81ea \u52a8 \u6a21 \u5f0f \u4e71 \u62cd \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5355 \u53cd \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u6211 \u6700 \u8fd1 \u5f00 \u59cb \u7ec3 \u4e60 \u6444 \u5f71 \u4e86 \uff0c \u627e \u5230 \u4e86 \u5bb6 \u91cc \u5f88 \u65e9 \u4ee5 \u524d \u4e70 \u7684 \u5355 \u53cd \u3002 \u4f46 \u53ea \u4f1a \u7528 \u81ea \u52a8 \u6a21 \u5f0f \u4e71 \u62cd \u3002 \u4e0d \u68c0 \u7d22 tv \u6a21 \u5f0f \uff1f \u6211 \u770b \u6211 \u8fd9 \u4e2a \u597d \u50cf \u4e5f \u6709 \u8bf6 \uff0c \u4f46 \u754c \u9762 \u6211 \u4e0d \u592a \u61c2 \uff0c \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u6a21 \u5f0f \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5355 \u53cd \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u6211 \u6700 \u8fd1 \u5f00 \u59cb \u7ec3 \u4e60 \u6444 \u5f71 \u4e86 \uff0c \u627e \u5230 \u4e86 \u5bb6 \u91cc \u5f88 \u65e9 \u4ee5 \u524d \u4e70 \u7684 \u5355 \u53cd \u3002 \u4f46 \u53ea \u4f1a \u7528 \u81ea \u52a8 \u6a21 \u5f0f \u4e71 \u62cd \u3002 \u4e0d \u68c0 \u7d22 tv \u6a21 \u5f0f \uff1f \u6211 \u770b \u6211 \u8fd9 \u4e2a \u597d \u50cf \u4e5f \u6709 \u8bf6 \uff0c \u4f46 \u754c \u9762 \u6211 \u4e0d \u592a \u61c2 \uff0c \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u6a21 \u5f0f \u554a \uff1f \u68c0 \u7d22 \u660e \u767d \u4e86 \uff0c \u5c31 \u6bd4 \u5982 \u8bf4 \u6211 \u8981 \u66dd \u5149 \uff0c \u5c31 \u53ef \u4ee5 \u8bbe \u7f6e \u957f \u4e00 \u70b9 \u7684 \u5feb \u95e8 \u5bf9 \u5427 \u3002 \u4f46 \u6211 \u4e0d \u592a \u61c2 \u5149 \u5708 \u5230 \u5e95 \u662f \u4e2a \u4ec0 \u4e48 \u4e1c \u897f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5355 \u53cd \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u6211 \u6700 \u8fd1 \u5f00 \u59cb \u7ec3 \u4e60 \u6444 \u5f71 \u4e86 \uff0c \u627e \u5230 \u4e86 \u5bb6 \u91cc \u5f88 \u65e9 \u4ee5 \u524d \u4e70 \u7684 \u5355 \u53cd \u3002 \u4f46 \u53ea \u4f1a \u7528 \u81ea \u52a8 \u6a21 \u5f0f \u4e71 \u62cd \u3002 \u4e0d \u68c0 \u7d22 tv \u6a21 \u5f0f \uff1f \u6211 \u770b \u6211 \u8fd9 \u4e2a \u597d \u50cf \u4e5f \u6709 \u8bf6 \uff0c \u4f46 \u754c \u9762 \u6211 \u4e0d \u592a \u61c2 \uff0c \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u6a21 \u5f0f \u554a \uff1f \u68c0 \u7d22 \u660e \u767d \u4e86 \uff0c \u5c31 \u6bd4 \u5982 \u8bf4 \u6211 \u8981 \u66dd \u5149 \uff0c \u5c31 \u53ef \u4ee5 \u8bbe \u7f6e \u957f \u4e00 \u70b9 \u7684 \u5feb \u95e8 \u5bf9 \u5427 \u3002 \u4f46 \u6211 \u4e0d \u592a \u61c2 \u5149 \u5708 \u5230 \u5e95 \u662f \u4e2a \u4ec0 \u4e48 \u4e1c \u897f \u68c0 \u7d22 \u55ef \uff0c \u5927 \u6982 \u660e \u767d \u4e86 \u3002 \u6211 \u8fd8 \u633a \u60f3 \u5b66 \u98ce \u5149 \u6444 \u5f71 \u7684 \uff0c \u611f \u89c9 tv \u6a21 \u5f0f \u4e4b \u540e \u4f1a \u975e \u5e38 \u6709 \u7528 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u5355 \u53cd \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u6211 \u6700 \u8fd1 \u5f00 \u59cb \u7ec3 \u4e60 \u6444 \u5f71 \u4e86 \uff0c \u627e \u5230 \u4e86 \u5bb6 \u91cc \u5f88 \u65e9 \u4ee5 \u524d \u4e70 \u7684 \u5355 \u53cd \u3002 \u4f46 \u53ea \u4f1a \u7528 \u81ea \u52a8 \u6a21 \u5f0f \u4e71 \u62cd \u3002 \u4e0d \u68c0 \u7d22 tv \u6a21 \u5f0f \uff1f \u6211 \u770b \u6211 \u8fd9 \u4e2a \u597d \u50cf \u4e5f \u6709 \u8bf6 \uff0c \u4f46 \u754c \u9762 \u6211 \u4e0d \u592a \u61c2 \uff0c \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u6a21 \u5f0f \u554a \uff1f \u68c0 \u7d22 \u660e \u767d \u4e86 \uff0c \u5c31 \u6bd4 \u5982 \u8bf4 \u6211 \u8981 \u66dd \u5149 \uff0c \u5c31 \u53ef \u4ee5 \u8bbe \u7f6e \u957f \u4e00 \u70b9 \u7684 \u5feb \u95e8 \u5bf9 \u5427 \u3002 \u4f46 \u6211 \u4e0d \u592a \u61c2 \u5149 \u5708 \u5230 \u5e95 \u662f \u4e2a \u4ec0 \u4e48 \u4e1c \u897f \u68c0 \u7d22 \u55ef \uff0c \u5927 \u6982 \u660e \u767d \u4e86 \u3002 \u6211 \u8fd8 \u633a \u60f3 \u5b66 \u98ce \u5149 \u6444 \u5f71 \u7684 \uff0c \u611f \u89c9 tv \u6a21 \u5f0f \u4e4b \u540e \u4f1a \u975e \u5e38 \u6709 \u7528 \u68c0 \u7d22 \u662f \u7684 \uff01 \u611f \u8c22 \u4f60 \u7684 \u5efa \u8bae \uff0c \u7ed9 \u4e86 \u6211 \u5f88 \u591a \u542f \u53d1 \u3002 \u5468 \u672b \u6211 \u5c31 \u53bb \u90ca \u533a \u8bd5 \u8bd5 \u770b \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u4eba \u8138 \u8bc6 \u522b \u6e56 \u5317 \u7701 \u9102 \u5dde \u5e02 \u73b0 \u5728 \u6211 \u4eec \u5c0f \u533a \u4e5f \u88c5 \u4eba \u8138 \u8bc6 \u522b \u95f8 \u673a \u4e86 \uff0c \u8fd9 \u73a9 \u610f \u771f \u662f \u8d8a \u6765 \u8d8a \u591a \u89c1 \u4e86 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u4eba \u8138 \u8bc6 \u522b \u6e56 \u5317 \u7701 \u9102 \u5dde \u5e02 \u73b0 \u5728 \u6211 \u4eec \u5c0f \u533a \u4e5f \u88c5 \u4eba \u8138 \u8bc6 \u522b \u95f8 \u673a \u4e86 \uff0c \u8fd9 \u73a9 \u610f \u771f \u662f \u8d8a \u6765 \u8d8a \u591a \u89c1 \u4e86 \u554a \u68c0 \u7d22 \u4ec0 \u4e48 \u552e \u5356 \u673a \u8fd8 \u80fd \u5237 \u8138 \u554a \uff1f \u5237 \u8138 \u6263 \u6b3e \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u4eba \u8138 \u8bc6 \u522b \u6e56 \u5317 \u7701 \u9102 \u5dde \u5e02 \u73b0 \u5728 \u6211 \u4eec \u5c0f \u533a \u4e5f \u88c5 \u4eba \u8138 \u8bc6 \u522b \u95f8 \u673a \u4e86 \uff0c \u8fd9 \u73a9 \u610f \u771f \u662f \u8d8a \u6765 \u8d8a \u591a \u89c1 \u4e86 \u554a \u68c0 \u7d22 \u4ec0 \u4e48 \u552e \u5356 \u673a \u8fd8 \u80fd \u5237 \u8138 \u554a \uff1f \u5237 \u8138 \u6263 \u6b3e \u5417 \u4e0d \u68c0 \u7d22 \u5b83 \u600e \u4e48 \u5c31 \u80fd \u4fdd \u8bc1 \u81ea \u5df1 \u80fd \u51c6 \u786e \u5224 \u65ad \u5462 \uff0c \u4e0d \u4f1a \u5237 \u6210 \u522b \u4eba \u5417 \uff1f \u8fd9 \u662f \u600e \u4e48 \u5b9e \u73b0 \u7684 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u4eba \u8138 \u8bc6 \u522b \u6e56 \u5317 \u7701 \u9102 \u5dde \u5e02 \u73b0 \u5728 \u6211 \u4eec \u5c0f \u533a \u4e5f \u88c5 \u4eba \u8138 \u8bc6 \u522b \u95f8 \u673a \u4e86 \uff0c \u8fd9 \u73a9 \u610f \u771f \u662f \u8d8a \u6765 \u8d8a \u591a \u89c1 \u4e86 \u554a \u68c0 \u7d22 \u4ec0 \u4e48 \u552e \u5356 \u673a \u8fd8 \u80fd \u5237 \u8138 \u554a \uff1f \u5237 \u8138 \u6263 \u6b3e \u5417 \u4e0d \u68c0 \u7d22 \u5b83 \u600e \u4e48 \u5c31 \u80fd \u4fdd \u8bc1 \u81ea \u5df1 \u80fd \u51c6 \u786e \u5224 \u65ad \u5462 \uff0c \u4e0d \u4f1a \u5237 \u6210 \u522b \u4eba \u5417 \uff1f \u8fd9 \u662f \u600e \u4e48 \u5b9e \u73b0 \u7684 \u554a \u68c0 \u7d22 \u8fd9 \u4e9b \u6211 \u8fd8 \u771f \u4e0d \u592a \u4e86 \u89e3 \u3002 \u8fd9 \u4e2a \u7b97 \u662f \u4eba \u5de5 \u667a \u80fd \u5427 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u4eba \u8138 \u8bc6 \u522b \u6e56 \u5317 \u7701 \u9102 \u5dde \u5e02 \u73b0 \u5728 \u6211 \u4eec \u5c0f \u533a \u4e5f \u88c5 \u4eba \u8138 \u8bc6 \u522b \u95f8 \u673a \u4e86 \uff0c \u8fd9 \u73a9 \u610f \u771f \u662f \u8d8a \u6765 \u8d8a \u591a \u89c1 \u4e86 \u554a \u68c0 \u7d22 \u4ec0 \u4e48 \u552e \u5356 \u673a \u8fd8 \u80fd \u5237 \u8138 \u554a \uff1f \u5237 \u8138 \u6263 \u6b3e \u5417 \u4e0d \u68c0 \u7d22 \u5b83 \u600e \u4e48 \u5c31 \u80fd \u4fdd \u8bc1 \u81ea \u5df1 \u80fd \u51c6 \u786e \u5224 \u65ad \u5462 \uff0c \u4e0d \u4f1a \u5237 \u6210 \u522b \u4eba \u5417 \uff1f \u8fd9 \u662f \u600e \u4e48 \u5b9e \u73b0 \u7684 \u554a \u68c0 \u7d22 \u8fd9 \u4e9b \u6211 \u8fd8 \u771f \u4e0d \u592a \u4e86 \u89e3 \u3002 \u8fd9 \u4e2a \u7b97 \u662f \u4eba \u5de5 \u667a \u80fd \u5427 \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u597d \u9ad8 \u7aef \uff0c \u4e0d \u77e5 \u9053 \u6df1 \u5ea6 \u5b66 \u4e60 \u6df1 \u5ea6 \u5728 \u54ea \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u4eba \u8138 \u8bc6 \u522b \u6e56 \u5317 \u7701 \u9102 \u5dde \u5e02 \u73b0 \u5728 \u6211 \u4eec \u5c0f \u533a \u4e5f \u88c5 \u4eba \u8138 \u8bc6 \u522b \u95f8 \u673a \u4e86 \uff0c \u8fd9 \u73a9 \u610f \u771f \u662f \u8d8a \u6765 \u8d8a \u591a \u89c1 \u4e86 \u554a \u68c0 \u7d22 \u4ec0 \u4e48 \u552e \u5356 \u673a \u8fd8 \u80fd \u5237 \u8138 \u554a \uff1f \u5237 \u8138 \u6263 \u6b3e \u5417 \u4e0d \u68c0 \u7d22 \u5b83 \u600e \u4e48 \u5c31 \u80fd \u4fdd \u8bc1 \u81ea \u5df1 \u80fd \u51c6 \u786e \u5224 \u65ad \u5462 \uff0c \u4e0d \u4f1a \u5237 \u6210 \u522b \u4eba \u5417 \uff1f \u8fd9 \u662f \u600e \u4e48 \u5b9e \u73b0 \u7684 \u554a \u68c0 \u7d22 \u8fd9 \u4e9b \u6211 \u8fd8 \u771f \u4e0d \u592a \u4e86 \u89e3 \u3002 \u8fd9 \u4e2a \u7b97 \u662f \u4eba \u5de5 \u667a \u80fd \u5427 \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u597d \u9ad8 \u7aef \uff0c \u4e0d \u77e5 \u9053 \u6df1 \u5ea6 \u5b66 \u4e60 \u6df1 \u5ea6 \u5728 \u54ea \u68c0 \u7d22 \u539f \u6765 \u5982 \u6b64 \uff0c \u597d \u50cf \u660e \u767d \u4e86 \u3002 \u90a3 \u8fd9 \u4e2a \u6280 \u672f \u771f \u7684 \u975e \u5e38 \u6709 \u4ef7 \u503c \u5462 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u5218 \u6b22 \u8fbd \u5b81 \u7701 \u629a \u987a \u5e02 \u6700 \u8fd1 \u5728 \u770b \u7504 \u5b1b \u4f20 \uff0c \u7247 \u5934 \u66f2 \u771f \u597d \u542c \u554a \uff0c \u53eb \u4ec0 \u4e48 \u6765 \u7740 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u5218 \u6b22 \u8fbd \u5b81 \u7701 \u629a \u987a \u5e02 \u6700 \u8fd1 \u5728 \u770b \u7504 \u5b1b \u4f20 \uff0c \u7247 \u5934 \u66f2 \u771f \u597d \u542c \u554a \uff0c \u53eb \u4ec0 \u4e48 \u6765 \u7740 \uff1f \u68c0 \u7d22 \u4e0d \u6127 \u662f \u5218 \u6b22 \u8001 \u5e08 \uff0c \u8fd9 \u9996 \u6b4c \u7684 \u8bcd \u66f2 \u90fd \u5f88 \u5389 \u5bb3 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u5218 \u6b22 \u8fbd \u5b81 \u7701 \u629a \u987a \u5e02 \u6700 \u8fd1 \u5728 \u770b \u7504 \u5b1b \u4f20 \uff0c \u7247 \u5934 \u66f2 \u771f \u597d \u542c \u554a \uff0c \u53eb \u4ec0 \u4e48 \u6765 \u7740 \uff1f \u68c0 \u7d22 \u4e0d \u6127 \u662f \u5218 \u6b22 \u8001 \u5e08 \uff0c \u8fd9 \u9996 \u6b4c \u7684 \u8bcd \u66f2 \u90fd \u5f88 \u5389 \u5bb3 \u3002 \u68c0 \u7d22 \u6211 \u7ec8 \u4e8e \u7406 \u89e3 \u4e3a \u4ec0 \u4e48 \u6211 \u7238 \u5988 \u4e00 \u76f4 \u90fd \u5f88 \u559c \u6b22 \u5218 \u6b22 \u4e86 \uff0c \u592a \u5389 \u5bb3 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u5218 \u6b22 \u8fbd \u5b81 \u7701 \u629a \u987a \u5e02 \u6700 \u8fd1 \u5728 \u770b \u7504 \u5b1b \u4f20 \uff0c \u7247 \u5934 \u66f2 \u771f \u597d \u542c \u554a \uff0c \u53eb \u4ec0 \u4e48 \u6765 \u7740 \uff1f \u68c0 \u7d22 \u4e0d \u6127 \u662f \u5218 \u6b22 \u8001 \u5e08 \uff0c \u8fd9 \u9996 \u6b4c \u7684 \u8bcd \u66f2 \u90fd \u5f88 \u5389 \u5bb3 \u3002 \u68c0 \u7d22 \u6211 \u7ec8 \u4e8e \u7406 \u89e3 \u4e3a \u4ec0 \u4e48 \u6211 \u7238 \u5988 \u4e00 \u76f4 \u90fd \u5f88 \u559c \u6b22 \u5218 \u6b22 \u4e86 \uff0c \u592a \u5389 \u5bb3 \u4e86 \u3002 \u68c0 \u7d22 \u679c \u7136 \u4e0d \u7b80 \u5355 \uff0c \u5f88 \u4f69 \u670d \u4ed6 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u5218 \u6b22 \u8fbd \u5b81 \u7701 \u629a \u987a \u5e02 \u6700 \u8fd1 \u5728 \u770b \u7504 \u5b1b \u4f20 \uff0c \u7247 \u5934 \u66f2 \u771f \u597d \u542c \u554a \uff0c \u53eb \u4ec0 \u4e48 \u6765 \u7740 \uff1f \u68c0 \u7d22 \u4e0d \u6127 \u662f \u5218 \u6b22 \u8001 \u5e08 \uff0c \u8fd9 \u9996 \u6b4c \u7684 \u8bcd \u66