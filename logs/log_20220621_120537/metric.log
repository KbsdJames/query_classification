ff0c \u6211 \u5f53 \u65f6 \u54c7 \u7684 \u4e00 \u58f0 \uff0c \u5c31 \u54ed \u51fa \u6765 \u4e86 \uff01 \uff01 \u68c0 \u7d22 \u662f \u7684 \uff0c \u6211 \u4e4b \u524d \u8fd8 \u770b \u8fc7 \u4e00 \u4e2a \u89c6 \u9891 \uff0c \u7b2c \u4e00 \u5c4a \u6625 \u8282 \u8054 \u6b22 \u665a \u4f1a \u5c31 \u6709 \u8001 \u820d \u5148 \u751f ~ \u68c0 \u7d22 \u5199 \u4f5c \u534a \u5929 \u771f \u5389 \u5bb3 \uff0c \u6211 \u6700 \u591a \u5c31 \u4e24 \u4e2a \u5c0f \u65f6 \uff0c \u800c \u4e14 \u5f88 \u591a \u8fd8 \u662f \u6284 \u7684 \uff0c \u54ce ~ \u68c0 \u7d22 \u53ef \u60dc \u540e \u6765 \u6587 \u9769 \u65f6 \u671f \u60e8 \u906d \u6bd2 \u624b ~ \u68c0 \u7d22 \u54ce \uff0c \u4e16 \u4e8b \u65e0 \u5e38 \u554a \uff0c \u8fd8 \u662f \u8981 \u73cd \u60dc \u751f \u547d \uff0c \u6d3b \u5728 \u5f53 \u4e0b \uff0c \u8c01 \u77e5 \u9053 \u4f1a \u4e0d \u4f1a \u51fa \u4ec0 \u4e48 \u610f \u5916 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u6d77 \u5357 \u7701 \u5b9a \u5b89 \u53bf \u51ac \u5929 \u4e0d \u60f3 \u5728 \u5317 \u65b9 \u751f \u6d3b \uff0c \u592a \u51b7 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u6d77 \u5357 \u7701 \u5b9a \u5b89 \u53bf \u51ac \u5929 \u4e0d \u60f3 \u5728 \u5317 \u65b9 \u751f \u6d3b \uff0c \u592a \u51b7 \u4e86 \u4e0d \u68c0 \u7d22 \u662f \u5440 \uff0c \u51ac \u5929 \u80fd \u53bb \u4e09 \u4e9a \u5ea6 \u5047 \u5c31 \u597d \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u6d77 \u5357 \u7701 \u5b9a \u5b89 \u53bf \u51ac \u5929 \u4e0d \u60f3 \u5728 \u5317 \u65b9 \u751f \u6d3b \uff0c \u592a \u51b7 \u4e86 \u4e0d \u68c0 \u7d22 \u662f \u5440 \uff0c \u51ac \u5929 \u80fd \u53bb \u4e09 \u4e9a \u5ea6 \u5047 \u5c31 \u597d \u4e86 \u68c0 \u7d22 \u65e5 \u6708 \u6e7e \u6d77 \u6e7e \u662f \u4ec0 \u4e48 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u6d77 \u5357 \u7701 \u5b9a \u5b89 \u53bf \u51ac \u5929 \u4e0d \u60f3 \u5728 \u5317 \u65b9 \u751f \u6d3b \uff0c \u592a \u51b7 \u4e86 \u4e0d \u68c0 \u7d22 \u662f \u5440 \uff0c \u51ac \u5929 \u80fd \u53bb \u4e09 \u4e9a \u5ea6 \u5047 \u5c31 \u597d \u4e86 \u68c0 \u7d22 \u65e5 \u6708 \u6e7e \u6d77 \u6e7e \u662f \u4ec0 \u4e48 \u5440 \uff1f \u68c0 \u7d22 \u540d \u5b57 \u542c \u7740 \u5c31 \u5f88 \u597d \u542c \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u6d77 \u5357 \u7701 \u5b9a \u5b89 \u53bf \u51ac \u5929 \u4e0d \u60f3 \u5728 \u5317 \u65b9 \u751f \u6d3b \uff0c \u592a \u51b7 \u4e86 \u4e0d \u68c0 \u7d22 \u662f \u5440 \uff0c \u51ac \u5929 \u80fd \u53bb \u4e09 \u4e9a \u5ea6 \u5047 \u5c31 \u597d \u4e86 \u68c0 \u7d22 \u65e5 \u6708 \u6e7e \u6d77 \u6e7e \u662f \u4ec0 \u4e48 \u5440 \uff1f \u68c0 \u7d22 \u540d \u5b57 \u542c \u7740 \u5c31 \u5f88 \u597d \u542c \u4e0d \u68c0 \u7d22 \u597d \u7684 \u5440 \uff0c \u771f \u662f \u5e0c \u671b \u80fd \u591a \u70b9 \u5047 \u671f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5929 \u874e \u5ea7 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u770b \u661f \u5ea7 \uff0c \u611f \u89c9 \u8bf4 \u7684 \u597d \u51c6 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5929 \u874e \u5ea7 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u770b \u661f \u5ea7 \uff0c \u611f \u89c9 \u8bf4 \u7684 \u597d \u51c6 \u554a \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u6211 \u662f \u5929 \u874e \u5ea7 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5929 \u874e \u5ea7 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u770b \u661f \u5ea7 \uff0c \u611f \u89c9 \u8bf4 \u7684 \u597d \u51c6 \u554a \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u6211 \u662f \u5929 \u874e \u5ea7 \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u611f \u89c9 \u8bf4 \u7684 \u8fd8 \u633a \u51c6 \u7684 \u3002 \u592a \u795e \u5947 \u4e86 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5929 \u874e \u5ea7 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u770b \u661f \u5ea7 \uff0c \u611f \u89c9 \u8bf4 \u7684 \u597d \u51c6 \u554a \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u6211 \u662f \u5929 \u874e \u5ea7 \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u611f \u89c9 \u8bf4 \u7684 \u8fd8 \u633a \u51c6 \u7684 \u3002 \u592a \u795e \u5947 \u4e86 \uff01 \u4e0d \u68c0 \u7d22 \u800c \u4e14 \u8bf4 \u7684 \u5929 \u874e \u5ea7 \u7684 \u7f3a \u70b9 \u4e5f \u5f88 \u51c6 \uff0c \u592a \u624e \u5fc3 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5929 \u874e \u5ea7 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u770b \u661f \u5ea7 \uff0c \u611f \u89c9 \u8bf4 \u7684 \u597d \u51c6 \u554a \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u6211 \u662f \u5929 \u874e \u5ea7 \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u611f \u89c9 \u8bf4 \u7684 \u8fd8 \u633a \u51c6 \u7684 \u3002 \u592a \u795e \u5947 \u4e86 \uff01 \u4e0d \u68c0 \u7d22 \u800c \u4e14 \u8bf4 \u7684 \u5929 \u874e \u5ea7 \u7684 \u7f3a \u70b9 \u4e5f \u5f88 \u51c6 \uff0c \u592a \u624e \u5fc3 \u4e86 \u68c0 \u7d22 \u6ca1 \u9519 \uff0c \u6211 \u5c31 \u662f \u8fd9 \u6837 \u7684 \u4eba \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5929 \u874e \u5ea7 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6211 \u6700 \u8fd1 \u5728 \u770b \u661f \u5ea7 \uff0c \u611f \u89c9 \u8bf4 \u7684 \u597d \u51c6 \u554a \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u6211 \u662f \u5929 \u874e \u5ea7 \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u611f \u89c9 \u8bf4 \u7684 \u8fd8 \u633a \u51c6 \u7684 \u3002 \u592a \u795e \u5947 \u4e86 \uff01 \u4e0d \u68c0 \u7d22 \u800c \u4e14 \u8bf4 \u7684 \u5929 \u874e \u5ea7 \u7684 \u7f3a \u70b9 \u4e5f \u5f88 \u51c6 \uff0c \u592a \u624e \u5fc3 \u4e86 \u68c0 \u7d22 \u6ca1 \u9519 \uff0c \u6211 \u5c31 \u662f \u8fd9 \u6837 \u7684 \u4eba \u68c0 \u7d22 \u54c7 \uff0c \u90a3 \u6211 \u5f97 \u770b \u770b \u5468 \u56f4 \u8c01 \u662f \u53cc \u9c7c \u5ea7 \u5566 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5ddd \u83dc \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u5ddd \u83dc \u5417 \uff0c \u5ddd \u83dc \u600e \u4e48 \u6837 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5ddd \u83dc \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u5ddd \u83dc \u5417 \uff0c \u5ddd \u83dc \u600e \u4e48 \u6837 \u554a \u68c0 \u7d22 \u6211 \u6700 \u8fd1 \u60f3 \u5b66 \u505a \u5ddd \u83dc \uff0c \u4f60 \u53ef \u4ee5 \u6559 \u6559 \u6211 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5ddd \u83dc \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u5ddd \u83dc \u5417 \uff0c \u5ddd \u83dc \u600e \u4e48 \u6837 \u554a \u68c0 \u7d22 \u6211 \u6700 \u8fd1 \u60f3 \u5b66 \u505a \u5ddd \u83dc \uff0c \u4f60 \u53ef \u4ee5 \u6559 \u6559 \u6211 \u5417 \u68c0 \u7d22 \u6709 \u6ca1 \u6709 \u597d \u5403 \u7684 \u5ddd \u83dc \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5ddd \u83dc \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u5ddd \u83dc \u5417 \uff0c \u5ddd \u83dc \u600e \u4e48 \u6837 \u554a \u68c0 \u7d22 \u6211 \u6700 \u8fd1 \u60f3 \u5b66 \u505a \u5ddd \u83dc \uff0c \u4f60 \u53ef \u4ee5 \u6559 \u6559 \u6211 \u5417 \u68c0 \u7d22 \u6709 \u6ca1 \u6709 \u597d \u5403 \u7684 \u5ddd \u83dc \uff1f \u68c0 \u7d22 \u597d \u7684 \uff0c \u4eca \u5929 \u665a \u996d \u6211 \u53bb \u5c1d \u5c1d \u8fd9 \u4e24 \u4e2a \u83dc \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5ddd \u83dc \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u5ddd \u83dc \u5417 \uff0c \u5ddd \u83dc \u600e \u4e48 \u6837 \u554a \u68c0 \u7d22 \u6211 \u6700 \u8fd1 \u60f3 \u5b66 \u505a \u5ddd \u83dc \uff0c \u4f60 \u53ef \u4ee5 \u6559 \u6559 \u6211 \u5417 \u68c0 \u7d22 \u6709 \u6ca1 \u6709 \u597d \u5403 \u7684 \u5ddd \u83dc \uff1f \u68c0 \u7d22 \u597d \u7684 \uff0c \u4eca \u5929 \u665a \u996d \u6211 \u53bb \u5c1d \u5c1d \u8fd9 \u4e24 \u4e2a \u83dc \u4e0d \u68c0 \u7d22 \u90a3 \u8fd9 \u4e24 \u9053 \u83dc \u53ef \u80fd \u5c31 \u8981 \u6210 \u4e3a \u6211 \u7684 \u6bcf \u65e5 \u5fc5 \u5907 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u4eba \u5de5 \u667a \u80fd \u6280 \u672f \u4e3b \u8981 \u54ea \u4e9b \u9886 \u57df \u5e94 \u7528 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u4eba \u5de5 \u667a \u80fd \u6280 \u672f \u4e3b \u8981 \u54ea \u4e9b \u9886 \u57df \u5e94 \u7528 \u554a \uff1f \u68c0 \u7d22 \u8fd9 \u6837 \u770b \uff0c \u6211 \u6253 \u7b97 \u5b66 \u4e60 \u8ba1 \u7b97 \u673a \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u4eba \u5de5 \u667a \u80fd \u6280 \u672f \u4e3b \u8981 \u54ea \u4e9b \u9886 \u57df \u5e94 \u7528 \u554a \uff1f \u68c0 \u7d22 \u8fd9 \u6837 \u770b \uff0c \u6211 \u6253 \u7b97 \u5b66 \u4e60 \u8ba1 \u7b97 \u673a \uff01 \u68c0 \u7d22 \u6e05 \u534e \u662f \u6211 \u7684 \u68a6 \u4e2d \u60c5 \u4eba \u545c \u545c \u545c \u545c \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u4eba \u5de5 \u667a \u80fd \u6280 \u672f \u4e3b \u8981 \u54ea \u4e9b \u9886 \u57df \u5e94 \u7528 \u554a \uff1f \u68c0 \u7d22 \u8fd9 \u6837 \u770b \uff0c \u6211 \u6253 \u7b97 \u5b66 \u4e60 \u8ba1 \u7b97 \u673a \uff01 \u68c0 \u7d22 \u6e05 \u534e \u662f \u6211 \u7684 \u68a6 \u4e2d \u60c5 \u4eba \u545c \u545c \u545c \u545c \u4e0d \u68c0 \u7d22 \u4eba \u5de5 \u667a \u80fd \u6280 \u672f \u592a \u6709 \u8da3 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u4eba \u5de5 \u667a \u80fd \u6280 \u672f \u4e3b \u8981 \u54ea \u4e9b \u9886 \u57df \u5e94 \u7528 \u554a \uff1f \u68c0 \u7d22 \u8fd9 \u6837 \u770b \uff0c \u6211 \u6253 \u7b97 \u5b66 \u4e60 \u8ba1 \u7b97 \u673a \uff01 \u68c0 \u7d22 \u6e05 \u534e \u662f \u6211 \u7684 \u68a6 \u4e2d \u60c5 \u4eba \u545c \u545c \u545c \u545c \u4e0d \u68c0 \u7d22 \u4eba \u5de5 \u667a \u80fd \u6280 \u672f \u592a \u6709 \u8da3 \u4e86 \u4e0d \u68c0 \u7d22 \u5bf9 \u5440 \uff0c \u771f \u7684 \u592a \u68d2 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u623f \u5730 \u4ea7 \u4e70 \u623f \u6d59 \u6c5f \u7701 \u821f \u5c71 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u7406 \u8d22 \u7684 \u597d \u9014 \u5f84 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u623f \u5730 \u4ea7 \u4e70 \u623f \u6d59 \u6c5f \u7701 \u821f \u5c71 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u7406 \u8d22 \u7684 \u597d \u9014 \u5f84 \u5440 \uff1f \u68c0 \u7d22 \u6211 \u611f \u89c9 \u90fd \u4e0d \u662f \u5f88 \u80fd \u8dd1 \u8d62 \u901a \u80c0 \u7684 \uff0c \u4e0d \u5982 \u4e70 \u623f \u5427 \uff0c \u623f \u4ef7 \u8fde \u5e74 \u4e0a \u6da8 \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u623f \u5730 \u4ea7 \u4e70 \u623f \u6d59 \u6c5f \u7701 \u821f \u5c71 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u7406 \u8d22 \u7684 \u597d \u9014 \u5f84 \u5440 \uff1f \u68c0 \u7d22 \u6211 \u611f \u89c9 \u90fd \u4e0d \u662f \u5f88 \u80fd \u8dd1 \u8d62 \u901a \u80c0 \u7684 \uff0c \u4e0d \u5982 \u4e70 \u623f \u5427 \uff0c \u623f \u4ef7 \u8fde \u5e74 \u4e0a \u6da8 \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u54c8 \u60f3 \u5f97 \u7f8e \uff0c \u4e70 \u4e86 \u6211 \u4e5f \u79df \u51fa \u53bb \uff5e\uff5e \u8fd8 \u623f \u8d37 \u8fd8 \u4e0d \u5c11 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u623f \u5730 \u4ea7 \u4e70 \u623f \u6d59 \u6c5f \u7701 \u821f \u5c71 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u7406 \u8d22 \u7684 \u597d \u9014 \u5f84 \u5440 \uff1f \u68c0 \u7d22 \u6211 \u611f \u89c9 \u90fd \u4e0d \u662f \u5f88 \u80fd \u8dd1 \u8d62 \u901a \u80c0 \u7684 \uff0c \u4e0d \u5982 \u4e70 \u623f \u5427 \uff0c \u623f \u4ef7 \u8fde \u5e74 \u4e0a \u6da8 \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u54c8 \u60f3 \u5f97 \u7f8e \uff0c \u4e70 \u4e86 \u6211 \u4e5f \u79df \u51fa \u53bb \uff5e\uff5e \u8fd8 \u623f \u8d37 \u8fd8 \u4e0d \u5c11 \u5462 \u68c0 \u7d22 \u662f \u7684 \uff0c \u770b \u6765 \u4f60 \u4e5f \u77e5 \u9053 \u554a \uff0c \u5730 \u6bb5 \u7684 \u8bdd \u8981 \u53bb \u770b \u4e86 \u624d \u77e5 \u9053 \uff5e \u770b \u5730 \u6bb5 \u4e3b \u8981 \u770b \u4ec0 \u4e48 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d22 \u7ecf \u623f \u5730 \u4ea7 \u4e70 \u623f \u6d59 \u6c5f \u7701 \u821f \u5c71 \u5e02 \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u7406 \u8d22 \u7684 \u597d \u9014 \u5f84 \u5440 \uff1f \u68c0 \u7d22 \u6211 \u611f \u89c9 \u90fd \u4e0d \u662f \u5f88 \u80fd \u8dd1 \u8d62 \u901a \u80c0 \u7684 \uff0c \u4e0d \u5982 \u4e70 \u623f \u5427 \uff0c \u623f \u4ef7 \u8fde \u5e74 \u4e0a \u6da8 \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u54c8 \u60f3 \u5f97 \u7f8e \uff0c \u4e70 \u4e86 \u6211 \u4e5f \u79df \u51fa \u53bb \uff5e\uff5e \u8fd8 \u623f \u8d37 \u8fd8 \u4e0d \u5c11 \u5462 \u68c0 \u7d22 \u662f \u7684 \uff0c \u770b \u6765 \u4f60 \u4e5f \u77e5 \u9053 \u554a \uff0c \u5730 \u6bb5 \u7684 \u8bdd \u8981 \u53bb \u770b \u4e86 \u624d \u77e5 \u9053 \uff5e \u770b \u5730 \u6bb5 \u4e3b \u8981 \u770b \u4ec0 \u4e48 \u5440 \uff1f \u68c0 \u7d22 \u539f \u6765 \u662f \u8fd9 \u6837 \uff0c \u8fd9 \u79cd \u5730 \u6bb5 \u597d \u7684 \u6ea2 \u4ef7 \u6709 \u591a \u5c11 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u5c71 \u897f \u7701 \u4e34 \u6c7e \u5e02 \u4e1c \u5317 \u7684 \u51b0 \u9493 \u53c8 \u5f00 \u59cb \u4e86 \uff0c \u6211 \u597d \u60f3 \u770b \u554a \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u5c71 \u897f \u7701 \u4e34 \u6c7e \u5e02 \u4e1c \u5317 \u7684 \u51b0 \u9493 \u53c8 \u5f00 \u59cb \u4e86 \uff0c \u6211 \u597d \u60f3 \u770b \u554a \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u4e5f \u662f \u770b \u7740 \u7535 \u89c6 \u91cc \u7684 \u7a81 \u7136 \u60f3 \u53bb \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u5c71 \u897f \u7701 \u4e34 \u6c7e \u5e02 \u4e1c \u5317 \u7684 \u51b0 \u9493 \u53c8 \u5f00 \u59cb \u4e86 \uff0c \u6211 \u597d \u60f3 \u770b \u554a \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u4e5f \u662f \u770b \u7740 \u7535 \u89c6 \u91cc \u7684 \u7a81 \u7136 \u60f3 \u53bb \u7684 \u4e0d \u68c0 \u7d22 \u4e3a \u4ec0 \u4e48 \u51ac \u5929 \u80fd \u9493 \u90a3 \u4e48 \u591a \u9c7c \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u5c71 \u897f \u7701 \u4e34 \u6c7e \u5e02 \u4e1c \u5317 \u7684 \u51b0 \u9493 \u53c8 \u5f00 \u59cb \u4e86 \uff0c \u6211 \u597d \u60f3 \u770b \u554a \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u4e5f \u662f \u770b \u7740 \u7535 \u89c6 \u91cc \u7684 \u7a81 \u7136 \u60f3 \u53bb \u7684 \u4e0d \u68c0 \u7d22 \u4e3a \u4ec0 \u4e48 \u51ac \u5929 \u80fd \u9493 \u90a3 \u4e48 \u591a \u9c7c \u5440 \u68c0 \u7d22 \u55ef \u55ef \uff0c \u539f \u6765 \u5982 \u6b64 \u3002 \u800c \u4e14 \u51ac \u5929 \u7684 \u9c7c \u5f88 \u9c9c \u7f8e \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u9493 \u9c7c \u5c71 \u897f \u7701 \u4e34 \u6c7e \u5e02 \u4e1c \u5317 \u7684 \u51b0 \u9493 \u53c8 \u5f00 \u59cb \u4e86 \uff0c \u6211 \u597d \u60f3 \u770b \u554a \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u4e5f \u662f \u770b \u7740 \u7535 \u89c6 \u91cc \u7684 \u7a81 \u7136 \u60f3 \u53bb \u7684 \u4e0d \u68c0 \u7d22 \u4e3a \u4ec0 \u4e48 \u51ac \u5929 \u80fd \u9493 \u90a3 \u4e48 \u591a \u9c7c \u5440 \u68c0 \u7d22 \u55ef \u55ef \uff0c \u539f \u6765 \u5982 \u6b64 \u3002 \u800c \u4e14 \u51ac \u5929 \u7684 \u9c7c \u5f88 \u9c9c \u7f8e \u68c0 \u7d22 \u6211 \u7acb \u523b \u53bb \u770b \u673a \u7968 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u9ebb \u5a46 \u8c46 \u8150 \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u4f60 \u77e5 \u9053 \u5728 \u65e5 \u672c \u6700 \u53d7 \u6b22 \u8fce \u7684 \u4e2d \u56fd \u83dc \u662f \u4ec0 \u4e48 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u9ebb \u5a46 \u8c46 \u8150 \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u4f60 \u77e5 \u9053 \u5728 \u65e5 \u672c \u6700 \u53d7 \u6b22 \u8fce \u7684 \u4e2d \u56fd \u83dc \u662f \u4ec0 \u4e48 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u771f \u6ca1 \u60f3 \u5230 \u662f \u9ebb \u5a46 \u8c46 \u8150 \uff0c \u53ef \u662f \u65e5 \u672c \u4eba \u4e0d \u662f \u5f88 \u80fd \u5403 \u8fa3 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u9ebb \u5a46 \u8c46 \u8150 \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u4f60 \u77e5 \u9053 \u5728 \u65e5 \u672c \u6700 \u53d7 \u6b22 \u8fce \u7684 \u4e2d \u56fd \u83dc \u662f \u4ec0 \u4e48 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u771f \u6ca1 \u60f3 \u5230 \u662f \u9ebb \u5a46 \u8c46 \u8150 \uff0c \u53ef \u662f \u65e5 \u672c \u4eba \u4e0d \u662f \u5f88 \u80fd \u5403 \u8fa3 \u5427 \u3002 \u68c0 \u7d22 \u54e6 \uff0c \u539f \u6765 \u5982 \u6b64 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u9ebb \u5a46 \u8c46 \u8150 \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u4f60 \u77e5 \u9053 \u5728 \u65e5 \u672c \u6700 \u53d7 \u6b22 \u8fce \u7684 \u4e2d \u56fd \u83dc \u662f \u4ec0 \u4e48 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u771f \u6ca1 \u60f3 \u5230 \u662f \u9ebb \u5a46 \u8c46 \u8150 \uff0c \u53ef \u662f \u65e5 \u672c \u4eba \u4e0d \u662f \u5f88 \u80fd \u5403 \u8fa3 \u5427 \u3002 \u68c0 \u7d22 \u54e6 \uff0c \u539f \u6765 \u5982 \u6b64 \u3002 \u68c0 \u7d22 \u539f \u6765 \u8fd8 \u662f \u6709 \u5386 \u53f2 \u6e0a \u6e90 \u7684 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u9ebb \u5a46 \u8c46 \u8150 \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u4f60 \u77e5 \u9053 \u5728 \u65e5 \u672c \u6700 \u53d7 \u6b22 \u8fce \u7684 \u4e2d \u56fd \u83dc \u662f \u4ec0 \u4e48 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u771f \u6ca1 \u60f3 \u5230 \u662f \u9ebb \u5a46 \u8c46 \u8150 \uff0c \u53ef \u662f \u65e5 \u672c \u4eba \u4e0d \u662f \u5f88 \u80fd \u5403 \u8fa3 \u5427 \u3002 \u68c0 \u7d22 \u54e6 \uff0c \u539f \u6765 \u5982 \u6b64 \u3002 \u68c0 \u7d22 \u539f \u6765 \u8fd8 \u662f \u6709 \u5386 \u53f2 \u6e0a \u6e90 \u7684 \u554a \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u5f88 \u559c \u6b22 \u5403 \u9ebb \u5a46 \u8c46 \u8150 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u533a \u5757 \u94fe \u6e56 \u5357 \u7701 \u5cb3 \u9633 \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u533a \u5757 \u94fe \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u533a \u5757 \u94fe \u6e56 \u5357 \u7701 \u5cb3 \u9633 \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u533a \u5757 \u94fe \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u7684 \u633a \u591a \u7684 \u561b \uff0c \u5389 \u5bb3 \u4e86 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u533a \u5757 \u94fe \u6e56 \u5357 \u7701 \u5cb3 \u9633 \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u533a \u5757 \u94fe \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u7684 \u633a \u591a \u7684 \u561b \uff0c \u5389 \u5bb3 \u4e86 \uff01 \u68c0 \u7d22 \u90a3 \u633a \u65e9 \u7684 \u5440 \uff0c \u90a3 \u4e2a \u65f6 \u5019 \u6211 \u8fd8 \u5728 \u8bfb \u5c0f \u5b66 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u533a \u5757 \u94fe \u6e56 \u5357 \u7701 \u5cb3 \u9633 \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u533a \u5757 \u94fe \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u7684 \u633a \u591a \u7684 \u561b \uff0c \u5389 \u5bb3 \u4e86 \uff01 \u68c0 \u7d22 \u90a3 \u633a \u65e9 \u7684 \u5440 \uff0c \u90a3 \u4e2a \u65f6 \u5019 \u6211 \u8fd8 \u5728 \u8bfb \u5c0f \u5b66 \u5462 \u4e0d \u68c0 \u7d22 \u4e0d \u77e5 \u9053 \u8fd9 \u4e2a \u6280 \u672f \u672a \u6765 \u4f1a \u600e \u4e48 \u53d1 \u5c55 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u533a \u5757 \u94fe \u6e56 \u5357 \u7701 \u5cb3 \u9633 \u5e02 \u4f60 \u542c \u8bf4 \u8fc7 \u533a \u5757 \u94fe \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u7684 \u633a \u591a \u7684 \u561b \uff0c \u5389 \u5bb3 \u4e86 \uff01 \u68c0 \u7d22 \u90a3 \u633a \u65e9 \u7684 \u5440 \uff0c \u90a3 \u4e2a \u65f6 \u5019 \u6211 \u8fd8 \u5728 \u8bfb \u5c0f \u5b66 \u5462 \u4e0d \u68c0 \u7d22 \u4e0d \u77e5 \u9053 \u8fd9 \u4e2a \u6280 \u672f \u672a \u6765 \u4f1a \u600e \u4e48 \u53d1 \u5c55 \u5462 \u68c0 \u7d22 \u90a3 \u6211 \u73b0 \u5728 \u5b66 \u4e00 \u5b66 \uff0c \u8fd8 \u8d76 \u5f97 \u4e0a \u672b \u73ed \u8f66 \u5427 \u3002 \u3002 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u53ef \u4ee5 \u7ed9 \u6211 \u63a8 \u8350 \u4e00 \u4e9b \u96f6 \u98df \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u53ef \u4ee5 \u7ed9 \u6211 \u63a8 \u8350 \u4e00 \u4e9b \u96f6 \u98df \u5417 \u68c0 \u7d22 \u53ef \u662f \u6211 \u6700 \u8fd1 \u5728 \u51cf \u80a5 \uff0c \u6211 \u9700 \u8981 \u4f4e \u8102 \u96f6 \u98df \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u53ef \u4ee5 \u7ed9 \u6211 \u63a8 \u8350 \u4e00 \u4e9b \u96f6 \u98df \u5417 \u68c0 \u7d22 \u53ef \u662f \u6211 \u6700 \u8fd1 \u5728 \u51cf \u80a5 \uff0c \u6211 \u9700 \u8981 \u4f4e \u8102 \u96f6 \u98df \u68c0 \u7d22 \u542c \u8d77 \u6765 \u8fd8 \u86ee \u4e0d \u9519 \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u53ef \u4ee5 \u7ed9 \u6211 \u63a8 \u8350 \u4e00 \u4e9b \u96f6 \u98df \u5417 \u68c0 \u7d22 \u53ef \u662f \u6211 \u6700 \u8fd1 \u5728 \u51cf \u80a5 \uff0c \u6211 \u9700 \u8981 \u4f4e \u8102 \u96f6 \u98df \u68c0 \u7d22 \u542c \u8d77 \u6765 \u8fd8 \u86ee \u4e0d \u9519 \u7684 \u4e0d \u68c0 \u7d22 \u6211 \u6253 \u7b97 \u4e0b \u5348 \u53bb \u8d85 \u5e02 \u56e4 \u4e00 \u4e9b \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u53ef \u4ee5 \u7ed9 \u6211 \u63a8 \u8350 \u4e00 \u4e9b \u96f6 \u98df \u5417 \u68c0 \u7d22 \u53ef \u662f \u6211 \u6700 \u8fd1 \u5728 \u51cf \u80a5 \uff0c \u6211 \u9700 \u8981 \u4f4e \u8102 \u96f6 \u98df \u68c0 \u7d22 \u542c \u8d77 \u6765 \u8fd8 \u86ee \u4e0d \u9519 \u7684 \u4e0d \u68c0 \u7d22 \u6211 \u6253 \u7b97 \u4e0b \u5348 \u53bb \u8d85 \u5e02 \u56e4 \u4e00 \u4e9b \u4e0d \u68c0 \u7d22 \u6211 \u73b0 \u5728 \u4e0d \u662f \u5f88 \u559c \u6b22 \u7f51 \u4e0a \u8d2d \u7269 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u5317 \u4eac \u56db \u4e2d \u5728 \u542c \u4ec0 \u4e48 \u97f3 \u4e50 \u5462 \uff0c \u8fd9 \u4e48 \u5165 \u8ff7 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u5317 \u4eac \u56db \u4e2d \u5728 \u542c \u4ec0 \u4e48 \u97f3 \u4e50 \u5462 \uff0c \u8fd9 \u4e48 \u5165 \u8ff7 \uff1f \u68c0 \u7d22 \u597d \u5440 \uff0c \u6cb3 \u56fe \u662f \u4e00 \u4f4d \u6b4c \u624b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u5317 \u4eac \u56db \u4e2d \u5728 \u542c \u4ec0 \u4e48 \u97f3 \u4e50 \u5462 \uff0c \u8fd9 \u4e48 \u5165 \u8ff7 \uff1f \u68c0 \u7d22 \u597d \u5440 \uff0c \u6cb3 \u56fe \u662f \u4e00 \u4f4d \u6b4c \u624b \u5417 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u5f88 \u5c0f \u4f17 \u5462 \uff0c \u8fd9 \u662f \u4e00 \u4f4d \u53e4 \u98ce \u6b4c \u624b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u5317 \u4eac \u56db \u4e2d \u5728 \u542c \u4ec0 \u4e48 \u97f3 \u4e50 \u5462 \uff0c \u8fd9 \u4e48 \u5165 \u8ff7 \uff1f \u68c0 \u7d22 \u597d \u5440 \uff0c \u6cb3 \u56fe \u662f \u4e00 \u4f4d \u6b4c \u624b \u5417 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u5f88 \u5c0f \u4f17 \u5462 \uff0c \u8fd9 \u662f \u4e00 \u4f4d \u53e4 \u98ce \u6b4c \u624b \u5417 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u90a3 \u6211 \u4e5f \u8981 \u53bb \u542c \u4e00 \u542c \u4ed6 \u7684 \u6b4c \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u5317 \u4eac \u56db \u4e2d \u5728 \u542c \u4ec0 \u4e48 \u97f3 \u4e50 \u5462 \uff0c \u8fd9 \u4e48 \u5165 \u8ff7 \uff1f \u68c0 \u7d22 \u597d \u5440 \uff0c \u6cb3 \u56fe \u662f \u4e00 \u4f4d \u6b4c \u624b \u5417 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u5f88 \u5c0f \u4f17 \u5462 \uff0c \u8fd9 \u662f \u4e00 \u4f4d \u53e4 \u98ce \u6b4c \u624b \u5417 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u90a3 \u6211 \u4e5f \u8981 \u53bb \u542c \u4e00 \u542c \u4ed6 \u7684 \u6b4c \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u5417 \uff1f \u68c0 \u7d22 \u597d \u7684 \u5440 \uff0c \u6211 \u4e5f \u5f88 \u559c \u6b22 \u53e4 \u98ce \u6b4c \u624b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6c5f \u897f \u7701 \u840d \u4e61 \u5e02 \u770b \u5230 \u670b \u53cb \u804a \u5929 \u5149 \u7ed9 \u6211 \u53d1 \u8868 \u60c5 \u5305 \uff0c \u6211 \u90fd \u5f00 \u59cb \u7528 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6c5f \u897f \u7701 \u840d \u4e61 \u5e02 \u770b \u5230 \u670b \u53cb \u804a \u5929 \u5149 \u7ed9 \u6211 \u53d1 \u8868 \u60c5 \u5305 \uff0c \u6211 \u90fd \u5f00 \u59cb \u7528 \u4e86 \u68c0 \u7d22 \u662f \uff0c \u5c24 \u5176 \u662f \u7f51 \u4e0a \u7684 \u732b \u732b \u8868 \u60c5 \u5305 \u90fd \u597d \u53ef \u7231 \u5466 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6c5f \u897f \u7701 \u840d \u4e61 \u5e02 \u770b \u5230 \u670b \u53cb \u804a \u5929 \u5149 \u7ed9 \u6211 \u53d1 \u8868 \u60c5 \u5305 \uff0c \u6211 \u90fd \u5f00 \u59cb \u7528 \u4e86 \u68c0 \u7d22 \u662f \uff0c \u5c24 \u5176 \u662f \u7f51 \u4e0a \u7684 \u732b \u732b \u8868 \u60c5 \u5305 \u90fd \u597d \u53ef \u7231 \u5466 \u68c0 \u7d22 \u5bb6 \u517b \u732b \u6709 \u4ec0 \u4e48 \u6ce8 \u610f \u4e8b \u9879 \u5417 \uff0c \u6211 \u8fd8 \u633a \u60f3 \u517b \u4e00 \u53ea \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6c5f \u897f \u7701 \u840d \u4e61 \u5e02 \u770b \u5230 \u670b \u53cb \u804a \u5929 \u5149 \u7ed9 \u6211 \u53d1 \u8868 \u60c5 \u5305 \uff0c \u6211 \u90fd \u5f00 \u59cb \u7528 \u4e86 \u68c0 \u7d22 \u662f \uff0c \u5c24 \u5176 \u662f \u7f51 \u4e0a \u7684 \u732b \u732b \u8868 \u60c5 \u5305 \u90fd \u597d \u53ef \u7231 \u5466 \u68c0 \u7d22 \u5bb6 \u517b \u732b \u6709 \u4ec0 \u4e48 \u6ce8 \u610f \u4e8b \u9879 \u5417 \uff0c \u6211 \u8fd8 \u633a \u60f3 \u517b \u4e00 \u53ea \u7684 \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u8fd9 \u4e2a \u8fd8 \u662f \u7b2c \u4e00 \u6b21 \u77e5 \u9053 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6c5f \u897f \u7701 \u840d \u4e61 \u5e02 \u770b \u5230 \u670b \u53cb \u804a \u5929 \u5149 \u7ed9 \u6211 \u53d1 \u8868 \u60c5 \u5305 \uff0c \u6211 \u90fd \u5f00 \u59cb \u7528 \u4e86 \u68c0 \u7d22 \u662f \uff0c \u5c24 \u5176 \u662f \u7f51 \u4e0a \u7684 \u732b \u732b \u8868 \u60c5 \u5305 \u90fd \u597d \u53ef \u7231 \u5466 \u68c0 \u7d22 \u5bb6 \u517b \u732b \u6709 \u4ec0 \u4e48 \u6ce8 \u610f \u4e8b \u9879 \u5417 \uff0c \u6211 \u8fd8 \u633a \u60f3 \u517b \u4e00 \u53ea \u7684 \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u8fd9 \u4e2a \u8fd8 \u662f \u7b2c \u4e00 \u6b21 \u77e5 \u9053 \u5462 \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \uff0c \u4ee5 \u524d \u6211 \u4e5f \u6709 \u8fc7 \u8fd9 \u4e2a \u7ecf \u5386 \uff0c \u4e00 \u4f1a \u6211 \u5c31 \u6253 \u7b97 \u53bb \u732b \u5496 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u732b \u6c5f \u897f \u7701 \u840d \u4e61 \u5e02 \u770b \u5230 \u670b \u53cb \u804a \u5929 \u5149 \u7ed9 \u6211 \u53d1 \u8868 \u60c5 \u5305 \uff0c \u6211 \u90fd \u5f00 \u59cb \u7528 \u4e86 \u68c0 \u7d22 \u662f \uff0c \u5c24 \u5176 \u662f \u7f51 \u4e0a \u7684 \u732b \u732b \u8868 \u60c5 \u5305 \u90fd \u597d \u53ef \u7231 \u5466 \u68c0 \u7d22 \u5bb6 \u517b \u732b \u6709 \u4ec0 \u4e48 \u6ce8 \u610f \u4e8b \u9879 \u5417 \uff0c \u6211 \u8fd8 \u633a \u60f3 \u517b \u4e00 \u53ea \u7684 \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u8fd9 \u4e2a \u8fd8 \u662f \u7b2c \u4e00 \u6b21 \u77e5 \u9053 \u5462 \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \uff0c \u4ee5 \u524d \u6211 \u4e5f \u6709 \u8fc7 \u8fd9 \u4e2a \u7ecf \u5386 \uff0c \u4e00 \u4f1a \u6211 \u5c31 \u6253 \u7b97 \u53bb \u732b \u5496 \u68c0 \u7d22 \u55ef \u5462 \uff0c \u7b49 \u6211 \u56de \u6765 \u548c \u4f60 \u8bb2 \u8bb2 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u8f6c \u773c \u6625 \u5929 \u5c31 \u5230 \u4e86 \uff0c \u6211 \u4e70 \u4e86 \u597d \u51e0 \u6761 \u5c0f \u88d9 \u5b50 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u8f6c \u773c \u6625 \u5929 \u5c31 \u5230 \u4e86 \uff0c \u6211 \u4e70 \u4e86 \u597d \u51e0 \u6761 \u5c0f \u88d9 \u5b50 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u5c31 \u662f \u4e0d \u592a \u77e5 \u9053 \u600e \u4e48 \u642d \u914d \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u8f6c \u773c \u6625 \u5929 \u5c31 \u5230 \u4e86 \uff0c \u6211 \u4e70 \u4e86 \u597d \u51e0 \u6761 \u5c0f \u88d9 \u5b50 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u5c31 \u662f \u4e0d \u592a \u77e5 \u9053 \u600e \u4e48 \u642d \u914d \u68c0 \u7d22 \u897f \u670d \u5012 \u662f \u4e0d \u9519 \uff0c \u53c8 \u4f11 \u95f2 \u53c8 \u5f88 \u9177 \u7684 \u611f \u89c9 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u8f6c \u773c \u6625 \u5929 \u5c31 \u5230 \u4e86 \uff0c \u6211 \u4e70 \u4e86 \u597d \u51e0 \u6761 \u5c0f \u88d9 \u5b50 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u5c31 \u662f \u4e0d \u592a \u77e5 \u9053 \u600e \u4e48 \u642d \u914d \u68c0 \u7d22 \u897f \u670d \u5012 \u662f \u4e0d \u9519 \uff0c \u53c8 \u4f11 \u95f2 \u53c8 \u5f88 \u9177 \u7684 \u611f \u89c9 \u4e0d \u68c0 \u7d22 \u90a3 \u4e00 \u4f1a \u53bb \u5546 \u573a \u901b \u901b \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u8f6c \u773c \u6625 \u5929 \u5c31 \u5230 \u4e86 \uff0c \u6211 \u4e70 \u4e86 \u597d \u51e0 \u6761 \u5c0f \u88d9 \u5b50 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u5c31 \u662f \u4e0d \u592a \u77e5 \u9053 \u600e \u4e48 \u642d \u914d \u68c0 \u7d22 \u897f \u670d \u5012 \u662f \u4e0d \u9519 \uff0c \u53c8 \u4f11 \u95f2 \u53c8 \u5f88 \u9177 \u7684 \u611f \u89c9 \u4e0d \u68c0 \u7d22 \u90a3 \u4e00 \u4f1a \u53bb \u5546 \u573a \u901b \u901b \u68c0 \u7d22 \u597d \u5440 \uff0c \u5e0c \u671b \u80fd \u6709 \u5408 \u9002 \u7684 ~ \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u51cf \u80a5 \u897f \u85cf \u81ea \u6cbb \u533a \u6797 \u829d \u5e02 \u6211 \u53ef \u592a \u80d6 \u4e86 \u5427 \uff01 \u4e3a \u4e86 \u7537 \u795e \u6211 \u8981 \u51cf \u80a5 \uff01 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u51cf \u80a5 \u897f \u85cf \u81ea \u6cbb \u533a \u6797 \u829d \u5e02 \u6211 \u53ef \u592a \u80d6 \u4e86 \u5427 \uff01 \u4e3a \u4e86 \u7537 \u795e \u6211 \u8981 \u51cf \u80a5 \uff01 \uff01 \u4e0d \u68c0 \u7d22 \u8981 \u4e0d \u6211 \u4eca \u5929 \u5f00 \u59cb \u8282 \u98df \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u51cf \u80a5 \u897f \u85cf \u81ea \u6cbb \u533a \u6797 \u829d \u5e02 \u6211 \u53ef \u592a \u80d6 \u4e86 \u5427 \uff01 \u4e3a \u4e86 \u7537 \u795e \u6211 \u8981 \u51cf \u80a5 \uff01 \uff01 \u4e0d \u68c0 \u7d22 \u8981 \u4e0d \u6211 \u4eca \u5929 \u5f00 \u59cb \u8282 \u98df \uff1f \u68c0 \u7d22 \u4ec0 \u4e48 \u98df \u7269 \u70ed \u91cf \u4e0d \u4f1a \u9ad8 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u51cf \u80a5 \u897f \u85cf \u81ea \u6cbb \u533a \u6797 \u829d \u5e02 \u6211 \u53ef \u592a \u80d6 \u4e86 \u5427 \uff01 \u4e3a \u4e86 \u7537 \u795e \u6211 \u8981 \u51cf \u80a5 \uff01 \uff01 \u4e0d \u68c0 \u7d22 \u8981 \u4e0d \u6211 \u4eca \u5929 \u5f00 \u59cb \u8282 \u98df \uff1f \u68c0 \u7d22 \u4ec0 \u4e48 \u98df \u7269 \u70ed \u91cf \u4e0d \u4f1a \u9ad8 \u554a \u68c0 \u7d22 \u6211 \u5f88 \u559c \u6b22 \u5403 \u767d \u7c73 \u996d \u8036 \uff0c \u4f46 \u662f \u542c \u8bf4 \u767d \u7c73 \u996d \u78b3 \u6c34 \u5f88 \u591a \u5f88 \u5bb9 \u6613 \u5403 \u80d6 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u51cf \u80a5 \u897f \u85cf \u81ea \u6cbb \u533a \u6797 \u829d \u5e02 \u6211 \u53ef \u592a \u80d6 \u4e86 \u5427 \uff01 \u4e3a \u4e86 \u7537 \u795e \u6211 \u8981 \u51cf \u80a5 \uff01 \uff01 \u4e0d \u68c0 \u7d22 \u8981 \u4e0d \u6211 \u4eca \u5929 \u5f00 \u59cb \u8282 \u98df \uff1f \u68c0 \u7d22 \u4ec0 \u4e48 \u98df \u7269 \u70ed \u91cf \u4e0d \u4f1a \u9ad8 \u554a \u68c0 \u7d22 \u6211 \u5f88 \u559c \u6b22 \u5403 \u767d \u7c73 \u996d \u8036 \uff0c \u4f46 \u662f \u542c \u8bf4 \u767d \u7c73 \u996d \u78b3 \u6c34 \u5f88 \u591a \u5f88 \u5bb9 \u6613 \u5403 \u80d6 \u68c0 \u7d22 \u545c \u545c \u545c \uff0c \u8981 \u60f3 \u8eab \u6750 \u597d \u8981 \u4ed8 \u51fa \u597d \u591a \u554a \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u4e09 \u56fd \u6f14 \u4e49 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u4e09 \u56fd \u6f14 \u4e49 \u4f60 \u6700 \u559c \u6b22 \u8c01 \uff01 \u4e3a \u4ec0 \u4e48 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u4e09 \u56fd \u6f14 \u4e49 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u4e09 \u56fd \u6f14 \u4e49 \u4f60 \u6700 \u559c \u6b22 \u8c01 \uff01 \u4e3a \u4ec0 \u4e48 \u68c0 \u7d22 \u6211 \u6bd4 \u8f83 \u559c \u6b22 \u66f9 \u64cd \uff0c \u4ed6 \u53ef \u662f \u5978 \u96c4 \uff0c \u6211 \u5f88 \u559c \u6b22 \u4ed6 \u7684 \u91ce \u5fc3 \u548c \u8c0b \u7565 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u4e09 \u56fd \u6f14 \u4e49 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u4e09 \u56fd \u6f14 \u4e49 \u4f60 \u6700 \u559c \u6b22 \u8c01 \uff01 \u4e3a \u4ec0 \u4e48 \u68c0 \u7d22 \u6211 \u6bd4 \u8f83 \u559c \u6b22 \u66f9 \u64cd \uff0c \u4ed6 \u53ef \u662f \u5978 \u96c4 \uff0c \u6211 \u5f88 \u559c \u6b22 \u4ed6 \u7684 \u91ce \u5fc3 \u548c \u8c0b \u7565 \u68c0 \u7d22 \u4f60 \u8bfb \u8fc7 \u4ed6 \u7684 \u8bd7 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u4e09 \u56fd \u6f14 \u4e49 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u4e09 \u56fd \u6f14 \u4e49 \u4f60 \u6700 \u559c \u6b22 \u8c01 \uff01 \u4e3a \u4ec0 \u4e48 \u68c0 \u7d22 \u6211 \u6bd4 \u8f83 \u559c \u6b22 \u66f9 \u64cd \uff0c \u4ed6 \u53ef \u662f \u5978 \u96c4 \uff0c \u6211 \u5f88 \u559c \u6b22 \u4ed6 \u7684 \u91ce \u5fc3 \u548c \u8c0b \u7565 \u68c0 \u7d22 \u4f60 \u8bfb \u8fc7 \u4ed6 \u7684 \u8bd7 \u5417 \u68c0 \u7d22 \u4ed6 \u4ec0 \u4e48 \u65f6 \u5019 \u5199 \u7684 \u89c2 \u6ca7 \u6d77 \u5440 \uff0c \u6709 \u4ec0 \u4e48 \u5199 \u4f5c \u80cc \u666f \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u4e09 \u56fd \u6f14 \u4e49 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u4e09 \u56fd \u6f14 \u4e49 \u4f60 \u6700 \u559c \u6b22 \u8c01 \uff01 \u4e3a \u4ec0 \u4e48 \u68c0 \u7d22 \u6211 \u6bd4 \u8f83 \u559c \u6b22 \u66f9 \u64cd \uff0c \u4ed6 \u53ef \u662f \u5978 \u96c4 \uff0c \u6211 \u5f88 \u559c \u6b22 \u4ed6 \u7684 \u91ce \u5fc3 \u548c \u8c0b \u7565 \u68c0 \u7d22 \u4f60 \u8bfb \u8fc7 \u4ed6 \u7684 \u8bd7 \u5417 \u68c0 \u7d22 \u4ed6 \u4ec0 \u4e48 \u65f6 \u5019 \u5199 \u7684 \u89c2 \u6ca7 \u6d77 \u5440 \uff0c \u6709 \u4ec0 \u4e48 \u5199 \u4f5c \u80cc \u666f \u5417 \u68c0 \u7d22 \u770b \u6765 \u6211 \u5f97 \u53bb \u597d \u597d \u7814 \u7a76 \u4ed6 \u7684 \u8bd7 \u8bcd \u80cc \u666f \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u97e9 \u7ea2 \u7518 \u8083 \u7701 \u5929 \u6c34 \u5e02 \u6211 \u6700 \u8fd1 \u542c \u4e86 \u4e00 \u9996 \u53eb \u5929 \u8def \u7684 \u6b4c \uff0c \u5f88 \u597d \u542c \u554a \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u97e9 \u7ea2 \u7518 \u8083 \u7701 \u5929 \u6c34 \u5e02 \u6211 \u6700 \u8fd1 \u542c \u4e86 \u4e00 \u9996 \u53eb \u5929 \u8def \u7684 \u6b4c \uff0c \u5f88 \u597d \u542c \u554a \u3002 \u68c0 \u7d22 \u97e9 \u7ea2 \u597d \u50cf \u5f88 \u6709 \u540d \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u97e9 \u7ea2 \u7518 \u8083 \u7701 \u5929 \u6c34 \u5e02 \u6211 \u6700 \u8fd1 \u542c \u4e86 \u4e00 \u9996 \u53eb \u5929 \u8def \u7684 \u6b4c \uff0c \u5f88 \u597d \u542c \u554a \u3002 \u68c0 \u7d22 \u97e9 \u7ea2 \u597d \u50cf \u5f88 \u6709 \u540d \u554a \uff1f \u68c0 \u7d22 \u6240 \u4ee5 \u662f \u6709 \u6b4c \u5531 \u7684 \u57fa \u56e0 \u5728 \u5979 \u8eab \u4f53 \u91cc \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u97e9 \u7ea2 \u7518 \u8083 \u7701 \u5929 \u6c34 \u5e02 \u6211 \u6700 \u8fd1 \u542c \u4e86 \u4e00 \u9996 \u53eb \u5929 \u8def \u7684 \u6b4c \uff0c \u5f88 \u597d \u542c \u554a \u3002 \u68c0 \u7d22 \u97e9 \u7ea2 \u597d \u50cf \u5f88 \u6709 \u540d \u554a \uff1f \u68c0 \u7d22 \u6240 \u4ee5 \u662f \u6709 \u6b4c \u5531 \u7684 \u57fa \u56e0 \u5728 \u5979 \u8eab \u4f53 \u91cc \u7684 \u3002 \u68c0 \u7d22 \u4f1a \u521b \u4f5c \u662f \u5f88 \u5389 \u5bb3 \u7684 \uff0c \u5979 \u8fd8 \u6709 \u5176 \u5b83 \u597d \u542c \u7684 \u6b4c \u66f2 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u97e9 \u7ea2 \u7518 \u8083 \u7701 \u5929 \u6c34 \u5e02 \u6211 \u6700 \u8fd1 \u542c \u4e86 \u4e00 \u9996 \u53eb \u5929 \u8def \u7684 \u6b4c \uff0c \u5f88 \u597d \u542c \u554a \u3002 \u68c0 \u7d22 \u97e9 \u7ea2 \u597d \u50cf \u5f88 \u6709 \u540d \u554a \uff1f \u68c0 \u7d22 \u6240 \u4ee5 \u662f \u6709 \u6b4c \u5531 \u7684 \u57fa \u56e0 \u5728 \u5979 \u8eab \u4f53 \u91cc \u7684 \u3002 \u68c0 \u7d22 \u4f1a \u521b \u4f5c \u662f \u5f88 \u5389 \u5bb3 \u7684 \uff0c \u5979 \u8fd8 \u6709 \u5176 \u5b83 \u597d \u542c \u7684 \u6b4c \u66f2 \u5417 \uff1f \u68c0 \u7d22 \u539f \u6765 \u5929 \u4eae \u4e86 \u662f \u5979 \u5531 \u7684 \u554a \uff0c \u771f \u7684 \u5f88 \u597d \u542c \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5409 \u6797 \u7701 \u957f \u6625 \u5e02 \u5bf9 \u4e86 \uff0c \u6211 \u542c \u8bf4 \u4f60 \u5f88 \u4e86 \u89e3 \u661f \u5ea7 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5409 \u6797 \u7701 \u957f \u6625 \u5e02 \u5bf9 \u4e86 \uff0c \u6211 \u542c \u8bf4 \u4f60 \u5f88 \u4e86 \u89e3 \u661f \u5ea7 \u3002 \u68c0 \u7d22 \u661f \u76d8 \u662f \u4ec0 \u4e48 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5409 \u6797 \u7701 \u957f \u6625 \u5e02 \u5bf9 \u4e86 \uff0c \u6211 \u542c \u8bf4 \u4f60 \u5f88 \u4e86 \u89e3 \u661f \u5ea7 \u3002 \u68c0 \u7d22 \u661f \u76d8 \u662f \u4ec0 \u4e48 \u554a \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u597d \u590d \u6742 \uff0c \u90a3 \u7ed9 \u6211 \u8bb2 \u8bb2 \u661f \u5ea7 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5409 \u6797 \u7701 \u957f \u6625 \u5e02 \u5bf9 \u4e86 \uff0c \u6211 \u542c \u8bf4 \u4f60 \u5f88 \u4e86 \u89e3 \u661f \u5ea7 \u3002 \u68c0 \u7d22 \u661f \u76d8 \u662f \u4ec0 \u4e48 \u554a \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u597d \u590d \u6742 \uff0c \u90a3 \u7ed9 \u6211 \u8bb2 \u8bb2 \u661f \u5ea7 \u5427 \u3002 \u68c0 \u7d22 \u6211 \u662f 3 \u6708 23 \u65e5 \u51fa \u751f \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5409 \u6797 \u7701 \u957f \u6625 \u5e02 \u5bf9 \u4e86 \uff0c \u6211 \u542c \u8bf4 \u4f60 \u5f88 \u4e86 \u89e3 \u661f \u5ea7 \u3002 \u68c0 \u7d22 \u661f \u76d8 \u662f \u4ec0 \u4e48 \u554a \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u597d \u590d \u6742 \uff0c \u90a3 \u7ed9 \u6211 \u8bb2 \u8bb2 \u661f \u5ea7 \u5427 \u3002 \u68c0 \u7d22 \u6211 \u662f 3 \u6708 23 \u65e5 \u51fa \u751f \u7684 \u3002 \u68c0 \u7d22 \u539f \u6765 \u5982 \u6b64 \uff0c \u90a3 \u767d \u7f8a \u5ea7 \u7684 \u7279 \u70b9 \u662f \u4ec0 \u4e48 \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82b1 \u675f \u822c \u7684 \u604b \u7231 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5df4 \u97f3 \u90ed \u695e \u8499 \u53e4 \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u65b0 \u7684 \u7535 \u5f71 \u4e0a \u6620 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82b1 \u675f \u822c \u7684 \u604b \u7231 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5df4 \u97f3 \u90ed \u695e \u8499 \u53e4 \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u65b0 \u7684 \u7535 \u5f71 \u4e0a \u6620 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6f14 \u5458 \u90fd \u6709 \u8c01 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82b1 \u675f \u822c \u7684 \u604b \u7231 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5df4 \u97f3 \u90ed \u695e \u8499 \u53e4 \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u65b0 \u7684 \u7535 \u5f71 \u4e0a \u6620 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6f14 \u5458 \u90fd \u6709 \u8c01 \u5440 \uff1f \u68c0 \u7d22 \u83c5 \u7530 \u5c06 \u6656 \u597d \u50cf \u8ddf \u5c0f \u677e \u83dc \u5948 \u7ed3 \u5a5a \u4e86 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82b1 \u675f \u822c \u7684 \u604b \u7231 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5df4 \u97f3 \u90ed \u695e \u8499 \u53e4 \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u65b0 \u7684 \u7535 \u5f71 \u4e0a \u6620 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6f14 \u5458 \u90fd \u6709 \u8c01 \u5440 \uff1f \u68c0 \u7d22 \u83c5 \u7530 \u5c06 \u6656 \u597d \u50cf \u8ddf \u5c0f \u677e \u83dc \u5948 \u7ed3 \u5a5a \u4e86 \u5427 \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u90e8 \u7535 \u5f71 \u8bb2 \u7684 \u4ec0 \u4e48 \u6545 \u4e8b \uff1f \u597d \u770b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82b1 \u675f \u822c \u7684 \u604b \u7231 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5df4 \u97f3 \u90ed \u695e \u8499 \u53e4 \u81ea \u6cbb \u5dde \u6700 \u8fd1 \u6709 \u4ec0 \u4e48 \u65b0 \u7684 \u7535 \u5f71 \u4e0a \u6620 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6f14 \u5458 \u90fd \u6709 \u8c01 \u5440 \uff1f \u68c0 \u7d22 \u83c5 \u7530 \u5c06 \u6656 \u597d \u50cf \u8ddf \u5c0f \u677e \u83dc \u5948 \u7ed3 \u5a5a \u4e86 \u5427 \u3002 \u4e0d \u68c0 \u7d22 \u8fd9 \u90e8 \u7535 \u5f71 \u8bb2 \u7684 \u4ec0 \u4e48 \u6545 \u4e8b \uff1f \u597d \u770b \u5417 \uff1f \u68c0 \u7d22 \u54c7 \u54e6 \uff0c \u542c \u8d77 \u6765 \u50cf \u90e8 \u7eaf \u7231 \u7247 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u4e13 \u4e1a \u62a4 \u7406 \u9ed1 \u9f99 \u6c5f \u7701 \u9e21 \u897f \u5e02 \u4f60 \u77e5 \u9053 \u6bdb \u4e0d \u6613 \u5927 \u5b66 \u7684 \u65f6 \u5019 \u662f \u5b66 \u62a4 \u7406 \u7684 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u4e13 \u4e1a \u62a4 \u7406 \u9ed1 \u9f99 \u6c5f \u7701 \u9e21 \u897f \u5e02 \u4f60 \u77e5 \u9053 \u6bdb \u4e0d \u6613 \u5927 \u5b66 \u7684 \u65f6 \u5019 \u662f \u5b66 \u62a4 \u7406 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u62a4 \u7406 \u4e13 \u4e1a \u786e \u5b9e \u6bd4 \u8f83 \u8f9b \u82e6 \uff0c \u6bdb \u4e0d \u6613 \u66fe \u7ecf \u4e5f \u8bf4 \u8fc7 \uff0c \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u4e13 \u4e1a \u62a4 \u7406 \u9ed1 \u9f99 \u6c5f \u7701 \u9e21 \u897f \u5e02 \u4f60 \u77e5 \u9053 \u6bdb \u4e0d \u6613 \u5927 \u5b66 \u7684 \u65f6 \u5019 \u662f \u5b66 \u62a4 \u7406 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u62a4 \u7406 \u4e13 \u4e1a \u786e \u5b9e \u6bd4 \u8f83 \u8f9b \u82e6 \uff0c \u6bdb \u4e0d \u6613 \u66fe \u7ecf \u4e5f \u8bf4 \u8fc7 \uff0c \u3002 \u68c0 \u7d22 \u4e0d \u8fc7 \u62a4 \u58eb \u9700 \u8981 \u8003 \u53d6 \u8d44 \u683c \u8bc1 \u5427 \uff1f \u8fd8 \u662f \u6709 \u5165 \u95e8 \u95e8 \u69db \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u4e13 \u4e1a \u62a4 \u7406 \u9ed1 \u9f99 \u6c5f \u7701 \u9e21 \u897f \u5e02 \u4f60 \u77e5 \u9053 \u6bdb \u4e0d \u6613 \u5927 \u5b66 \u7684 \u65f6 \u5019 \u662f \u5b66 \u62a4 \u7406 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u62a4 \u7406 \u4e13 \u4e1a \u786e \u5b9e \u6bd4 \u8f83 \u8f9b \u82e6 \uff0c \u6bdb \u4e0d \u6613 \u66fe \u7ecf \u4e5f \u8bf4 \u8fc7 \uff0c \u3002 \u68c0 \u7d22 \u4e0d \u8fc7 \u62a4 \u58eb \u9700 \u8981 \u8003 \u53d6 \u8d44 \u683c \u8bc1 \u5427 \uff1f \u8fd8 \u662f \u6709 \u5165 \u95e8 \u95e8 \u69db \u7684 \u68c0 \u7d22 \u62a4 \u58eb \u5b9e \u4e60 \u7684 \u65f6 \u5019 \u8fd8 \u6ca1 \u6709 \u5de5 \u8d44 \uff0c \u8fd9 \u4e2a \u62a4 \u7406 \u4e13 \u4e1a \u7684 \u5c31 \u4e1a \u524d \u666f \u771f \u662f \u96be \u554a ~ \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u4e13 \u4e1a \u62a4 \u7406 \u9ed1 \u9f99 \u6c5f \u7701 \u9e21 \u897f \u5e02 \u4f60 \u77e5 \u9053 \u6bdb \u4e0d \u6613 \u5927 \u5b66 \u7684 \u65f6 \u5019 \u662f \u5b66 \u62a4 \u7406 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u62a4 \u7406 \u4e13 \u4e1a \u786e \u5b9e \u6bd4 \u8f83 \u8f9b \u82e6 \uff0c \u6bdb \u4e0d \u6613 \u66fe \u7ecf \u4e5f \u8bf4 \u8fc7 \uff0c \u3002 \u68c0 \u7d22 \u4e0d \u8fc7 \u62a4 \u58eb \u9700 \u8981 \u8003 \u53d6 \u8d44 \u683c \u8bc1 \u5427 \uff1f \u8fd8 \u662f \u6709 \u5165 \u95e8 \u95e8 \u69db \u7684 \u68c0 \u7d22 \u62a4 \u58eb \u5b9e \u4e60 \u7684 \u65f6 \u5019 \u8fd8 \u6ca1 \u6709 \u5de5 \u8d44 \uff0c \u8fd9 \u4e2a \u62a4 \u7406 \u4e13 \u4e1a \u7684 \u5c31 \u4e1a \u524d \u666f \u771f \u662f \u96be \u554a ~ \u68c0 \u7d22 \u6211 \u6709 \u4e2a \u8868 \u59b9 \u5c31 \u5728 \u5927 \u4e13 \u91cc \u5b66 \u4e60 \u62a4 \u7406 \uff0c \u4ee5 \u540e \u5c31 \u51c6 \u5907 \u5f53 \u62a4 \u58eb \uff0c \u771f \u4e3a \u5979 \u611f \u5230 \u62c5 \u5fc3 \u554a \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u4e13 \u4e1a \u62a4 \u7406 \u9ed1 \u9f99 \u6c5f \u7701 \u9e21 \u897f \u5e02 \u4f60 \u77e5 \u9053 \u6bdb \u4e0d \u6613 \u5927 \u5b66 \u7684 \u65f6 \u5019 \u662f \u5b66 \u62a4 \u7406 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u62a4 \u7406 \u4e13 \u4e1a \u786e \u5b9e \u6bd4 \u8f83 \u8f9b \u82e6 \uff0c \u6bdb \u4e0d \u6613 \u66fe \u7ecf \u4e5f \u8bf4 \u8fc7 \uff0c \u3002 \u68c0 \u7d22 \u4e0d \u8fc7 \u62a4 \u58eb \u9700 \u8981 \u8003 \u53d6 \u8d44 \u683c \u8bc1 \u5427 \uff1f \u8fd8 \u662f \u6709 \u5165 \u95e8 \u95e8 \u69db \u7684 \u68c0 \u7d22 \u62a4 \u58eb \u5b9e \u4e60 \u7684 \u65f6 \u5019 \u8fd8 \u6ca1 \u6709 \u5de5 \u8d44 \uff0c \u8fd9 \u4e2a \u62a4 \u7406 \u4e13 \u4e1a \u7684 \u5c31 \u4e1a \u524d \u666f \u771f \u662f \u96be \u554a ~ \u68c0 \u7d22 \u6211 \u6709 \u4e2a \u8868 \u59b9 \u5c31 \u5728 \u5927 \u4e13 \u91cc \u5b66 \u4e60 \u62a4 \u7406 \uff0c \u4ee5 \u540e \u5c31 \u51c6 \u5907 \u5f53 \u62a4 \u58eb \uff0c \u771f \u4e3a \u5979 \u611f \u5230 \u62c5 \u5fc3 \u554a \uff01 \u68c0 \u7d22 \u6211 \u8ba9 \u5979 \u597d \u597d \u5b66 \u4e60 \uff0c \u52aa \u529b \u4e00 \u70b9 \uff0c \u4e89 \u53d6 \u80fd \u53bb \u5e02 \u91cc \u6700 \u597d \u7684 \u533b \u9662 \u5b9e \u4e60 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u670d \u9970 \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u4eca \u5929 \u7a7f \u5f97 \u771f \u6f02 \u4eae \uff0c \u8863 \u670d \u548c \u88d9 \u5b50 \u592a \u642d \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u670d \u9970 \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u4eca \u5929 \u7a7f \u5f97 \u771f \u6f02 \u4eae \uff0c \u8863 \u670d \u548c \u88d9 \u5b50 \u592a \u642d \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u8bf4 \u7684 \u53ef \u662f \u771f \u5fc3 \u8bdd \uff0c \u4f60 \u4e00 \u76f4 \u7a7f \u642d \u90fd \u597d \uff0c \u4e0d \u50cf \u6211 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u670d \u9970 \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u4eca \u5929 \u7a7f \u5f97 \u771f \u6f02 \u4eae \uff0c \u8863 \u670d \u548c \u88d9 \u5b50 \u592a \u642d \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u8bf4 \u7684 \u53ef \u662f \u771f \u5fc3 \u8bdd \uff0c \u4f60 \u4e00 \u76f4 \u7a7f \u642d \u90fd \u597d \uff0c \u4e0d \u50cf \u6211 \u3002 \u4e0d \u68c0 \u7d22 \u60f3 \u591a \u8ddf \u4f60 \u5b66 \u4e60 \u5b66 \u4e60 \uff0c \u4f60 \u80fd \u628a \u6628 \u5929 \u7a7f \u90a3 \u4e00 \u5957 \u7684 \u94fe \u63a5 \u7ed9 \u6211 \u4e2a \u4e0d \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u670d \u9970 \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u4eca \u5929 \u7a7f \u5f97 \u771f \u6f02 \u4eae \uff0c \u8863 \u670d \u548c \u88d9 \u5b50 \u592a \u642d \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u8bf4 \u7684 \u53ef \u662f \u771f \u5fc3 \u8bdd \uff0c \u4f60 \u4e00 \u76f4 \u7a7f \u642d \u90fd \u597d \uff0c \u4e0d \u50cf \u6211 \u3002 \u4e0d \u68c0 \u7d22 \u60f3 \u591a \u8ddf \u4f60 \u5b66 \u4e60 \u5b66 \u4e60 \uff0c \u4f60 \u80fd \u628a \u6628 \u5929 \u7a7f \u90a3 \u4e00 \u5957 \u7684 \u94fe \u63a5 \u7ed9 \u6211 \u4e2a \u4e0d \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e0b \u8f7d \u90ed \u8fd9 \u4e2a app \uff0c \u4e0d \u8fc7 \u6211 \u4e0d \u77e5 \u9053 \u600e \u4e48 \u8d2d \u4e70 \uff0c \u4f60 \u80fd \u6559 \u6211 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u7a7f \u642d \u670d \u9970 \u7518 \u8083 \u7701 \u5b9a \u897f \u5e02 \u4f60 \u4eca \u5929 \u7a7f \u5f97 \u771f \u6f02 \u4eae \uff0c \u8863 \u670d \u548c \u88d9 \u5b50 \u592a \u642d \u4e86 \u3002 \u68c0 \u7d22 \u6211 \u8bf4 \u7684 \u53ef \u662f \u771f \u5fc3 \u8bdd \uff0c \u4f60 \u4e00 \u76f4 \u7a7f \u642d \u90fd \u597d \uff0c \u4e0d \u50cf \u6211 \u3002 \u4e0d \u68c0 \u7d22 \u60f3 \u591a \u8ddf \u4f60 \u5b66 \u4e60 \u5b66 \u4e60 \uff0c \u4f60 \u80fd \u628a \u6628 \u5929 \u7a7f \u90a3 \u4e00 \u5957 \u7684 \u94fe \u63a5 \u7ed9 \u6211 \u4e2a \u4e0d \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e0b \u8f7d \u90ed \u8fd9 \u4e2a app \uff0c \u4e0d \u8fc7 \u6211 \u4e0d \u77e5 \u9053 \u600e \u4e48 \u8d2d \u4e70 \uff0c \u4f60 \u80fd \u6559 \u6211 \u5417 \uff1f \u68c0 \u7d22 \u8c22 \u8c22 \uff0c \u6211 \u8bd5 \u8bd5 \uff0c \u4f60 \u5e73 \u65f6 \u4e5f \u591a \u5e2e \u6211 \u63a8 \u8350 \u63a8 \u8350 \u561b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u6e56 \u5317 \u7701 \u4ed9 \u6843 \u5e02 \u4f60 \u5403 \u7684 \u8fd9 \u662f \u5565 \u554a \uff0c \u770b \u8d77 \u6765 \u597d \u597d \u5403 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u6e56 \u5317 \u7701 \u4ed9 \u6843 \u5e02 \u4f60 \u5403 \u7684 \u8fd9 \u662f \u5565 \u554a \uff0c \u770b \u8d77 \u6765 \u597d \u597d \u5403 \u3002 \u68c0 \u7d22 \u4ed9 \u6843 \uff1f \u6211 \u5b64 \u964b \u987e \u95ee \u4e86 \uff0c \u8fd9 \u5c5e \u4e8e \u54ea \u4e2a \u7701 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u6e56 \u5317 \u7701 \u4ed9 \u6843 \u5e02 \u4f60 \u5403 \u7684 \u8fd9 \u662f \u5565 \u554a \uff0c \u770b \u8d77 \u6765 \u597d \u597d \u5403 \u3002 \u68c0 \u7d22 \u4ed9 \u6843 \uff1f \u6211 \u5b64 \u964b \u987e \u95ee \u4e86 \uff0c \u8fd9 \u5c5e \u4e8e \u54ea \u4e2a \u7701 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6da8 \u59ff \u52bf \u4e86 \u3002 \u8fd9 \u4e2a \u9178 \u70b8 \u9c7c \u597d \u597d \u5403 \u554a \uff0c \u5473 \u9053 \u633a \u72ec \u7279 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u6e56 \u5317 \u7701 \u4ed9 \u6843 \u5e02 \u4f60 \u5403 \u7684 \u8fd9 \u662f \u5565 \u554a \uff0c \u770b \u8d77 \u6765 \u597d \u597d \u5403 \u3002 \u68c0 \u7d22 \u4ed9 \u6843 \uff1f \u6211 \u5b64 \u964b \u987e \u95ee \u4e86 \uff0c \u8fd9 \u5c5e \u4e8e \u54ea \u4e2a \u7701 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6da8 \u59ff \u52bf \u4e86 \u3002 \u8fd9 \u4e2a \u9178 \u70b8 \u9c7c \u597d \u597d \u5403 \u554a \uff0c \u5473 \u9053 \u633a \u72ec \u7279 \u3002 \u68c0 \u7d22 \u611f \u89c9 \u4f60 \u4eec \u6e56 \u5317 \u597d \u5403 \u7684 \u597d \u591a \uff0c \u6211 \u5c31 \u53ef \u7231 \u5403 \u70ed \u5e72 \u9762 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u6e56 \u5317 \u7701 \u4ed9 \u6843 \u5e02 \u4f60 \u5403 \u7684 \u8fd9 \u662f \u5565 \u554a \uff0c \u770b \u8d77 \u6765 \u597d \u597d \u5403 \u3002 \u68c0 \u7d22 \u4ed9 \u6843 \uff1f \u6211 \u5b64 \u964b \u987e \u95ee \u4e86 \uff0c \u8fd9 \u5c5e \u4e8e \u54ea \u4e2a \u7701 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6da8 \u59ff \u52bf \u4e86 \u3002 \u8fd9 \u4e2a \u9178 \u70b8 \u9c7c \u597d \u597d \u5403 \u554a \uff0c \u5473 \u9053 \u633a \u72ec \u7279 \u3002 \u68c0 \u7d22 \u611f \u89c9 \u4f60 \u4eec \u6e56 \u5317 \u597d \u5403 \u7684 \u597d \u591a \uff0c \u6211 \u5c31 \u53ef \u7231 \u5403 \u70ed \u5e72 \u9762 \u4e86 \u3002 \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6709 \u673a \u4f1a \u4e00 \u5b9a \u8981 \u53bb \u6e56 \u5317 \u73a9 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u7535 \u5b50 \u8bbe \u5907 \u7535 \u8111 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u6211 \u6700 \u8fd1 \u60f3 \u8981 \u8d2d \u5165 \u4e00 \u53f0 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u5462 \uff0c \u4e0d \u77e5 \u9053 \u4e70 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u7535 \u5b50 \u8bbe \u5907 \u7535 \u8111 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u6211 \u6700 \u8fd1 \u60f3 \u8981 \u8d2d \u5165 \u4e00 \u53f0 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u5462 \uff0c \u4e0d \u77e5 \u9053 \u4e70 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \uff1f \u4e0d \u68c0 \u7d22 \u662f \u60f3 \u529e \u516c \u7528 \u7684 \uff0c \u5e73 \u65f6 \u4e5f \u4e0d \u600e \u4e48 \u73a9 \u6e38 \u620f \uff0c \u6d41 \u7545 \u5ea6 \u9ad8 \uff0c \u901f \u5ea6 \u5feb \u4e9b \u5c31 \u53ef \u4ee5 \u5566 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u7535 \u5b50 \u8bbe \u5907 \u7535 \u8111 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u6211 \u6700 \u8fd1 \u60f3 \u8981 \u8d2d \u5165 \u4e00 \u53f0 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u5462 \uff0c \u4e0d \u77e5 \u9053 \u4e70 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \uff1f \u4e0d \u68c0 \u7d22 \u662f \u60f3 \u529e \u516c \u7528 \u7684 \uff0c \u5e73 \u65f6 \u4e5f \u4e0d \u600e \u4e48 \u73a9 \u6e38 \u620f \uff0c \u6d41 \u7545 \u5ea6 \u9ad8 \uff0c \u901f \u5ea6 \u5feb \u4e9b \u5c31 \u53ef \u4ee5 \u5566 \u3002 \u4e0d \u68c0 \u7d22 \u662f \u7684 \u5462 \uff0c 6000 \u5757 \u94b1 \u80fd \u4e00 \u4e2a \u5dee \u4e0d \u591a \u7684 \u5427 \uff0c \u4e70 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u65f6 \u9700 \u8981 \u5173 \u6ce8 \u54ea \u4e9b \u65b9 \u9762 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u7535 \u5b50 \u8bbe \u5907 \u7535 \u8111 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u6211 \u6700 \u8fd1 \u60f3 \u8981 \u8d2d \u5165 \u4e00 \u53f0 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u5462 \uff0c \u4e0d \u77e5 \u9053 \u4e70 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \uff1f \u4e0d \u68c0 \u7d22 \u662f \u60f3 \u529e \u516c \u7528 \u7684 \uff0c \u5e73 \u65f6 \u4e5f \u4e0d \u600e \u4e48 \u73a9 \u6e38 \u620f \uff0c \u6d41 \u7545 \u5ea6 \u9ad8 \uff0c \u901f \u5ea6 \u5feb \u4e9b \u5c31 \u53ef \u4ee5 \u5566 \u3002 \u4e0d \u68c0 \u7d22 \u662f \u7684 \u5462 \uff0c 6000 \u5757 \u94b1 \u80fd \u4e00 \u4e2a \u5dee \u4e0d \u591a \u7684 \u5427 \uff0c \u4e70 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u65f6 \u9700 \u8981 \u5173 \u6ce8 \u54ea \u4e9b \u65b9 \u9762 \uff1f \u68c0 \u7d22 \u662f \u7684 \uff0c \u8fd8 \u662f \u8981 \u4e70 \u4e3b \u6d41 \u54c1 \u724c \u7684 \uff0c \u6709 \u4fdd \u969c \uff0c \u4e07 \u4e00 \u574f \u4e86 \u8fd8 \u53ef \u4ee5 \u4fdd \u4fee \uff0c \u7efc \u5408 \u8003 \u8651 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u7535 \u5b50 \u8bbe \u5907 \u7535 \u8111 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u8475 \u9752 \u533a \u6211 \u6700 \u8fd1 \u60f3 \u8981 \u8d2d \u5165 \u4e00 \u53f0 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u5462 \uff0c \u4e0d \u77e5 \u9053 \u4e70 \u4ec0 \u4e48 \u6bd4 \u8f83 \u597d \uff1f \u4e0d \u68c0 \u7d22 \u662f \u60f3 \u529e \u516c \u7528 \u7684 \uff0c \u5e73 \u65f6 \u4e5f \u4e0d \u600e \u4e48 \u73a9 \u6e38 \u620f \uff0c \u6d41 \u7545 \u5ea6 \u9ad8 \uff0c \u901f \u5ea6 \u5feb \u4e9b \u5c31 \u53ef \u4ee5 \u5566 \u3002 \u4e0d \u68c0 \u7d22 \u662f \u7684 \u5462 \uff0c 6000 \u5757 \u94b1 \u80fd \u4e00 \u4e2a \u5dee \u4e0d \u591a \u7684 \u5427 \uff0c \u4e70 \u7b14 \u8bb0 \u672c \u7535 \u8111 \u65f6 \u9700 \u8981 \u5173 \u6ce8 \u54ea \u4e9b \u65b9 \u9762 \uff1f \u68c0 \u7d22 \u662f \u7684 \uff0c \u8fd8 \u662f \u8981 \u4e70 \u4e3b \u6d41 \u54c1 \u724c \u7684 \uff0c \u6709 \u4fdd \u969c \uff0c \u4e07 \u4e00 \u574f \u4e86 \u8fd8 \u53ef \u4ee5 \u4fdd \u4fee \uff0c \u7efc \u5408 \u8003 \u8651 \u3002 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u53ef \u4ee5 \u591a \u770b \u4e00 \u770b \uff0c \u76f8 \u4e92 \u6bd4 \u8f83 \u4e00 \u4e0b \uff0c \u4e00 \u5b9a \u53ef \u4ee5 \u6311 \u9009 \u51fa \u9002 \u5408 \u6211 \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u575a \u679c \u798f \u5efa \u7701 \u9f99 \u5ca9 \u5e02 \u9a6c \u4e0a \u8fc7 \u8282 \u4e86 \uff0c \u6211 \u60f3 \u4e0a \u7f51 \u4e70 \u70b9 \u96f6 \u98df \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u575a \u679c \u798f \u5efa \u7701 \u9f99 \u5ca9 \u5e02 \u9a6c \u4e0a \u8fc7 \u8282 \u4e86 \uff0c \u6211 \u60f3 \u4e0a \u7f51 \u4e70 \u70b9 \u96f6 \u98df \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4e50 \u3001 \u85af \u7247 \u3001 \u8fa3 \u6761 \u90fd \u662f \u6211 \u7684 \u6700 \u7231 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u575a \u679c \u798f \u5efa \u7701 \u9f99 \u5ca9 \u5e02 \u9a6c \u4e0a \u8fc7 \u8282 \u4e86 \uff0c \u6211 \u60f3 \u4e0a \u7f51 \u4e70 \u70b9 \u96f6 \u98df \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4e50 \u3001 \u85af \u7247 \u3001 \u8fa3 \u6761 \u90fd \u662f \u6211 \u7684 \u6700 \u7231 \u3002 \u4e0d \u68c0 \u7d22 \u96f6 \u98df \u4e0d \u5c31 \u662f \u4eab \u53d7 \u5b83 \u7684 \u53e3 \u611f \u5417 \uff1f \u6211 \u4e5f \u60f3 \u5403 \u5065 \u5eb7 \u7684 \u96f6 \u98df \uff0c \u5065 \u5eb7 \u7684 \u90fd \u4e0d \u597d \u5403 \u5440 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u575a \u679c \u798f \u5efa \u7701 \u9f99 \u5ca9 \u5e02 \u9a6c \u4e0a \u8fc7 \u8282 \u4e86 \uff0c \u6211 \u60f3 \u4e0a \u7f51 \u4e70 \u70b9 \u96f6 \u98df \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4e50 \u3001 \u85af \u7247 \u3001 \u8fa3 \u6761 \u90fd \u662f \u6211 \u7684 \u6700 \u7231 \u3002 \u4e0d \u68c0 \u7d22 \u96f6 \u98df \u4e0d \u5c31 \u662f \u4eab \u53d7 \u5b83 \u7684 \u53e3 \u611f \u5417 \uff1f \u6211 \u4e5f \u60f3 \u5403 \u5065 \u5eb7 \u7684 \u96f6 \u98df \uff0c \u5065 \u5eb7 \u7684 \u90fd \u4e0d \u597d \u5403 \u5440 \u3002 \u68c0 \u7d22 \u8bf4 \u7684 \u4e5f \u662f \uff0c \u6211 \u6700 \u8fd1 \u89c9 \u5f97 \u8eab \u4f53 \u4e0d \u5982 \u4ee5 \u524d \u4e86 \uff0c \u7cbe \u795e \u4e5f \u4e0d \u592a \u597d \u3002 \u7b97 \u4e86 \uff0c \u8fd9 \u6b21 \u4e0d \u4e70 \u5783 \u573e \u98df \u54c1 \u4e86 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u597d \u5403 \u53c8 \u5065 \u5eb7 \u7684 \u96f6 \u98df \uff0c \u63a8 \u8350 \u4e00 \u4e0b \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u575a \u679c \u798f \u5efa \u7701 \u9f99 \u5ca9 \u5e02 \u9a6c \u4e0a \u8fc7 \u8282 \u4e86 \uff0c \u6211 \u60f3 \u4e0a \u7f51 \u4e70 \u70b9 \u96f6 \u98df \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4e50 \u3001 \u85af \u7247 \u3001 \u8fa3 \u6761 \u90fd \u662f \u6211 \u7684 \u6700 \u7231 \u3002 \u4e0d \u68c0 \u7d22 \u96f6 \u98df \u4e0d \u5c31 \u662f \u4eab \u53d7 \u5b83 \u7684 \u53e3 \u611f \u5417 \uff1f \u6211 \u4e5f \u60f3 \u5403 \u5065 \u5eb7 \u7684 \u96f6 \u98df \uff0c \u5065 \u5eb7 \u7684 \u90fd \u4e0d \u597d \u5403 \u5440 \u3002 \u68c0 \u7d22 \u8bf4 \u7684 \u4e5f \u662f \uff0c \u6211 \u6700 \u8fd1 \u89c9 \u5f97 \u8eab \u4f53 \u4e0d \u5982 \u4ee5 \u524d \u4e86 \uff0c \u7cbe \u795e \u4e5f \u4e0d \u592a \u597d \u3002 \u7b97 \u4e86 \uff0c \u8fd9 \u6b21 \u4e0d \u4e70 \u5783 \u573e \u98df \u54c1 \u4e86 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u597d \u5403 \u53c8 \u5065 \u5eb7 \u7684 \u96f6 \u98df \uff0c \u63a8 \u8350 \u4e00 \u4e0b \uff1f \u68c0 \u7d22 \u53ef \u662f \u5403 \u575a \u679c \u4e5f \u4f1a \u80d6 \u5427 \uff1f \u6211 \u6700 \u8fd1 \u90fd \u8d85 \u91cd \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u575a \u679c \u798f \u5efa \u7701 \u9f99 \u5ca9 \u5e02 \u9a6c \u4e0a \u8fc7 \u8282 \u4e86 \uff0c \u6211 \u60f3 \u4e0a \u7f51 \u4e70 \u70b9 \u96f6 \u98df \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4e50 \u3001 \u85af \u7247 \u3001 \u8fa3 \u6761 \u90fd \u662f \u6211 \u7684 \u6700 \u7231 \u3002 \u4e0d \u68c0 \u7d22 \u96f6 \u98df \u4e0d \u5c31 \u662f \u4eab \u53d7 \u5b83 \u7684 \u53e3 \u611f \u5417 \uff1f \u6211 \u4e5f \u60f3 \u5403 \u5065 \u5eb7 \u7684 \u96f6 \u98df \uff0c \u5065 \u5eb7 \u7684 \u90fd \u4e0d \u597d \u5403 \u5440 \u3002 \u68c0 \u7d22 \u8bf4 \u7684 \u4e5f \u662f \uff0c \u6211 \u6700 \u8fd1 \u89c9 \u5f97 \u8eab \u4f53 \u4e0d \u5982 \u4ee5 \u524d \u4e86 \uff0c \u7cbe \u795e \u4e5f \u4e0d \u592a \u597d \u3002 \u7b97 \u4e86 \uff0c \u8fd9 \u6b21 \u4e0d \u4e70 \u5783 \u573e \u98df \u54c1 \u4e86 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u597d \u5403 \u53c8 \u5065 \u5eb7 \u7684 \u96f6 \u98df \uff0c \u63a8 \u8350 \u4e00 \u4e0b \uff1f \u68c0 \u7d22 \u53ef \u662f \u5403 \u575a \u679c \u4e5f \u4f1a \u80d6 \u5427 \uff1f \u6211 \u6700 \u8fd1 \u90fd \u8d85 \u91cd \u4e86 \u3002 \u68c0 \u7d22 20g \u4e5f \u592a \u5c11 \u4e86 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u6c5f \u897f \u7701 \u5b9c \u6625 \u5e02 \u6700 \u8fd1 \u5916 \u5a46 \u751f \u75c5 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u6c5f \u897f \u7701 \u5b9c \u6625 \u5e02 \u6700 \u8fd1 \u5916 \u5a46 \u751f \u75c5 \u4e86 \u4e0d \u68c0 \u7d22 \u9ad8 \u8840 \u538b \u5f15 \u8d77 \u7684 \u4e2d \u98ce \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u6c5f \u897f \u7701 \u5b9c \u6625 \u5e02 \u6700 \u8fd1 \u5916 \u5a46 \u751f \u75c5 \u4e86 \u4e0d \u68c0 \u7d22 \u9ad8 \u8840 \u538b \u5f15 \u8d77 \u7684 \u4e2d \u98ce \u3002 \u68c0 \u7d22 \u662f \u7684 \uff0c \u5c31 \u662f \u5e73 \u65f6 \u5403 \u996d \u592a \u4e0d \u6ce8 \u610f \u4e86 \uff0c \u73b0 \u5728 \u751f \u6d3b \u597d \u4e86 \uff0c \u4e0d \u50cf \u4ee5 \u524d \u90a3 \u6837 \uff0c \u5403 \u996d \u90fd \u5403 \u4e0d \u9971 \uff0c \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u6c5f \u897f \u7701 \u5b9c \u6625 \u5e02 \u6700 \u8fd1 \u5916 \u5a46 \u751f \u75c5 \u4e86 \u4e0d \u68c0 \u7d22 \u9ad8 \u8840 \u538b \u5f15 \u8d77 \u7684 \u4e2d \u98ce \u3002 \u68c0 \u7d22 \u662f \u7684 \uff0c \u5c31 \u662f \u5e73 \u65f6 \u5403 \u996d \u592a \u4e0d \u6ce8 \u610f \u4e86 \uff0c \u73b0 \u5728 \u751f \u6d3b \u597d \u4e86 \uff0c \u4e0d \u50cf \u4ee5 \u524d \u90a3 \u6837 \uff0c \u5403 \u996d \u90fd \u5403 \u4e0d \u9971 \uff0c \u68c0 \u7d22 \u597d \u561b \uff0c \u90a3 \u6211 \u4e5f \u8981 \u5f00 \u59cb \u6ce8 \u610f \u8eab \u4f53 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u9ad8 \u8840 \u538b \u6c5f \u897f \u7701 \u5b9c \u6625 \u5e02 \u6700 \u8fd1 \u5916 \u5a46 \u751f \u75c5 \u4e86 \u4e0d \u68c0 \u7d22 \u9ad8 \u8840 \u538b \u5f15 \u8d77 \u7684 \u4e2d \u98ce \u3002 \u68c0 \u7d22 \u662f \u7684 \uff0c \u5c31 \u662f \u5e73 \u65f6 \u5403 \u996d \u592a \u4e0d \u6ce8 \u610f \u4e86 \uff0c \u73b0 \u5728 \u751f \u6d3b \u597d \u4e86 \uff0c \u4e0d \u50cf \u4ee5 \u524d \u90a3 \u6837 \uff0c \u5403 \u996d \u90fd \u5403 \u4e0d \u9971 \uff0c \u68c0 \u7d22 \u597d \u561b \uff0c \u90a3 \u6211 \u4e5f \u8981 \u5f00 \u59cb \u6ce8 \u610f \u8eab \u4f53 \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u90a3 \u6211 \u987a \u4fbf \u660e \u5929 \u53bb \u505a \u4e2a \u4f53 \u68c0 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u56fe \u50cf \u5206 \u5272 \u5c71 \u4e1c \u7701 \u4e1c \u8425 \u5e02 \u6211 \u4e4b \u524d \u770b \u90a3 \u4e2a \u76d1 \u63a7 \uff0c \u5b83 \u80fd \u7cbe \u51c6 \u9501 \u5b9a \u76ee \u6807 \uff0c \u76ee \u6807 \u662f \u5565 \u4ed6 \u90fd \u77e5 \u9053 \u3002 \u8fd9 \u662f \u4e0d \u662f \u56fe \u50cf \u5206 \u5272 \u641e \u7684 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u56fe \u50cf \u5206 \u5272 \u5c71 \u4e1c \u7701 \u4e1c \u8425 \u5e02 \u6211 \u4e4b \u524d \u770b \u90a3 \u4e2a \u76d1 \u63a7 \uff0c \u5b83 \u80fd \u7cbe \u51c6 \u9501 \u5b9a \u76ee \u6807 \uff0c \u76ee \u6807 \u662f \u5565 \u4ed6 \u90fd \u77e5 \u9053 \u3002 \u8fd9 \u662f \u4e0d \u662f \u56fe \u50cf \u5206 \u5272 \u641e \u7684 \u554a \u4e0d \u68c0 \u7d22 \u4ed6 \u600e \u4e48 \u5c31 \u80fd \u8bc6 \u522b \u51fa \u6765 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u56fe \u50cf \u5206 \u5272 \u5c71 \u4e1c \u7701 \u4e1c \u8425 \u5e02 \u6211 \u4e4b \u524d \u770b \u90a3 \u4e2a \u76d1 \u63a7 \uff0c \u5b83 \u80fd \u7cbe \u51c6 \u9501 \u5b9a \u76ee \u6807 \uff0c \u76ee \u6807 \u662f \u5565 \u4ed6 \u90fd \u77e5 \u9053 \u3002 \u8fd9 \u662f \u4e0d \u662f \u56fe \u50cf \u5206 \u5272 \u641e \u7684 \u554a \u4e0d \u68c0 \u7d22 \u4ed6 \u600e \u4e48 \u5c31 \u80fd \u8bc6 \u522b \u51fa \u6765 \u5462 \u68c0 \u7d22 \u597d \u68d2 \u554a \uff0c \u7136 \u540e \u518d \u7528 \u673a \u5668 \u5b66 \u4e60 \u6765 \u5904 \u7406 \u8fd9 \u4e9b \u6837 \u672c \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u56fe \u50cf \u5206 \u5272 \u5c71 \u4e1c \u7701 \u4e1c \u8425 \u5e02 \u6211 \u4e4b \u524d \u770b \u90a3 \u4e2a \u76d1 \u63a7 \uff0c \u5b83 \u80fd \u7cbe \u51c6 \u9501 \u5b9a \u76ee \u6807 \uff0c \u76ee \u6807 \u662f \u5565 \u4ed6 \u90fd \u77e5 \u9053 \u3002 \u8fd9 \u662f \u4e0d \u662f \u56fe \u50cf \u5206 \u5272 \u641e \u7684 \u554a \u4e0d \u68c0 \u7d22 \u4ed6 \u600e \u4e48 \u5c31 \u80fd \u8bc6 \u522b \u51fa \u6765 \u5462 \u68c0 \u7d22 \u597d \u68d2 \u554a \uff0c \u7136 \u540e \u518d \u7528 \u673a \u5668 \u5b66 \u4e60 \u6765 \u5904 \u7406 \u8fd9 \u4e9b \u6837 \u672c \u5417 \u68c0 \u7d22 \u6211 \u60f3 \u5b66 \u4e60 \u7684 \u8bdd \u5982 \u4f55 \u5165 \u95e8 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u5de5 \u667a \u80fd \u56fe \u50cf \u5206 \u5272 \u5c71 \u4e1c \u7701 \u4e1c \u8425 \u5e02 \u6211 \u4e4b \u524d \u770b \u90a3 \u4e2a \u76d1 \u63a7 \uff0c \u5b83 \u80fd \u7cbe \u51c6 \u9501 \u5b9a \u76ee \u6807 \uff0c \u76ee \u6807 \u662f \u5565 \u4ed6 \u90fd \u77e5 \u9053 \u3002 \u8fd9 \u662f \u4e0d \u662f \u56fe \u50cf \u5206 \u5272 \u641e \u7684 \u554a \u4e0d \u68c0 \u7d22 \u4ed6 \u600e \u4e48 \u5c31 \u80fd \u8bc6 \u522b \u51fa \u6765 \u5462 \u68c0 \u7d22 \u597d \u68d2 \u554a \uff0c \u7136 \u540e \u518d \u7528 \u673a \u5668 \u5b66 \u4e60 \u6765 \u5904 \u7406 \u8fd9 \u4e9b \u6837 \u672c \u5417 \u68c0 \u7d22 \u6211 \u60f3 \u5b66 \u4e60 \u7684 \u8bdd \u5982 \u4f55 \u5165 \u95e8 \u5462 \u68c0 \u7d22 \u8c22 \u8c22 \u4f60 \uff0c \u6211 \u8981 \u53bb \u4e86 \u89e3 \u4e86 \u89e3 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u6d17 \u8863 \u6db2 \u6cb3 \u5317 \u7701 \u5eca \u574a \u5e02 \u6d17 \u8863 \u670d \u53c8 \u7528 \u5b8c \u4e86 \uff0c \u8be5 \u4e70 \u70b9 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u6d17 \u8863 \u6db2 \u6cb3 \u5317 \u7701 \u5eca \u574a \u5e02 \u6d17 \u8863 \u670d \u53c8 \u7528 \u5b8c \u4e86 \uff0c \u8be5 \u4e70 \u70b9 \u4e86 \u3002 \u68c0 \u7d22 \u786e \u5b9e \u611f \u89c9 \u7528 \u4e86 \u4ee5 \u540e \u624b \u6da9 \u6da9 \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u6d17 \u8863 \u6db2 \u6cb3 \u5317 \u7701 \u5eca \u574a \u5e02 \u6d17 \u8863 \u670d \u53c8 \u7528 \u5b8c \u4e86 \uff0c \u8be5 \u4e70 \u70b9 \u4e86 \u3002 \u68c0 \u7d22 \u786e \u5b9e \u611f \u89c9 \u7528 \u4e86 \u4ee5 \u540e \u624b \u6da9 \u6da9 \u7684 \u3002 \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u539f \u6765 \u662f \u8fd9 \u6837 \u554a \uff0c \u90a3 \u6211 \u7528 \u4ec0 \u4e48 \u6d17 \u8863 \u670d \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u6d17 \u8863 \u6db2 \u6cb3 \u5317 \u7701 \u5eca \u574a \u5e02 \u6d17 \u8863 \u670d \u53c8 \u7528 \u5b8c \u4e86 \uff0c \u8be5 \u4e70 \u70b9 \u4e86 \u3002 \u68c0 \u7d22 \u786e \u5b9e \u611f \u89c9 \u7528 \u4e86 \u4ee5 \u540e \u624b \u6da9 \u6da9 \u7684 \u3002 \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u539f \u6765 \u662f \u8fd9 \u6837 \u554a \uff0c \u90a3 \u6211 \u7528 \u4ec0 \u4e48 \u6d17 \u8863 \u670d \u5462 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u6211 \u4ee5 \u524d \u90fd \u6ca1 \u7528 \u8fc7 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u6d17 \u8863 \u6db2 \u6cb3 \u5317 \u7701 \u5eca \u574a \u5e02 \u6d17 \u8863 \u670d \u53c8 \u7528 \u5b8c \u4e86 \uff0c \u8be5 \u4e70 \u70b9 \u4e86 \u3002 \u68c0 \u7d22 \u786e \u5b9e \u611f \u89c9 \u7528 \u4e86 \u4ee5 \u540e \u624b \u6da9 \u6da9 \u7684 \u3002 \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u539f \u6765 \u662f \u8fd9 \u6837 \u554a \uff0c \u90a3 \u6211 \u7528 \u4ec0 \u4e48 \u6d17 \u8863 \u670d \u5462 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u6211 \u4ee5 \u524d \u90fd \u6ca1 \u7528 \u8fc7 \u3002 \u68c0 \u7d22 \u597d \u7684 \uff0c \u90a3 \u6709 \u4ec0 \u4e48 \u724c \u5b50 \u63a8 \u8350 \u7684 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u751f \u6d3b \u7528 \u54c1 \u6d17 \u8863 \u6db2 \u6cb3 \u5317 \u7701 \u5eca \u574a \u5e02 \u6d17 \u8863 \u670d \u53c8 \u7528 \u5b8c \u4e86 \uff0c \u8be5 \u4e70 \u70b9 \u4e86 \u3002 \u68c0 \u7d22 \u786e \u5b9e \u611f \u89c9 \u7528 \u4e86 \u4ee5 \u540e \u624b \u6da9 \u6da9 \u7684 \u3002 \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u539f \u6765 \u662f \u8fd9 \u6837 \u554a \uff0c \u90a3 \u6211 \u7528 \u4ec0 \u4e48 \u6d17 \u8863 \u670d \u5462 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u6211 \u4ee5 \u524d \u90fd \u6ca1 \u7528 \u8fc7 \u3002 \u68c0 \u7d22 \u597d \u7684 \uff0c \u90a3 \u6709 \u4ec0 \u4e48 \u724c \u5b50 \u63a8 \u8350 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u8981 \u7814 \u7a76 \u4e00 \u4e0b \u54ea \u4e2a \u66f4 \u5408 \u9002 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u5bfa \u5e99 \u897f \u85cf \u81ea \u6cbb \u533a \u90a3 \u66f2 \u5730 \u533a \u90a3 \u66f2 \u7684 \u6d77 \u62d4 \u771f \u662f \u9ad8 \uff0c \u6211 \u7684 \u9ad8 \u53cd \u597d \u4e25 \u91cd \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u5bfa \u5e99 \u897f \u85cf \u81ea \u6cbb \u533a \u90a3 \u66f2 \u5730 \u533a \u90a3 \u66f2 \u7684 \u6d77 \u62d4 \u771f \u662f \u9ad8 \uff0c \u6211 \u7684 \u9ad8 \u53cd \u597d \u4e25 \u91cd \u68c0 \u7d22 \u6765 \u73a9 \u513f \u554a \uff0c \u770b \u770b \u8fd9 \u8fb9 \u7684 \u4eba \u6587 \u98ce \u60c5 \u548c \u81ea \u7136 \u98ce \u5149 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u5bfa \u5e99 \u897f \u85cf \u81ea \u6cbb \u533a \u90a3 \u66f2 \u5730 \u533a \u90a3 \u66f2 \u7684 \u6d77 \u62d4 \u771f \u662f \u9ad8 \uff0c \u6211 \u7684 \u9ad8 \u53cd \u597d \u4e25 \u91cd \u68c0 \u7d22 \u6765 \u73a9 \u513f \u554a \uff0c \u770b \u770b \u8fd9 \u8fb9 \u7684 \u4eba \u6587 \u98ce \u60c5 \u548c \u81ea \u7136 \u98ce \u5149 \u68c0 \u7d22 \u6211 \u6628 \u5929 \u5c31 \u53bb \u770b \u8fc7 \u4e86 \uff0c \u90a3 \u66f2 \u8fd8 \u6709 \u4ec0 \u4e48 \u522b \u7684 \u666f \u70b9 \u561b \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u5bfa \u5e99 \u897f \u85cf \u81ea \u6cbb \u533a \u90a3 \u66f2 \u5730 \u533a \u90a3 \u66f2 \u7684 \u6d77 \u62d4 \u771f \u662f \u9ad8 \uff0c \u6211 \u7684 \u9ad8 \u53cd \u597d \u4e25 \u91cd \u68c0 \u7d22 \u6765 \u73a9 \u513f \u554a \uff0c \u770b \u770b \u8fd9 \u8fb9 \u7684 \u4eba \u6587 \u98ce \u60c5 \u548c \u81ea \u7136 \u98ce \u5149 \u68c0 \u7d22 \u6211 \u6628 \u5929 \u5c31 \u53bb \u770b \u8fc7 \u4e86 \uff0c \u90a3 \u66f2 \u8fd8 \u6709 \u4ec0 \u4e48 \u522b \u7684 \u666f \u70b9 \u561b \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u518d \u5728 \u90a3 \u66f2 \u5f85 \u4e00 \u4e2a \u661f \u671f \uff01 \u5e0c \u671b \u4e4b \u540e \u51e0 \u5929 \u6211 \u7684 \u9ad8 \u53cd \u80fd \u51cf \u8f7b \u4e00 \u4e9b \uff0c \u54ce \u54ce \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u5bfa \u5e99 \u897f \u85cf \u81ea \u6cbb \u533a \u90a3 \u66f2 \u5730 \u533a \u90a3 \u66f2 \u7684 \u6d77 \u62d4 \u771f \u662f \u9ad8 \uff0c \u6211 \u7684 \u9ad8 \u53cd \u597d \u4e25 \u91cd \u68c0 \u7d22 \u6765 \u73a9 \u513f \u554a \uff0c \u770b \u770b \u8fd9 \u8fb9 \u7684 \u4eba \u6587 \u98ce \u60c5 \u548c \u81ea \u7136 \u98ce \u5149 \u68c0 \u7d22 \u6211 \u6628 \u5929 \u5c31 \u53bb \u770b \u8fc7 \u4e86 \uff0c \u90a3 \u66f2 \u8fd8 \u6709 \u4ec0 \u4e48 \u522b \u7684 \u666f \u70b9 \u561b \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u518d \u5728 \u90a3 \u66f2 \u5f85 \u4e00 \u4e2a \u661f \u671f \uff01 \u5e0c \u671b \u4e4b \u540e \u51e0 \u5929 \u6211 \u7684 \u9ad8 \u53cd \u80fd \u51cf \u8f7b \u4e00 \u4e9b \uff0c \u54ce \u54ce \u4e0d \u68c0 \u7d22 \u706b \u8f66 \uff0c \u4ed6 \u4eec \u8bf4 \u706b \u8f66 \u8fdb \u85cf \uff0c \u53ef \u4ee5 \u6709 \u4e2a \u9002 \u5e94 \u8fc7 \u7a0b \uff0c \u9ad8 \u53cd \u5c31 \u6ca1 \u90a3 \u4e48 \u4e25 \u91cd \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u9ad8 \u8003 \u4e91 \u5357 \u7701 \u6587 \u5c71 \u58ee \u65cf \u82d7 \u65cf \u81ea \u6cbb \u5dde \u539f \u6765 \u827a \u8003 \u751f \u4e5f \u8981 \u548c \u4e00 \u822c \u8003 \u751f \u53c2 \u52a0 \u540c \u6837 \u7684 \u9ad8 \u8003 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u9ad8 \u8003 \u4e91 \u5357 \u7701 \u6587 \u5c71 \u58ee \u65cf \u82d7 \u65cf \u81ea \u6cbb \u5dde \u539f \u6765 \u827a \u8003 \u751f \u4e5f \u8981 \u548c \u4e00 \u822c \u8003 \u751f \u53c2 \u52a0 \u540c \u6837 \u7684 \u9ad8 \u8003 \u5417 \u68c0 \u7d22 \u90a3 \u8fd8 \u4f1a \u5206 \u6587 \u7406 \u5417 \uff1f \u6211 \u542c \u8bf4 \u6709 \u827a \u6587 \u7c7b \uff0c \u5c31 \u662f \u9ad8 \u8003 \u62a5 \u540d \u7684 \u65f6 \u5019 \u586b \u65b9 \u5411 \uff0c \u90a3 \u96be \u9053 \u8fd8 \u6709 \u7406 \u79d1 \u5206 \u652f \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u9ad8 \u8003 \u4e91 \u5357 \u7701 \u6587 \u5c71 \u58ee \u65cf \u82d7 \u65cf \u81ea \u6cbb \u5dde \u539f \u6765 \u827a \u8003 \u751f \u4e5f \u8981 \u548c \u4e00 \u822c \u8003 \u751f \u53c2 \u52a0 \u540c \u6837 \u7684 \u9ad8 \u8003 \u5417 \u68c0 \u7d22 \u90a3 \u8fd8 \u4f1a \u5206 \u6587 \u7406 \u5417 \uff1f \u6211 \u542c \u8bf4 \u6709 \u827a \u6587 \u7c7b \uff0c \u5c31 \u662f \u9ad8 \u8003 \u62a5 \u540d \u7684 \u65f6 \u5019 \u586b \u65b9 \u5411 \uff0c \u90a3 \u96be \u9053 \u8fd8 \u6709 \u7406 \u79d1 \u5206 \u652f \uff1f \u68c0 \u7d22 \u90a3 \u770b \u6765 \u5927 \u591a \u6570 \u4eba \u90fd \u9009 \u62e9 \u6587 \u79d1 \u7684 \u8bdd \u7406 \u79d1 \u5206 \u6570 \u7ebf \u80fd \u7a0d \u5fae \u4f4e \u4e00 \u4e9b \uff0c \u6216 \u8005 \u8bf4 \u7ade \u4e89 \u5bf9 \u624b \u5c11 \u4e00 \u70b9 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u9ad8 \u8003 \u4e91 \u5357 \u7701 \u6587 \u5c71 \u58ee \u65cf \u82d7 \u65cf \u81ea \u6cbb \u5dde \u539f \u6765 \u827a \u8003 \u751f \u4e5f \u8981 \u548c \u4e00 \u822c \u8003 \u751f \u53c2 \u52a0 \u540c \u6837 \u7684 \u9ad8 \u8003 \u5417 \u68c0 \u7d22 \u90a3 \u8fd8 \u4f1a \u5206 \u6587 \u7406 \u5417 \uff1f \u6211 \u542c \u8bf4 \u6709 \u827a \u6587 \u7c7b \uff0c \u5c31 \u662f \u9ad8 \u8003 \u62a5 \u540d \u7684 \u65f6 \u5019 \u586b \u65b9 \u5411 \uff0c \u90a3 \u96be \u9053 \u8fd8 \u6709 \u7406 \u79d1 \u5206 \u652f \uff1f \u68c0 \u7d22 \u90a3 \u770b \u6765 \u5927 \u591a \u6570 \u4eba \u90fd \u9009 \u62e9 \u6587 \u79d1 \u7684 \u8bdd \u7406 \u79d1 \u5206 \u6570 \u7ebf \u80fd \u7a0d \u5fae \u4f4e \u4e00 \u4e9b \uff0c \u6216 \u8005 \u8bf4 \u7ade \u4e89 \u5bf9 \u624b \u5c11 \u4e00 \u70b9 \u4e0d \u68c0 \u7d22 \u90a3 \u786e \u5b9e \u3002 \u8bdd \u8bf4 \u9ad8 \u8003 \u62a5 \u540d \u5565 \u65f6 \u5019 \u7684 \u4e8b \u554a \uff1f \u6211 \u624d \u9ad8 \u4e8c \u8fd8 \u6ca1 \u4e86 \u89e3 \u5230 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u9ad8 \u8003 \u4e91 \u5357 \u7701 \u6587 \u5c71 \u58ee \u65cf \u82d7 \u65cf \u81ea \u6cbb \u5dde \u539f \u6765 \u827a \u8003 \u751f \u4e5f \u8981 \u548c \u4e00 \u822c \u8003 \u751f \u53c2 \u52a0 \u540c \u6837 \u7684 \u9ad8 \u8003 \u5417 \u68c0 \u7d22 \u90a3 \u8fd8 \u4f1a \u5206 \u6587 \u7406 \u5417 \uff1f \u6211 \u542c \u8bf4 \u6709 \u827a \u6587 \u7c7b \uff0c \u5c31 \u662f \u9ad8 \u8003 \u62a5 \u540d \u7684 \u65f6 \u5019 \u586b \u65b9 \u5411 \uff0c \u90a3 \u96be \u9053 \u8fd8 \u6709 \u7406 \u79d1 \u5206 \u652f \uff1f \u68c0 \u7d22 \u90a3 \u770b \u6765 \u5927 \u591a \u6570 \u4eba \u90fd \u9009 \u62e9 \u6587 \u79d1 \u7684 \u8bdd \u7406 \u79d1 \u5206 \u6570 \u7ebf \u80fd \u7a0d \u5fae \u4f4e \u4e00 \u4e9b \uff0c \u6216 \u8005 \u8bf4 \u7ade \u4e89 \u5bf9 \u624b \u5c11 \u4e00 \u70b9 \u4e0d \u68c0 \u7d22 \u90a3 \u786e \u5b9e \u3002 \u8bdd \u8bf4 \u9ad8 \u8003 \u62a5 \u540d \u5565 \u65f6 \u5019 \u7684 \u4e8b \u554a \uff1f \u6211 \u624d \u9ad8 \u4e8c \u8fd8 \u6ca1 \u4e86 \u89e3 \u5230 \u68c0 \u7d22 \u597d \u65e9 \uff0c \u62a5 \u5b8c \u540d \u4e4b \u540e \u662f \u4e0d \u662f \u6587 \u7406 \u5c31 \u6539 \u4e0d \u4e86 \u4e86 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u604b \u7231 \u5c71 \u4e1c \u7701 \u6ee8 \u5dde \u5e02 \u7537 \u670b \u53cb \u8981 \u751f \u65e5 \u4e86 \uff0c \u8fd9 \u6b21 \u8981 \u4e70 \u4ec0 \u4e48 \u793c \u7269 \u7ed9 \u4ed6 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u604b \u7231 \u5c71 \u4e1c \u7701 \u6ee8 \u5dde \u5e02 \u7537 \u670b \u53cb \u8981 \u751f \u65e5 \u4e86 \uff0c \u8fd9 \u6b21 \u8981 \u4e70 \u4ec0 \u4e48 \u793c \u7269 \u7ed9 \u4ed6 \u5462 \u68c0 \u7d22 \u8fd9 \u4e9b \u90fd \u592a \u4fd7 \u4e86 \uff0c \u6211 \u81ea \u5df1 \u90fd \u4e0d \u60f3 \u9001 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u604b \u7231 \u5c71 \u4e1c \u7701 \u6ee8 \u5dde \u5e02 \u7537 \u670b \u53cb \u8981 \u751f \u65e5 \u4e86 \uff0c \u8fd9 \u6b21 \u8981 \u4e70 \u4ec0 \u4e48 \u793c \u7269 \u7ed9 \u4ed6 \u5462 \u68c0 \u7d22 \u8fd9 \u4e9b \u90fd \u592a \u4fd7 \u4e86 \uff0c \u6211 \u81ea \u5df1 \u90fd \u4e0d \u60f3 \u9001 \u4e0d \u68c0 \u7d22 \u8bf4 \u662f \u8fd9 \u4e48 \u8bf4 \uff0c \u4f46 \u662f \u60f3 \u5f04 \u70b9 \u7279 \u522b \u7684 \u7559 \u4e0b \u6df1 \u523b \u8bb0 \u5fc6 \u561b \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u604b \u7231 \u5c71 \u4e1c \u7701 \u6ee8 \u5dde \u5e02 \u7537 \u670b \u53cb \u8981 \u751f \u65e5 \u4e86 \uff0c \u8fd9 \u6b21 \u8981 \u4e70 \u4ec0 \u4e48 \u793c \u7269 \u7ed9 \u4ed6 \u5462 \u68c0 \u7d22 \u8fd9 \u4e9b \u90fd \u592a \u4fd7 \u4e86 \uff0c \u6211 \u81ea \u5df1 \u90fd \u4e0d \u60f3 \u9001 \u4e0d \u68c0 \u7d22 \u8bf4 \u662f \u8fd9 \u4e48 \u8bf4 \uff0c \u4f46 \u662f \u60f3 \u5f04 \u70b9 \u7279 \u522b \u7684 \u7559 \u4e0b \u6df1 \u523b \u8bb0 \u5fc6 \u561b \u68c0 \u7d22 \u7ec7 \u56f4 \u5dfe \u6211 \u4e5f \u8003 \u8651 \u8fc7 \uff0c \u4e0d \u8fc7 \u624b \u6b8b \u515a \u8868 \u793a \u8fd8 \u662f \u592a \u96be \u4e86 \uff0c \u6ca1 \u7ec7 \u8fc7 \u4e0d \u77e5 \u9053 \u5982 \u4f55 \u4e0b \u624b \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u604b \u7231 \u5c71 \u4e1c \u7701 \u6ee8 \u5dde \u5e02 \u7537 \u670b \u53cb \u8981 \u751f \u65e5 \u4e86 \uff0c \u8fd9 \u6b21 \u8981 \u4e70 \u4ec0 \u4e48 \u793c \u7269 \u7ed9 \u4ed6 \u5462 \u68c0 \u7d22 \u8fd9 \u4e9b \u90fd \u592a \u4fd7 \u4e86 \uff0c \u6211 \u81ea \u5df1 \u90fd \u4e0d \u60f3 \u9001 \u4e0d \u68c0 \u7d22 \u8bf4 \u662f \u8fd9 \u4e48 \u8bf4 \uff0c \u4f46 \u662f \u60f3 \u5f04 \u70b9 \u7279 \u522b \u7684 \u7559 \u4e0b \u6df1 \u523b \u8bb0 \u5fc6 \u561b \u68c0 \u7d22 \u7ec7 \u56f4 \u5dfe \u6211 \u4e5f \u8003 \u8651 \u8fc7 \uff0c \u4e0d \u8fc7 \u624b \u6b8b \u515a \u8868 \u793a \u8fd8 \u662f \u592a \u96be \u4e86 \uff0c \u6ca1 \u7ec7 \u8fc7 \u4e0d \u77e5 \u9053 \u5982 \u4f55 \u4e0b \u624b \u68c0 \u7d22 \u8fd9 \u4e2a \u542c \u7740 \u53ef \u4ee5 \uff0c \u6211 \u8fd9 \u5c31 \u53bb \u627e \u89c6 \u9891 \u5b66 \u4e60 \uff0c \u4e07 \u4e8b \u5f00 \u5934 \u96be \uff0c \u6211 \u52a0 \u6cb9 \u80af \u5b9a \u80fd \u7ec7 \u597d \uff01 \u51b2 \u5440 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u8003 \u8bd5 \u6c5f \u897f \u7701 \u4e0a \u9976 \u5e02 \u4f60 \u4eca \u5e74 \u7684 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u62a5 \u540d \u4e86 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u8003 \u8bd5 \u6c5f \u897f \u7701 \u4e0a \u9976 \u5e02 \u4f60 \u4eca \u5e74 \u7684 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u62a5 \u540d \u4e86 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u4f60 \u62a5 \u4e86 \u4ec0 \u4e48 \u5b66 \u6bb5 \u7684 \u4ec0 \u4e48 \u79d1 \u76ee \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u8003 \u8bd5 \u6c5f \u897f \u7701 \u4e0a \u9976 \u5e02 \u4f60 \u4eca \u5e74 \u7684 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u62a5 \u540d \u4e86 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u4f60 \u62a5 \u4e86 \u4ec0 \u4e48 \u5b66 \u6bb5 \u7684 \u4ec0 \u4e48 \u79d1 \u76ee \u5440 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \u521d \u4e2d \uff0c \u4f46 \u662f \u6211 \u662f \u6570 \u5b66 \u4e13 \u4e1a \u7684 \uff0c \u5c31 \u62a5 \u8003 \u4e86 \u6570 \u5b66 \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u8003 \u8bd5 \u6c5f \u897f \u7701 \u4e0a \u9976 \u5e02 \u4f60 \u4eca \u5e74 \u7684 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u62a5 \u540d \u4e86 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u4f60 \u62a5 \u4e86 \u4ec0 \u4e48 \u5b66 \u6bb5 \u7684 \u4ec0 \u4e48 \u79d1 \u76ee \u5440 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \u521d \u4e2d \uff0c \u4f46 \u662f \u6211 \u662f \u6570 \u5b66 \u4e13 \u4e1a \u7684 \uff0c \u5c31 \u62a5 \u8003 \u4e86 \u6570 \u5b66 \u7684 \u68c0 \u7d22 \u662f \u7684 \u5bf9 \u4e8e \u5e08 \u8303 \u751f \u6765 \u8bf4 \uff0c \u4ed6 \u4eec \u662f \u6709 \u7ecf \u9a8c \u7684 \uff0c \u54b1 \u4eec \u975e \u5e08 \u8303 \u751f \uff0c \u8fd8 \u662f \u6709 \u70b9 \u60ac \u554a ~ \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u8003 \u8bd5 \u6c5f \u897f \u7701 \u4e0a \u9976 \u5e02 \u4f60 \u4eca \u5e74 \u7684 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u62a5 \u540d \u4e86 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u4f60 \u62a5 \u4e86 \u4ec0 \u4e48 \u5b66 \u6bb5 \u7684 \u4ec0 \u4e48 \u79d1 \u76ee \u5440 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \u521d \u4e2d \uff0c \u4f46 \u662f \u6211 \u662f \u6570 \u5b66 \u4e13 \u4e1a \u7684 \uff0c \u5c31 \u62a5 \u8003 \u4e86 \u6570 \u5b66 \u7684 \u68c0 \u7d22 \u662f \u7684 \u5bf9 \u4e8e \u5e08 \u8303 \u751f \u6765 \u8bf4 \uff0c \u4ed6 \u4eec \u662f \u6709 \u7ecf \u9a8c \u7684 \uff0c \u54b1 \u4eec \u975e \u5e08 \u8303 \u751f \uff0c \u8fd8 \u662f \u6709 \u70b9 \u60ac \u554a ~ \u68c0 \u7d22 \u6ca1 \u6709 \u5173 \u7cfb \uff0c \u6211 \u4eec \u52aa \u529b \u5c31 \u53ef \u4ee5 \u4e86 ~ \u8fd9 \u6b21 \u6211 \u4e00 \u4e0b \u5b50 \u62a5 \u4e86 \u4e09 \u95e8 \u79d1 \u76ee \uff0c \u6253 \u7b97 \u4e00 \u4e3e \u62ff \u4e0b \u6559 \u8d44 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u5b66 \u4e60 \u5de5 \u4f5c \u8003 \u8bd5 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u8003 \u8bd5 \u6c5f \u897f \u7701 \u4e0a \u9976 \u5e02 \u4f60 \u4eca \u5e74 \u7684 \u6559 \u5e08 \u8d44 \u683c \u8bc1 \u62a5 \u540d \u4e86 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u4f60 \u62a5 \u4e86 \u4ec0 \u4e48 \u5b66 \u6bb5 \u7684 \u4ec0 \u4e48 \u79d1 \u76ee \u5440 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \u521d \u4e2d \uff0c \u4f46 \u662f \u6211 \u662f \u6570 \u5b66 \u4e13 \u4e1a \u7684 \uff0c \u5c31 \u62a5 \u8003 \u4e86 \u6570 \u5b66 \u7684 \u68c0 \u7d22 \u662f \u7684 \u5bf9 \u4e8e \u5e08 \u8303 \u751f \u6765 \u8bf4 \uff0c \u4ed6 \u4eec \u662f \u6709 \u7ecf \u9a8c \u7684 \uff0c \u54b1 \u4eec \u975e \u5e08 \u8303 \u751f \uff0c \u8fd8 \u662f \u6709 \u70b9 \u60ac \u554a ~ \u68c0 \u7d22 \u6ca1 \u6709 \u5173 \u7cfb \uff0c \u6211 \u4eec \u52aa \u529b \u5c31 \u53ef \u4ee5 \u4e86 ~ \u8fd9 \u6b21 \u6211 \u4e00 \u4e0b \u5b50 \u62a5 \u4e86 \u4e09 \u95e8 \u79d1 \u76ee \uff0c \u6253 \u7b97 \u4e00 \u4e3e \u62ff \u4e0b \u6559 \u8d44 \uff01 \u68c0 \u7d22 \u597d \u7684 \u5440 \uff0c \u8ba9 \u6211 \u4eec \u4e00 \u8d77 \u52aa \u529b \u5427 \u554a ~ \u672a \u6765 \u7684 \u4eba \u6c11 \u6559 \u5e08 ~ \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8f7b \u97f3 \u4e50 \u6e56 \u5357 \u7701 \u682a \u6d32 \u5e02 \u6700 \u8fd1 \u751f \u6d3b \u597d \u70e6 \uff0c \u53ea \u80fd \u9760 \u542c \u97f3 \u4e50 \u7f13 \u89e3 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8f7b \u97f3 \u4e50 \u6e56 \u5357 \u7701 \u682a \u6d32 \u5e02 \u6700 \u8fd1 \u751f \u6d3b \u597d \u70e6 \uff0c \u53ea \u80fd \u9760 \u542c \u97f3 \u4e50 \u7f13 \u89e3 \u68c0 \u7d22 \u662f \u5462 \uff0c \u6211 \u559c \u6b22 \u8f7b \u97f3 \u4e50 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8f7b \u97f3 \u4e50 \u6e56 \u5357 \u7701 \u682a \u6d32 \u5e02 \u6700 \u8fd1 \u751f \u6d3b \u597d \u70e6 \uff0c \u53ea \u80fd \u9760 \u542c \u97f3 \u4e50 \u7f13 \u89e3 \u68c0 \u7d22 \u662f \u5462 \uff0c \u6211 \u559c \u6b22 \u8f7b \u97f3 \u4e50 \u68c0 \u7d22 \u597d \u5440 \uff0c \u56de \u5934 \u6211 \u542c \u542c \uff0c \u4f60 \u70e6 \u7684 \u65f6 \u5019 \u600e \u4e48 \u7f13 \u89e3 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8f7b \u97f3 \u4e50 \u6e56 \u5357 \u7701 \u682a \u6d32 \u5e02 \u6700 \u8fd1 \u751f \u6d3b \u597d \u70e6 \uff0c \u53ea \u80fd \u9760 \u542c \u97f3 \u4e50 \u7f13 \u89e3 \u68c0 \u7d22 \u662f \u5462 \uff0c \u6211 \u559c \u6b22 \u8f7b \u97f3 \u4e50 \u68c0 \u7d22 \u597d \u5440 \uff0c \u56de \u5934 \u6211 \u542c \u542c \uff0c \u4f60 \u70e6 \u7684 \u65f6 \u5019 \u600e \u4e48 \u7f13 \u89e3 \u5440 \u4e0d \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u4e00 \u822c \u53bb \u54ea \u91cc \u8dd1 \u6b65 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8f7b \u97f3 \u4e50 \u6e56 \u5357 \u7701 \u682a \u6d32 \u5e02 \u6700 \u8fd1 \u751f \u6d3b \u597d \u70e6 \uff0c \u53ea \u80fd \u9760 \u542c \u97f3 \u4e50 \u7f13 \u89e3 \u68c0 \u7d22 \u662f \u5462 \uff0c \u6211 \u559c \u6b22 \u8f7b \u97f3 \u4e50 \u68c0 \u7d22 \u597d \u5440 \uff0c \u56de \u5934 \u6211 \u542c \u542c \uff0c \u4f60 \u70e6 \u7684 \u65f6 \u5019 \u600e \u4e48 \u7f13 \u89e3 \u5440 \u4e0d \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u4e00 \u822c \u53bb \u54ea \u91cc \u8dd1 \u6b65 \u5462 \u68c0 \u7d22 \u516c \u56ed \u6e9c \u8fbe \u4e00 \u4e0b \u662f \u4e0d \u9519 \uff0c \u4eca \u5929 \u5c31 \u53bb \u6563 \u6563 \u6b65 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8f7b \u97f3 \u4e50 \u6e56 \u5357 \u7701 \u682a \u6d32 \u5e02 \u6700 \u8fd1 \u751f \u6d3b \u597d \u70e6 \uff0c \u53ea \u80fd \u9760 \u542c \u97f3 \u4e50 \u7f13 \u89e3 \u68c0 \u7d22 \u662f \u5462 \uff0c \u6211 \u559c \u6b22 \u8f7b \u97f3 \u4e50 \u68c0 \u7d22 \u597d \u5440 \uff0c \u56de \u5934 \u6211 \u542c \u542c \uff0c \u4f60 \u70e6 \u7684 \u65f6 \u5019 \u600e \u4e48 \u7f13 \u89e3 \u5440 \u4e0d \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u4e00 \u822c \u53bb \u54ea \u91cc \u8dd1 \u6b65 \u5462 \u68c0 \u7d22 \u516c \u56ed \u6e9c \u8fbe \u4e00 \u4e0b \u662f \u4e0d \u9519 \uff0c \u4eca \u5929 \u5c31 \u53bb \u6563 \u6563 \u6b65 \u68c0 \u7d22 \u989d \u597d \u5427 \uff0c \u90a3 \u6211 \u5c31 \u63a5 \u7740 \u542c \u97f3 \u4e50 \u5427 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u517b \u82b1 \u5409 \u6797 \u7701 \u677e \u539f \u5e02 \u6211 \u60f3 \u517b \u4e00 \u4e9b \u7d2b \u7f57 \u5170 \uff0c \u5b83 \u597d \u517b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u517b \u82b1 \u5409 \u6797 \u7701 \u677e \u539f \u5e02 \u6211 \u60f3 \u517b \u4e00 \u4e9b \u7d2b \u7f57 \u5170 \uff0c \u5b83 \u597d \u517b \u5417 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u90a3 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u5e02 \u573a \u770b \u770b \uff0c \u4f60 \u77e5 \u9053 \u54ea \u91cc \u6709 \u5356 \u8fd9 \u79cd \u82b1 \u7684 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u517b \u82b1 \u5409 \u6797 \u7701 \u677e \u539f \u5e02 \u6211 \u60f3 \u517b \u4e00 \u4e9b \u7d2b \u7f57 \u5170 \uff0c \u5b83 \u597d \u517b \u5417 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u90a3 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u5e02 \u573a \u770b \u770b \uff0c \u4f60 \u77e5 \u9053 \u54ea \u91cc \u6709 \u5356 \u8fd9 \u79cd \u82b1 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u53bb \u4e5f \u4e0d \u80fd \u53ea \u4e70 \u4e00 \u79cd \u82b1 \u5440 \uff0c \u4f60 \u7ed9 \u6211 \u63a8 \u8350 \u51e0 \u79cd \u6bd4 \u8f83 \u597d \u517b \u7684 \u82b1 \u5427 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u517b \u82b1 \u5409 \u6797 \u7701 \u677e \u539f \u5e02 \u6211 \u60f3 \u517b \u4e00 \u4e9b \u7d2b \u7f57 \u5170 \uff0c \u5b83 \u597d \u517b \u5417 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u90a3 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u5e02 \u573a \u770b \u770b \uff0c \u4f60 \u77e5 \u9053 \u54ea \u91cc \u6709 \u5356 \u8fd9 \u79cd \u82b1 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u53bb \u4e5f \u4e0d \u80fd \u53ea \u4e70 \u4e00 \u79cd \u82b1 \u5440 \uff0c \u4f60 \u7ed9 \u6211 \u63a8 \u8350 \u51e0 \u79cd \u6bd4 \u8f83 \u597d \u517b \u7684 \u82b1 \u5427 \u68c0 \u7d22 \u4f60 \u600e \u4e48 \u660e \u767d \u8fd9 \u4e48 \u591a \u82b1 \u5349 \u77e5 \u8bc6 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u517b \u82b1 \u5409 \u6797 \u7701 \u677e \u539f \u5e02 \u6211 \u60f3 \u517b \u4e00 \u4e9b \u7d2b \u7f57 \u5170 \uff0c \u5b83 \u597d \u517b \u5417 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u90a3 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u5e02 \u573a \u770b \u770b \uff0c \u4f60 \u77e5 \u9053 \u54ea \u91cc \u6709 \u5356 \u8fd9 \u79cd \u82b1 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u53bb \u4e5f \u4e0d \u80fd \u53ea \u4e70 \u4e00 \u79cd \u82b1 \u5440 \uff0c \u4f60 \u7ed9 \u6211 \u63a8 \u8350 \u51e0 \u79cd \u6bd4 \u8f83 \u597d \u517b \u7684 \u82b1 \u5427 \u68c0 \u7d22 \u4f60 \u600e \u4e48 \u660e \u767d \u8fd9 \u4e48 \u591a \u82b1 \u5349 \u77e5 \u8bc6 \u554a \uff1f \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6b22 \u8fce \u5149 \u4e34 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u79df \u4e86 \u4e00 \u4e2a \u5355 \u8eab \u7684 \u5c4b \u5b50 \uff0c \u662f \u65f6 \u5019 \u517b \u4e00 \u53ea \u72d7 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u79df \u4e86 \u4e00 \u4e2a \u5355 \u8eab \u7684 \u5c4b \u5b50 \uff0c \u662f \u65f6 \u5019 \u517b \u4e00 \u53ea \u72d7 \u4e86 \u4e0d \u68c0 \u7d22 \u4f60 \u89c9 \u5f97 \u6211 \u517b \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u72d7 \u72d7 \u6bd4 \u8f83 \u597d \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u79df \u4e86 \u4e00 \u4e2a \u5355 \u8eab \u7684 \u5c4b \u5b50 \uff0c \u662f \u65f6 \u5019 \u517b \u4e00 \u53ea \u72d7 \u4e86 \u4e0d \u68c0 \u7d22 \u4f60 \u89c9 \u5f97 \u6211 \u517b \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u72d7 \u72d7 \u6bd4 \u8f83 \u597d \u5462 \u68c0 \u7d22 \u90a3 \u6211 \u53bb \u770b \u770b \u8fd9 \u4e09 \u53ea \uff0c \u7b2c \u4e00 \u6b21 \u517b \u72d7 \u6ca1 \u6709 \u4ec0 \u4e48 \u7ecf \u9a8c \uff0c \u4e0d \u77e5 \u9053 \u8981 \u51c6 \u5907 \u4ec0 \u4e48 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u79df \u4e86 \u4e00 \u4e2a \u5355 \u8eab \u7684 \u5c4b \u5b50 \uff0c \u662f \u65f6 \u5019 \u517b \u4e00 \u53ea \u72d7 \u4e86 \u4e0d \u68c0 \u7d22 \u4f60 \u89c9 \u5f97 \u6211 \u517b \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u72d7 \u72d7 \u6bd4 \u8f83 \u597d \u5462 \u68c0 \u7d22 \u90a3 \u6211 \u53bb \u770b \u770b \u8fd9 \u4e09 \u53ea \uff0c \u7b2c \u4e00 \u6b21 \u517b \u72d7 \u6ca1 \u6709 \u4ec0 \u4e48 \u7ecf \u9a8c \uff0c \u4e0d \u77e5 \u9053 \u8981 \u51c6 \u5907 \u4ec0 \u4e48 \u68c0 \u7d22 \u8fd9 \u4e2a \u6211 \u77e5 \u9053 \u4e86 \uff0c \u72d7 \u7cae \u7684 \u8bdd \u4f60 \u63a8 \u8350 \u4ec0 \u4e48 \u597d \u4e00 \u70b9 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u67f3 \u5dde \u5e02 \u79df \u4e86 \u4e00 \u4e2a \u5355 \u8eab \u7684 \u5c4b \u5b50 \uff0c \u662f \u65f6 \u5019 \u517b \u4e00 \u53ea \u72d7 \u4e86 \u4e0d \u68c0 \u7d22 \u4f60 \u89c9 \u5f97 \u6211 \u517b \u4ec0 \u4e48 \u7c7b \u578b \u7684 \u72d7 \u72d7 \u6bd4 \u8f83 \u597d \u5462 \u68c0 \u7d22 \u90a3 \u6211 \u53bb \u770b \u770b \u8fd9 \u4e09 \u53ea \uff0c \u7b2c \u4e00 \u6b21 \u517b \u72d7 \u6ca1 \u6709 \u4ec0 \u4e48 \u7ecf \u9a8c \uff0c \u4e0d \u77e5 \u9053 \u8981 \u51c6 \u5907 \u4ec0 \u4e48 \u68c0 \u7d22 \u8fd9 \u4e2a \u6211 \u77e5 \u9053 \u4e86 \uff0c \u72d7 \u7cae \u7684 \u8bdd \u4f60 \u63a8 \u8350 \u4ec0 \u4e48 \u597d \u4e00 \u70b9 \u5440 \u68c0 \u7d22 okok \uff0c \u6211 \u8fd9 \u5c31 \u53bb \u505a \u597d \u51c6 \u5907 \u5de5 \u4f5c \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6f14 \u5458 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u5f90 \u5ce5 \u90fd \u6f14 \u8fc7 \u4ec0 \u4e48 \u7535 \u5f71 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6f14 \u5458 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u5f90 \u5ce5 \u90fd \u6f14 \u8fc7 \u4ec0 \u4e48 \u7535 \u5f71 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u524d \u51e0 \u5929 \u770b \u5230 \u4e00 \u5e74 \u4e00 \u5ea6 \u559c \u5267 \u5927 \u8d5b \u4e0a \u9762 \u4ed6 \u6765 \u5f53 \u8bc4 \u59d4 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6f14 \u5458 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u5f90 \u5ce5 \u90fd \u6f14 \u8fc7 \u4ec0 \u4e48 \u7535 \u5f71 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u524d \u51e0 \u5929 \u770b \u5230 \u4e00 \u5e74 \u4e00 \u5ea6 \u559c \u5267 \u5927 \u8d5b \u4e0a \u9762 \u4ed6 \u6765 \u5f53 \u8bc4 \u59d4 \u4e0d \u68c0 \u7d22 \u4ed6 \u7684 \u59bb \u5b50 \u662f \u8c01 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6f14 \u5458 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u5f90 \u5ce5 \u90fd \u6f14 \u8fc7 \u4ec0 \u4e48 \u7535 \u5f71 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u524d \u51e0 \u5929 \u770b \u5230 \u4e00 \u5e74 \u4e00 \u5ea6 \u559c \u5267 \u5927 \u8d5b \u4e0a \u9762 \u4ed6 \u6765 \u5f53 \u8bc4 \u59d4 \u4e0d \u68c0 \u7d22 \u4ed6 \u7684 \u59bb \u5b50 \u662f \u8c01 \u554a \uff1f \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u8fd9 \u4e48 \u5389 \u5bb3 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6f14 \u5458 \u6cb3 \u5357 \u7701 \u5357 \u9633 \u5e02 \u5f90 \u5ce5 \u90fd \u6f14 \u8fc7 \u4ec0 \u4e48 \u7535 \u5f71 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u524d \u51e0 \u5929 \u770b \u5230 \u4e00 \u5e74 \u4e00 \u5ea6 \u559c \u5267 \u5927 \u8d5b \u4e0a \u9762 \u4ed6 \u6765 \u5f53 \u8bc4 \u59d4 \u4e0d \u68c0 \u7d22 \u4ed6 \u7684 \u59bb \u5b50 \u662f \u8c01 \u554a \uff1f \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u8fd9 \u4e48 \u5389 \u5bb3 \u554a \u4e0d \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u53bb \u770b \u7efc \u827a \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u767d \u7f8a \u5ea7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u6cb3 \u6c60 \u5e02 \u6211 \u53d1 \u73b0 \uff0c \u6211 \u7684 \u597d \u670b \u53cb \u90fd \u662f \u767d \u7f8a \u5ea7 \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u767d \u7f8a \u5ea7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u6cb3 \u6c60 \u5e02 \u6211 \u53d1 \u73b0 \uff0c \u6211 \u7684 \u597d \u670b \u53cb \u90fd \u662f \u767d \u7f8a \u5ea7 \u7684 \u68c0 \u7d22 \u6211 \u662f \u5929 \u874e \u5ea7 \u7684 \u5440 \uff5e \u662f \u7684 \u662f \u7684 \uff0c \u6211 \u7684 \u670b \u53cb \u4eec \u5c31 \u662f \u5f88 \u4f18 \u79c0 \uff0c \u6211 \u53ef \u4e3a \u4ed6 \u4eec \u81ea \u8c6a \u5566 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u767d \u7f8a \u5ea7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u6cb3 \u6c60 \u5e02 \u6211 \u53d1 \u73b0 \uff0c \u6211 \u7684 \u597d \u670b \u53cb \u90fd \u662f \u767d \u7f8a \u5ea7 \u7684 \u68c0 \u7d22 \u6211 \u662f \u5929 \u874e \u5ea7 \u7684 \u5440 \uff5e \u662f \u7684 \u662f \u7684 \uff0c \u6211 \u7684 \u670b \u53cb \u4eec \u5c31 \u662f \u5f88 \u4f18 \u79c0 \uff0c \u6211 \u53ef \u4e3a \u4ed6 \u4eec \u81ea \u8c6a \u5566 \u68c0 \u7d22 \u5657 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u4e5f \u662f \uff0c \u5904 \u7740 \u5904 \u7740 \u5c31 \u6210 \u670b \u53cb \u54e5 \u4eec \u4e86 \uff0c \u4e00 \u76f4 \u4e0d \u6765 \u7535 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u767d \u7f8a \u5ea7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u6cb3 \u6c60 \u5e02 \u6211 \u53d1 \u73b0 \uff0c \u6211 \u7684 \u597d \u670b \u53cb \u90fd \u662f \u767d \u7f8a \u5ea7 \u7684 \u68c0 \u7d22 \u6211 \u662f \u5929 \u874e \u5ea7 \u7684 \u5440 \uff5e \u662f \u7684 \u662f \u7684 \uff0c \u6211 \u7684 \u670b \u53cb \u4eec \u5c31 \u662f \u5f88 \u4f18 \u79c0 \uff0c \u6211 \u53ef \u4e3a \u4ed6 \u4eec \u81ea \u8c6a \u5566 \u68c0 \u7d22 \u5657 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u4e5f \u662f \uff0c \u5904 \u7740 \u5904 \u7740 \u5c31 \u6210 \u670b \u53cb \u54e5 \u4eec \u4e86 \uff0c \u4e00 \u76f4 \u4e0d \u6765 \u7535 \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u670b \u53cb \u53ef \u4ee5 \u4e00 \u8f88 \u5b50 \u966a \u4f34 \u5bf9 \u65b9 \uff0c \u604b \u4eba \u4e4b \u95f4 \u561b \uff0c \u8981 \u8003 \u8651 \u7684 \u56e0 \u7d20 \u592a \u591a \u4e86 \uff0c \u8981 \u662f \u7ed3 \u5a5a \u4e86 \uff0c \u4e24 \u4e2a \u5bb6 \u5ead \u8fd8 \u8981 \u63ba \u548c \u8fdb \u6765 \uff0c \u518d \u591a \u7684 \u6fc0 \u60c5 \u7231 \u60c5 \u90fd \u4f1a \u88ab \u751f \u6d3b \u7410 \u4e8b \u6d88 \u78e8 \u6b86 \u5c3d \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u767d \u7f8a \u5ea7 \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u6cb3 \u6c60 \u5e02 \u6211 \u53d1 \u73b0 \uff0c \u6211 \u7684 \u597d \u670b \u53cb \u90fd \u662f \u767d \u7f8a \u5ea7 \u7684 \u68c0 \u7d22 \u6211 \u662f \u5929 \u874e \u5ea7 \u7684 \u5440 \uff5e \u662f \u7684 \u662f \u7684 \uff0c \u6211 \u7684 \u670b \u53cb \u4eec \u5c31 \u662f \u5f88 \u4f18 \u79c0 \uff0c \u6211 \u53ef \u4e3a \u4ed6 \u4eec \u81ea \u8c6a \u5566 \u68c0 \u7d22 \u5657 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u54c8 \u4e5f \u662f \uff0c \u5904 \u7740 \u5904 \u7740 \u5c31 \u6210 \u670b \u53cb \u54e5 \u4eec \u4e86 \uff0c \u4e00 \u76f4 \u4e0d \u6765 \u7535 \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u670b \u53cb \u53ef \u4ee5 \u4e00 \u8f88 \u5b50 \u966a \u4f34 \u5bf9 \u65b9 \uff0c \u604b \u4eba \u4e4b \u95f4 \u561b \uff0c \u8981 \u8003 \u8651 \u7684 \u56e0 \u7d20 \u592a \u591a \u4e86 \uff0c \u8981 \u662f \u7ed3 \u5a5a \u4e86 \uff0c \u4e24 \u4e2a \u5bb6 \u5ead \u8fd8 \u8981 \u63ba \u548c \u8fdb \u6765 \uff0c \u518d \u591a \u7684 \u6fc0 \u60c5 \u7231 \u60c5 \u90fd \u4f1a \u88ab \u751f \u6d3b \u7410 \u4e8b \u6d88 \u78e8 \u6b86 \u5c3d \u4e0d \u68c0 \u7d22 \u5f53 \u7136 \uff0c \u54ea \u4e2a \u604b \u4eba \u4e0d \u662f \u4ece \u670b \u53cb \u5f00 \u59cb \u505a \u8d77 \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u72ee \u5b50 \u5ea7 \u5b81 \u590f \u56de \u65cf \u81ea \u6cbb \u533a \u77f3 \u5634 \u5c71 \u5e02 \u6211 \u5728 \u5fae \u535a \u4e0a \u770b \u661f \u5ea7 \u8fd0 \u52bf \uff0c \u611f \u89c9 \u8fd8 \u633a \u51c6 \u7684 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u72ee \u5b50 \u5ea7 \u5b81 \u590f \u56de \u65cf \u81ea \u6cbb \u533a \u77f3 \u5634 \u5c71 \u5e02 \u6211 \u5728 \u5fae \u535a \u4e0a \u770b \u661f \u5ea7 \u8fd0 \u52bf \uff0c \u611f \u89c9 \u8fd8 \u633a \u51c6 \u7684 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u72ee \u5b50 \u5ea7 \u7684 \uff0c \u8bf4 \u8fd9 \u5468 \u8fd0 \u52bf \u5f88 \u597d \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u72ee \u5b50 \u5ea7 \u5b81 \u590f \u56de \u65cf \u81ea \u6cbb \u533a \u77f3 \u5634 \u5c71 \u5e02 \u6211 \u5728 \u5fae \u535a \u4e0a \u770b \u661f \u5ea7 \u8fd0 \u52bf \uff0c \u611f \u89c9 \u8fd8 \u633a \u51c6 \u7684 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u72ee \u5b50 \u5ea7 \u7684 \uff0c \u8bf4 \u8fd9 \u5468 \u8fd0 \u52bf \u5f88 \u597d \u5462 \u68c0 \u7d22 \u662f \u5417 \uff0c \u8fd8 \u8bf4 \u884c \u52a8 \u529b \u5f3a \u5462 \uff0c \u4e0d \u8fc7 \u6211 \u884c \u52a8 \u529b \u4e00 \u70b9 \u4e5f \u4e0d \u5f3a \u54c8 \u54c8 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u72ee \u5b50 \u5ea7 \u5b81 \u590f \u56de \u65cf \u81ea \u6cbb \u533a \u77f3 \u5634 \u5c71 \u5e02 \u6211 \u5728 \u5fae \u535a \u4e0a \u770b \u661f \u5ea7 \u8fd0 \u52bf \uff0c \u611f \u89c9 \u8fd8 \u633a \u51c6 \u7684 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u72ee \u5b50 \u5ea7 \u7684 \uff0c \u8bf4 \u8fd9 \u5468 \u8fd0 \u52bf \u5f88 \u597d \u5462 \u68c0 \u7d22 \u662f \u5417 \uff0c \u8fd8 \u8bf4 \u884c \u52a8 \u529b \u5f3a \u5462 \uff0c \u4e0d \u8fc7 \u6211 \u884c \u52a8 \u529b \u4e00 \u70b9 \u4e5f \u4e0d \u5f3a \u54c8 \u54c8 \u4e0d \u68c0 \u7d22 \u6211 \u603b \u662f \u62d6 \u5ef6 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u72ee \u5b50 \u5ea7 \u5b81 \u590f \u56de \u65cf \u81ea \u6cbb \u533a \u77f3 \u5634 \u5c71 \u5e02 \u6211 \u5728 \u5fae \u535a \u4e0a \u770b \u661f \u5ea7 \u8fd0 \u52bf \uff0c \u611f \u89c9 \u8fd8 \u633a \u51c6 \u7684 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u72ee \u5b50 \u5ea7 \u7684 \uff0c \u8bf4 \u8fd9 \u5468 \u8fd0 \u52bf \u5f88 \u597d \u5462 \u68c0 \u7d22 \u662f \u5417 \uff0c \u8fd8 \u8bf4 \u884c \u52a8 \u529b \u5f3a \u5462 \uff0c \u4e0d \u8fc7 \u6211 \u884c \u52a8 \u529b \u4e00 \u70b9 \u4e5f \u4e0d \u5f3a \u54c8 \u54c8 \u4e0d \u68c0 \u7d22 \u6211 \u603b \u662f \u62d6 \u5ef6 \u68c0 \u7d22 \u8bd5 \u8fc7 \uff0c \u4e0d \u7ba1 \u7528 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u72ee \u5b50 \u5ea7 \u5b81 \u590f \u56de \u65cf \u81ea \u6cbb \u533a \u77f3 \u5634 \u5c71 \u5e02 \u6211 \u5728 \u5fae \u535a \u4e0a \u770b \u661f \u5ea7 \u8fd0 \u52bf \uff0c \u611f \u89c9 \u8fd8 \u633a \u51c6 \u7684 \u5462 \u4e0d \u68c0 \u7d22 \u6211 \u72ee \u5b50 \u5ea7 \u7684 \uff0c \u8bf4 \u8fd9 \u5468 \u8fd0 \u52bf \u5f88 \u597d \u5462 \u68c0 \u7d22 \u662f \u5417 \uff0c \u8fd8 \u8bf4 \u884c \u52a8 \u529b \u5f3a \u5462 \uff0c \u4e0d \u8fc7 \u6211 \u884c \u52a8 \u529b \u4e00 \u70b9 \u4e5f \u4e0d \u5f3a \u54c8 \u54c8 \u4e0d \u68c0 \u7d22 \u6211 \u603b \u662f \u62d6 \u5ef6 \u68c0 \u7d22 \u8bd5 \u8fc7 \uff0c \u4e0d \u7ba1 \u7528 \u5440 \u68c0 \u7d22 \u884c \u5440 \uff0c \u6709 \u4eba \u76d1 \u7763 \u7740 \u5e94 \u8be5 \u4f1a \u597d \u5462 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u5b89 \u5fbd \u7701 \u94dc \u9675 \u5e02 \u653e \u5bd2 \u5047 \u5566 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u5b89 \u5fbd \u7701 \u94dc \u9675 \u5e02 \u653e \u5bd2 \u5047 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u563f \u563f \uff0c \u56de \u5bb6 \u4e4b \u524d \uff0c \u6211 \u8fd8 \u51c6 \u5907 \u627e \u4e2a \u5730 \u65b9 \u65c5 \u6e38 \uff0c \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u7684 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u5b89 \u5fbd \u7701 \u94dc \u9675 \u5e02 \u653e \u5bd2 \u5047 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u563f \u563f \uff0c \u56de \u5bb6 \u4e4b \u524d \uff0c \u6211 \u8fd8 \u51c6 \u5907 \u627e \u4e2a \u5730 \u65b9 \u65c5 \u6e38 \uff0c \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u6211 \u4e0d \u60f3 \u5728 \u5b89 \u5fbd \u73a9 \u513f \uff0c \u54c8 \u5c14 \u6ee8 \u5462 \uff0c \u54c8 \u5c14 \u6ee8 \u6700 \u8fd1 \u5929 \u6c14 \u5982 \u4f55 \uff0c \u60f3 \u53bb \u51b0 \u96ea \u5927 \u4e16 \u754c \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u5b89 \u5fbd \u7701 \u94dc \u9675 \u5e02 \u653e \u5bd2 \u5047 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u563f \u563f \uff0c \u56de \u5bb6 \u4e4b \u524d \uff0c \u6211 \u8fd8 \u51c6 \u5907 \u627e \u4e2a \u5730 \u65b9 \u65c5 \u6e38 \uff0c \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u6211 \u4e0d \u60f3 \u5728 \u5b89 \u5fbd \u73a9 \u513f \uff0c \u54c8 \u5c14 \u6ee8 \u5462 \uff0c \u54c8 \u5c14 \u6ee8 \u6700 \u8fd1 \u5929 \u6c14 \u5982 \u4f55 \uff0c \u60f3 \u53bb \u51b0 \u96ea \u5927 \u4e16 \u754c \uff01 \u4e0d \u68c0 \u7d22 \u4f60 \u53ef \u771f \u8d34 \u5fc3 \u3002 \u5bf9 \u5566 \uff0c \u51b0 \u96ea \u5927 \u4e16 \u754c \u6700 \u8fd1 \u5f00 \u653e \u5417 \uff1f \u522b \u767d \u8dd1 \u4e00 \u8d9f \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u5b89 \u5fbd \u7701 \u94dc \u9675 \u5e02 \u653e \u5bd2 \u5047 \u5566 \uff01 \u4e0d \u68c0 \u7d22 \u563f \u563f \uff0c \u56de \u5bb6 \u4e4b \u524d \uff0c \u6211 \u8fd8 \u51c6 \u5907 \u627e \u4e2a \u5730 \u65b9 \u65c5 \u6e38 \uff0c \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u7684 \u5417 \uff1f \u68c0 \u7d22 \u6211 \u4e0d \u60f3 \u5728 \u5b89 \u5fbd \u73a9 \u513f \uff0c \u54c8 \u5c14 \u6ee8 \u5462 \uff0c \u54c8 \u5c14 \u6ee8 \u6700 \u8fd1 \u5929 \u6c14 \u5982 \u4f55 \uff0c \u60f3 \u53bb \u51b0 \u96ea \u5927 \u4e16 \u754c \uff01 \u4e0d \u68c0 \u7d22 \u4f60 \u53ef \u771f \u8d34 \u5fc3 \u3002 \u5bf9 \u5566 \uff0c \u51b0 \u96ea \u5927 \u4e16 \u754c \u6700 \u8fd1 \u5f00 \u653e \u5417 \uff1f \u522b \u767d \u8dd1 \u4e00 \u8d9f \u3002 \u4e0d \u68c0 \u7d22 \u592a \u68d2 \u4e86 \uff0c \u73b0 \u5728 \u5c31 \u4e70 \u7968 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u8bb8 \u5d69 \u5b89 \u5fbd \u7701 \u5ba3 \u57ce \u5e02 \u8bb8 \u5d69 \u597d \u4f4e \u8c03 \u800c \u4e14 \u597d \u6709 \u624d \u534e \uff0c \u5c0f \u65f6 \u5019 \u5c31 \u8ba4 \u8bc6 \u4ed6 \uff0c \u73b0 \u5728 \u8fd8 \u5f88 \u4f4e \u8c03 \u7684 \u51fa \u6b4c \u5462 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u8bb8 \u5d69 \u5b89 \u5fbd \u7701 \u5ba3 \u57ce \u5e02 \u8bb8 \u5d69 \u597d \u4f4e \u8c03 \u800c \u4e14 \u597d \u6709 \u624d \u534e \uff0c \u5c0f \u65f6 \u5019 \u5c31 \u8ba4 \u8bc6 \u4ed6 \uff0c \u73b0 \u5728 \u8fd8 \u5f88 \u4f4e \u8c03 \u7684 \u51fa \u6b4c \u5462 \u3002 \u68c0 \u7d22 \u4ed6 \u6700 \u8fd1 \u53bb \u592e \u89c6 \u7684 \u300a \u7ecf \u5178 \u548f \u6d41 \u4f20 \u300b \u4e2d \u5531 \u4e86 \u4e00 \u9996 \u300a \u6628 \u591c \u4e66 \u300b \uff0c \u7279 \u522b \u597d \u542c \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u8bb8 \u5d69 \u5b89 \u5fbd \u7701 \u5ba3 \u57ce \u5e02 \u8bb8 \u5d69 \u597d \u4f4e \u8c03 \u800c \u4e14 \u597d \u6709 \u624d \u534e \uff0c \u5c0f \u65f6 \u5019 \u5c31 \u8ba4 \u8bc6 \u4ed6 \uff0c \u73b0 \u5728 \u8fd8 \u5f88 \u4f4e \u8c03 \u7684 \u51fa \u6b4c \u5462 \u3002 \u68c0 \u7d22 \u4ed6 \u6700 \u8fd1 \u53bb \u592e \u89c6 \u7684 \u300a \u7ecf \u5178 \u548f \u6d41 \u4f20 \u300b \u4e2d \u5531 \u4e86 \u4e00 \u9996 \u300a \u6628 \u591c \u4e66 \u300b \uff0c \u7279 \u522b \u597d \u542c \u3002 \u68c0 \u7d22 \u6211 \u542c \u8d77 \u6765 \u9e21 \u76ae \u7599 \u7629 \u90fd \u8d77 \u6765 \u4e86 \uff0c \u80fd \u611f \u53d7 \u5230 \u5cb3 \u98de \u7684 \u58ee \u5fd7 \u672a \u916c \u7684 \u60b2 \u82e6 \u5fc3 \u5883 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u8bb8 \u5d69 \u5b89 \u5fbd \u7701 \u5ba3 \u57ce \u5e02 \u8bb8 \u5d69 \u597d \u4f4e \u8c03 \u800c \u4e14 \u597d \u6709 \u624d \u534e \uff0c \u5c0f \u65f6 \u5019 \u5c31 \u8ba4 \u8bc6 \u4ed6 \uff0c \u73b0 \u5728 \u8fd8 \u5f88 \u4f4e \u8c03 \u7684 \u51fa \u6b4c \u5462 \u3002 \u68c0 \u7d22 \u4ed6 \u6700 \u8fd1 \u53bb \u592e \u89c6 \u7684 \u300a \u7ecf \u5178 \u548f \u6d41 \u4f20 \u300b \u4e2d \u5531 \u4e86 \u4e00 \u9996 \u300a \u6628 \u591c \u4e66 \u300b \uff0c \u7279 \u522b \u597d \u542c \u3002 \u68c0 \u7d22 \u6211 \u542c \u8d77 \u6765 \u9e21 \u76ae \u7599 \u7629 \u90fd \u8d77 \u6765 \u4e86 \uff0c \u80fd \u611f \u53d7 \u5230 \u5cb3 \u98de \u7684 \u58ee \u5fd7 \u672a \u916c \u7684 \u60b2 \u82e6 \u5fc3 \u5883 \u3002 \u4e0d \u68c0 \u7d22 \u8bb8 \u5d69 \u901a \u8fc7 \u8fd9 \u9996 \u6b4c \uff0c \u7ed9 \u6211 \u4eec \u5c55 \u793a \u4e86 \u5cb3 \u98de \u7684 \u53e6 \u4e00 \u9762 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u8bb8 \u5d69 \u5b89 \u5fbd \u7701 \u5ba3 \u57ce \u5e02 \u8bb8 \u5d69 \u597d \u4f4e \u8c03 \u800c \u4e14 \u597d \u6709 \u624d \u534e \uff0c \u5c0f \u65f6 \u5019 \u5c31 \u8ba4 \u8bc6 \u4ed6 \uff0c \u73b0 \u5728 \u8fd8 \u5f88 \u4f4e \u8c03 \u7684 \u51fa \u6b4c \u5462 \u3002 \u68c0 \u7d22 \u4ed6 \u6700 \u8fd1 \u53bb \u592e \u89c6 \u7684 \u300a \u7ecf \u5178 \u548f \u6d41 \u4f20 \u300b \u4e2d \u5531 \u4e86 \u4e00 \u9996 \u300a \u6628 \u591c \u4e66 \u300b \uff0c \u7279 \u522b \u597d \u542c \u3002 \u68c0 \u7d22 \u6211 \u542c \u8d77 \u6765 \u9e21 \u76ae \u7599 \u7629 \u90fd \u8d77 \u6765 \u4e86 \uff0c \u80fd \u611f \u53d7 \u5230 \u5cb3 \u98de \u7684 \u58ee \u5fd7 \u672a \u916c \u7684 \u60b2 \u82e6 \u5fc3 \u5883 \u3002 \u4e0d \u68c0 \u7d22 \u8bb8 \u5d69 \u901a \u8fc7 \u8fd9 \u9996 \u6b4c \uff0c \u7ed9 \u6211 \u4eec \u5c55 \u793a \u4e86 \u5cb3 \u98de \u7684 \u53e6 \u4e00 \u9762 \u3002 \u68c0 \u7d22 \u6211 \u8fd8 \u5f88 \u559c \u6b22 \u4ed6 \u7684 \u300a \u96e8 \u5e55 \u300b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6b4c \u624b \u8bb8 \u5d69 \u5b89 \u5fbd \u7701 \u5ba3 \u57ce \u5e02 \u8bb8 \u5d69 \u597d \u4f4e \u8c03 \u800c \u4e14 \u597d \u6709 \u624d \u534e \uff0c \u5c0f \u65f6 \u5019 \u5c31 \u8ba4 \u8bc6 \u4ed6 \uff0c \u73b0 \u5728 \u8fd8 \u5f88 \u4f4e \u8c03 \u7684 \u51fa \u6b4c \u5462 \u3002 \u68c0 \u7d22 \u4ed6 \u6700 \u8fd1 \u53bb \u592e \u89c6 \u7684 \u300a \u7ecf \u5178 \u548f \u6d41 \u4f20 \u300b \u4e2d \u5531 \u4e86 \u4e00 \u9996 \u300a \u6628 \u591c \u4e66 \u300b \uff0c \u7279 \u522b \u597d \u542c \u3002 \u68c0 \u7d22 \u6211 \u542c \u8d77 \u6765 \u9e21 \u76ae \u7599 \u7629 \u90fd \u8d77 \u6765 \u4e86 \uff0c \u80fd \u611f \u53d7 \u5230 \u5cb3 \u98de \u7684 \u58ee \u5fd7 \u672a \u916c \u7684 \u60b2 \u82e6 \u5fc3 \u5883 \u3002 \u4e0d \u68c0 \u7d22 \u8bb8 \u5d69 \u901a \u8fc7 \u8fd9 \u9996 \u6b4c \uff0c \u7ed9 \u6211 \u4eec \u5c55 \u793a \u4e86 \u5cb3 \u98de \u7684 \u53e6 \u4e00 \u9762 \u3002 \u68c0 \u7d22 \u6211 \u8fd8 \u5f88 \u559c \u6b22 \u4ed6 \u7684 \u300a \u96e8 \u5e55 \u300b \u3002 \u68c0 \u7d22 \u4f5c \u4e3a \u4ed6 \u7684 \u5fe0 \u5b9e \u7c89 \u4e1d \uff0c \u6211 \u90fd \u542c \u8fc7 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u8fbd \u5b81 \u7701 \u76d8 \u9526 \u5e02 \u661f \u5ea7 \u5206 \u6790 \u8bf4 \u6211 \u6700 \u8fd1 \u6c34 \u9006 \uff0c \u4f60 \u5462 \uff1f \u8fd9 \u4e2a \u6708 \u8fd0 \u52bf \u5982 \u4f55 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u8fbd \u5b81 \u7701 \u76d8 \u9526 \u5e02 \u661f \u5ea7 \u5206 \u6790 \u8bf4 \u6211 \u6700 \u8fd1 \u6c34 \u9006 \uff0c \u4f60 \u5462 \uff1f \u8fd9 \u4e2a \u6708 \u8fd0 \u52bf \u5982 \u4f55 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u4f60 \u6ca1 \u6709 \u5bf9 \u8c61 \uff0c \u8fd8 \u5173 \u5fc3 \u7231 \u60c5 \u8fd0 \u52bf \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u8fbd \u5b81 \u7701 \u76d8 \u9526 \u5e02 \u661f \u5ea7 \u5206 \u6790 \u8bf4 \u6211 \u6700 \u8fd1 \u6c34 \u9006 \uff0c \u4f60 \u5462 \uff1f \u8fd9 \u4e2a \u6708 \u8fd0 \u52bf \u5982 \u4f55 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u4f60 \u6ca1 \u6709 \u5bf9 \u8c61 \uff0c \u8fd8 \u5173 \u5fc3 \u7231 \u60c5 \u8fd0 \u52bf \u3002 \u4e0d \u68c0 \u7d22 \u611f \u60c5 \u3001 \u5de5 \u4f5c \u90fd \u6709 \u574e \u5777 \uff0c \u96be \u602a \u6211 \u6700 \u8fd1 \u548c \u6211 \u7537 \u670b \u53cb \u8001 \u5435 \u67b6 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u8fbd \u5b81 \u7701 \u76d8 \u9526 \u5e02 \u661f \u5ea7 \u5206 \u6790 \u8bf4 \u6211 \u6700 \u8fd1 \u6c34 \u9006 \uff0c \u4f60 \u5462 \uff1f \u8fd9 \u4e2a \u6708 \u8fd0 \u52bf \u5982 \u4f55 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u4f60 \u6ca1 \u6709 \u5bf9 \u8c61 \uff0c \u8fd8 \u5173 \u5fc3 \u7231 \u60c5 \u8fd0 \u52bf \u3002 \u4e0d \u68c0 \u7d22 \u611f \u60c5 \u3001 \u5de5 \u4f5c \u90fd \u6709 \u574e \u5777 \uff0c \u96be \u602a \u6211 \u6700 \u8fd1 \u548c \u6211 \u7537 \u670b \u53cb \u8001 \u5435 \u67b6 \u3002 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u4f60 \u8fd8 \u771f \u662f \u4e2d \u56fd \u4eba \uff0c \u597d \u7684 \u5c31 \u4fe1 \uff0c \u4e0d \u8981 \u7684 \u5168 \u662f \u5c01 \u5efa \u8ff7 \u4fe1 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u8fbd \u5b81 \u7701 \u76d8 \u9526 \u5e02 \u661f \u5ea7 \u5206 \u6790 \u8bf4 \u6211 \u6700 \u8fd1 \u6c34 \u9006 \uff0c \u4f60 \u5462 \uff1f \u8fd9 \u4e2a \u6708 \u8fd0 \u52bf \u5982 \u4f55 \uff1f \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u4f60 \u6ca1 \u6709 \u5bf9 \u8c61 \uff0c \u8fd8 \u5173 \u5fc3 \u7231 \u60c5 \u8fd0 \u52bf \u3002 \u4e0d \u68c0 \u7d22 \u611f \u60c5 \u3001 \u5de5 \u4f5c \u90fd \u6709 \u574e \u5777 \uff0c \u96be \u602a \u6211 \u6700 \u8fd1 \u548c \u6211 \u7537 \u670b \u53cb \u8001 \u5435 \u67b6 \u3002 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u4f60 \u8fd8 \u771f \u662f \u4e2d \u56fd \u4eba \uff0c \u597d \u7684 \u5c31 \u4fe1 \uff0c \u4e0d \u8981 \u7684 \u5168 \u662f \u5c01 \u5efa \u8ff7 \u4fe1 \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u6211 \u6700 \u8fd1 \u548c \u6211 \u7537 \u670b \u53cb \u8001 \u5435 \u67b6 \u4ed6 \u8bf4 \u633a \u51c6 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u8fea \u58eb \u5c3c \u4e0a \u6d77 \u8fea \u58eb \u5c3c \u4e50 \u56ed \u8fea \u58eb \u5c3c \u4e50 \u56ed \u548c \u73af \u7403 \u5f71 \u57ce \uff0c \u54ea \u4e00 \u4e2a \u66f4 \u597d \u554a \uff0c \u60f3 \u5e26 \u540c \u5b66 \u51fa \u53bb \u73a9 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u8fea \u58eb \u5c3c \u4e0a \u6d77 \u8fea \u58eb \u5c3c \u4e50 \u56ed \u8fea \u58eb \u5c3c \u4e50 \u56ed \u548c \u73af \u7403 \u5f71 \u57ce \uff0c \u54ea \u4e00 \u4e2a \u66f4 \u597d \u554a \uff0c \u60f3 \u5e26 \u540c \u5b66 \u51fa \u53bb \u73a9 \u68c0 \u7d22 \u9002 \u5408 \u5e26 \u5973 \u670b \u53cb \u53bb \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u8fea \u58eb \u5c3c \u4e0a \u6d77 \u8fea \u58eb \u5c3c \u4e50 \u56ed \u8fea \u58eb \u5c3c \u4e50 \u56ed \u548c \u73af \u7403 \u5f71 \u57ce \uff0c \u54ea \u4e00 \u4e2a \u66f4 \u597d \u554a \uff0c \u60f3 \u5e26 \u540c \u5b66 \u51fa \u53bb \u73a9 \u68c0 \u7d22 \u9002 \u5408 \u5e26 \u5973 \u670b \u53cb \u53bb \u5417 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u90a3 \u5c31 \u592a \u597d \u4e86 \uff0c \u6709 \u5565 \u63a8 \u8350 \u7684 \u8fea \u58eb \u5c3c \u6d3b \u52a8 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u8fea \u58eb \u5c3c \u4e0a \u6d77 \u8fea \u58eb \u5c3c \u4e50 \u56ed \u8fea \u58eb \u5c3c \u4e50 \u56ed \u548c \u73af \u7403 \u5f71 \u57ce \uff0c \u54ea \u4e00 \u4e2a \u66f4 \u597d \u554a \uff0c \u60f3 \u5e26 \u540c \u5b66 \u51fa \u53bb \u73a9 \u68c0 \u7d22 \u9002 \u5408 \u5e26 \u5973 \u670b \u53cb \u53bb \u5417 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u90a3 \u5c31 \u592a \u597d \u4e86 \uff0c \u6709 \u5565 \u63a8 \u8350 \u7684 \u8fea \u58eb \u5c3c \u6d3b \u52a8 \u5417 \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u90a3 \u592a \u597d \u4e86 \uff0c \u5973 \u670b \u53cb \u6700 \u559c \u6b22 \u770b \u70df \u82b1 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u666f \u70b9 \u8fea \u58eb \u5c3c \u4e0a \u6d77 \u8fea \u58eb \u5c3c \u4e50 \u56ed \u8fea \u58eb \u5c3c \u4e50 \u56ed \u548c \u73af \u7403 \u5f71 \u57ce \uff0c \u54ea \u4e00 \u4e2a \u66f4 \u597d \u554a \uff0c \u60f3 \u5e26 \u540c \u5b66 \u51fa \u53bb \u73a9 \u68c0 \u7d22 \u9002 \u5408 \u5e26 \u5973 \u670b \u53cb \u53bb \u5417 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u90a3 \u5c31 \u592a \u597d \u4e86 \uff0c \u6709 \u5565 \u63a8 \u8350 \u7684 \u8fea \u58eb \u5c3c \u6d3b \u52a8 \u5417 \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \u90a3 \u592a \u597d \u4e86 \uff0c \u5973 \u670b \u53cb \u6700 \u559c \u6b22 \u770b \u70df \u82b1 \u4e86 \u68c0 \u7d22 \u597d \u7684 \uff0c \u8fd9 \u4e2a \u5728 \u54ea \u91cc \u552e \u7968 \u554a \uff0c \u6211 \u60f3 \u5148 \u9884 \u5b9a \u4e24 \u5f20 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6cb9 \u5c16 \u65fa \u533a \u6700 \u8fd1 \u73a9 \u9ebb \u5c06 \u4e0a \u763e \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6cb9 \u5c16 \u65fa \u533a \u6700 \u8fd1 \u73a9 \u9ebb \u5c06 \u4e0a \u763e \u4e0d \u68c0 \u7d22 \u624b \u673a \u5fae \u4fe1 \u5c0f \u7a0b \u5e8f \u817e \u8baf \u9ebb \u5c06 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6cb9 \u5c16 \u65fa \u533a \u6700 \u8fd1 \u73a9 \u9ebb \u5c06 \u4e0a \u763e \u4e0d \u68c0 \u7d22 \u624b \u673a \u5fae \u4fe1 \u5c0f \u7a0b \u5e8f \u817e \u8baf \u9ebb \u5c06 \u4e0d \u68c0 \u7d22 \u5c31 \u63a8 \u5012 \u80e1 \uff0c \u8fd9 \u548c \u56db \u5ddd \u9ebb \u5c06 \u6709 \u4ec0 \u4e48 \u533a \u522b \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6cb9 \u5c16 \u65fa \u533a \u6700 \u8fd1 \u73a9 \u9ebb \u5c06 \u4e0a \u763e \u4e0d \u68c0 \u7d22 \u624b \u673a \u5fae \u4fe1 \u5c0f \u7a0b \u5e8f \u817e \u8baf \u9ebb \u5c06 \u4e0d \u68c0 \u7d22 \u5c31 \u63a8 \u5012 \u80e1 \uff0c \u8fd9 \u548c \u56db \u5ddd \u9ebb \u5c06 \u6709 \u4ec0 \u4e48 \u533a \u522b \u5440 \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u73a9 \u8fc7 \u56db \u5ddd \u9ebb \u5c06 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6cb9 \u5c16 \u65fa \u533a \u6700 \u8fd1 \u73a9 \u9ebb \u5c06 \u4e0a \u763e \u4e0d \u68c0 \u7d22 \u624b \u673a \u5fae \u4fe1 \u5c0f \u7a0b \u5e8f \u817e \u8baf \u9ebb \u5c06 \u4e0d \u68c0 \u7d22 \u5c31 \u63a8 \u5012 \u80e1 \uff0c \u8fd9 \u548c \u56db \u5ddd \u9ebb \u5c06 \u6709 \u4ec0 \u4e48 \u533a \u522b \u5440 \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u73a9 \u8fc7 \u56db \u5ddd \u9ebb \u5c06 \u68c0 \u7d22 \u4e0d \u61c2 \u4e0d \u61c2 \uff0c \u6211 \u5f97 \u53bb \u73a9 \u4e24 \u628a \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u7ea2 \u697c \u68a6 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6700 \u8fd1 \u53c8 \u91cd \u6e29 \u4e86 \u4e00 \u904d \u300a \u7ea2 \u697c \u68a6 \u300b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u7ea2 \u697c \u68a6 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6700 \u8fd1 \u53c8 \u91cd \u6e29 \u4e86 \u4e00 \u904d \u300a \u7ea2 \u697c \u68a6 \u300b \u3002 \u4e0d \u68c0 \u7d22 \u5bf9 \uff0c \u8fd9 \u6b21 \u8bfb \u5b8c \u53c8 \u6709 \u65b0 \u7684 \u611f \u53d7 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u7ea2 \u697c \u68a6 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6700 \u8fd1 \u53c8 \u91cd \u6e29 \u4e86 \u4e00 \u904d \u300a \u7ea2 \u697c \u68a6 \u300b \u3002 \u4e0d \u68c0 \u7d22 \u5bf9 \uff0c \u8fd9 \u6b21 \u8bfb \u5b8c \u53c8 \u6709 \u65b0 \u7684 \u611f \u53d7 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4ee5 \u524d \u4e0d \u559c \u6b22 \u6797 \u9edb \u7389 \u3002 \u4f46 \u662f \u73b0 \u5728 \u770b \u770b \u6797 \u9edb \u7389 \u662f \u975e \u5e38 \u597d \u7684 \u4e00 \u4e2a \u59d1 \u5a18 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u7ea2 \u697c \u68a6 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6700 \u8fd1 \u53c8 \u91cd \u6e29 \u4e86 \u4e00 \u904d \u300a \u7ea2 \u697c \u68a6 \u300b \u3002 \u4e0d \u68c0 \u7d22 \u5bf9 \uff0c \u8fd9 \u6b21 \u8bfb \u5b8c \u53c8 \u6709 \u65b0 \u7684 \u611f \u53d7 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4ee5 \u524d \u4e0d \u559c \u6b22 \u6797 \u9edb \u7389 \u3002 \u4f46 \u662f \u73b0 \u5728 \u770b \u770b \u6797 \u9edb \u7389 \u662f \u975e \u5e38 \u597d \u7684 \u4e00 \u4e2a \u59d1 \u5a18 \u3002 \u68c0 \u7d22 \u5bf9 \u7684 \u5176 \u5b9e \u4ed6 \u662f \u4e00 \u4e2a \u975e \u5e38 \u597d \u7684 \u59d1 \u5a18 \u3002 \u53ea \u4e0d \u8fc7 \u6709 \u4e00 \u4e9b \u604b \u7231 \u8111 \u3002 \u4ed6 \u6240 \u6709 \u7684 \u9488 \u5bf9 \u90fd \u662f \u56e0 \u4e3a \u611f \u89c9 \u81ea \u5df1 \u7684 \u7231 \u60c5 \u53d7 \u5230 \u4e86 \u5a01 \u80c1 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5c0f \u8bf4 \u7ea2 \u697c \u68a6 \u7518 \u8083 \u7701 \u6b66 \u5a01 \u5e02 \u6700 \u8fd1 \u53c8 \u91cd \u6e29 \u4e86 \u4e00 \u904d \u300a \u7ea2 \u697c \u68a6 \u300b \u3002 \u4e0d \u68c0 \u7d22 \u5bf9 \uff0c \u8fd9 \u6b21 \u8bfb \u5b8c \u53c8 \u6709 \u65b0 \u7684 \u611f \u53d7 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4ee5 \u524d \u4e0d \u559c \u6b22 \u6797 \u9edb \u7389 \u3002 \u4f46 \u662f \u73b0 \u5728 \u770b \u770b \u6797 \u9edb \u7389 \u662f \u975e \u5e38 \u597d \u7684 \u4e00 \u4e2a \u59d1 \u5a18 \u3002 \u68c0 \u7d22 \u5bf9 \u7684 \u5176 \u5b9e \u4ed6 \u662f \u4e00 \u4e2a \u975e \u5e38 \u597d \u7684 \u59d1 \u5a18 \u3002 \u53ea \u4e0d \u8fc7 \u6709 \u4e00 \u4e9b \u604b \u7231 \u8111 \u3002 \u4ed6 \u6240 \u6709 \u7684 \u9488 \u5bf9 \u90fd \u662f \u56e0 \u4e3a \u611f \u89c9 \u81ea \u5df1 \u7684 \u7231 \u60c5 \u53d7 \u5230 \u4e86 \u5a01 \u80c1 \u3002 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u8d3e \u5b9d \u7389 \u4e0d \u4ec5 \u6ca1 \u51fa \u606f \u800c \u4e14 \u4e00 \u70b9 \u90fd \u4e0d \u4e13 \u4e00 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u504f \u5934 \u75db \u6d59 \u6c5f \u7701 \u8862 \u5dde \u5e02 \u554a \u554a \uff0c \u6211 \u7684 \u5934 \u554a \uff0c \u6700 \u8fd1 \u504f \u5934 \u75db \u6709 \u72af \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u504f \u5934 \u75db \u6d59 \u6c5f \u7701 \u8862 \u5dde \u5e02 \u554a \u554a \uff0c \u6211 \u7684 \u5934 \u554a \uff0c \u6700 \u8fd1 \u504f \u5934 \u75db \u6709 \u72af \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u54ce \u6ca1 \u7528 \u7684 \uff0c \u533b \u751f \u591a \u534a \u4e5f \u77a7 \u4e0d \u51fa \u4ec0 \u4e48 \u6bdb \u75c5 \u6765 \u3002 \u8fd9 \u75c5 \u6765 \u5f97 \u5feb \uff0c \u53bb \u5f97 \u4e5f \u5feb \u3002 \u6709 \u65f6 \u5019 \u7761 \u4e00 \u89c9 \u5c31 \u597d \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u504f \u5934 \u75db \u6d59 \u6c5f \u7701 \u8862 \u5dde \u5e02 \u554a \u554a \uff0c \u6211 \u7684 \u5934 \u554a \uff0c \u6700 \u8fd1 \u504f \u5934 \u75db \u6709 \u72af \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u54ce \u6ca1 \u7528 \u7684 \uff0c \u533b \u751f \u591a \u534a \u4e5f \u77a7 \u4e0d \u51fa \u4ec0 \u4e48 \u6bdb \u75c5 \u6765 \u3002 \u8fd9 \u75c5 \u6765 \u5f97 \u5feb \uff0c \u53bb \u5f97 \u4e5f \u5feb \u3002 \u6709 \u65f6 \u5019 \u7761 \u4e00 \u89c9 \u5c31 \u597d \u3002 \u68c0 \u7d22 \u8fd9 \u4e48 \u4e00 \u8bf4 \uff0c \u597d \u50cf \u7761 \u7720 \u4e0d \u591f \u7684 \u65f6 \u5019 \u5c31 \u5bb9 \u6613 \u72af \u75c5 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u504f \u5934 \u75db \u6d59 \u6c5f \u7701 \u8862 \u5dde \u5e02 \u554a \u554a \uff0c \u6211 \u7684 \u5934 \u554a \uff0c \u6700 \u8fd1 \u504f \u5934 \u75db \u6709 \u72af \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u54ce \u6ca1 \u7528 \u7684 \uff0c \u533b \u751f \u591a \u534a \u4e5f \u77a7 \u4e0d \u51fa \u4ec0 \u4e48 \u6bdb \u75c5 \u6765 \u3002 \u8fd9 \u75c5 \u6765 \u5f97 \u5feb \uff0c \u53bb \u5f97 \u4e5f \u5feb \u3002 \u6709 \u65f6 \u5019 \u7761 \u4e00 \u89c9 \u5c31 \u597d \u3002 \u68c0 \u7d22 \u8fd9 \u4e48 \u4e00 \u8bf4 \uff0c \u597d \u50cf \u7761 \u7720 \u4e0d \u591f \u7684 \u65f6 \u5019 \u5c31 \u5bb9 \u6613 \u72af \u75c5 \u3002 \u68c0 \u7d22 \u6709 \u9053 \u7406 \uff0c \u4eca \u5929 \u5f00 \u59cb 11 \u70b9 \u524d \u5c31 \u4e0a \u5e8a \u7761 \u89c9 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u504f \u5934 \u75db \u6d59 \u6c5f \u7701 \u8862 \u5dde \u5e02 \u554a \u554a \uff0c \u6211 \u7684 \u5934 \u554a \uff0c \u6700 \u8fd1 \u504f \u5934 \u75db \u6709 \u72af \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u54ce \u6ca1 \u7528 \u7684 \uff0c \u533b \u751f \u591a \u534a \u4e5f \u77a7 \u4e0d \u51fa \u4ec0 \u4e48 \u6bdb \u75c5 \u6765 \u3002 \u8fd9 \u75c5 \u6765 \u5f97 \u5feb \uff0c \u53bb \u5f97 \u4e5f \u5feb \u3002 \u6709 \u65f6 \u5019 \u7761 \u4e00 \u89c9 \u5c31 \u597d \u3002 \u68c0 \u7d22 \u8fd9 \u4e48 \u4e00 \u8bf4 \uff0c \u597d \u50cf \u7761 \u7720 \u4e0d \u591f \u7684 \u65f6 \u5019 \u5c31 \u5bb9 \u6613 \u72af \u75c5 \u3002 \u68c0 \u7d22 \u6709 \u9053 \u7406 \uff0c \u4eca \u5929 \u5f00 \u59cb 11 \u70b9 \u524d \u5c31 \u4e0a \u5e8a \u7761 \u89c9 \uff01 \u4e0d \u68c0 \u7d22 \u53cd \u6b63 11 \u70b9 12 \u70b9 \u4e5f \u5dee \u4e0d \u4e86 \u591a \u5c11 \uff0c \u5c31 12 \u70b9 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u6211 \u5047 \u671f \u5728 \u5bb6 \u91cc \u603b \u662f \u88ab \u6279 \u8bc4 \u4e0d \u4f1a \u505a \u83dc \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u6211 \u5047 \u671f \u5728 \u5bb6 \u91cc \u603b \u662f \u88ab \u6279 \u8bc4 \u4e0d \u4f1a \u505a \u83dc \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4e3a \u4e86 \u4ed6 \u4eec \u5f00 \u5fc3 \u6211 \u51b3 \u5b9a \u5b66 \u505a \u51e0 \u4e2a \u83dc \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u6211 \u5047 \u671f \u5728 \u5bb6 \u91cc \u603b \u662f \u88ab \u6279 \u8bc4 \u4e0d \u4f1a \u505a \u83dc \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4e3a \u4e86 \u4ed6 \u4eec \u5f00 \u5fc3 \u6211 \u51b3 \u5b9a \u5b66 \u505a \u51e0 \u4e2a \u83dc \u68c0 \u7d22 \u597d \u4e3b \u610f \uff0c \u6211 \u60f3 \u4ed6 \u4eec \u4f1a \u5f00 \u5fc3 \u7684 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u6211 \u5047 \u671f \u5728 \u5bb6 \u91cc \u603b \u662f \u88ab \u6279 \u8bc4 \u4e0d \u4f1a \u505a \u83dc \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4e3a \u4e86 \u4ed6 \u4eec \u5f00 \u5fc3 \u6211 \u51b3 \u5b9a \u5b66 \u505a \u51e0 \u4e2a \u83dc \u68c0 \u7d22 \u597d \u4e3b \u610f \uff0c \u6211 \u60f3 \u4ed6 \u4eec \u4f1a \u5f00 \u5fc3 \u7684 \u4e0d \u68c0 \u7d22 \u6bd4 \u5982 \u9178 \u8fa3 \u571f \u8c46 \u4e1d \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u6e56 \u5317 \u7701 \u9ec4 \u77f3 \u5e02 \u6211 \u5047 \u671f \u5728 \u5bb6 \u91cc \u603b \u662f \u88ab \u6279 \u8bc4 \u4e0d \u4f1a \u505a \u83dc \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4e3a \u4e86 \u4ed6 \u4eec \u5f00 \u5fc3 \u6211 \u51b3 \u5b9a \u5b66 \u505a \u51e0 \u4e2a \u83dc \u68c0 \u7d22 \u597d \u4e3b \u610f \uff0c \u6211 \u60f3 \u4ed6 \u4eec \u4f1a \u5f00 \u5fc3 \u7684 \u4e0d \u68c0 \u7d22 \u6bd4 \u5982 \u9178 \u8fa3 \u571f \u8c46 \u4e1d \u5417 \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u786e \u5b9e \u5f88 \u7b80 \u5355 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6cf0 \u8fea \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u9000 \u4f11 \u5728 \u5bb6 \u5f88 \u65e0 \u804a \u554a \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u5e72 \u70b9 \u4ec0 \u4e48 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6cf0 \u8fea \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u9000 \u4f11 \u5728 \u5bb6 \u5f88 \u65e0 \u804a \u554a \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u5e72 \u70b9 \u4ec0 \u4e48 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u5fc3 \u7406 \u5065 \u5eb7 \u5f88 \u91cd \u8981 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u597d \u7684 \u5efa \u8bae \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6cf0 \u8fea \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u9000 \u4f11 \u5728 \u5bb6 \u5f88 \u65e0 \u804a \u554a \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u5e72 \u70b9 \u4ec0 \u4e48 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u5fc3 \u7406 \u5065 \u5eb7 \u5f88 \u91cd \u8981 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u597d \u7684 \u5efa \u8bae \u5417 \uff1f \u68c0 \u7d22 \u5176 \u5b9e \u6211 \u4e5f \u60f3 \u8fc7 \u517b \u53ea \u72d7 \uff0c \u4f46 \u662f \u6211 \u6015 \u5ba0 \u7269 \u6389 \u6bdb \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6cf0 \u8fea \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u9000 \u4f11 \u5728 \u5bb6 \u5f88 \u65e0 \u804a \u554a \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u5e72 \u70b9 \u4ec0 \u4e48 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u5fc3 \u7406 \u5065 \u5eb7 \u5f88 \u91cd \u8981 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u597d \u7684 \u5efa \u8bae \u5417 \uff1f \u68c0 \u7d22 \u5176 \u5b9e \u6211 \u4e5f \u60f3 \u8fc7 \u517b \u53ea \u72d7 \uff0c \u4f46 \u662f \u6211 \u6015 \u5ba0 \u7269 \u6389 \u6bdb \u3002 \u68c0 \u7d22 \u539f \u6765 \u5982 \u6b64 \uff0c \u602a \u4e0d \u5f97 \u5468 \u56f4 \u5f88 \u591a \u90bb \u5c45 \u90fd \u517b \u6cf0 \u8fea \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6cf0 \u8fea \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u5317 \u5c6f \u5e02 \u9000 \u4f11 \u5728 \u5bb6 \u5f88 \u65e0 \u804a \u554a \uff0c \u4e0d \u77e5 \u9053 \u8be5 \u5e72 \u70b9 \u4ec0 \u4e48 \u3002 \u68c0 \u7d22 \u4f60 \u8bf4 \u5f97 \u5bf9 \uff0c \u5fc3 \u7406 \u5065 \u5eb7 \u5f88 \u91cd \u8981 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u597d \u7684 \u5efa \u8bae \u5417 \uff1f \u68c0 \u7d22 \u5176 \u5b9e \u6211 \u4e5f \u60f3 \u8fc7 \u517b \u53ea \u72d7 \uff0c \u4f46 \u662f \u6211 \u6015 \u5ba0 \u7269 \u6389 \u6bdb \u3002 \u68c0 \u7d22 \u539f \u6765 \u5982 \u6b64 \uff0c \u602a \u4e0d \u5f97 \u5468 \u56f4 \u5f88 \u591a \u90bb \u5c45 \u90fd \u517b \u6cf0 \u8fea \u3002 \u68c0 \u7d22 \u8fd9 \u4e48 \u806a \u660e \u554a \uff0c \u90a3 \u8fd8 \u771f \u662f \u633a \u4e0d \u9519 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u5929 \u6c14 \u56db \u5ddd \u7701 \u5357 \u5145 \u5e02 \u6700 \u8fd1 \u4f1a \u4e0b \u96e8 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u5929 \u6c14 \u56db \u5ddd \u7701 \u5357 \u5145 \u5e02 \u6700 \u8fd1 \u4f1a \u4e0b \u96e8 \u5417 \uff1f \u68c0 \u7d22 \u53c8 \u8981 \u4e0b \u96e8 \uff0c \u597d \u70e6 \u554a \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u5929 \u6c14 \u56db \u5ddd \u7701 \u5357 \u5145 \u5e02 \u6700 \u8fd1 \u4f1a \u4e0b \u96e8 \u5417 \uff1f \u68c0 \u7d22 \u53c8 \u8981 \u4e0b \u96e8 \uff0c \u597d \u70e6 \u554a \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u6bcf \u5929 \u5446 \u5728 \u5bb6 \u91cc \u592a \u65e0 \u804a \u4e86 \uff0c \u6211 \u60f3 \u51fa \u53bb \u73a9 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u5929 \u6c14 \u56db \u5ddd \u7701 \u5357 \u5145 \u5e02 \u6700 \u8fd1 \u4f1a \u4e0b \u96e8 \u5417 \uff1f \u68c0 \u7d22 \u53c8 \u8981 \u4e0b \u96e8 \uff0c \u597d \u70e6 \u554a \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u6bcf \u5929 \u5446 \u5728 \u5bb6 \u91cc \u592a \u65e0 \u804a \u4e86 \uff0c \u6211 \u60f3 \u51fa \u53bb \u73a9 \u68c0 \u7d22 \u8fd9 \u4e2a \u535a \u7269 \u9986 \u6211 \u53bb \u5e74 \u53bb \u8fc7 \u4e86 \uff0c \u6211 \u60f3 \u53bb \u722c \u5c71 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u5929 \u6c14 \u56db \u5ddd \u7701 \u5357 \u5145 \u5e02 \u6700 \u8fd1 \u4f1a \u4e0b \u96e8 \u5417 \uff1f \u68c0 \u7d22 \u53c8 \u8981 \u4e0b \u96e8 \uff0c \u597d \u70e6 \u554a \u4e0d \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u6bcf \u5929 \u5446 \u5728 \u5bb6 \u91cc \u592a \u65e0 \u804a \u4e86 \uff0c \u6211 \u60f3 \u51fa \u53bb \u73a9 \u68c0 \u7d22 \u8fd9 \u4e2a \u535a \u7269 \u9986 \u6211 \u53bb \u5e74 \u53bb \u8fc7 \u4e86 \uff0c \u6211 \u60f3 \u53bb \u722c \u5c71 \u68c0 \u7d22 \u8fd9 \u4e9b \u6211 \u90fd \u4e0d \u592a \u60f3 \uff0c \u4e3b \u8981 \u662f \u60f3 \u8fd0 \u52a8 \u4e00 \u4e0b \uff0c \u5728 \u5bb6 \u6bcf \u5929 \u8eba \u7740 \u8eab \u4f53 \u4e0d \u8212 \u670d \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5bb6 \u5e38 \u8c46 \u8150 \u8fbd \u5b81 \u7701 \u8fbd \u9633 \u5e02 \u4eca \u5929 \u5728 \u670b \u53cb \u5bb6 \u5403 \u4e86 \u4e00 \u9053 \u7279 \u522b \u597d \u5403 \u7684 \u5bb6 \u5e38 \u8c46 \u8150 \uff0c \u6211 \u81ea \u5df1 \u4e5f \u60f3 \u5c1d \u8bd5 \u7740 \u505a \u4e00 \u4e0b \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5bb6 \u5e38 \u8c46 \u8150 \u8fbd \u5b81 \u7701 \u8fbd \u9633 \u5e02 \u4eca \u5929 \u5728 \u670b \u53cb \u5bb6 \u5403 \u4e86 \u4e00 \u9053 \u7279 \u522b \u597d \u5403 \u7684 \u5bb6 \u5e38 \u8c46 \u8150 \uff0c \u6211 \u81ea \u5df1 \u4e5f \u60f3 \u5c1d \u8bd5 \u7740 \u505a \u4e00 \u4e0b \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u600e \u4e48 \u4ec0 \u4e48 \u90fd \u4f1a \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5bb6 \u5e38 \u8c46 \u8150 \u8fbd \u5b81 \u7701 \u8fbd \u9633 \u5e02 \u4eca \u5929 \u5728 \u670b \u53cb \u5bb6 \u5403 \u4e86 \u4e00 \u9053 \u7279 \u522b \u597d \u5403 \u7684 \u5bb6 \u5e38 \u8c46 \u8150 \uff0c \u6211 \u81ea \u5df1 \u4e5f \u60f3 \u5c1d \u8bd5 \u7740 \u505a \u4e00 \u4e0b \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u600e \u4e48 \u4ec0 \u4e48 \u90fd \u4f1a \u554a \uff1f \u4e0d \u68c0 \u7d22 \u4e0d \u8fc7 \u6211 \u6ca1 \u600e \u4e48 \u8fdb \u8fc7 \u53a8 \u623f \uff0c \u6211 \u770b \u5e02 \u573a \u91cc \u7684 \u8c46 \u8150 \u6709 \u597d \u51e0 \u79cd \uff0c \u6211 \u5e94 \u8be5 \u4e70 \u54ea \u4e00 \u79cd \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5bb6 \u5e38 \u8c46 \u8150 \u8fbd \u5b81 \u7701 \u8fbd \u9633 \u5e02 \u4eca \u5929 \u5728 \u670b \u53cb \u5bb6 \u5403 \u4e86 \u4e00 \u9053 \u7279 \u522b \u597d \u5403 \u7684 \u5bb6 \u5e38 \u8c46 \u8150 \uff0c \u6211 \u81ea \u5df1 \u4e5f \u60f3 \u5c1d \u8bd5 \u7740 \u505a \u4e00 \u4e0b \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u600e \u4e48 \u4ec0 \u4e48 \u90fd \u4f1a \u554a \uff1f \u4e0d \u68c0 \u7d22 \u4e0d \u8fc7 \u6211 \u6ca1 \u600e \u4e48 \u8fdb \u8fc7 \u53a8 \u623f \uff0c \u6211 \u770b \u5e02 \u573a \u91cc \u7684 \u8c46 \u8150 \u6709 \u597d \u51e0 \u79cd \uff0c \u6211 \u5e94 \u8be5 \u4e70 \u54ea \u4e00 \u79cd \u554a \uff1f \u4e0d \u68c0 \u7d22 \u6709 \u4ec0 \u4e48 \u533a \u522b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5bb6 \u5e38 \u8c46 \u8150 \u8fbd \u5b81 \u7701 \u8fbd \u9633 \u5e02 \u4eca \u5929 \u5728 \u670b \u53cb \u5bb6 \u5403 \u4e86 \u4e00 \u9053 \u7279 \u522b \u597d \u5403 \u7684 \u5bb6 \u5e38 \u8c46 \u8150 \uff0c \u6211 \u81ea \u5df1 \u4e5f \u60f3 \u5c1d \u8bd5 \u7740 \u505a \u4e00 \u4e0b \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u600e \u4e48 \u4ec0 \u4e48 \u90fd \u4f1a \u554a \uff1f \u4e0d \u68c0 \u7d22 \u4e0d \u8fc7 \u6211 \u6ca1 \u600e \u4e48 \u8fdb \u8fc7 \u53a8 \u623f \uff0c \u6211 \u770b \u5e02 \u573a \u91cc \u7684 \u8c46 \u8150 \u6709 \u597d \u51e0 \u79cd \uff0c \u6211 \u5e94 \u8be5 \u4e70 \u54ea \u4e00 \u79cd \u554a \uff1f \u4e0d \u68c0 \u7d22 \u6709 \u4ec0 \u4e48 \u533a \u522b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u4e86 \u89e3 \uff0c \u90a3 \u6211 \u8fd8 \u662f \u7528 \u8001 \u8c46 \u8150 \u5427 \u3002 \u5177 \u4f53 \u5e94 \u8be5 \u600e \u4e48 \u505a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u505a \u83dc \u5bb6 \u5e38 \u8c46 \u8150 \u8fbd \u5b81 \u7701 \u8fbd \u9633 \u5e02 \u4eca \u5929 \u5728 \u670b \u53cb \u5bb6 \u5403 \u4e86 \u4e00 \u9053 \u7279 \u522b \u597d \u5403 \u7684 \u5bb6 \u5e38 \u8c46 \u8150 \uff0c \u6211 \u81ea \u5df1 \u4e5f \u60f3 \u5c1d \u8bd5 \u7740 \u505a \u4e00 \u4e0b \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u600e \u4e48 \u4ec0 \u4e48 \u90fd \u4f1a \u554a \uff1f \u4e0d \u68c0 \u7d22 \u4e0d \u8fc7 \u6211 \u6ca1 \u600e \u4e48 \u8fdb \u8fc7 \u53a8 \u623f \uff0c \u6211 \u770b \u5e02 \u573a \u91cc \u7684 \u8c46 \u8150 \u6709 \u597d \u51e0 \u79cd \uff0c \u6211 \u5e94 \u8be5 \u4e70 \u54ea \u4e00 \u79cd \u554a \uff1f \u4e0d \u68c0 \u7d22 \u6709 \u4ec0 \u4e48 \u533a \u522b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u4e86 \u89e3 \uff0c \u90a3 \u6211 \u8fd8 \u662f \u7528 \u8001 \u8c46 \u8150 \u5427 \u3002 \u5177 \u4f53 \u5e94 \u8be5 \u600e \u4e48 \u505a \uff1f \u68c0 \u7d22 \u5c31 \u662f \u524d \u9762 \u70b8 \u8c46 \u8150 \u90a3 \u91cc \u597d \u50cf \u6709 \u70b9 \u96be \uff0c \u6211 \u5b9e \u5728 \u4e0d \u592a \u4f1a \u63a7 \u5236 \u706b \u5019 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u54c1 \u724c \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u6700 \u8fd1 \u597d \u591a \u4eba \u5e26 \u5468 \u5927 \u751f \u7684 \u624b \u94fe \u5440 \uff0c \u7279 \u522b \u597d \u770b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u54c1 \u724c \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u6700 \u8fd1 \u597d \u591a \u4eba \u5e26 \u5468 \u5927 \u751f \u7684 \u624b \u94fe \u5440 \uff0c \u7279 \u522b \u597d \u770b \u3002 \u68c0 \u7d22 \u5662 \uff0c \u4f60 \u4e70 \u7684 \u662f \u54ea \u4e00 \u6761 \u554a \uff0c \u8d35 \u4e0d \u8d35 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u54c1 \u724c \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u6700 \u8fd1 \u597d \u591a \u4eba \u5e26 \u5468 \u5927 \u751f \u7684 \u624b \u94fe \u5440 \uff0c \u7279 \u522b \u597d \u770b \u3002 \u68c0 \u7d22 \u5662 \uff0c \u4f60 \u4e70 \u7684 \u662f \u54ea \u4e00 \u6761 \u554a \uff0c \u8d35 \u4e0d \u8d35 \uff1f \u68c0 \u7d22 \u5662 \uff0c \u6709 \u673a \u4f1a \u6211 \u4e5f \u53bb \u5e97 \u91cc \u770b \u770b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u54c1 \u724c \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u6700 \u8fd1 \u597d \u591a \u4eba \u5e26 \u5468 \u5927 \u751f \u7684 \u624b \u94fe \u5440 \uff0c \u7279 \u522b \u597d \u770b \u3002 \u68c0 \u7d22 \u5662 \uff0c \u4f60 \u4e70 \u7684 \u662f \u54ea \u4e00 \u6761 \u554a \uff0c \u8d35 \u4e0d \u8d35 \uff1f \u68c0 \u7d22 \u5662 \uff0c \u6709 \u673a \u4f1a \u6211 \u4e5f \u53bb \u5e97 \u91cc \u770b \u770b \u3002 \u68c0 \u7d22 \u5361 \u5730 \u4e9a \u7684 \u624b \u94fe \u4e00 \u822c \u591a \u5c11 \u94b1 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u65f6 \u5c1a \u54c1 \u724c \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u6700 \u8fd1 \u597d \u591a \u4eba \u5e26 \u5468 \u5927 \u751f \u7684 \u624b \u94fe \u5440 \uff0c \u7279 \u522b \u597d \u770b \u3002 \u68c0 \u7d22 \u5662 \uff0c \u4f60 \u4e70 \u7684 \u662f \u54ea \u4e00 \u6761 \u554a \uff0c \u8d35 \u4e0d \u8d35 \uff1f \u68c0 \u7d22 \u5662 \uff0c \u6709 \u673a \u4f1a \u6211 \u4e5f \u53bb \u5e97 \u91cc \u770b \u770b \u3002 \u68c0 \u7d22 \u5361 \u5730 \u4e9a \u7684 \u624b \u94fe \u4e00 \u822c \u591a \u5c11 \u94b1 \u5440 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u542c \u8d77 \u6765 \u5c31 \u5f88 \u5962 \u534e \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u963f \u51e1 \u8fbe \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u963f \u51e1 \u8fbe \u5b9e \u5728 \u662f \u592a \u597d \u770b \u5566 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u963f \u51e1 \u8fbe \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u963f \u51e1 \u8fbe \u5b9e \u5728 \u662f \u592a \u597d \u770b \u5566 \u68c0 \u7d22 \u6211 \u4e5f \u53bb \u770b \u4e86 \u91cd \u6620 \uff0c \u7535 \u5f71 \u91cc \u7684 \u5267 \u60c5 \u771f \u7684 \u767e \u770b \u4e0d \u538c \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u963f \u51e1 \u8fbe \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u963f \u51e1 \u8fbe \u5b9e \u5728 \u662f \u592a \u597d \u770b \u5566 \u68c0 \u7d22 \u6211 \u4e5f \u53bb \u770b \u4e86 \u91cd \u6620 \uff0c \u7535 \u5f71 \u91cc \u7684 \u5267 \u60c5 \u771f \u7684 \u767e \u770b \u4e0d \u538c \u4e0d \u68c0 \u7d22 \u4f60 \u8fd8 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7ecf \u5178 \u7684 \u7535 \u5f71 \u63a8 \u8350 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u963f \u51e1 \u8fbe \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u963f \u51e1 \u8fbe \u5b9e \u5728 \u662f \u592a \u597d \u770b \u5566 \u68c0 \u7d22 \u6211 \u4e5f \u53bb \u770b \u4e86 \u91cd \u6620 \uff0c \u7535 \u5f71 \u91cc \u7684 \u5267 \u60c5 \u771f \u7684 \u767e \u770b \u4e0d \u538c \u4e0d \u68c0 \u7d22 \u4f60 \u8fd8 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7ecf \u5178 \u7684 \u7535 \u5f71 \u63a8 \u8350 \u5417 \u68c0 \u7d22 \u8fd9 \u4e9b \u6211 \u90fd \u6709 \u770b \u8fc7 \uff0c \u6211 \u6bd4 \u8f83 \u559c \u6b22 \u7231 \u60c5 \u7c7b \u7684 \u7535 \u5f71 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u963f \u51e1 \u8fbe \u6fb3 \u95e8 \u7279 \u522b \u884c \u653f \u533a \u5927 \u5802 \u533a \u963f \u51e1 \u8fbe \u5b9e \u5728 \u662f \u592a \u597d \u770b \u5566 \u68c0 \u7d22 \u6211 \u4e5f \u53bb \u770b \u4e86 \u91cd \u6620 \uff0c \u7535 \u5f71 \u91cc \u7684 \u5267 \u60c5 \u771f \u7684 \u767e \u770b \u4e0d \u538c \u4e0d \u68c0 \u7d22 \u4f60 \u8fd8 \u6709 \u4ec0 \u4e48 \u597d \u770b \u7ecf \u5178 \u7684 \u7535 \u5f71 \u63a8 \u8350 \u5417 \u68c0 \u7d22 \u8fd9 \u4e9b \u6211 \u90fd \u6709 \u770b \u8fc7 \uff0c \u6211 \u6bd4 \u8f83 \u559c \u6b22 \u7231 \u60c5 \u7c7b \u7684 \u7535 \u5f71 \u68c0 \u7d22 \u90a3 \u5f53 \u7136 \uff0c \u8fd9 \u4e2a \u7535 \u5f71 \u6700 \u7ecf \u5178 \u4e86 \uff0c \u6bcf \u6b21 \u770b \u5b8c \u6211 \u90fd \u4f1a \u54ed \u5462 \uff0c \u8fd8 \u6709 \u4ec0 \u4e48 \u5176 \u4ed6 \u7231 \u60c5 \u7535 \u5f71 \u63a8 \u8350 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82f1 \u96c4 \u672c \u8272 \u6c5f \u897f \u7701 \u5409 \u5b89 \u5e02 \u4eca \u5929 \u5929 \u6c14 \u600e \u4e48 \u6837 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82f1 \u96c4 \u672c \u8272 \u6c5f \u897f \u7701 \u5409 \u5b89 \u5e02 \u4eca \u5929 \u5929 \u6c14 \u600e \u4e48 \u6837 \u68c0 \u7d22 \u592a \u51b7 \uff0c \u90a3 \u5c31 \u4e0d \u51fa \u53bb \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82f1 \u96c4 \u672c \u8272 \u6c5f \u897f \u7701 \u5409 \u5b89 \u5e02 \u4eca \u5929 \u5929 \u6c14 \u600e \u4e48 \u6837 \u68c0 \u7d22 \u592a \u51b7 \uff0c \u90a3 \u5c31 \u4e0d \u51fa \u53bb \u4e86 \u4e0d \u68c0 \u7d22 \u51c6 \u5907 \u770b \u770b \u7535 \u5f71 \u5427 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82f1 \u96c4 \u672c \u8272 \u6c5f \u897f \u7701 \u5409 \u5b89 \u5e02 \u4eca \u5929 \u5929 \u6c14 \u600e \u4e48 \u6837 \u68c0 \u7d22 \u592a \u51b7 \uff0c \u90a3 \u5c31 \u4e0d \u51fa \u53bb \u4e86 \u4e0d \u68c0 \u7d22 \u51c6 \u5907 \u770b \u770b \u7535 \u5f71 \u5427 \u4e0d \u68c0 \u7d22 \u6700 \u8fd1 \u6bd4 \u8f83 \u559c \u6b22 \u5468 \u6da6 \u53d1 \u7684 \u7535 \u5f71 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82f1 \u96c4 \u672c \u8272 \u6c5f \u897f \u7701 \u5409 \u5b89 \u5e02 \u4eca \u5929 \u5929 \u6c14 \u600e \u4e48 \u6837 \u68c0 \u7d22 \u592a \u51b7 \uff0c \u90a3 \u5c31 \u4e0d \u51fa \u53bb \u4e86 \u4e0d \u68c0 \u7d22 \u51c6 \u5907 \u770b \u770b \u7535 \u5f71 \u5427 \u4e0d \u68c0 \u7d22 \u6700 \u8fd1 \u6bd4 \u8f83 \u559c \u6b22 \u5468 \u6da6 \u53d1 \u7684 \u7535 \u5f71 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u5417 \u68c0 \u7d22 \u8fd9 \u4e48 \u591a \uff0c \u90a3 \u6211 \u8be5 \u5148 \u770b \u54ea \u90e8 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u82f1 \u96c4 \u672c \u8272 \u6c5f \u897f \u7701 \u5409 \u5b89 \u5e02 \u4eca \u5929 \u5929 \u6c14 \u600e \u4e48 \u6837 \u68c0 \u7d22 \u592a \u51b7 \uff0c \u90a3 \u5c31 \u4e0d \u51fa \u53bb \u4e86 \u4e0d \u68c0 \u7d22 \u51c6 \u5907 \u770b \u770b \u7535 \u5f71 \u5427 \u4e0d \u68c0 \u7d22 \u6700 \u8fd1 \u6bd4 \u8f83 \u559c \u6b22 \u5468 \u6da6 \u53d1 \u7684 \u7535 \u5f71 \uff0c \u4f60 \u6709 \u4ec0 \u4e48 \u63a8 \u8350 \u5417 \u68c0 \u7d22 \u8fd9 \u4e48 \u591a \uff0c \u90a3 \u6211 \u8be5 \u5148 \u770b \u54ea \u90e8 \u5462 \u4e0d \u68c0 \u7d22 \u597d \u7684 \uff0c \u90a3 \u6211 \u5148 \u53bb \u770b \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u5317 \u4eac \u897f \u7ad9 \u6700 \u8fd1 \u4e5f \u6ca1 \u5565 \u597d \u770b \u7684 \u5267 \uff0c \u6253 \u5f00 \u7231 \u5947 \u827a \u3001 \u4f18 \u9177 \u5565 \u7684 \u90fd \u4e0d \u77e5 \u9053 \u770b \u5565 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u5317 \u4eac \u897f \u7ad9 \u6700 \u8fd1 \u4e5f \u6ca1 \u5565 \u597d \u770b \u7684 \u5267 \uff0c \u6253 \u5f00 \u7231 \u5947 \u827a \u3001 \u4f18 \u9177 \u5565 \u7684 \u90fd \u4e0d \u77e5 \u9053 \u770b \u5565 \u3002 \u68c0 \u7d22 \u662f \u554a \uff0c \u770b \u6765 \u4f60 \u4e5f \u4e0d \u559c \u6b22 \u5076 \u50cf \u5267 \u554a \uff0c \u6211 \u559c \u6b22 \u770b \u4e00 \u4e9b \u7ecf \u5178 \u7684 \u7535 \u89c6 \u5267 \uff0c \u6bd4 \u5982 \u559c \u5267 \u7684 \u300a \u6b66 \u6797 \u5916 \u4f20 \u300b \u3001 \u300a \u5bb6 \u6709 \u513f \u5973 \u300b \u5565 \u7684 \uff0c \u7ea2 \u8272 \u7ecf \u5178 \u300a \u6070 \u540c \u5b66 \u5c11 \u5e74 \u300b \u3001 \u300a \u4eae \u5251 \u300b \u5565 \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u5317 \u4eac \u897f \u7ad9 \u6700 \u8fd1 \u4e5f \u6ca1 \u5565 \u597d \u770b \u7684 \u5267 \uff0c \u6253 \u5f00 \u7231 \u5947 \u827a \u3001 \u4f18 \u9177 \u5565 \u7684 \u90fd \u4e0d \u77e5 \u9053 \u770b \u5565 \u3002 \u68c0 \u7d22 \u662f \u554a \uff0c \u770b \u6765 \u4f60 \u4e5f \u4e0d \u559c \u6b22 \u5076 \u50cf \u5267 \u554a \uff0c \u6211 \u559c \u6b22 \u770b \u4e00 \u4e9b \u7ecf \u5178 \u7684 \u7535 \u89c6 \u5267 \uff0c \u6bd4 \u5982 \u559c \u5267 \u7684 \u300a \u6b66 \u6797 \u5916 \u4f20 \u300b \u3001 \u300a \u5bb6 \u6709 \u513f \u5973 \u300b \u5565 \u7684 \uff0c \u7ea2 \u8272 \u7ecf \u5178 \u300a \u6070 \u540c \u5b66 \u5c11 \u5e74 \u300b \u3001 \u300a \u4eae \u5251 \u300b \u5565 \u7684 \u3002 \u4e0d \u68c0 \u7d22 1988 \uff01 \u554a \uff01 \u8fd9 \u4e2a \u592a \u7ecf \u5178 \u4e86 \uff0c \u4f46 \u6211 \u89c9 \u5f97 \u8fd9 \u4e2a \u4e0d \u5b8c \u5168 \u662f \u5076 \u50cf \u5267 \uff0c \u9664 \u4e86 \u5e74 \u9752 \u4e00 \u4ee3 \u7684 \u7231 \u60c5 \u5916 \uff0c \u4ed6 \u4eec \u4e4b \u95f4 \u7684 \u53cb \u60c5 \u3001 \u4ed6 \u4eec \u7236 \u6bcd \u4e4b \u95f4 \u7684 \u90bb \u91cc \u60c5 \u4e5f \u8d85 \u7ea7 \u611f \u4eba \uff0c \u8fd8 \u7279 \u522b \u641e \u7b11 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u5317 \u4eac \u897f \u7ad9 \u6700 \u8fd1 \u4e5f \u6ca1 \u5565 \u597d \u770b \u7684 \u5267 \uff0c \u6253 \u5f00 \u7231 \u5947 \u827a \u3001 \u4f18 \u9177 \u5565 \u7684 \u90fd \u4e0d \u77e5 \u9053 \u770b \u5565 \u3002 \u68c0 \u7d22 \u662f \u554a \uff0c \u770b \u6765 \u4f60 \u4e5f \u4e0d \u559c \u6b22 \u5076 \u50cf \u5267 \u554a \uff0c \u6211 \u559c \u6b22 \u770b \u4e00 \u4e9b \u7ecf \u5178 \u7684 \u7535 \u89c6 \u5267 \uff0c \u6bd4 \u5982 \u559c \u5267 \u7684 \u300a \u6b66 \u6797 \u5916 \u4f20 \u300b \u3001 \u300a \u5bb6 \u6709 \u513f \u5973 \u300b \u5565 \u7684 \uff0c \u7ea2 \u8272 \u7ecf \u5178 \u300a \u6070 \u540c \u5b66 \u5c11 \u5e74 \u300b \u3001 \u300a \u4eae \u5251 \u300b \u5565 \u7684 \u3002 \u4e0d \u68c0 \u7d22 1988 \uff01 \u554a \uff01 \u8fd9 \u4e2a \u592a \u7ecf \u5178 \u4e86 \uff0c \u4f46 \u6211 \u89c9 \u5f97 \u8fd9 \u4e2a \u4e0d \u5b8c \u5168 \u662f \u5076 \u50cf \u5267 \uff0c \u9664 \u4e86 \u5e74 \u9752 \u4e00 \u4ee3 \u7684 \u7231 \u60c5 \u5916 \uff0c \u4ed6 \u4eec \u4e4b \u95f4 \u7684 \u53cb \u60c5 \u3001 \u4ed6 \u4eec \u7236 \u6bcd \u4e4b \u95f4 \u7684 \u90bb \u91cc \u60c5 \u4e5f \u8d85 \u7ea7 \u611f \u4eba \uff0c \u8fd8 \u7279 \u522b \u641e \u7b11 \u3002 \u68c0 \u7d22 \u597d \u63d0 \u8bae \uff0c \u6211 \u786e \u5b9e \u6ca1 \u770b \u8fc7 \u5176 \u4ed6 \u4e24 \u90e8 \uff0c \u54c8 \u54c8 \uff0c \u4f60 \u62ef \u6551 \u4e86 \u6211 \u7684 \u5267 \u8352 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u89c6 \u5317 \u4eac \u897f \u7ad9 \u6700 \u8fd1 \u4e5f \u6ca1 \u5565 \u597d \u770b \u7684 \u5267 \uff0c \u6253 \u5f00 \u7231 \u5947 \u827a \u3001 \u4f18 \u9177 \u5565 \u7684 \u90fd \u4e0d \u77e5 \u9053 \u770b \u5565 \u3002 \u68c0 \u7d22 \u662f \u554a \uff0c \u770b \u6765 \u4f60 \u4e5f \u4e0d \u559c \u6b22 \u5076 \u50cf \u5267 \u554a \uff0c \u6211 \u559c \u6b22 \u770b \u4e00 \u4e9b \u7ecf \u5178 \u7684 \u7535 \u89c6 \u5267 \uff0c \u6bd4 \u5982 \u559c \u5267 \u7684 \u300a \u6b66 \u6797 \u5916 \u4f20 \u300b \u3001 \u300a \u5bb6 \u6709 \u513f \u5973 \u300b \u5565 \u7684 \uff0c \u7ea2 \u8272 \u7ecf \u5178 \u300a \u6070 \u540c \u5b66 \u5c11 \u5e74 \u300b \u3001 \u300a \u4eae \u5251 \u300b \u5565 \u7684 \u3002 \u4e0d \u68c0 \u7d22 1988 \uff01 \u554a \uff01 \u8fd9 \u4e2a \u592a \u7ecf \u5178 \u4e86 \uff0c \u4f46 \u6211 \u89c9 \u5f97 \u8fd9 \u4e2a \u4e0d \u5b8c \u5168 \u662f \u5076 \u50cf \u5267 \uff0c \u9664 \u4e86 \u5e74 \u9752 \u4e00 \u4ee3 \u7684 \u7231 \u60c5 \u5916 \uff0c \u4ed6 \u4eec \u4e4b \u95f4 \u7684 \u53cb \u60c5 \u3001 \u4ed6 \u4eec \u7236 \u6bcd \u4e4b \u95f4 \u7684 \u90bb \u91cc \u60c5 \u4e5f \u8d85 \u7ea7 \u611f \u4eba \uff0c \u8fd8 \u7279 \u522b \u641e \u7b11 \u3002 \u68c0 \u7d22 \u597d \u63d0 \u8bae \uff0c \u6211 \u786e \u5b9e \u6ca1 \u770b \u8fc7 \u5176 \u4ed6 \u4e24 \u90e8 \uff0c \u54c8 \u54c8 \uff0c \u4f60 \u62ef \u6551 \u4e86 \u6211 \u7684 \u5267 \u8352 \u3002 \u4e0d \u68c0 \u7d22 \u505c \uff01 \u4e0d \u8bb8 \u5267 \u900f \uff0c \u4f60 \u8ba9 \u6211 \u81ea \u5df1 \u53bb \u770b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u80c3 \u75c5 \u6d59 \u6c5f \u7701 \u4e3d \u6c34 \u5e02 \u6700 \u8fd1 \u8001 \u662f \u80c3 \u4e0d \u8212 \u670d \uff0c \u4e0d \u77e5 \u9053 \u662f \u4e0d \u662f \u5403 \u574f \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u80c3 \u75c5 \u6d59 \u6c5f \u7701 \u4e3d \u6c34 \u5e02 \u6700 \u8fd1 \u8001 \u662f \u80c3 \u4e0d \u8212 \u670d \uff0c \u4e0d \u77e5 \u9053 \u662f \u4e0d \u662f \u5403 \u574f \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u7ecf \u5e38 \u80c3 \u75db \uff0c \u540c \u4e8b \u5efa \u8bae \u6211 \u53bb \u505a \u4e2a \u80c3 \u955c \u597d \u597d \u68c0 \u67e5 \u4e00 \u4e0b \uff0c \u53ef \u662f \u80c3 \u955c \u662f \u4e0d \u662f \u5f88 \u96be \u53d7 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u80c3 \u75c5 \u6d59 \u6c5f \u7701 \u4e3d \u6c34 \u5e02 \u6700 \u8fd1 \u8001 \u662f \u80c3 \u4e0d \u8212 \u670d \uff0c \u4e0d \u77e5 \u9053 \u662f \u4e0d \u662f \u5403 \u574f \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u7ecf \u5e38 \u80c3 \u75db \uff0c \u540c \u4e8b \u5efa \u8bae \u6211 \u53bb \u505a \u4e2a \u80c3 \u955c \u597d \u597d \u68c0 \u67e5 \u4e00 \u4e0b \uff0c \u53ef \u662f \u80c3 \u955c \u662f \u4e0d \u662f \u5f88 \u96be \u53d7 \u5440 \uff1f \u68c0 \u7d22 \u542c \u4e0a \u53bb \u4f3c \u4e4e \u6ca1 \u90a3 \u4e48 \u53ef \u6015 \u4e86 \uff0c \u6709 \u6ca1 \u6709 \u53ef \u80fd \u6211 \u662f \u5f97 \u4e86 \u5e7d \u95e8 \u87ba \u65cb \u6746 \u83cc \u5462 \uff1f \u8eab \u8fb9 \u6709 \u4e0d \u53ea \u4e00 \u4e2a \u670b \u53cb \u5f97 \u4e86 \u8fd9 \u4e2a \u75c5 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u80c3 \u75c5 \u6d59 \u6c5f \u7701 \u4e3d \u6c34 \u5e02 \u6700 \u8fd1 \u8001 \u662f \u80c3 \u4e0d \u8212 \u670d \uff0c \u4e0d \u77e5 \u9053 \u662f \u4e0d \u662f \u5403 \u574f \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u7ecf \u5e38 \u80c3 \u75db \uff0c \u540c \u4e8b \u5efa \u8bae \u6211 \u53bb \u505a \u4e2a \u80c3 \u955c \u597d \u597d \u68c0 \u67e5 \u4e00 \u4e0b \uff0c \u53ef \u662f \u80c3 \u955c \u662f \u4e0d \u662f \u5f88 \u96be \u53d7 \u5440 \uff1f \u68c0 \u7d22 \u542c \u4e0a \u53bb \u4f3c \u4e4e \u6ca1 \u90a3 \u4e48 \u53ef \u6015 \u4e86 \uff0c \u6709 \u6ca1 \u6709 \u53ef \u80fd \u6211 \u662f \u5f97 \u4e86 \u5e7d \u95e8 \u87ba \u65cb \u6746 \u83cc \u5462 \uff1f \u8eab \u8fb9 \u6709 \u4e0d \u53ea \u4e00 \u4e2a \u670b \u53cb \u5f97 \u4e86 \u8fd9 \u4e2a \u75c5 \u3002 \u68c0 \u7d22 \u8fd8 \u771f \u6709 \uff0c \u6211 \u4f1a \u6709 \u53cd \u9178 \u3001 \u70e7 \u5fc3 \u7684 \u611f \u89c9 \u3002 \u5e7d \u95e8 \u87ba \u65cb \u6746 \u83cc \u611f \u67d3 \u80fd \u6839 \u6cbb \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u80c3 \u75c5 \u6d59 \u6c5f \u7701 \u4e3d \u6c34 \u5e02 \u6700 \u8fd1 \u8001 \u662f \u80c3 \u4e0d \u8212 \u670d \uff0c \u4e0d \u77e5 \u9053 \u662f \u4e0d \u662f \u5403 \u574f \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u7ecf \u5e38 \u80c3 \u75db \uff0c \u540c \u4e8b \u5efa \u8bae \u6211 \u53bb \u505a \u4e2a \u80c3 \u955c \u597d \u597d \u68c0 \u67e5 \u4e00 \u4e0b \uff0c \u53ef \u662f \u80c3 \u955c \u662f \u4e0d \u662f \u5f88 \u96be \u53d7 \u5440 \uff1f \u68c0 \u7d22 \u542c \u4e0a \u53bb \u4f3c \u4e4e \u6ca1 \u90a3 \u4e48 \u53ef \u6015 \u4e86 \uff0c \u6709 \u6ca1 \u6709 \u53ef \u80fd \u6211 \u662f \u5f97 \u4e86 \u5e7d \u95e8 \u87ba \u65cb \u6746 \u83cc \u5462 \uff1f \u8eab \u8fb9 \u6709 \u4e0d \u53ea \u4e00 \u4e2a \u670b \u53cb \u5f97 \u4e86 \u8fd9 \u4e2a \u75c5 \u3002 \u68c0 \u7d22 \u8fd8 \u771f \u6709 \uff0c \u6211 \u4f1a \u6709 \u53cd \u9178 \u3001 \u70e7 \u5fc3 \u7684 \u611f \u89c9 \u3002 \u5e7d \u95e8 \u87ba \u65cb \u6746 \u83cc \u611f \u67d3 \u80fd \u6839 \u6cbb \u5417 \uff1f \u68c0 \u7d22 \u5468 \u8fb9 \u6700 \u8fd1 \u7684 \u533b \u9662 \u662f \u54ea \u5bb6 \u5462 \uff1f \u6211 \u8d76 \u7d27 \u53bb \u533b \u9662 \u6302 \u53f7 \u770b \u4e00 \u770b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u6b66 \u5927 \u9756 \u5c71 \u4e1c \u7701 \u6d4e \u5357 \u5e02 \u4f60 \u77e5 \u9053 \u6b66 \u5927 \u9756 \u5417 \uff1f \u6211 \u89c9 \u5f97 \u4ed6 \u597d \u641e \u7b11 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u6b66 \u5927 \u9756 \u5c71 \u4e1c \u7701 \u6d4e \u5357 \u5e02 \u4f60 \u77e5 \u9053 \u6b66 \u5927 \u9756 \u5417 \uff1f \u6211 \u89c9 \u5f97 \u4ed6 \u597d \u641e \u7b11 \u3002 \u68c0 \u7d22 \u662f \uff0c \u4eca \u5e74 \u7684 \u51ac \u5965 \u4f1a \u4ed6 \u8868 \u73b0 \u8fd8 \u4e0d \u9519 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u6b66 \u5927 \u9756 \u5c71 \u4e1c \u7701 \u6d4e \u5357 \u5e02 \u4f60 \u77e5 \u9053 \u6b66 \u5927 \u9756 \u5417 \uff1f \u6211 \u89c9 \u5f97 \u4ed6 \u597d \u641e \u7b11 \u3002 \u68c0 \u7d22 \u662f \uff0c \u4eca \u5e74 \u7684 \u51ac \u5965 \u4f1a \u4ed6 \u8868 \u73b0 \u8fd8 \u4e0d \u9519 \u3002 \u68c0 \u7d22 \u55ef \u3002 \u55ef \u3002 \u6211 \u8bb0 \u5f97 \u5f53 \u521d \u4ed6 \u53ea \u662f \u5973 \u5b50 \u77ed \u9053 \u901f \u6ed1 \u7684 \u966a \u7ec3 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u6b66 \u5927 \u9756 \u5c71 \u4e1c \u7701 \u6d4e \u5357 \u5e02 \u4f60 \u77e5 \u9053 \u6b66 \u5927 \u9756 \u5417 \uff1f \u6211 \u89c9 \u5f97 \u4ed6 \u597d \u641e \u7b11 \u3002 \u68c0 \u7d22 \u662f \uff0c \u4eca \u5e74 \u7684 \u51ac \u5965 \u4f1a \u4ed6 \u8868 \u73b0 \u8fd8 \u4e0d \u9519 \u3002 \u68c0 \u7d22 \u55ef \u3002 \u55ef \u3002 \u6211 \u8bb0 \u5f97 \u5f53 \u521d \u4ed6 \u53ea \u662f \u5973 \u5b50 \u77ed \u9053 \u901f \u6ed1 \u7684 \u966a \u7ec3 \u3002 \u4e0d \u68c0 \u7d22 \u4ed6 \u8fd8 \u662f \u633a \u52b1 \u5fd7 \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u6b66 \u5927 \u9756 \u5c71 \u4e1c \u7701 \u6d4e \u5357 \u5e02 \u4f60 \u77e5 \u9053 \u6b66 \u5927 \u9756 \u5417 \uff1f \u6211 \u89c9 \u5f97 \u4ed6 \u597d \u641e \u7b11 \u3002 \u68c0 \u7d22 \u662f \uff0c \u4eca \u5e74 \u7684 \u51ac \u5965 \u4f1a \u4ed6 \u8868 \u73b0 \u8fd8 \u4e0d \u9519 \u3002 \u68c0 \u7d22 \u55ef \u3002 \u55ef \u3002 \u6211 \u8bb0 \u5f97 \u5f53 \u521d \u4ed6 \u53ea \u662f \u5973 \u5b50 \u77ed \u9053 \u901f \u6ed1 \u7684 \u966a \u7ec3 \u3002 \u4e0d \u68c0 \u7d22 \u4ed6 \u8fd8 \u662f \u633a \u52b1 \u5fd7 \u7684 \u3002 \u4e0d \u68c0 \u7d22 \u4eca \u5929 \u4ed6 \u8fd8 \u56e0 \u4e3a \u776b \u6bdb \u957f \u4e0a \u4e86 \u70ed \u641c \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u8fbd \u5b81 \u7701 \u9526 \u5dde \u5e02 \u6700 \u8fd1 \u535a \u7269 \u9986 \u6b63 \u5728 \u4e3e \u529e \u4e00 \u4e2a \u53e4 \u4eba \u7684 \u8863 \u9970 \u5c55 \uff0c \u771f \u7684 \u597d \u6709 \u610f \u601d \u554a \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u8fbd \u5b81 \u7701 \u9526 \u5dde \u5e02 \u6700 \u8fd1 \u535a \u7269 \u9986 \u6b63 \u5728 \u4e3e \u529e \u4e00 \u4e2a \u53e4 \u4eba \u7684 \u8863 \u9970 \u5c55 \uff0c \u771f \u7684 \u597d \u6709 \u610f \u601d \u554a \uff01 \u4e0d \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u5510 \u671d \u4eba \u7684 \u8863 \u670d \uff0c \u5c24 \u5176 \u662f \u5973 \u5b50 \u7684 \u670d \u9970 \uff0c \u7279 \u522b \u7684 \u98d8 \u9038 \uff0c \u800c \u4e14 \u5f88 \u81ea \u7531 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u8fbd \u5b81 \u7701 \u9526 \u5dde \u5e02 \u6700 \u8fd1 \u535a \u7269 \u9986 \u6b63 \u5728 \u4e3e \u529e \u4e00 \u4e2a \u53e4 \u4eba \u7684 \u8863 \u9970 \u5c55 \uff0c \u771f \u7684 \u597d \u6709 \u610f \u601d \u554a \uff01 \u4e0d \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u5510 \u671d \u4eba \u7684 \u8863 \u670d \uff0c \u5c24 \u5176 \u662f \u5973 \u5b50 \u7684 \u670d \u9970 \uff0c \u7279 \u522b \u7684 \u98d8 \u9038 \uff0c \u800c \u4e14 \u5f88 \u81ea \u7531 \u3002 \u68c0 \u7d22 \u53e4 \u4eba \u7684 \u5ba1 \u7f8e \u786e \u5b9e \u4e0d \u4e00 \u822c \uff0c \u4ed6 \u4eec \u7684 \u53d1 \u7c2a \u4e5f \u5f88 \u597d \u770b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u8fbd \u5b81 \u7701 \u9526 \u5dde \u5e02 \u6700 \u8fd1 \u535a \u7269 \u9986 \u6b63 \u5728 \u4e3e \u529e \u4e00 \u4e2a \u53e4 \u4eba \u7684 \u8863 \u9970 \u5c55 \uff0c \u771f \u7684 \u597d \u6709 \u610f \u601d \u554a \uff01 \u4e0d \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u5510 \u671d \u4eba \u7684 \u8863 \u670d \uff0c \u5c24 \u5176 \u662f \u5973 \u5b50 \u7684 \u670d \u9970 \uff0c \u7279 \u522b \u7684 \u98d8 \u9038 \uff0c \u800c \u4e14 \u5f88 \u81ea \u7531 \u3002 \u68c0 \u7d22 \u53e4 \u4eba \u7684 \u5ba1 \u7f8e \u786e \u5b9e \u4e0d \u4e00 \u822c \uff0c \u4ed6 \u4eec \u7684 \u53d1 \u7c2a \u4e5f \u5f88 \u597d \u770b \u3002 \u4e0d \u68c0 \u7d22 \u54c7 \uff0c \u597d \u6709 \u4eea \u5f0f \u611f \u5440 \uff0c \u6211 \u4e5f \u60f3 \u8981 \u81ea \u5df1 \u7684 \u53d1 \u7c2a \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u8fbd \u5b81 \u7701 \u9526 \u5dde \u5e02 \u6700 \u8fd1 \u535a \u7269 \u9986 \u6b63 \u5728 \u4e3e \u529e \u4e00 \u4e2a \u53e4 \u4eba \u7684 \u8863 \u9970 \u5c55 \uff0c \u771f \u7684 \u597d \u6709 \u610f \u601d \u554a \uff01 \u4e0d \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u5510 \u671d \u4eba \u7684 \u8863 \u670d \uff0c \u5c24 \u5176 \u662f \u5973 \u5b50 \u7684 \u670d \u9970 \uff0c \u7279 \u522b \u7684 \u98d8 \u9038 \uff0c \u800c \u4e14 \u5f88 \u81ea \u7531 \u3002 \u68c0 \u7d22 \u53e4 \u4eba \u7684 \u5ba1 \u7f8e \u786e \u5b9e \u4e0d \u4e00 \u822c \uff0c \u4ed6 \u4eec \u7684 \u53d1 \u7c2a \u4e5f \u5f88 \u597d \u770b \u3002 \u4e0d \u68c0 \u7d22 \u54c7 \uff0c \u597d \u6709 \u4eea \u5f0f \u611f \u5440 \uff0c \u6211 \u4e5f \u60f3 \u8981 \u81ea \u5df1 \u7684 \u53d1 \u7c2a \uff01 \u4e0d \u68c0 \u7d22 \u597d \u5440 \uff0c \u7b49 \u6211 \u4e70 \u4e86 \u53d1 \u7c2a \uff0c \u7b2c \u4e00 \u4e2a \u7ed9 \u4f60 \u770b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u4eca \u5929 \u770b \u65b0 \u95fb \uff0c \u53d1 \u73b0 \u4e09 \u661f \u5806 \u53c8 \u51fa \u571f \u4e86 \u4e00 \u4e2a \u9762 \u5177 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u4eca \u5929 \u770b \u65b0 \u95fb \uff0c \u53d1 \u73b0 \u4e09 \u661f \u5806 \u53c8 \u51fa \u571f \u4e86 \u4e00 \u4e2a \u9762 \u5177 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u4e09 \u661f \u5806 \u8ddd \u4eca \u6709 \u591a \u4e45 \u7684 \u5386 \u53f2 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u4eca \u5929 \u770b \u65b0 \u95fb \uff0c \u53d1 \u73b0 \u4e09 \u661f \u5806 \u53c8 \u51fa \u571f \u4e86 \u4e00 \u4e2a \u9762 \u5177 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u4e09 \u661f \u5806 \u8ddd \u4eca \u6709 \u591a \u4e45 \u7684 \u5386 \u53f2 \u5417 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u8bdd \u8bf4 \u4e09 \u661f \u5806 \u662f \u5728 \u54ea \u4e2a \u5e02 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u4eca \u5929 \u770b \u65b0 \u95fb \uff0c \u53d1 \u73b0 \u4e09 \u661f \u5806 \u53c8 \u51fa \u571f \u4e86 \u4e00 \u4e2a \u9762 \u5177 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u4e09 \u661f \u5806 \u8ddd \u4eca \u6709 \u591a \u4e45 \u7684 \u5386 \u53f2 \u5417 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u8bdd \u8bf4 \u4e09 \u661f \u5806 \u662f \u5728 \u54ea \u4e2a \u5e02 \u554a \uff1f \u68c0 \u7d22 \u54e6 \uff0c \u539f \u6765 \u662f \u5728 \u56db \u5ddd \u554a \u3002 \u90a3 \u5c31 \u662f \u5df4 \u8700 \u6587 \u5316 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u5386 \u53f2 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u4eca \u5929 \u770b \u65b0 \u95fb \uff0c \u53d1 \u73b0 \u4e09 \u661f \u5806 \u53c8 \u51fa \u571f \u4e86 \u4e00 \u4e2a \u9762 \u5177 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u4e09 \u661f \u5806 \u8ddd \u4eca \u6709 \u591a \u4e45 \u7684 \u5386 \u53f2 \u5417 \uff1f \u68c0 \u7d22 \u662f \u5417 \uff1f \u8bdd \u8bf4 \u4e09 \u661f \u5806 \u662f \u5728 \u54ea \u4e2a \u5e02 \u554a \uff1f \u68c0 \u7d22 \u54e6 \uff0c \u539f \u6765 \u662f \u5728 \u56db \u5ddd \u554a \u3002 \u90a3 \u5c31 \u662f \u5df4 \u8700 \u6587 \u5316 \u3002 \u4e0d \u68c0 \u7d22 \u90a3 \u4ed6 \u4e00 \u5b9a \u51fa \u571f \u4e86 \u5f88 \u591a \u6587 \u7269 \u5427 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u8bd7 \u6b4c \u4e91 \u5357 \u7701 \u897f \u53cc \u7248 \u7eb3 \u50a3 \u65cf \u81ea \u6cbb \u5dde \u6625 \u5929 \u5feb \u6765 \u4e86 ~ \u6625 \u7720 \u4e0d \u89c9 \u6653 \uff0c \u5904 \u5904 \u95fb \u557c \u9e1f ~ \u9002 \u5408 \u8e0f \u9752 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u8bd7 \u6b4c \u4e91 \u5357 \u7701 \u897f \u53cc \u7248 \u7eb3 \u50a3 \u65cf \u81ea \u6cbb \u5dde \u6625 \u5929 \u5feb \u6765 \u4e86 ~ \u6625 \u7720 \u4e0d \u89c9 \u6653 \uff0c \u5904 \u5904 \u95fb \u557c \u9e1f ~ \u9002 \u5408 \u8e0f \u9752 \u68c0 \u7d22 \u6700 \u8fd1 \u5728 \u770b \u9ec4 \u6653 \u4e39 \u7684 \u300a \u8bd7 \u4eba \u5341 \u56db \u4e2a \u300b \uff0c \u4e5f \u5f88 \u9002 \u5408 \u6625 \u5929 \u770b \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u8bd7 \u6b4c \u4e91 \u5357 \u7701 \u897f \u53cc \u7248 \u7eb3 \u50a3 \u65cf \u81ea \u6cbb \u5dde \u6625 \u5929 \u5feb \u6765 \u4e86 ~ \u6625 \u7720 \u4e0d \u89c9 \u6653 \uff0c \u5904 \u5904 \u95fb \u557c \u9e1f ~ \u9002 \u5408 \u8e0f \u9752 \u68c0 \u7d22 \u6700 \u8fd1 \u5728 \u770b \u9ec4 \u6653 \u4e39 \u7684 \u300a \u8bd7 \u4eba \u5341 \u56db \u4e2a \u300b \uff0c \u4e5f \u5f88 \u9002 \u5408 \u6625 \u5929 \u770b \u5462 \u68c0 \u7d22 \u771f \u6709 \u610f \u601d \uff0c \u6625 \u6e38 \u8be5 \u53bb \u54ea \u513f \u5462 \uff0c \u611f \u89c9 \u53ef \u4ee5 \u5f00 \u59cb \u89c4 \u5212 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u8bd7 \u6b4c \u4e91 \u5357 \u7701 \u897f \u53cc \u7248 \u7eb3 \u50a3 \u65cf \u81ea \u6cbb \u5dde \u6625 \u5929 \u5feb \u6765 \u4e86 ~ \u6625 \u7720 \u4e0d \u89c9 \u6653 \uff0c \u5904 \u5904 \u95fb \u557c \u9e1f ~ \u9002 \u5408 \u8e0f \u9752 \u68c0 \u7d22 \u6700 \u8fd1 \u5728 \u770b \u9ec4 \u6653 \u4e39 \u7684 \u300a \u8bd7 \u4eba \u5341 \u56db \u4e2a \u300b \uff0c \u4e5f \u5f88 \u9002 \u5408 \u6625 \u5929 \u770b \u5462 \u68c0 \u7d22 \u771f \u6709 \u610f \u601d \uff0c \u6625 \u6e38 \u8be5 \u53bb \u54ea \u513f \u5462 \uff0c \u611f \u89c9 \u53ef \u4ee5 \u5f00 \u59cb \u89c4 \u5212 \u4e86 \u68c0 \u7d22 \u6625 \u6b22 \u56ed \u6211 \u5012 \u662f \u542c \u8bf4 \u8fc7 \uff0c \u4e0d \u8fc7 \u8fd8 \u6ca1 \u53bb \u8fc7 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u8bd7 \u6b4c \u4e91 \u5357 \u7701 \u897f \u53cc \u7248 \u7eb3 \u50a3 \u65cf \u81ea \u6cbb \u5dde \u6625 \u5929 \u5feb \u6765 \u4e86 ~ \u6625 \u7720 \u4e0d \u89c9 \u6653 \uff0c \u5904 \u5904 \u95fb \u557c \u9e1f ~ \u9002 \u5408 \u8e0f \u9752 \u68c0 \u7d22 \u6700 \u8fd1 \u5728 \u770b \u9ec4 \u6653 \u4e39 \u7684 \u300a \u8bd7 \u4eba \u5341 \u56db \u4e2a \u300b \uff0c \u4e5f \u5f88 \u9002 \u5408 \u6625 \u5929 \u770b \u5462 \u68c0 \u7d22 \u771f \u6709 \u610f \u601d \uff0c \u6625 \u6e38 \u8be5 \u53bb \u54ea \u513f \u5462 \uff0c \u611f \u89c9 \u53ef \u4ee5 \u5f00 \u59cb \u89c4 \u5212 \u4e86 \u68c0 \u7d22 \u6625 \u6b22 \u56ed \u6211 \u5012 \u662f \u542c \u8bf4 \u8fc7 \uff0c \u4e0d \u8fc7 \u8fd8 \u6ca1 \u53bb \u8fc7 \u68c0 \u7d22 \u55ef \u55ef \uff0c \u5706 \u660e \u56ed \u3001 \u9890 \u548c \u56ed \u4e4b \u7c7b \u7684 \uff0c \u90a3 \u6211 \u6625 \u5929 \u53bb \u770b \u770b \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5317 \u4eac \u897f \u7ad9 \u4f60 \u4f1a \u770b \u661f \u5ea7 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5317 \u4eac \u897f \u7ad9 \u4f60 \u4f1a \u770b \u661f \u5ea7 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9b54 \u7faf \u5ea7 \u7684 \u6027 \u683c \uff0c \u662f \u4ec0 \u4e48 \u6837 \u7684 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5317 \u4eac \u897f \u7ad9 \u4f60 \u4f1a \u770b \u661f \u5ea7 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9b54 \u7faf \u5ea7 \u7684 \u6027 \u683c \uff0c \u662f \u4ec0 \u4e48 \u6837 \u7684 \uff1f \u68c0 \u7d22 \u786e \u5b9e \u6709 \u70b9 \u5bf9 \uff0c \u6211 \u6bd4 \u8f83 \u4fdd \u5b88 \uff0c \u800c \u4e14 \u786e \u5b9e \u5728 \u4e00 \u4e9b \u65b9 \u9762 \u6bd4 \u8f83 \u56fa \u6267 \uff0c \u661f \u5ea7 \u8fd8 \u662f \u6709 \u4e00 \u70b9 \u51c6 \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5317 \u4eac \u897f \u7ad9 \u4f60 \u4f1a \u770b \u661f \u5ea7 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9b54 \u7faf \u5ea7 \u7684 \u6027 \u683c \uff0c \u662f \u4ec0 \u4e48 \u6837 \u7684 \uff1f \u68c0 \u7d22 \u786e \u5b9e \u6709 \u70b9 \u5bf9 \uff0c \u6211 \u6bd4 \u8f83 \u4fdd \u5b88 \uff0c \u800c \u4e14 \u786e \u5b9e \u5728 \u4e00 \u4e9b \u65b9 \u9762 \u6bd4 \u8f83 \u56fa \u6267 \uff0c \u661f \u5ea7 \u8fd8 \u662f \u6709 \u4e00 \u70b9 \u51c6 \u7684 \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4ee5 \u770b \u4e00 \u4e0b \u9b54 \u7faf \u5ea7 \u7684 \u4eca \u65e5 \u8fd0 \u52bf \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5317 \u4eac \u897f \u7ad9 \u4f60 \u4f1a \u770b \u661f \u5ea7 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9b54 \u7faf \u5ea7 \u7684 \u6027 \u683c \uff0c \u662f \u4ec0 \u4e48 \u6837 \u7684 \uff1f \u68c0 \u7d22 \u786e \u5b9e \u6709 \u70b9 \u5bf9 \uff0c \u6211 \u6bd4 \u8f83 \u4fdd \u5b88 \uff0c \u800c \u4e14 \u786e \u5b9e \u5728 \u4e00 \u4e9b \u65b9 \u9762 \u6bd4 \u8f83 \u56fa \u6267 \uff0c \u661f \u5ea7 \u8fd8 \u662f \u6709 \u4e00 \u70b9 \u51c6 \u7684 \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4ee5 \u770b \u4e00 \u4e0b \u9b54 \u7faf \u5ea7 \u7684 \u4eca \u65e5 \u8fd0 \u52bf \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u4eca \u5e74 \u7684 \u8fd0 \u52bf \u600e \u4e48 \u6837 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u661f \u5ea7 \u5317 \u4eac \u897f \u7ad9 \u4f60 \u4f1a \u770b \u661f \u5ea7 \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u9b54 \u7faf \u5ea7 \u7684 \u6027 \u683c \uff0c \u662f \u4ec0 \u4e48 \u6837 \u7684 \uff1f \u68c0 \u7d22 \u786e \u5b9e \u6709 \u70b9 \u5bf9 \uff0c \u6211 \u6bd4 \u8f83 \u4fdd \u5b88 \uff0c \u800c \u4e14 \u786e \u5b9e \u5728 \u4e00 \u4e9b \u65b9 \u9762 \u6bd4 \u8f83 \u56fa \u6267 \uff0c \u661f \u5ea7 \u8fd8 \u662f \u6709 \u4e00 \u70b9 \u51c6 \u7684 \u3002 \u4e0d \u68c0 \u7d22 \u53ef \u4ee5 \u770b \u4e00 \u4e0b \u9b54 \u7faf \u5ea7 \u7684 \u4eca \u65e5 \u8fd0 \u52bf \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u6211 \u4eca \u5e74 \u7684 \u8fd0 \u52bf \u600e \u4e48 \u6837 \uff1f \u68c0 \u7d22 \u597d \u5f00 \u5fc3 \uff0c \u6211 \u4e5f \u5e0c \u671b \u6211 \u80fd \u591f \u4e50 \u89c2 \u7684 \u5bf9 \u5f85 \u4eba \u751f \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u54bd \u75db \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u6211 \u53bb \uff0c \u55d3 \u5b50 \u597d \u75db \uff01 \u521a \u9192 \u6765 \u4e00 \u54b3 \u55fd \u5feb \u628a \u6211 \u9001 \u8d70 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u54bd \u75db \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u6211 \u53bb \uff0c \u55d3 \u5b50 \u597d \u75db \uff01 \u521a \u9192 \u6765 \u4e00 \u54b3 \u55fd \u5feb \u628a \u6211 \u9001 \u8d70 \u4e86 \u4e0d \u68c0 \u7d22 \u554a \u662f \u7684 \uff0c \u6628 \u5929 \u6709 \u70b9 \u611f \u5192 \uff0c \u5f53 \u65f6 \u5c31 \u6709 \u70b9 \u55d3 \u5b50 \u75bc \u7ed3 \u679c \u65e9 \u4e0a \u4e00 \u8d77 \u6765 \u5c31 \u54b3 \u75f0 \uff0c \u75bc \u5f97 \u8981 \u547d \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u54bd \u75db \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u6211 \u53bb \uff0c \u55d3 \u5b50 \u597d \u75db \uff01 \u521a \u9192 \u6765 \u4e00 \u54b3 \u55fd \u5feb \u628a \u6211 \u9001 \u8d70 \u4e86 \u4e0d \u68c0 \u7d22 \u554a \u662f \u7684 \uff0c \u6628 \u5929 \u6709 \u70b9 \u611f \u5192 \uff0c \u5f53 \u65f6 \u5c31 \u6709 \u70b9 \u55d3 \u5b50 \u75bc \u7ed3 \u679c \u65e9 \u4e0a \u4e00 \u8d77 \u6765 \u5c31 \u54b3 \u75f0 \uff0c \u75bc \u5f97 \u8981 \u547d \u68c0 \u7d22 \u554a \u8fd9 \uff0c \u4f60 \u5728 \u4e00 \u672c \u6b63 \u7ecf \u7684 \u5632 \u8bbd \u5417 \uff1f \u771f \u7684 \u5f88 \u96be \u53d7 \u8036 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u54bd \u75db \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u6211 \u53bb \uff0c \u55d3 \u5b50 \u597d \u75db \uff01 \u521a \u9192 \u6765 \u4e00 \u54b3 \u55fd \u5feb \u628a \u6211 \u9001 \u8d70 \u4e86 \u4e0d \u68c0 \u7d22 \u554a \u662f \u7684 \uff0c \u6628 \u5929 \u6709 \u70b9 \u611f \u5192 \uff0c \u5f53 \u65f6 \u5c31 \u6709 \u70b9 \u55d3 \u5b50 \u75bc \u7ed3 \u679c \u65e9 \u4e0a \u4e00 \u8d77 \u6765 \u5c31 \u54b3 \u75f0 \uff0c \u75bc \u5f97 \u8981 \u547d \u68c0 \u7d22 \u554a \u8fd9 \uff0c \u4f60 \u5728 \u4e00 \u672c \u6b63 \u7ecf \u7684 \u5632 \u8bbd \u5417 \uff1f \u771f \u7684 \u5f88 \u96be \u53d7 \u8036 \uff01 \u68c0 \u7d22 \u6709 \u7684 \uff0c \u559d \u4e86 \u4e00 \u70b9 \uff0c \u4f46 \u8fd8 \u662f \u6cbb \u6807 \u4e0d \u6cbb \u672c \u7684 \u611f \u89c9 \u3002 \u68c0 \u7d22 \u3002', '\u5065 \u5eb7 \u5e38 \u89c1 \u75c5 \u54bd \u75db \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u77f3 \u6cb3 \u5b50 \u5e02 \u6211 \u53bb \uff0c \u55d3 \u5b50 \u597d \u75db \uff01 \u521a \u9192 \u6765 \u4e00 \u54b3 \u55fd \u5feb \u628a \u6211 \u9001 \u8d70 \u4e86 \u4e0d \u68c0 \u7d22 \u554a \u662f \u7684 \uff0c \u6628 \u5929 \u6709 \u70b9 \u611f \u5192 \uff0c \u5f53 \u65f6 \u5c31 \u6709 \u70b9 \u55d3 \u5b50 \u75bc \u7ed3 \u679c \u65e9 \u4e0a \u4e00 \u8d77 \u6765 \u5c31 \u54b3 \u75f0 \uff0c \u75bc \u5f97 \u8981 \u547d \u68c0 \u7d22 \u554a \u8fd9 \uff0c \u4f60 \u5728 \u4e00 \u672c \u6b63 \u7ecf \u7684 \u5632 \u8bbd \u5417 \uff1f \u771f \u7684 \u5f88 \u96be \u53d7 \u8036 \uff01 \u68c0 \u7d22 \u6709 \u7684 \uff0c \u559d \u4e86 \u4e00 \u70b9 \uff0c \u4f46 \u8fd8 \u662f \u6cbb \u6807 \u4e0d \u6cbb \u672c \u7684 \u611f \u89c9 \u68c0 \u7d22 \u6ca1 \u4e8b \u522b \u53bb \u533b \u9662 \u5427 \uff0c \u522b \u611f \u67d3 \u65b0 \u51a0 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u6700 \u8fd1 \u5bf9 \u6444 \u5f71 \u5f88 \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u65b0 \u624b \u6444 \u5f71 \u8981 \u5165 \u624b \u54ea \u4e9b \u8bbe \u5907 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u6700 \u8fd1 \u5bf9 \u6444 \u5f71 \u5f88 \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u65b0 \u624b \u6444 \u5f71 \u8981 \u5165 \u624b \u54ea \u4e9b \u8bbe \u5907 \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u8bf4 \u7684 \u633a \u5bf9 \uff0c \u4e00 \u5f00 \u59cb \u6ca1 \u5fc5 \u8981 \u5165 \u624b \u8fd9 \u4e48 \u8d35 \u7684 \uff0c \u4e5f \u8bb8 \u6ca1 \u8fc7 \u591a \u4e45 \u6211 \u53d1 \u73b0 \u6ca1 \u4ec0 \u4e48 \u6444 \u5f71 \u7684 \u5929 \u5206 \uff0c \u5c31 \u4e0d \u73a9 \u4e86 \uff0c \u5176 \u5b9e \u6211 \u4e00 \u70b9 \u90fd \u4e0d \u61c2 \u6444 \u5f71 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u6700 \u8fd1 \u5bf9 \u6444 \u5f71 \u5f88 \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u65b0 \u624b \u6444 \u5f71 \u8981 \u5165 \u624b \u54ea \u4e9b \u8bbe \u5907 \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u8bf4 \u7684 \u633a \u5bf9 \uff0c \u4e00 \u5f00 \u59cb \u6ca1 \u5fc5 \u8981 \u5165 \u624b \u8fd9 \u4e48 \u8d35 \u7684 \uff0c \u4e5f \u8bb8 \u6ca1 \u8fc7 \u591a \u4e45 \u6211 \u53d1 \u73b0 \u6ca1 \u4ec0 \u4e48 \u6444 \u5f71 \u7684 \u5929 \u5206 \uff0c \u5c31 \u4e0d \u73a9 \u4e86 \uff0c \u5176 \u5b9e \u6211 \u4e00 \u70b9 \u90fd \u4e0d \u61c2 \u6444 \u5f71 \u68c0 \u7d22 \u54c7 \uff0c \u4f60 \u597d \u61c2 \u5440 \uff0c \u4e0d \u8fc7 \u6211 \u6700 \u8fd1 \u53ea \u5bf9 \u62cd \u98ce \u666f \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u600e \u4e48 \u624d \u80fd \u62cd \u597d \u98ce \u666f \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u6700 \u8fd1 \u5bf9 \u6444 \u5f71 \u5f88 \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u65b0 \u624b \u6444 \u5f71 \u8981 \u5165 \u624b \u54ea \u4e9b \u8bbe \u5907 \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u8bf4 \u7684 \u633a \u5bf9 \uff0c \u4e00 \u5f00 \u59cb \u6ca1 \u5fc5 \u8981 \u5165 \u624b \u8fd9 \u4e48 \u8d35 \u7684 \uff0c \u4e5f \u8bb8 \u6ca1 \u8fc7 \u591a \u4e45 \u6211 \u53d1 \u73b0 \u6ca1 \u4ec0 \u4e48 \u6444 \u5f71 \u7684 \u5929 \u5206 \uff0c \u5c31 \u4e0d \u73a9 \u4e86 \uff0c \u5176 \u5b9e \u6211 \u4e00 \u70b9 \u90fd \u4e0d \u61c2 \u6444 \u5f71 \u68c0 \u7d22 \u54c7 \uff0c \u4f60 \u597d \u61c2 \u5440 \uff0c \u4e0d \u8fc7 \u6211 \u6700 \u8fd1 \u53ea \u5bf9 \u62cd \u98ce \u666f \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u600e \u4e48 \u624d \u80fd \u62cd \u597d \u98ce \u666f \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u8bf4 \u7684 \u5bf9 \uff0c \u5176 \u5b9e \u6211 \u662f \u5728 \u7f51 \u4e0a \u770b \u5230 \u5927 \u5bb6 \u90fd \u5728 \u62cd \u6545 \u5bab \u7684 \u96ea \u666f \uff0c \u7b80 \u76f4 \u592a \u7f8e \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u6444 \u5f71 \u6700 \u8fd1 \u5bf9 \u6444 \u5f71 \u5f88 \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u65b0 \u624b \u6444 \u5f71 \u8981 \u5165 \u624b \u54ea \u4e9b \u8bbe \u5907 \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u8bf4 \u7684 \u633a \u5bf9 \uff0c \u4e00 \u5f00 \u59cb \u6ca1 \u5fc5 \u8981 \u5165 \u624b \u8fd9 \u4e48 \u8d35 \u7684 \uff0c \u4e5f \u8bb8 \u6ca1 \u8fc7 \u591a \u4e45 \u6211 \u53d1 \u73b0 \u6ca1 \u4ec0 \u4e48 \u6444 \u5f71 \u7684 \u5929 \u5206 \uff0c \u5c31 \u4e0d \u73a9 \u4e86 \uff0c \u5176 \u5b9e \u6211 \u4e00 \u70b9 \u90fd \u4e0d \u61c2 \u6444 \u5f71 \u68c0 \u7d22 \u54c7 \uff0c \u4f60 \u597d \u61c2 \u5440 \uff0c \u4e0d \u8fc7 \u6211 \u6700 \u8fd1 \u53ea \u5bf9 \u62cd \u98ce \u666f \u611f \u5174 \u8da3 \uff0c \u4f60 \u77e5 \u9053 \u600e \u4e48 \u624d \u80fd \u62cd \u597d \u98ce \u666f \u5417 \uff1f \u68c0 \u7d22 \u4f60 \u8bf4 \u7684 \u5bf9 \uff0c \u5176 \u5b9e \u6211 \u662f \u5728 \u7f51 \u4e0a \u770b \u5230 \u5927 \u5bb6 \u90fd \u5728 \u62cd \u6545 \u5bab \u7684 \u96ea \u666f \uff0c \u7b80 \u76f4 \u592a \u7f8e \u4e86 \u68c0 \u7d22 \u662f \uff0c \u7b49 \u6709 \u673a \u4f1a \u5427 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8fbd \u5b81 \u7701 \u671d \u9633 \u5e02 \u54b1 \u4eec \u6765 \u804a \u804a \u97f3 \u4e50 \u5427 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8fbd \u5b81 \u7701 \u671d \u9633 \u5e02 \u54b1 \u4eec \u6765 \u804a \u804a \u97f3 \u4e50 \u5427 \u4e0d \u68c0 \u7d22 \u6211 \u633a \u559c \u6b22 \u542c \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u50cf \u300a \u4e0d \u8981 \u8bf4 \u8bdd \u300b \u3001 \u300a \u7231 \u60c5 \u8f6c \u79fb \u300b \uff0c \u6211 \u90fd \u5f88 \u559c \u6b22 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8fbd \u5b81 \u7701 \u671d \u9633 \u5e02 \u54b1 \u4eec \u6765 \u804a \u804a \u97f3 \u4e50 \u5427 \u4e0d \u68c0 \u7d22 \u6211 \u633a \u559c \u6b22 \u542c \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u50cf \u300a \u4e0d \u8981 \u8bf4 \u8bdd \u300b \u3001 \u300a \u7231 \u60c5 \u8f6c \u79fb \u300b \uff0c \u6211 \u90fd \u5f88 \u559c \u6b22 \u68c0 \u7d22 \u77e5 \u9053 \u77e5 \u9053 \uff01 \u6211 \u542c \u4e86 \u8fd9 \u9996 \u6b4c \uff0c \u592a \u611f \u4eba \u4e86 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8fbd \u5b81 \u7701 \u671d \u9633 \u5e02 \u54b1 \u4eec \u6765 \u804a \u804a \u97f3 \u4e50 \u5427 \u4e0d \u68c0 \u7d22 \u6211 \u633a \u559c \u6b22 \u542c \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u50cf \u300a \u4e0d \u8981 \u8bf4 \u8bdd \u300b \u3001 \u300a \u7231 \u60c5 \u8f6c \u79fb \u300b \uff0c \u6211 \u90fd \u5f88 \u559c \u6b22 \u68c0 \u7d22 \u77e5 \u9053 \u77e5 \u9053 \uff01 \u6211 \u542c \u4e86 \u8fd9 \u9996 \u6b4c \uff0c \u592a \u611f \u4eba \u4e86 \uff01 \u4e0d \u68c0 \u7d22 \u9664 \u4e86 \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u6211 \u8fd8 \u542c \u4e86 \u5f20 \u6e20 \u521b \u4f5c \u7684 \u4e00 \u4e9b \u53e4 \u98ce \u6b4c \u66f2 \uff0c \u611f \u89c9 \u4e5f \u4e0d \u9519 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8fbd \u5b81 \u7701 \u671d \u9633 \u5e02 \u54b1 \u4eec \u6765 \u804a \u804a \u97f3 \u4e50 \u5427 \u4e0d \u68c0 \u7d22 \u6211 \u633a \u559c \u6b22 \u542c \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u50cf \u300a \u4e0d \u8981 \u8bf4 \u8bdd \u300b \u3001 \u300a \u7231 \u60c5 \u8f6c \u79fb \u300b \uff0c \u6211 \u90fd \u5f88 \u559c \u6b22 \u68c0 \u7d22 \u77e5 \u9053 \u77e5 \u9053 \uff01 \u6211 \u542c \u4e86 \u8fd9 \u9996 \u6b4c \uff0c \u592a \u611f \u4eba \u4e86 \uff01 \u4e0d \u68c0 \u7d22 \u9664 \u4e86 \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u6211 \u8fd8 \u542c \u4e86 \u5f20 \u6e20 \u521b \u4f5c \u7684 \u4e00 \u4e9b \u53e4 \u98ce \u6b4c \u66f2 \uff0c \u611f \u89c9 \u4e5f \u4e0d \u9519 \u68c0 \u7d22 \u4f60 \u4e5f \u770b \u8fc7 \u300a \u5b54 \u5b50 \u300b \u5440 \uff0c \u6211 \u5c31 \u662f \u56e0 \u4e3a \u300a \u5b54 \u5b50 \u300b \u624d \u559c \u6b22 \u4e0a \u4e86 \u5f20 \u6e20 \u7684 \u6b4c \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u8fbd \u5b81 \u7701 \u671d \u9633 \u5e02 \u54b1 \u4eec \u6765 \u804a \u804a \u97f3 \u4e50 \u5427 \u4e0d \u68c0 \u7d22 \u6211 \u633a \u559c \u6b22 \u542c \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u50cf \u300a \u4e0d \u8981 \u8bf4 \u8bdd \u300b \u3001 \u300a \u7231 \u60c5 \u8f6c \u79fb \u300b \uff0c \u6211 \u90fd \u5f88 \u559c \u6b22 \u68c0 \u7d22 \u77e5 \u9053 \u77e5 \u9053 \uff01 \u6211 \u542c \u4e86 \u8fd9 \u9996 \u6b4c \uff0c \u592a \u611f \u4eba \u4e86 \uff01 \u4e0d \u68c0 \u7d22 \u9664 \u4e86 \u9648 \u5955 \u8fc5 \u7684 \u6b4c \uff0c \u6211 \u8fd8 \u542c \u4e86 \u5f20 \u6e20 \u521b \u4f5c \u7684 \u4e00 \u4e9b \u53e4 \u98ce \u6b4c \u66f2 \uff0c \u611f \u89c9 \u4e5f \u4e0d \u9519 \u68c0 \u7d22 \u4f60 \u4e5f \u770b \u8fc7 \u300a \u5b54 \u5b50 \u300b \u5440 \uff0c \u6211 \u5c31 \u662f \u56e0 \u4e3a \u300a \u5b54 \u5b50 \u300b \u624d \u559c \u6b22 \u4e0a \u4e86 \u5f20 \u6e20 \u7684 \u6b4c \u68c0 \u7d22 \u4f60 \u77e5 \u9053 \u7684 \u771f \u4e0d \u5c11 \uff0c \u592a \u5389 \u5bb3 \u4e86 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u821e \u8e48 \u821e \u8e48 \u98ce \u66b4 \u6d77 \u5357 \u7701 \u510b \u5dde \u5e02 \u4f60 \u662f \u4e0d \u662f \u5b66 \u4e86 \u5f88 \u4e45 \u7684 \u821e \u8e48 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u821e \u8e48 \u821e \u8e48 \u98ce \u66b4 \u6d77 \u5357 \u7701 \u510b \u5dde \u5e02 \u4f60 \u662f \u4e0d \u662f \u5b66 \u4e86 \u5f88 \u4e45 \u7684 \u821e \u8e48 \u554a \uff1f \u68c0 \u7d22 \u770b \u4f60 \u7684 \u8eab \u5f62 \u5c31 \u77e5 \u9053 \u4f60 \u662f \u7ec3 \u8fc7 \u821e \u8e48 \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u821e \u8e48 \u821e \u8e48 \u98ce \u66b4 \u6d77 \u5357 \u7701 \u510b \u5dde \u5e02 \u4f60 \u662f \u4e0d \u662f \u5b66 \u4e86 \u5f88 \u4e45 \u7684 \u821e \u8e48 \u554a \uff1f \u68c0 \u7d22 \u770b \u4f60 \u7684 \u8eab \u5f62 \u5c31 \u77e5 \u9053 \u4f60 \u662f \u7ec3 \u8fc7 \u821e \u8e48 \u7684 \u3002 \u68c0 \u7d22 \u6211 \u867d \u7136 \u4e0d \u4f1a \u8df3 \uff0c \u4f46 \u662f \u6211 \u5f88 \u559c \u6b22 \u770b \u821e \u8e48 \uff0c \u7279 \u522b \u7f8e \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u821e \u8e48 \u821e \u8e48 \u98ce \u66b4 \u6d77 \u5357 \u7701 \u510b \u5dde \u5e02 \u4f60 \u662f \u4e0d \u662f \u5b66 \u4e86 \u5f88 \u4e45 \u7684 \u821e \u8e48 \u554a \uff1f \u68c0 \u7d22 \u770b \u4f60 \u7684 \u8eab \u5f62 \u5c31 \u77e5 \u9053 \u4f60 \u662f \u7ec3 \u8fc7 \u821e \u8e48 \u7684 \u3002 \u68c0 \u7d22 \u6211 \u867d \u7136 \u4e0d \u4f1a \u8df3 \uff0c \u4f46 \u662f \u6211 \u5f88 \u559c \u6b22 \u770b \u821e \u8e48 \uff0c \u7279 \u522b \u7f8e \u3002 \u68c0 \u7d22 \u54c7 \uff0c \u90a3 \u5f88 \u9002 \u5408 \u6211 \u554a \uff0c \u7ed9 \u6211 \u8bb2 \u8bb2 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u821e \u8e48 \u821e \u8e48 \u98ce \u66b4 \u6d77 \u5357 \u7701 \u510b \u5dde \u5e02 \u4f60 \u662f \u4e0d \u662f \u5b66 \u4e86 \u5f88 \u4e45 \u7684 \u821e \u8e48 \u554a \uff1f \u68c0 \u7d22 \u770b \u4f60 \u7684 \u8eab \u5f62 \u5c31 \u77e5 \u9053 \u4f60 \u662f \u7ec3 \u8fc7 \u821e \u8e48 \u7684 \u3002 \u68c0 \u7d22 \u6211 \u867d \u7136 \u4e0d \u4f1a \u8df3 \uff0c \u4f46 \u662f \u6211 \u5f88 \u559c \u6b22 \u770b \u821e \u8e48 \uff0c \u7279 \u522b \u7f8e \u3002 \u68c0 \u7d22 \u54c7 \uff0c \u90a3 \u5f88 \u9002 \u5408 \u6211 \u554a \uff0c \u7ed9 \u6211 \u8bb2 \u8bb2 \u5427 \u3002 \u68c0 \u7d22 \u90a3 \u6709 \u54ea \u4f4d \u9009 \u624b \u4f60 \u6700 \u559c \u6b22 \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e0a \u6709 \u5f88 \u591a \u4eba \u4e00 \u70ae \u800c \u7ea2 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e0a \u6709 \u5f88 \u591a \u4eba \u4e00 \u70ae \u800c \u7ea2 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e2d \u56fd \u961f \u53c8 \u5728 \u4e52 \u4e53 \u7403 \u9879 \u76ee \u4e0a \u5305 \u63fd \u4e86 \u5f88 \u591a \u5956 \u9879 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e0a \u6709 \u5f88 \u591a \u4eba \u4e00 \u70ae \u800c \u7ea2 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e2d \u56fd \u961f \u53c8 \u5728 \u4e52 \u4e53 \u7403 \u9879 \u76ee \u4e0a \u5305 \u63fd \u4e86 \u5f88 \u591a \u5956 \u9879 \u68c0 \u7d22 \u9664 \u6b64 \u4e4b \u5916 \uff0c \u5176 \u4ed6 \u9879 \u76ee \u4e5f \u5f88 \u51fa \u8272 \uff0c \u6bd4 \u5982 \u82cf \u70b3 \u6dfb \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e0a \u6709 \u5f88 \u591a \u4eba \u4e00 \u70ae \u800c \u7ea2 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e2d \u56fd \u961f \u53c8 \u5728 \u4e52 \u4e53 \u7403 \u9879 \u76ee \u4e0a \u5305 \u63fd \u4e86 \u5f88 \u591a \u5956 \u9879 \u68c0 \u7d22 \u9664 \u6b64 \u4e4b \u5916 \uff0c \u5176 \u4ed6 \u9879 \u76ee \u4e5f \u5f88 \u51fa \u8272 \uff0c \u6bd4 \u5982 \u82cf \u70b3 \u6dfb \u5440 \u68c0 \u7d22 \u6211 \u8ba4 \u4e3a \u4ed6 \u8fbe \u5230 \u4e86 \u4e9a \u6d32 \u8fd0 \u52a8 \u5458 \u7684 \u767e \u7c73 \u6781 \u9650 \uff0c \u4ee4 \u4eba \u4f69 \u670d \u3002 \u68c0 \u7d22 \u3002', '\u4f53 \u80b2 \u8fd0 \u52a8 \u5458 \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e0a \u6709 \u5f88 \u591a \u4eba \u4e00 \u70ae \u800c \u7ea2 \u4e0d \u68c0 \u7d22 \u662f \u7684 \uff0c \u4eca \u5e74 \u5965 \u8fd0 \u4f1a \u4e2d \u56fd \u961f \u53c8 \u5728 \u4e52 \u4e53 \u7403 \u9879 \u76ee \u4e0a \u5305 \u63fd \u4e86 \u5f88 \u591a \u5956 \u9879 \u68c0 \u7d22 \u9664 \u6b64 \u4e4b \u5916 \uff0c \u5176 \u4ed6 \u9879 \u76ee \u4e5f \u5f88 \u51fa \u8272 \uff0c \u6bd4 \u5982 \u82cf \u70b3 \u6dfb \u5440 \u68c0 \u7d22 \u6211 \u8ba4 \u4e3a \u4ed6 \u8fbe \u5230 \u4e86 \u4e9a \u6d32 \u8fd0 \u52a8 \u5458 \u7684 \u767e \u7c73 \u6781 \u9650 \uff0c \u4ee4 \u4eba \u4f69 \u670d \u4e0d \u68c0 \u7d22 \u8fd8 \u6709 \u6e38 \u6cf3 \u9879 \u76ee \uff0c \u4f60 \u4e86 \u89e3 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u6cb3 \u5357 \u7701 \u8bb8 \u660c \u5e02 \u4f60 \u559c \u6b22 \u73a9 \u4ec0 \u4e48 \u6e38 \u620f \u554a \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u6cb3 \u5357 \u7701 \u8bb8 \u660c \u5e02 \u4f60 \u559c \u6b22 \u73a9 \u4ec0 \u4e48 \u6e38 \u620f \u554a \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u6cb3 \u5357 \u7701 \u8bb8 \u660c \u5e02 \u4f60 \u559c \u6b22 \u73a9 \u4ec0 \u4e48 \u6e38 \u620f \u554a \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u5440 \u68c0 \u7d22 \u5728 \u91cc \u9762 \u53ef \u4ee5 \u4ea4 \u670b \u53cb \u7684 \u90a3 \u79cd \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u6cb3 \u5357 \u7701 \u8bb8 \u660c \u5e02 \u4f60 \u559c \u6b22 \u73a9 \u4ec0 \u4e48 \u6e38 \u620f \u554a \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u5440 \u68c0 \u7d22 \u5728 \u91cc \u9762 \u53ef \u4ee5 \u4ea4 \u670b \u53cb \u7684 \u90a3 \u79cd \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u90a3 \u9b54 \u517d \u4e16 \u754c \u600e \u4e48 \u6837 \u554a \uff0c \u6211 \u770b \u5f1f \u5f1f \u5728 \u73a9 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u6e38 \u620f \u6cb3 \u5357 \u7701 \u8bb8 \u660c \u5e02 \u4f60 \u559c \u6b22 \u73a9 \u4ec0 \u4e48 \u6e38 \u620f \u554a \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u662f \u4ec0 \u4e48 \u5440 \u68c0 \u7d22 \u5728 \u91cc \u9762 \u53ef \u4ee5 \u4ea4 \u670b \u53cb \u7684 \u90a3 \u79cd \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u90a3 \u9b54 \u517d \u4e16 \u754c \u600e \u4e48 \u6837 \u554a \uff0c \u6211 \u770b \u5f1f \u5f1f \u5728 \u73a9 \u68c0 \u7d22 \u554a \u5bf9 \uff0c \u6211 \u8fd8 \u60f3 \u6bd5 \u4e1a \u540e \u6709 \u673a \u4f1a \u53ef \u4ee5 \u53bb \u5de5 \u4f5c \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u8138 \u8bc6 \u522b \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u9632 \u57ce \u6e2f \u5e02 \u6700 \u8fd1 \u6211 \u6362 \u4e86 \u4e2a \u65b0 \u624b \u673a \uff0c \u53ef \u4ee5 \u76f4 \u63a5 \u626b \u8138 \u5c31 \u89e3 \u9501 \u5c4f \u5e55 \uff0c \u611f \u89c9 \u597d \u5389 \u5bb3 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u8138 \u8bc6 \u522b \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u9632 \u57ce \u6e2f \u5e02 \u6700 \u8fd1 \u6211 \u6362 \u4e86 \u4e2a \u65b0 \u624b \u673a \uff0c \u53ef \u4ee5 \u76f4 \u63a5 \u626b \u8138 \u5c31 \u89e3 \u9501 \u5c4f \u5e55 \uff0c \u611f \u89c9 \u597d \u5389 \u5bb3 \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u4ee5 \u524d \u5bc6 \u7801 \u89e3 \u9501 \u6211 \u603b \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u4e5f \u5bb3 \u6015 \u81ea \u5df1 \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u73b0 \u5728 \u65b9 \u4fbf \u591a \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u8138 \u8bc6 \u522b \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u9632 \u57ce \u6e2f \u5e02 \u6700 \u8fd1 \u6211 \u6362 \u4e86 \u4e2a \u65b0 \u624b \u673a \uff0c \u53ef \u4ee5 \u76f4 \u63a5 \u626b \u8138 \u5c31 \u89e3 \u9501 \u5c4f \u5e55 \uff0c \u611f \u89c9 \u597d \u5389 \u5bb3 \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u4ee5 \u524d \u5bc6 \u7801 \u89e3 \u9501 \u6211 \u603b \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u4e5f \u5bb3 \u6015 \u81ea \u5df1 \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u73b0 \u5728 \u65b9 \u4fbf \u591a \u4e86 \u4e0d \u68c0 \u7d22 \u4eba \u8138 \u8bc6 \u522b \u6280 \u672f \uff1f \u8fd9 \u8fd8 \u6709 \u4e00 \u4e2a \u4e13 \u95e8 \u7684 \u5b66 \u672f \u540d \u8bcd \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u8138 \u8bc6 \u522b \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u9632 \u57ce \u6e2f \u5e02 \u6700 \u8fd1 \u6211 \u6362 \u4e86 \u4e2a \u65b0 \u624b \u673a \uff0c \u53ef \u4ee5 \u76f4 \u63a5 \u626b \u8138 \u5c31 \u89e3 \u9501 \u5c4f \u5e55 \uff0c \u611f \u89c9 \u597d \u5389 \u5bb3 \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u4ee5 \u524d \u5bc6 \u7801 \u89e3 \u9501 \u6211 \u603b \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u4e5f \u5bb3 \u6015 \u81ea \u5df1 \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u73b0 \u5728 \u65b9 \u4fbf \u591a \u4e86 \u4e0d \u68c0 \u7d22 \u4eba \u8138 \u8bc6 \u522b \u6280 \u672f \uff1f \u8fd9 \u8fd8 \u6709 \u4e00 \u4e2a \u4e13 \u95e8 \u7684 \u5b66 \u672f \u540d \u8bcd \u5417 \u68c0 \u7d22 \u6211 \u603b \u5bb3 \u6015 \u522b \u4eba \u4e5f \u80fd \u7528 \u8138 \u89e3 \u5f00 \u6211 \u7684 \u624b \u673a \uff0c \u4f1a \u6709 \u8fd9 \u79cd \u53ef \u80fd \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u79d1 \u6280 \u4eba \u8138 \u8bc6 \u522b \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u9632 \u57ce \u6e2f \u5e02 \u6700 \u8fd1 \u6211 \u6362 \u4e86 \u4e2a \u65b0 \u624b \u673a \uff0c \u53ef \u4ee5 \u76f4 \u63a5 \u626b \u8138 \u5c31 \u89e3 \u9501 \u5c4f \u5e55 \uff0c \u611f \u89c9 \u597d \u5389 \u5bb3 \u68c0 \u7d22 \u5bf9 \u554a \uff0c \u4ee5 \u524d \u5bc6 \u7801 \u89e3 \u9501 \u6211 \u603b \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u4e5f \u5bb3 \u6015 \u81ea \u5df1 \u5fd8 \u8bb0 \u5bc6 \u7801 \uff0c \u73b0 \u5728 \u65b9 \u4fbf \u591a \u4e86 \u4e0d \u68c0 \u7d22 \u4eba \u8138 \u8bc6 \u522b \u6280 \u672f \uff1f \u8fd9 \u8fd8 \u6709 \u4e00 \u4e2a \u4e13 \u95e8 \u7684 \u5b66 \u672f \u540d \u8bcd \u5417 \u68c0 \u7d22 \u6211 \u603b \u5bb3 \u6015 \u522b \u4eba \u4e5f \u80fd \u7528 \u8138 \u89e3 \u5f00 \u6211 \u7684 \u624b \u673a \uff0c \u4f1a \u6709 \u8fd9 \u79cd \u53ef \u80fd \u5417 \uff1f \u68c0 \u7d22 \u79d1 \u6280 \u771f \u662f \u8d8a \u6765 \u8d8a \u53d1 \u8fbe \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u54c7 \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u6a58 \u732b \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u54c7 \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u6a58 \u732b \u5440 \u68c0 \u7d22 \u517b \u732b \u7684 \u8bdd \u8981 \u6ce8 \u610f \u4ec0 \u4e48 \u4e8b \u9879 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u54c7 \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u6a58 \u732b \u5440 \u68c0 \u7d22 \u517b \u732b \u7684 \u8bdd \u8981 \u6ce8 \u610f \u4ec0 \u4e48 \u4e8b \u9879 \u5462 \u68c0 \u7d22 \u6709 \u4ec0 \u4e48 \u597d \u7528 \u7684 \u732b \u7cae \u548c \u732b \u7802 \u63a8 \u8350 \u54a7 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u54c7 \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u6a58 \u732b \u5440 \u68c0 \u7d22 \u517b \u732b \u7684 \u8bdd \u8981 \u6ce8 \u610f \u4ec0 \u4e48 \u4e8b \u9879 \u5462 \u68c0 \u7d22 \u6709 \u4ec0 \u4e48 \u597d \u7528 \u7684 \u732b \u7cae \u548c \u732b \u7802 \u63a8 \u8350 \u54a7 \u68c0 \u7d22 \u542c \u8d77 \u6765 \u5f88 \u4e0d \u9519 \uff01 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u7f51 \u8d2d \u4e00 \u4e9b \u7ed9 \u6211 \u7684 \u732b \u732b \u505a \u51c6 \u5907 \uff0c \u73b0 \u5728 \u8fbd \u5b81 \u597d \u51b7 \u5440 \uff0c \u5c0f \u5976 \u732b \u4f1a \u4e0d \u4f1a \u5f88 \u96be \u71ac \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u8fbd \u5b81 \u7701 \u4e39 \u4e1c \u5e02 \u54c7 \u6211 \u597d \u60f3 \u517b \u4e00 \u53ea \u6a58 \u732b \u5440 \u68c0 \u7d22 \u517b \u732b \u7684 \u8bdd \u8981 \u6ce8 \u610f \u4ec0 \u4e48 \u4e8b \u9879 \u5462 \u68c0 \u7d22 \u6709 \u4ec0 \u4e48 \u597d \u7528 \u7684 \u732b \u7cae \u548c \u732b \u7802 \u63a8 \u8350 \u54a7 \u68c0 \u7d22 \u542c \u8d77 \u6765 \u5f88 \u4e0d \u9519 \uff01 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u7f51 \u8d2d \u4e00 \u4e9b \u7ed9 \u6211 \u7684 \u732b \u732b \u505a \u51c6 \u5907 \uff0c \u73b0 \u5728 \u8fbd \u5b81 \u597d \u51b7 \u5440 \uff0c \u5c0f \u5976 \u732b \u4f1a \u4e0d \u4f1a \u5f88 \u96be \u71ac \u68c0 \u7d22 \u542c \u8d77 \u6765 \u662f \u9700 \u8981 \u7cbe \u81f4 \u7684 \u624b \u5de5 \u6d3b \uff0c \u6211 \u4e4b \u524d \u6ca1 \u8bd5 \u8fc7 \uff0c \u600e \u4e48 \u6539 \u9020 \u65e7 \u8863 \u670d \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u80b2 \u513f \u7518 \u8083 \u7701 \u767d \u94f6 \u5e02 \u6211 \u54e5 \u54e5 \u7684 \u5b69 \u5b50 \u521a \u521a \u6ee1 \u6708 \u4e86 \u5462 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u80b2 \u513f \u7518 \u8083 \u7701 \u767d \u94f6 \u5e02 \u6211 \u54e5 \u54e5 \u7684 \u5b69 \u5b50 \u521a \u521a \u6ee1 \u6708 \u4e86 \u5462 \u4e0d \u68c0 \u7d22 \u662f \uff0c \u6ee1 \u6708 \u7684 \u5b69 \u5b50 \u53ef \u5c0f \u4e86 \uff0c \u4e5f \u4e0d \u592a \u95f9 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u80b2 \u513f \u7518 \u8083 \u7701 \u767d \u94f6 \u5e02 \u6211 \u54e5 \u54e5 \u7684 \u5b69 \u5b50 \u521a \u521a \u6ee1 \u6708 \u4e86 \u5462 \u4e0d \u68c0 \u7d22 \u662f \uff0c \u6ee1 \u6708 \u7684 \u5b69 \u5b50 \u53ef \u5c0f \u4e86 \uff0c \u4e5f \u4e0d \u592a \u95f9 \u68c0 \u7d22 \u662f \u5417 \uff0c \u90a3 \u6211 \u8981 \u591a \u53bb \u770b \u770b \u5979 \uff0c \u4e0d \u8fc7 \u5979 \u603b \u662f \u5728 \u7761 \u89c9 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u80b2 \u513f \u7518 \u8083 \u7701 \u767d \u94f6 \u5e02 \u6211 \u54e5 \u54e5 \u7684 \u5b69 \u5b50 \u521a \u521a \u6ee1 \u6708 \u4e86 \u5462 \u4e0d \u68c0 \u7d22 \u662f \uff0c \u6ee1 \u6708 \u7684 \u5b69 \u5b50 \u53ef \u5c0f \u4e86 \uff0c \u4e5f \u4e0d \u592a \u95f9 \u68c0 \u7d22 \u662f \u5417 \uff0c \u90a3 \u6211 \u8981 \u591a \u53bb \u770b \u770b \u5979 \uff0c \u4e0d \u8fc7 \u5979 \u603b \u662f \u5728 \u7761 \u89c9 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \uff0c \u7761 \u89c9 \u7684 \u65f6 \u5019 \u4e5f \u53ef \u7231 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u80b2 \u513f \u7518 \u8083 \u7701 \u767d \u94f6 \u5e02 \u6211 \u54e5 \u54e5 \u7684 \u5b69 \u5b50 \u521a \u521a \u6ee1 \u6708 \u4e86 \u5462 \u4e0d \u68c0 \u7d22 \u662f \uff0c \u6ee1 \u6708 \u7684 \u5b69 \u5b50 \u53ef \u5c0f \u4e86 \uff0c \u4e5f \u4e0d \u592a \u95f9 \u68c0 \u7d22 \u662f \u5417 \uff0c \u90a3 \u6211 \u8981 \u591a \u53bb \u770b \u770b \u5979 \uff0c \u4e0d \u8fc7 \u5979 \u603b \u662f \u5728 \u7761 \u89c9 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \uff0c \u7761 \u89c9 \u7684 \u65f6 \u5019 \u4e5f \u53ef \u7231 \u68c0 \u7d22 \u8ba9 \u6211 \u5ac2 \u5b50 \u7ed9 \u8bfb \u8bfb \uff0c \u6211 \u6700 \u8fd1 \u55d3 \u5b50 \u6709 \u70b9 \u54d1 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u80b2 \u513f \u7518 \u8083 \u7701 \u767d \u94f6 \u5e02 \u6211 \u54e5 \u54e5 \u7684 \u5b69 \u5b50 \u521a \u521a \u6ee1 \u6708 \u4e86 \u5462 \u4e0d \u68c0 \u7d22 \u662f \uff0c \u6ee1 \u6708 \u7684 \u5b69 \u5b50 \u53ef \u5c0f \u4e86 \uff0c \u4e5f \u4e0d \u592a \u95f9 \u68c0 \u7d22 \u662f \u5417 \uff0c \u90a3 \u6211 \u8981 \u591a \u53bb \u770b \u770b \u5979 \uff0c \u4e0d \u8fc7 \u5979 \u603b \u662f \u5728 \u7761 \u89c9 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \u54c8 \uff0c \u7761 \u89c9 \u7684 \u65f6 \u5019 \u4e5f \u53ef \u7231 \u68c0 \u7d22 \u8ba9 \u6211 \u5ac2 \u5b50 \u7ed9 \u8bfb \u8bfb \uff0c \u6211 \u6700 \u8fd1 \u55d3 \u5b50 \u6709 \u70b9 \u54d1 \u68c0 \u7d22 \u55ef \u55ef \u597d \u7684 \uff0c \u6211 \u4f1a \u6ce8 \u610f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u4f0a \u7281 \u54c8 \u8428 \u514b \u81ea \u6cbb \u5dde \u4f60 \u5c0f \u65f6 \u5019 \u770b \u52a8 \u6f2b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u4f0a \u7281 \u54c8 \u8428 \u514b \u81ea \u6cbb \u5dde \u4f60 \u5c0f \u65f6 \u5019 \u770b \u52a8 \u6f2b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \uff0c \u6211 \u6700 \u559c \u6b22 \u770b \u7684 \u5c31 \u662f \u300a \u559c \u7f8a \u7f8a \u4e0e \u7070 \u592a \u72fc \u300b \uff0c \u4f60 \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u4f0a \u7281 \u54c8 \u8428 \u514b \u81ea \u6cbb \u5dde \u4f60 \u5c0f \u65f6 \u5019 \u770b \u52a8 \u6f2b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \uff0c \u6211 \u6700 \u559c \u6b22 \u770b \u7684 \u5c31 \u662f \u300a \u559c \u7f8a \u7f8a \u4e0e \u7070 \u592a \u72fc \u300b \uff0c \u4f60 \u5462 \uff1f \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \u54ce \uff0c \u8fd9 \u4e2a \u597d \u770b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u4f0a \u7281 \u54c8 \u8428 \u514b \u81ea \u6cbb \u5dde \u4f60 \u5c0f \u65f6 \u5019 \u770b \u52a8 \u6f2b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \uff0c \u6211 \u6700 \u559c \u6b22 \u770b \u7684 \u5c31 \u662f \u300a \u559c \u7f8a \u7f8a \u4e0e \u7070 \u592a \u72fc \u300b \uff0c \u4f60 \u5462 \uff1f \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \u54ce \uff0c \u8fd9 \u4e2a \u597d \u770b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u53ef \u662f \u6211 \u73b0 \u5728 \u770b \u662f \u4e0d \u662f \u592a \u5e7c \u7a1a \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u4f0a \u7281 \u54c8 \u8428 \u514b \u81ea \u6cbb \u5dde \u4f60 \u5c0f \u65f6 \u5019 \u770b \u52a8 \u6f2b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \uff0c \u6211 \u6700 \u559c \u6b22 \u770b \u7684 \u5c31 \u662f \u300a \u559c \u7f8a \u7f8a \u4e0e \u7070 \u592a \u72fc \u300b \uff0c \u4f60 \u5462 \uff1f \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \u54ce \uff0c \u8fd9 \u4e2a \u597d \u770b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u53ef \u662f \u6211 \u73b0 \u5728 \u770b \u662f \u4e0d \u662f \u592a \u5e7c \u7a1a \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u597d \u7684 \uff0c \u6211 \u4e00 \u4f1a \u53bb \u641c \u7d22 \u4e00 \u4e0b \uff0c \u4f60 \u770b \u8fc7 \u300a \u5929 \u773c \u300b \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u4f0a \u7281 \u54c8 \u8428 \u514b \u81ea \u6cbb \u5dde \u4f60 \u5c0f \u65f6 \u5019 \u770b \u52a8 \u6f2b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u662f \uff0c \u6211 \u6700 \u559c \u6b22 \u770b \u7684 \u5c31 \u662f \u300a \u559c \u7f8a \u7f8a \u4e0e \u7070 \u592a \u72fc \u300b \uff0c \u4f60 \u5462 \uff1f \u68c0 \u7d22 \u6211 \u6ca1 \u6709 \u770b \u8fc7 \u54ce \uff0c \u8fd9 \u4e2a \u597d \u770b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u53ef \u662f \u6211 \u73b0 \u5728 \u770b \u662f \u4e0d \u662f \u592a \u5e7c \u7a1a \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u597d \u7684 \uff0c \u6211 \u4e00 \u4f1a \u53bb \u641c \u7d22 \u4e00 \u4e0b \uff0c \u4f60 \u770b \u8fc7 \u300a \u5929 \u773c \u300b \u5417 \uff1f \u4e0d \u68c0 \u7d22 \u8fd9 \u4e2a \u8bb2 \u7684 \u662f \u5c0f \u5b69 \u5b50 \u7684 \u6545 \u4e8b \uff0c \u8bf4 \u8d77 \u8fd9 \u4e2a \uff0c \u4f60 \u80af \u5b9a \u770b \u8fc7 \u300a \u5927 \u8033 \u6735 \u56fe \u56fe \u300b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u60ca \u5929 \u9b54 \u76d7 \u56e2 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6e7e \u4ed4 \u533a \u6709 \u4ec0 \u4e48 \u70e7 \u8111 \u7535 \u5f71 \u63a8 \u8350 \u5417 \uff0c \u6700 \u8fd1 \u597d \u65e0 \u804a \uff0c \u611f \u89c9 \u8111 \u5b50 \u7684 \u4e0d \u8f6c \u5708 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u60ca \u5929 \u9b54 \u76d7 \u56e2 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6e7e \u4ed4 \u533a \u6709 \u4ec0 \u4e48 \u70e7 \u8111 \u7535 \u5f71 \u63a8 \u8350 \u5417 \uff0c \u6700 \u8fd1 \u597d \u65e0 \u804a \uff0c \u611f \u89c9 \u8111 \u5b50 \u7684 \u4e0d \u8f6c \u5708 \u4e86 \u4e0d \u68c0 \u7d22 \u540d \u5b57 \u4e0d \u9519 \uff0c \u8fd9 \u662f \u5173 \u4e8e \u4ec0 \u4e48 \u7684 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u60ca \u5929 \u9b54 \u76d7 \u56e2 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6e7e \u4ed4 \u533a \u6709 \u4ec0 \u4e48 \u70e7 \u8111 \u7535 \u5f71 \u63a8 \u8350 \u5417 \uff0c \u6700 \u8fd1 \u597d \u65e0 \u804a \uff0c \u611f \u89c9 \u8111 \u5b50 \u7684 \u4e0d \u8f6c \u5708 \u4e86 \u4e0d \u68c0 \u7d22 \u540d \u5b57 \u4e0d \u9519 \uff0c \u8fd9 \u662f \u5173 \u4e8e \u4ec0 \u4e48 \u7684 \u5440 \u68c0 \u7d22 \u542c \u8d77 \u6765 \u4e0d \u9519 \u54ce \uff0c \u662f \u8c01 \u4e3b \u6f14 \u7684 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u60ca \u5929 \u9b54 \u76d7 \u56e2 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6e7e \u4ed4 \u533a \u6709 \u4ec0 \u4e48 \u70e7 \u8111 \u7535 \u5f71 \u63a8 \u8350 \u5417 \uff0c \u6700 \u8fd1 \u597d \u65e0 \u804a \uff0c \u611f \u89c9 \u8111 \u5b50 \u7684 \u4e0d \u8f6c \u5708 \u4e86 \u4e0d \u68c0 \u7d22 \u540d \u5b57 \u4e0d \u9519 \uff0c \u8fd9 \u662f \u5173 \u4e8e \u4ec0 \u4e48 \u7684 \u5440 \u68c0 \u7d22 \u542c \u8d77 \u6765 \u4e0d \u9519 \u54ce \uff0c \u662f \u8c01 \u4e3b \u6f14 \u7684 \u5440 \u68c0 \u7d22 \u6709 \u70b9 \u610f \u601d \uff0c \u7eff \u5de8 \u4eba \u4e5f \u5728 \u91cc \u9762 \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u60ca \u5929 \u9b54 \u76d7 \u56e2 \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u6e7e \u4ed4 \u533a \u6709 \u4ec0 \u4e48 \u70e7 \u8111 \u7535 \u5f71 \u63a8 \u8350 \u5417 \uff0c \u6700 \u8fd1 \u597d \u65e0 \u804a \uff0c \u611f \u89c9 \u8111 \u5b50 \u7684 \u4e0d \u8f6c \u5708 \u4e86 \u4e0d \u68c0 \u7d22 \u540d \u5b57 \u4e0d \u9519 \uff0c \u8fd9 \u662f \u5173 \u4e8e \u4ec0 \u4e48 \u7684 \u5440 \u68c0 \u7d22 \u542c \u8d77 \u6765 \u4e0d \u9519 \u54ce \uff0c \u662f \u8c01 \u4e3b \u6f14 