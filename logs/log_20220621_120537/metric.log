\u8350 \u70b9 \u597d \u5403 \u7684 \u96f6 \u98df \u5427 \uff01 \u6700 \u8fd1 \u6ca1 \u5b58 \u8d27 \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u5357 \u5b81 \u5e02 \u7ed9 \u6211 \u63a8 \u8350 \u70b9 \u597d \u5403 \u7684 \u96f6 \u98df \u5427 \uff01 \u6700 \u8fd1 \u6ca1 \u5b58 \u8d27 \u4e86 \u3002 \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u771f \u662f \u95ee \u5bf9 \u4eba \u4e86 \u3002 \u6211 \u5c31 \u662f \u60f3 \u5403 \u70b9 \u5565 \uff0c \u4f46 \u4e00 \u70b9 \u60f3 \u6cd5 \u90fd \u6ca1 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u5357 \u5b81 \u5e02 \u7ed9 \u6211 \u63a8 \u8350 \u70b9 \u597d \u5403 \u7684 \u96f6 \u98df \u5427 \uff01 \u6700 \u8fd1 \u6ca1 \u5b58 \u8d27 \u4e86 \u3002 \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u771f \u662f \u95ee \u5bf9 \u4eba \u4e86 \u3002 \u6211 \u5c31 \u662f \u60f3 \u5403 \u70b9 \u5565 \uff0c \u4f46 \u4e00 \u70b9 \u60f3 \u6cd5 \u90fd \u6ca1 \u3002 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u8bf4 \u6765 \u542c \u542c \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u5357 \u5b81 \u5e02 \u7ed9 \u6211 \u63a8 \u8350 \u70b9 \u597d \u5403 \u7684 \u96f6 \u98df \u5427 \uff01 \u6700 \u8fd1 \u6ca1 \u5b58 \u8d27 \u4e86 \u3002 \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u771f \u662f \u95ee \u5bf9 \u4eba \u4e86 \u3002 \u6211 \u5c31 \u662f \u60f3 \u5403 \u70b9 \u5565 \uff0c \u4f46 \u4e00 \u70b9 \u60f3 \u6cd5 \u90fd \u6ca1 \u3002 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u8bf4 \u6765 \u542c \u542c \u3002 \u68c0 \u7d22 \u6211 \u4e0d \u4fe1 \uff0c \u4ec0 \u4e48 \u4e1c \u897f \u629b \u5f00 \u91cf \u8bf4 \u90fd \u662f \u800d \u6d41 \u6c13 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u96f6 \u98df \u5e7f \u897f \u58ee \u65cf \u81ea \u6cbb \u533a \u5357 \u5b81 \u5e02 \u7ed9 \u6211 \u63a8 \u8350 \u70b9 \u597d \u5403 \u7684 \u96f6 \u98df \u5427 \uff01 \u6700 \u8fd1 \u6ca1 \u5b58 \u8d27 \u4e86 \u3002 \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6211 \u771f \u662f \u95ee \u5bf9 \u4eba \u4e86 \u3002 \u6211 \u5c31 \u662f \u60f3 \u5403 \u70b9 \u5565 \uff0c \u4f46 \u4e00 \u70b9 \u60f3 \u6cd5 \u90fd \u6ca1 \u3002 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u8bf4 \u6765 \u542c \u542c \u3002 \u68c0 \u7d22 \u6211 \u4e0d \u4fe1 \uff0c \u4ec0 \u4e48 \u4e1c \u897f \u629b \u5f00 \u91cf \u8bf4 \u90fd \u662f \u800d \u6d41 \u6c13 \u3002 \u4e0d \u68c0 \u7d22 \u4e0d \u597d \u5403 \u4e3a \u5565 \u8981 \u4e70 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u56db \u5ddd \u4e91 \u5357 \u7701 \u8fea \u5e86 \u85cf \u65cf \u81ea \u6cbb \u5dde \u75ab \u60c5 \u671f \u95f4 \uff0c \u53bb \u54ea \u513f \u90fd \u4e0d \u65b9 \u4fbf \uff0c \u6000 \u5ff5 \u4ee5 \u524d \u53ef \u4ee5 \u5230 \u5904 \u65c5 \u6e38 \u7684 \u65e5 \u5b50 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u56db \u5ddd \u4e91 \u5357 \u7701 \u8fea \u5e86 \u85cf \u65cf \u81ea \u6cbb \u5dde \u75ab \u60c5 \u671f \u95f4 \uff0c \u53bb \u54ea \u513f \u90fd \u4e0d \u65b9 \u4fbf \uff0c \u6000 \u5ff5 \u4ee5 \u524d \u53ef \u4ee5 \u5230 \u5904 \u65c5 \u6e38 \u7684 \u65e5 \u5b50 \u3002 \u4e0d \u68c0 \u7d22 \u56db \u5ddd \u5417 \uff0c \u6211 \u4e00 \u76f4 \u5f88 \u60f3 \u53bb \u8fd9 \u4e2a \u5730 \u65b9 \uff0c \u4e0d \u8bba \u662f \u98ce \u666f \u8fd8 \u662f \u7f8e \u98df \uff0c \u6211 \u90fd \u5f88 \u60f3 \u53bb \u4f53 \u9a8c \u4e00 \u4e0b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u56db \u5ddd \u4e91 \u5357 \u7701 \u8fea \u5e86 \u85cf \u65cf \u81ea \u6cbb \u5dde \u75ab \u60c5 \u671f \u95f4 \uff0c \u53bb \u54ea \u513f \u90fd \u4e0d \u65b9 \u4fbf \uff0c \u6000 \u5ff5 \u4ee5 \u524d \u53ef \u4ee5 \u5230 \u5904 \u65c5 \u6e38 \u7684 \u65e5 \u5b50 \u3002 \u4e0d \u68c0 \u7d22 \u56db \u5ddd \u5417 \uff0c \u6211 \u4e00 \u76f4 \u5f88 \u60f3 \u53bb \u8fd9 \u4e2a \u5730 \u65b9 \uff0c \u4e0d \u8bba \u662f \u98ce \u666f \u8fd8 \u662f \u7f8e \u98df \uff0c \u6211 \u90fd \u5f88 \u60f3 \u53bb \u4f53 \u9a8c \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u771f \u7fa1 \u6155 \u554a \uff0c \u53ef \u60dc \u6211 \u8fd8 \u662f \u4e2a \u5b66 \u751f \uff0c \u7ecf \u8d39 \u4e0d \u592a \u8db3 \uff0c \u80fd \u4e0d \u80fd \u7ed9 \u6211 \u63a8 \u8350 \u6700 \u503c \u5f97 \u53bb \u7684 \u4e00 \u4e24 \u5904 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u56db \u5ddd \u4e91 \u5357 \u7701 \u8fea \u5e86 \u85cf \u65cf \u81ea \u6cbb \u5dde \u75ab \u60c5 \u671f \u95f4 \uff0c \u53bb \u54ea \u513f \u90fd \u4e0d \u65b9 \u4fbf \uff0c \u6000 \u5ff5 \u4ee5 \u524d \u53ef \u4ee5 \u5230 \u5904 \u65c5 \u6e38 \u7684 \u65e5 \u5b50 \u3002 \u4e0d \u68c0 \u7d22 \u56db \u5ddd \u5417 \uff0c \u6211 \u4e00 \u76f4 \u5f88 \u60f3 \u53bb \u8fd9 \u4e2a \u5730 \u65b9 \uff0c \u4e0d \u8bba \u662f \u98ce \u666f \u8fd8 \u662f \u7f8e \u98df \uff0c \u6211 \u90fd \u5f88 \u60f3 \u53bb \u4f53 \u9a8c \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u771f \u7fa1 \u6155 \u554a \uff0c \u53ef \u60dc \u6211 \u8fd8 \u662f \u4e2a \u5b66 \u751f \uff0c \u7ecf \u8d39 \u4e0d \u592a \u8db3 \uff0c \u80fd \u4e0d \u80fd \u7ed9 \u6211 \u63a8 \u8350 \u6700 \u503c \u5f97 \u53bb \u7684 \u4e00 \u4e24 \u5904 \u5440 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u6211 \u592a \u671f \u5f85 \u4e86 \uff01 \u8fd8 \u6709 \u6ca1 \u6709 \u4ec0 \u4e48 \u7f8e \u98df \u63a8 \u8350 \u5440 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u56db \u5ddd \u4e91 \u5357 \u7701 \u8fea \u5e86 \u85cf \u65cf \u81ea \u6cbb \u5dde \u75ab \u60c5 \u671f \u95f4 \uff0c \u53bb \u54ea \u513f \u90fd \u4e0d \u65b9 \u4fbf \uff0c \u6000 \u5ff5 \u4ee5 \u524d \u53ef \u4ee5 \u5230 \u5904 \u65c5 \u6e38 \u7684 \u65e5 \u5b50 \u3002 \u4e0d \u68c0 \u7d22 \u56db \u5ddd \u5417 \uff0c \u6211 \u4e00 \u76f4 \u5f88 \u60f3 \u53bb \u8fd9 \u4e2a \u5730 \u65b9 \uff0c \u4e0d \u8bba \u662f \u98ce \u666f \u8fd8 \u662f \u7f8e \u98df \uff0c \u6211 \u90fd \u5f88 \u60f3 \u53bb \u4f53 \u9a8c \u4e00 \u4e0b \u3002 \u68c0 \u7d22 \u771f \u7fa1 \u6155 \u554a \uff0c \u53ef \u60dc \u6211 \u8fd8 \u662f \u4e2a \u5b66 \u751f \uff0c \u7ecf \u8d39 \u4e0d \u592a \u8db3 \uff0c \u80fd \u4e0d \u80fd \u7ed9 \u6211 \u63a8 \u8350 \u6700 \u503c \u5f97 \u53bb \u7684 \u4e00 \u4e24 \u5904 \u5440 \uff1f \u68c0 \u7d22 \u54c7 \uff0c \u6211 \u592a \u671f \u5f85 \u4e86 \uff01 \u8fd8 \u6709 \u6ca1 \u6709 \u4ec0 \u4e48 \u7f8e \u98df \u63a8 \u8350 \u5440 \uff1f \u68c0 \u7d22 \u542c \u7684 \u6211 \u90fd \u997f \u4e86 \uff01 \u5e0c \u671b \u75ab \u60c5 \u8d76 \u7d27 \u7ed3 \u675f \uff0c \u53ef \u4ee5 \u8ba9 \u6211 \u597d \u597d \u901b \u901b \u56db \u5ddd \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u670b \u53cb \u5f88 \u559c \u6b22 \u72d7 \uff0c \u4ed6 \u5bb6 \u6709 \u4e00 \u53ea \u5c0f \u6cf0 \u8fea \uff0c \u5f88 \u53ef \u7231 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u670b \u53cb \u5f88 \u559c \u6b22 \u72d7 \uff0c \u4ed6 \u5bb6 \u6709 \u4e00 \u53ea \u5c0f \u6cf0 \u8fea \uff0c \u5f88 \u53ef \u7231 \u4e0d \u68c0 \u7d22 \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u8981 \u9009 \u4ec0 \u4e48 \u6837 \u7684 \u54c1 \u79cd \uff0c \u4f60 \u77e5 \u9053 \u4ec0 \u4e48 \u54c1 \u79cd \u7684 \u5c0f \u72d7 \u6027 \u60c5 \u6bd4 \u8f83 \u6e29 \u548c \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u670b \u53cb \u5f88 \u559c \u6b22 \u72d7 \uff0c \u4ed6 \u5bb6 \u6709 \u4e00 \u53ea \u5c0f \u6cf0 \u8fea \uff0c \u5f88 \u53ef \u7231 \u4e0d \u68c0 \u7d22 \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u8981 \u9009 \u4ec0 \u4e48 \u6837 \u7684 \u54c1 \u79cd \uff0c \u4f60 \u77e5 \u9053 \u4ec0 \u4e48 \u54c1 \u79cd \u7684 \u5c0f \u72d7 \u6027 \u60c5 \u6bd4 \u8f83 \u6e29 \u548c \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u4f60 \u77e5 \u9053 \u72d7 \u7cae \u7684 \u4ef7 \u683c \u662f \u591a \u5c11 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u670b \u53cb \u5f88 \u559c \u6b22 \u72d7 \uff0c \u4ed6 \u5bb6 \u6709 \u4e00 \u53ea \u5c0f \u6cf0 \u8fea \uff0c \u5f88 \u53ef \u7231 \u4e0d \u68c0 \u7d22 \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u8981 \u9009 \u4ec0 \u4e48 \u6837 \u7684 \u54c1 \u79cd \uff0c \u4f60 \u77e5 \u9053 \u4ec0 \u4e48 \u54c1 \u79cd \u7684 \u5c0f \u72d7 \u6027 \u60c5 \u6bd4 \u8f83 \u6e29 \u548c \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u4f60 \u77e5 \u9053 \u72d7 \u7cae \u7684 \u4ef7 \u683c \u662f \u591a \u5c11 \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u8fd8 \u4e0d \u9519 \uff0c \u4e0d \u662f \u5f88 \u8d35 \u54df \u3002 \u4f60 \u77e5 \u9053 \u9644 \u8fd1 \u54ea \u6709 \u5ba0 \u7269 \u5e97 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u6d77 \u5357 \u7701 \u743c \u6d77 \u5e02 \u670b \u53cb \u5f88 \u559c \u6b22 \u72d7 \uff0c \u4ed6 \u5bb6 \u6709 \u4e00 \u53ea \u5c0f \u6cf0 \u8fea \uff0c \u5f88 \u53ef \u7231 \u4e0d \u68c0 \u7d22 \u4f46 \u662f \u6211 \u4e0d \u77e5 \u9053 \u8981 \u9009 \u4ec0 \u4e48 \u6837 \u7684 \u54c1 \u79cd \uff0c \u4f60 \u77e5 \u9053 \u4ec0 \u4e48 \u54c1 \u79cd \u7684 \u5c0f \u72d7 \u6027 \u60c5 \u6bd4 \u8f83 \u6e29 \u548c \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u4f60 \u77e5 \u9053 \u72d7 \u7cae \u7684 \u4ef7 \u683c \u662f \u591a \u5c11 \u5417 \uff1f \u68c0 \u7d22 \u90a3 \u8fd8 \u4e0d \u9519 \uff0c \u4e0d \u662f \u5f88 \u8d35 \u54df \u3002 \u4f60 \u77e5 \u9053 \u9644 \u8fd1 \u54ea \u6709 \u5ba0 \u7269 \u5e97 \u5417 \uff1f \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6b63 \u597d \u79bb \u6211 \u5bb6 \u5f88 \u8fd1 \uff0c \u6211 \u9a6c \u4e0a \u5c31 \u53bb \u770b \u770b \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u6f6e \u6c55 \u7f8e \u98df \u771f \u7684 \u591a \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u6f6e \u6c55 \u7f8e \u98df \u771f \u7684 \u591a \uff01 \u68c0 \u7d22 \u563b \u563b \u563b \u563b \uff0c \u90a3 \u4f60 \u77e5 \u9053 \u869d \u70d9 \u600e \u4e48 \u505a \u7684 \u5417 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u6f6e \u6c55 \u7f8e \u98df \u771f \u7684 \u591a \uff01 \u68c0 \u7d22 \u563b \u563b \u563b \u563b \uff0c \u90a3 \u4f60 \u77e5 \u9053 \u869d \u70d9 \u600e \u4e48 \u505a \u7684 \u5417 \u68c0 \u7d22 \u4f60 \u5c45 \u7136 \u5403 \u8fc7 \uff0c \u8bdd \u8bf4 \u6211 \u5df2 \u7ecf \u79bb \u5f00 \u5bb6 \u5f88 \u4e45 \u4e86 \uff0c \u597d \u4e45 \u6ca1 \u5403 \u5230 \u869d \u70d9 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u6f6e \u6c55 \u7f8e \u98df \u771f \u7684 \u591a \uff01 \u68c0 \u7d22 \u563b \u563b \u563b \u563b \uff0c \u90a3 \u4f60 \u77e5 \u9053 \u869d \u70d9 \u600e \u4e48 \u505a \u7684 \u5417 \u68c0 \u7d22 \u4f60 \u5c45 \u7136 \u5403 \u8fc7 \uff0c \u8bdd \u8bf4 \u6211 \u5df2 \u7ecf \u79bb \u5f00 \u5bb6 \u5f88 \u4e45 \u4e86 \uff0c \u597d \u4e45 \u6ca1 \u5403 \u5230 \u869d \u70d9 \u4e86 \u4e0d \u68c0 \u7d22 \u4e91 \u5357 \u6709 \u4ec0 \u4e48 \u7f8e \u98df \u5417 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u5c71 \u897f \u7701 \u8fd0 \u57ce \u5e02 \u6f6e \u6c55 \u7f8e \u98df \u771f \u7684 \u591a \uff01 \u68c0 \u7d22 \u563b \u563b \u563b \u563b \uff0c \u90a3 \u4f60 \u77e5 \u9053 \u869d \u70d9 \u600e \u4e48 \u505a \u7684 \u5417 \u68c0 \u7d22 \u4f60 \u5c45 \u7136 \u5403 \u8fc7 \uff0c \u8bdd \u8bf4 \u6211 \u5df2 \u7ecf \u79bb \u5f00 \u5bb6 \u5f88 \u4e45 \u4e86 \uff0c \u597d \u4e45 \u6ca1 \u5403 \u5230 \u869d \u70d9 \u4e86 \u4e0d \u68c0 \u7d22 \u4e91 \u5357 \u6709 \u4ec0 \u4e48 \u7f8e \u98df \u5417 \uff01 \u68c0 \u7d22 \u597d \u5440 \u597d \u5440 \uff0c \u6211 \u4e00 \u5b9a \u8981 \u53bb \u5403 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u4e91 \u5357 \u65c5 \u884c \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u9f50 \u9f50 \u54c8 \u5c14 \u7684 \u51ac \u5929 \u5b9e \u5728 \u592a \u51b7 \u4e86 \uff0c \u4eca \u5929 \u4e0b \u73ed \u56de \u6765 \uff0c \u7b80 \u76f4 \u8981 \u51bb \u4f4f \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u4e91 \u5357 \u65c5 \u884c \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u9f50 \u9f50 \u54c8 \u5c14 \u7684 \u51ac \u5929 \u5b9e \u5728 \u592a \u51b7 \u4e86 \uff0c \u4eca \u5929 \u4e0b \u73ed \u56de \u6765 \uff0c \u7b80 \u76f4 \u8981 \u51bb \u4f4f \u4e86 \u3002 \u68c0 \u7d22 \u4e1c \u5317 \u7684 \u51ac \u5929 \u592a \u957f \u4e86 \uff0c \u6211 \u60f3 \u53bb \u5357 \u65b9 \u5f85 \u4e00 \u9635 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u4e91 \u5357 \u65c5 \u884c \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u9f50 \u9f50 \u54c8 \u5c14 \u7684 \u51ac \u5929 \u5b9e \u5728 \u592a \u51b7 \u4e86 \uff0c \u4eca \u5929 \u4e0b \u73ed \u56de \u6765 \uff0c \u7b80 \u76f4 \u8981 \u51bb \u4f4f \u4e86 \u3002 \u68c0 \u7d22 \u4e1c \u5317 \u7684 \u51ac \u5929 \u592a \u957f \u4e86 \uff0c \u6211 \u60f3 \u53bb \u5357 \u65b9 \u5f85 \u4e00 \u9635 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u8fd8 \u6ca1 \u786e \u5b9a \uff0c \u4f60 \u5e2e \u6211 \u60f3 \u60f3 \u5427 \u3002 \u60f3 \u627e \u4e2a \u56db \u5b63 \u5982 \u6625 \uff0c \u9e1f \u8bed \u82b1 \u9999 \u7684 \u5730 \u65b9 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u4e91 \u5357 \u65c5 \u884c \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u9f50 \u9f50 \u54c8 \u5c14 \u7684 \u51ac \u5929 \u5b9e \u5728 \u592a \u51b7 \u4e86 \uff0c \u4eca \u5929 \u4e0b \u73ed \u56de \u6765 \uff0c \u7b80 \u76f4 \u8981 \u51bb \u4f4f \u4e86 \u3002 \u68c0 \u7d22 \u4e1c \u5317 \u7684 \u51ac \u5929 \u592a \u957f \u4e86 \uff0c \u6211 \u60f3 \u53bb \u5357 \u65b9 \u5f85 \u4e00 \u9635 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u8fd8 \u6ca1 \u786e \u5b9a \uff0c \u4f60 \u5e2e \u6211 \u60f3 \u60f3 \u5427 \u3002 \u60f3 \u627e \u4e2a \u56db \u5b63 \u5982 \u6625 \uff0c \u9e1f \u8bed \u82b1 \u9999 \u7684 \u5730 \u65b9 \u3002 \u68c0 \u7d22 \u8bf4 \u7684 \u5bf9 \u554a \uff0c \u6211 \u5e74 \u5e95 \u8f9e \u804c \u540e \uff0c \u6709 \u5927 \u628a \u65f6 \u95f4 \u5230 \u5904 \u901b \u3002 \u6700 \u597d \u628a \u4e91 \u5357 \u90fd \u597d \u597d \u901b \u4e00 \u904d \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u4e91 \u5357 \u65c5 \u884c \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u9f50 \u9f50 \u54c8 \u5c14 \u7684 \u51ac \u5929 \u5b9e \u5728 \u592a \u51b7 \u4e86 \uff0c \u4eca \u5929 \u4e0b \u73ed \u56de \u6765 \uff0c \u7b80 \u76f4 \u8981 \u51bb \u4f4f \u4e86 \u3002 \u68c0 \u7d22 \u4e1c \u5317 \u7684 \u51ac \u5929 \u592a \u957f \u4e86 \uff0c \u6211 \u60f3 \u53bb \u5357 \u65b9 \u5f85 \u4e00 \u9635 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u8fd8 \u6ca1 \u786e \u5b9a \uff0c \u4f60 \u5e2e \u6211 \u60f3 \u60f3 \u5427 \u3002 \u60f3 \u627e \u4e2a \u56db \u5b63 \u5982 \u6625 \uff0c \u9e1f \u8bed \u82b1 \u9999 \u7684 \u5730 \u65b9 \u3002 \u68c0 \u7d22 \u8bf4 \u7684 \u5bf9 \u554a \uff0c \u6211 \u5e74 \u5e95 \u8f9e \u804c \u540e \uff0c \u6709 \u5927 \u628a \u65f6 \u95f4 \u5230 \u5904 \u901b \u3002 \u6700 \u597d \u628a \u4e91 \u5357 \u90fd \u597d \u597d \u901b \u4e00 \u904d \u3002 \u68c0 \u7d22 \u53bb \u4e91 \u5357 \u4e5f \u8981 \u9632 \u5bd2 \u5417 \uff1f \u8bdd \u8bf4 \u4e0d \u77e5 \u9053 \u6606 \u660e \u4eca \u5929 \u4ec0 \u4e48 \u5929 \u6c14 \u554a \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u65c5 \u884c \u4e91 \u5357 \u65c5 \u884c \u9ed1 \u9f99 \u6c5f \u7701 \u9f50 \u9f50 \u54c8 \u5c14 \u5e02 \u9f50 \u9f50 \u54c8 \u5c14 \u7684 \u51ac \u5929 \u5b9e \u5728 \u592a \u51b7 \u4e86 \uff0c \u4eca \u5929 \u4e0b \u73ed \u56de \u6765 \uff0c \u7b80 \u76f4 \u8981 \u51bb \u4f4f \u4e86 \u3002 \u68c0 \u7d22 \u4e1c \u5317 \u7684 \u51ac \u5929 \u592a \u957f \u4e86 \uff0c \u6211 \u60f3 \u53bb \u5357 \u65b9 \u5f85 \u4e00 \u9635 \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u8fd8 \u6ca1 \u786e \u5b9a \uff0c \u4f60 \u5e2e \u6211 \u60f3 \u60f3 \u5427 \u3002 \u60f3 \u627e \u4e2a \u56db \u5b63 \u5982 \u6625 \uff0c \u9e1f \u8bed \u82b1 \u9999 \u7684 \u5730 \u65b9 \u3002 \u68c0 \u7d22 \u8bf4 \u7684 \u5bf9 \u554a \uff0c \u6211 \u5e74 \u5e95 \u8f9e \u804c \u540e \uff0c \u6709 \u5927 \u628a \u65f6 \u95f4 \u5230 \u5904 \u901b \u3002 \u6700 \u597d \u628a \u4e91 \u5357 \u90fd \u597d \u597d \u901b \u4e00 \u904d \u3002 \u68c0 \u7d22 \u53bb \u4e91 \u5357 \u4e5f \u8981 \u9632 \u5bd2 \u5417 \uff1f \u8bdd \u8bf4 \u4e0d \u77e5 \u9053 \u6606 \u660e \u4eca \u5929 \u4ec0 \u4e48 \u5929 \u6c14 \u554a \uff1f \u68c0 \u7d22 \u4ec0 \u4e48 \uff1f \u6bd4 \u8f83 \u51b7 \u8fd8 \u9ad8 27 \u5ea6 \uff1f \u6211 \u8981 \u53bb \u6606 \u660e \uff0c \u8c01 \u90fd \u522b \u62e6 \u7740 \u6211 \uff01 \u6211 \u73b0 \u5728 \u5c31 \u53bb \u8ba2 \u673a \u7968 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u5927 \u72d7 \u72d7 \u597d \u6709 \u5b89 \u5168 \u611f \u554a \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u5927 \u72d7 \u72d7 \u597d \u6709 \u5b89 \u5168 \u611f \u554a \u68c0 \u7d22 \u6ca1 \u6709 \u5206 \u6e05 \u695a \u662f \u62c9 \u5e03 \u62c9 \u591a \u8fd8 \u662f \u91d1 \u6bdb \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u5927 \u72d7 \u72d7 \u597d \u6709 \u5b89 \u5168 \u611f \u554a \u68c0 \u7d22 \u6ca1 \u6709 \u5206 \u6e05 \u695a \u662f \u62c9 \u5e03 \u62c9 \u591a \u8fd8 \u662f \u91d1 \u6bdb \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u90a3 \u5b83 \u4eec \u7684 \u6027 \u683c \u6709 \u5dee \u8ddd \u4e48 \uff1f \u90a3 \u4e2a \u66f4 \u9002 \u5408 \u5bb6 \u517b \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u5927 \u72d7 \u72d7 \u597d \u6709 \u5b89 \u5168 \u611f \u554a \u68c0 \u7d22 \u6ca1 \u6709 \u5206 \u6e05 \u695a \u662f \u62c9 \u5e03 \u62c9 \u591a \u8fd8 \u662f \u91d1 \u6bdb \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u90a3 \u5b83 \u4eec \u7684 \u6027 \u683c \u6709 \u5dee \u8ddd \u4e48 \uff1f \u90a3 \u4e2a \u66f4 \u9002 \u5408 \u5bb6 \u517b \u5440 \u68c0 \u7d22 \u8fd8 \u6709 \u4ec0 \u4e48 \u5927 \u578b \u72ac \u9002 \u5408 \u5bb6 \u517b \u5440 \u3002 \u68c0 \u7d22 \u3002', '\u751f \u6d3b \u5ba0 \u7269 \u72d7 \u6df1 \u5733 \u767e \u5ea6 \u56fd \u9645 \u5927 \u53a6 \u5927 \u72d7 \u72d7 \u597d \u6709 \u5b89 \u5168 \u611f \u554a \u68c0 \u7d22 \u6ca1 \u6709 \u5206 \u6e05 \u695a \u662f \u62c9 \u5e03 \u62c9 \u591a \u8fd8 \u662f \u91d1 \u6bdb \u68c0 \u7d22 \u54e6 \u54e6 \uff0c \u90a3 \u5b83 \u4eec \u7684 \u6027 \u683c \u6709 \u5dee \u8ddd \u4e48 \uff1f \u90a3 \u4e2a \u66f4 \u9002 \u5408 \u5bb6 \u517b \u5440 \u68c0 \u7d22 \u8fd8 \u6709 \u4ec0 \u4e48 \u5927 \u578b \u72ac \u9002 \u5408 \u5bb6 \u517b \u5440 \u4e0d \u68c0 \u7d22 \u4e0d \u4e86 \u4e0d \u6765 \uff0c \u6015 \u5b83 \u62c6 \u5bb6 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u6c5f \u82cf \u7701 \u626c \u5dde \u5e02 \u521a \u624d \u4f60 \u54fc \u6b4c \u597d \u597d \u542c \u554a \uff0c \u54fc \u7684 \u5468 \u6770 \u4f26 \u5427 \uff0c \u600e \u4e48 \u505c \u4e86 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u6c5f \u82cf \u7701 \u626c \u5dde \u5e02 \u521a \u624d \u4f60 \u54fc \u6b4c \u597d \u597d \u542c \u554a \uff0c \u54fc \u7684 \u5468 \u6770 \u4f26 \u5427 \uff0c \u600e \u4e48 \u505c \u4e86 \uff1f \u4e0d \u68c0 \u7d22 \u54b1 \u4eec \u8fd9 \u4e00 \u4ee3 \u5f88 \u5c11 \u6709 \u4eba \u4e0d \u559c \u6b22 \u5468 \u6770 \u4f26 \u7684 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u6c5f \u82cf \u7701 \u626c \u5dde \u5e02 \u521a \u624d \u4f60 \u54fc \u6b4c \u597d \u597d \u542c \u554a \uff0c \u54fc \u7684 \u5468 \u6770 \u4f26 \u5427 \uff0c \u600e \u4e48 \u505c \u4e86 \uff1f \u4e0d \u68c0 \u7d22 \u54b1 \u4eec \u8fd9 \u4e00 \u4ee3 \u5f88 \u5c11 \u6709 \u4eba \u4e0d \u559c \u6b22 \u5468 \u6770 \u4f26 \u7684 \u3002 \u68c0 \u7d22 \u90a3 \u770b \u6765 \u4f60 \u786e \u5b9e \u662f \u8001 \u7c89 \u4e86 \uff0c \u96be \u602a \u4f60 \u5531 \u8fd9 \u4e48 \u597d \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u6c5f \u82cf \u7701 \u626c \u5dde \u5e02 \u521a \u624d \u4f60 \u54fc \u6b4c \u597d \u597d \u542c \u554a \uff0c \u54fc \u7684 \u5468 \u6770 \u4f26 \u5427 \uff0c \u600e \u4e48 \u505c \u4e86 \uff1f \u4e0d \u68c0 \u7d22 \u54b1 \u4eec \u8fd9 \u4e00 \u4ee3 \u5f88 \u5c11 \u6709 \u4eba \u4e0d \u559c \u6b22 \u5468 \u6770 \u4f26 \u7684 \u3002 \u68c0 \u7d22 \u90a3 \u770b \u6765 \u4f60 \u786e \u5b9e \u662f \u8001 \u7c89 \u4e86 \uff0c \u96be \u602a \u4f60 \u5531 \u8fd9 \u4e48 \u597d \u3002 \u68c0 \u7d22 \u725b \u4ed4 \u5f88 \u5fd9 \uff0c \u54c8 \u54c8 \uff0c \u6211 \u7b2c \u4e00 \u6b21 \u542c \u90fd \u611f \u89c9 \u70eb \u5634 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u97f3 \u4e50 \u6c5f \u82cf \u7701 \u626c \u5dde \u5e02 \u521a \u624d \u4f60 \u54fc \u6b4c \u597d \u597d \u542c \u554a \uff0c \u54fc \u7684 \u5468 \u6770 \u4f26 \u5427 \uff0c \u600e \u4e48 \u505c \u4e86 \uff1f \u4e0d \u68c0 \u7d22 \u54b1 \u4eec \u8fd9 \u4e00 \u4ee3 \u5f88 \u5c11 \u6709 \u4eba \u4e0d \u559c \u6b22 \u5468 \u6770 \u4f26 \u7684 \u3002 \u68c0 \u7d22 \u90a3 \u770b \u6765 \u4f60 \u786e \u5b9e \u662f \u8001 \u7c89 \u4e86 \uff0c \u96be \u602a \u4f60 \u5531 \u8fd9 \u4e48 \u597d \u3002 \u68c0 \u7d22 \u725b \u4ed4 \u5f88 \u5fd9 \uff0c \u54c8 \u54c8 \uff0c \u6211 \u7b2c \u4e00 \u6b21 \u542c \u90fd \u611f \u89c9 \u70eb \u5634 \u3002 \u4e0d \u68c0 \u7d22 \u4e0d \u8fc7 \u8fd9 \u4e48 \u8bf4 \u4f60 \u524d \u9762 \u4e3e \u7684 \u90a3 \u4e9b \u6b4c \u4f60 \u5e94 \u8be5 \u90fd \u5531 \u5f97 \u5f88 \u597d \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u79d1 \u5e7b \u798f \u5efa \u7701 \u8386 \u7530 \u5e02 \u4f60 \u7231 \u770b \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u6bd4 \u5982 \u8bf4 \u300a \u53d8 \u5f62 \u91d1 \u521a \u300b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u79d1 \u5e7b \u798f \u5efa \u7701 \u8386 \u7530 \u5e02 \u4f60 \u7231 \u770b \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u6bd4 \u5982 \u8bf4 \u300a \u53d8 \u5f62 \u91d1 \u521a \u300b \u3002 \u68c0 \u7d22 \u6211 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \u300a \u5927 \u9ec4 \u8702 \u300b \uff0c \u5927 \u9ec4 \u8702 \u592a \u53ef \u7231 \u4e86 \uff0c \u4e24 \u53ea \u5927 \u773c \u775b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u79d1 \u5e7b \u798f \u5efa \u7701 \u8386 \u7530 \u5e02 \u4f60 \u7231 \u770b \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u6bd4 \u5982 \u8bf4 \u300a \u53d8 \u5f62 \u91d1 \u521a \u300b \u3002 \u68c0 \u7d22 \u6211 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \u300a \u5927 \u9ec4 \u8702 \u300b \uff0c \u5927 \u9ec4 \u8702 \u592a \u53ef \u7231 \u4e86 \uff0c \u4e24 \u53ea \u5927 \u773c \u775b \u3002 \u68c0 \u7d22 \u300a \u73af \u592a \u5e73 \u6d0b \u300b \u4f60 \u770b \u8fc7 \u5417 \uff1f \u8fd9 \u4e2a \u4e5f \u975e \u5e38 \u597d \u770b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u79d1 \u5e7b \u798f \u5efa \u7701 \u8386 \u7530 \u5e02 \u4f60 \u7231 \u770b \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u6bd4 \u5982 \u8bf4 \u300a \u53d8 \u5f62 \u91d1 \u521a \u300b \u3002 \u68c0 \u7d22 \u6211 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \u300a \u5927 \u9ec4 \u8702 \u300b \uff0c \u5927 \u9ec4 \u8702 \u592a \u53ef \u7231 \u4e86 \uff0c \u4e24 \u53ea \u5927 \u773c \u775b \u3002 \u68c0 \u7d22 \u300a \u73af \u592a \u5e73 \u6d0b \u300b \u4f60 \u770b \u8fc7 \u5417 \uff1f \u8fd9 \u4e2a \u4e5f \u975e \u5e38 \u597d \u770b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u770b \u4e86 \uff0c \u6211 \u7b2c \u4e8c \u4e2a \u7535 \u5f71 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \uff0c 3d \u7535 \u5f71 \u7279 \u6548 \u975e \u5e38 \u597d \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u79d1 \u5e7b \u798f \u5efa \u7701 \u8386 \u7530 \u5e02 \u4f60 \u7231 \u770b \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u6bd4 \u5982 \u8bf4 \u300a \u53d8 \u5f62 \u91d1 \u521a \u300b \u3002 \u68c0 \u7d22 \u6211 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \u300a \u5927 \u9ec4 \u8702 \u300b \uff0c \u5927 \u9ec4 \u8702 \u592a \u53ef \u7231 \u4e86 \uff0c \u4e24 \u53ea \u5927 \u773c \u775b \u3002 \u68c0 \u7d22 \u300a \u73af \u592a \u5e73 \u6d0b \u300b \u4f60 \u770b \u8fc7 \u5417 \uff1f \u8fd9 \u4e2a \u4e5f \u975e \u5e38 \u597d \u770b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u770b \u4e86 \uff0c \u6211 \u7b2c \u4e8c \u4e2a \u7535 \u5f71 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \uff0c 3d \u7535 \u5f71 \u7279 \u6548 \u975e \u5e38 \u597d \u3002 \u4e0d \u68c0 \u7d22 \u8fd8 \u8981 \u4ec0 \u4e48 \u597d \u770b \u7684 \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u7535 \u5f71 \u79d1 \u5e7b \u798f \u5efa \u7701 \u8386 \u7530 \u5e02 \u4f60 \u7231 \u770b \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u6bd4 \u5982 \u8bf4 \u300a \u53d8 \u5f62 \u91d1 \u521a \u300b \u3002 \u68c0 \u7d22 \u6211 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \u300a \u5927 \u9ec4 \u8702 \u300b \uff0c \u5927 \u9ec4 \u8702 \u592a \u53ef \u7231 \u4e86 \uff0c \u4e24 \u53ea \u5927 \u773c \u775b \u3002 \u68c0 \u7d22 \u300a \u73af \u592a \u5e73 \u6d0b \u300b \u4f60 \u770b \u8fc7 \u5417 \uff1f \u8fd9 \u4e2a \u4e5f \u975e \u5e38 \u597d \u770b \u3002 \u4e0d \u68c0 \u7d22 \u6211 \u4e5f \u770b \u4e86 \uff0c \u6211 \u7b2c \u4e8c \u4e2a \u7535 \u5f71 \u5728 \u7535 \u5f71 \u9662 \u770b \u7684 \uff0c 3d \u7535 \u5f71 \u7279 \u6548 \u975e \u5e38 \u597d \u3002 \u4e0d \u68c0 \u7d22 \u8fd8 \u8981 \u4ec0 \u4e48 \u597d \u770b \u7684 \u79d1 \u5e7b \u7535 \u5f71 \u5417 \uff1f \u68c0 \u7d22 \u6211 \u77e5 \u9053 \uff0c \u4f46 \u662f \u6211 \u6ca1 \u6709 \u770b \u8fc7 \uff0c \u300a \u963f \u51e1 \u8fbe \u300b \u8bb2 \u7684 \u4ec0 \u4e48 \u6545 \u4e8b \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 80 \u5929 \u73af \u6e38 \u5730 \u7403 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u6211 \u4eca \u5929 \u53c8 \u8bfb \u5b8c \u4e86 \u4e00 \u672c \u540d \u8457 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 80 \u5929 \u73af \u6e38 \u5730 \u7403 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u6211 \u4eca \u5929 \u53c8 \u8bfb \u5b8c \u4e86 \u4e00 \u672c \u540d \u8457 \u3002 \u4e0d \u68c0 \u7d22 \u300a \u516b \u5341 \u5929 \u73af \u6e38 \u5730 \u7403 \u300b \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 80 \u5929 \u73af \u6e38 \u5730 \u7403 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u6211 \u4eca \u5929 \u53c8 \u8bfb \u5b8c \u4e86 \u4e00 \u672c \u540d \u8457 \u3002 \u4e0d \u68c0 \u7d22 \u300a \u516b \u5341 \u5929 \u73af \u6e38 \u5730 \u7403 \u300b \u3002 \u68c0 \u7d22 \u4f60 \u4e5f \u770b \u8fc7 \u8fd9 \u672c \u4e66 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 80 \u5929 \u73af \u6e38 \u5730 \u7403 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u6211 \u4eca \u5929 \u53c8 \u8bfb \u5b8c \u4e86 \u4e00 \u672c \u540d \u8457 \u3002 \u4e0d \u68c0 \u7d22 \u300a \u516b \u5341 \u5929 \u73af \u6e38 \u5730 \u7403 \u300b \u3002 \u68c0 \u7d22 \u4f60 \u4e5f \u770b \u8fc7 \u8fd9 \u672c \u4e66 \uff1f \u68c0 \u7d22 \u6211 \u770b \u7b80 \u4ecb \u8bf4 \uff0c \u662f \u4e00 \u90e8 \u79d1 \u5e7b \u5c0f \u8bf4 \uff0c \u7ed3 \u679c \u5185 \u5bb9 \u770b \u8d77 \u6765 \u66f4 \u50cf \u662f \u4e00 \u4e2a \u73b0 \u5b9e \u4e3b \u4e49 \u7684 \u6e38 \u51fb \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 80 \u5929 \u73af \u6e38 \u5730 \u7403 \u6cb3 \u5357 \u7701 \u5546 \u4e18 \u5e02 \u6211 \u4eca \u5929 \u53c8 \u8bfb \u5b8c \u4e86 \u4e00 \u672c \u540d \u8457 \u3002 \u4e0d \u68c0 \u7d22 \u300a \u516b \u5341 \u5929 \u73af \u6e38 \u5730 \u7403 \u300b \u3002 \u68c0 \u7d22 \u4f60 \u4e5f \u770b \u8fc7 \u8fd9 \u672c \u4e66 \uff1f \u68c0 \u7d22 \u6211 \u770b \u7b80 \u4ecb \u8bf4 \uff0c \u662f \u4e00 \u90e8 \u79d1 \u5e7b \u5c0f \u8bf4 \uff0c \u7ed3 \u679c \u5185 \u5bb9 \u770b \u8d77 \u6765 \u6