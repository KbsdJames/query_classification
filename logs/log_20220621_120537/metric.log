b \u5e02 \u87ba \u86f3 \u7c89 \u597d \u81ed \u554a \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u87ba \u86f3 \u7c89 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u87ba \u86f3 \u7c89 \u597d \u81ed \u554a \u68c0 \u7d22 \u5bf9 \u5440 \uff0c \u5728 \u5bb6 \u5403 \u4e00 \u5305 \u5473 \u9053 \u80fd \u7559 \u597d \u51e0 \u5929 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u87ba \u86f3 \u7c89 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u87ba \u86f3 \u7c89 \u597d \u81ed \u554a \u68c0 \u7d22 \u5bf9 \u5440 \uff0c \u5728 \u5bb6 \u5403 \u4e00 \u5305 \u5473 \u9053 \u80fd \u7559 \u597d \u51e0 \u5929 \u4e0d \u68c0 \u7d22 \u6211 \u4e70 \u4e86 \u597d \u51e0 \u4e2a \u54c1 \u724c \u7684 \u87ba \u86f3 \u7c89 \uff0c \u6253 \u7b97 \u8bd5 \u4e00 \u904d \u770b \u770b \u54ea \u4e2a \u6700 \u597d \u5403 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u87ba \u86f3 \u7c89 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u87ba \u86f3 \u7c89 \u597d \u81ed \u554a \u68c0 \u7d22 \u5bf9 \u5440 \uff0c \u5728 \u5bb6 \u5403 \u4e00 \u5305 \u5473 \u9053 \u80fd \u7559 \u597d \u51e0 \u5929 \u4e0d \u68c0 \u7d22 \u6211 \u4e70 \u4e86 \u597d \u51e0 \u4e2a \u54c1 \u724c \u7684 \u87ba \u86f3 \u7c89 \uff0c \u6253 \u7b97 \u8bd5 \u4e00 \u904d \u770b \u770b \u54ea \u4e2a \u6700 \u597d \u5403 \u68c0 \u7d22 \u6211 \u5403 \u4e86 \u87ba \u9738 \u738b \u7684 \u9ebb \u8fa3 \u87ba \u86f3 \u7c89 \uff0c \u771f \u4e0d \u9519 \u3002 \u81ed \u5473 \u6ca1 \u6709 \u90a3 \u4e48 \u5927 \uff0c \u53c8 \u5f88 \u8fa3 \u800c \u4e14 \u8fd8 \u6709 \u86cb \u767d \u68d2 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u87ba \u86f3 \u7c89 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u87ba \u86f3 \u7c89 \u597d \u81ed \u554a \u68c0 \u7d22 \u5bf9 \u5440 \uff0c \u5728 \u5bb6 \u5403 \u4e00 \u5305 \u5473 \u9053 \u80fd \u7559 \u597d \u51e0 \u5929 \u4e0d \u68c0 \u7d22 \u6211 \u4e70 \u4e86 \u597d \u51e0 \u4e2a \u54c1 \u724c \u7684 \u87ba \u86f3 \u7c89 \uff0c \u6253 \u7b97 \u8bd5 \u4e00 \u904d \u770b \u770b \u54ea \u4e2a \u6700 \u597d \u5403 \u68c0 \u7d22 \u6211 \u5403 \u4e86 \u87ba \u9738 \u738b \u7684 \u9ebb \u8fa3 \u87ba \u86f3 \u7c89 \uff0c \u771f \u4e0d \u9519 \u3002 \u81ed \u5473 \u6ca1 \u6709 \u90a3 \u4e48 \u5927 \uff0c \u53c8 \u5f88 \u8fa3 \u800c \u4e14 \u8fd8 \u6709 \u86cb \u767d \u68d2 \u3002 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6ca1 \u9519 \u3002 \u5982 \u679c \u4e00 \u4e0a \u6765 \u5c31 \u81ed \u5473 \u592a \u5927 \u7684 \u8bdd \u53ef \u80fd \u6211 \u5c31 \u653e \u5f03 \u4e86 \u3002 \u68c0 \u7d22 \u3002', '\u672c \u5730 \u751f \u6d3b \u7f8e \u98df \u87ba \u86f3 \u7c89 \u65b0 \u7586 \u7ef4 \u543e \u5c14 \u81ea \u6cbb \u533a \u56fe \u6728 \u8212 \u514b \u5e02 \u87ba \u86f3 \u7c89 \u597d \u81ed \u554a \u68c0 \u7d22 \u5bf9 \u5440 \uff0c \u5728 \u5bb6 \u5403 \u4e00 \u5305 \u5473 \u9053 \u80fd \u7559 \u597d \u51e0 \u5929 \u4e0d \u68c0 \u7d22 \u6211 \u4e70 \u4e86 \u597d \u51e0 \u4e2a \u54c1 \u724c \u7684 \u87ba \u86f3 \u7c89 \uff0c \u6253 \u7b97 \u8bd5 \u4e00 \u904d \u770b \u770b \u54ea \u4e2a \u6700 \u597d \u5403 \u68c0 \u7d22 \u6211 \u5403 \u4e86 \u87ba \u9738 \u738b \u7684 \u9ebb \u8fa3 \u87ba \u86f3 \u7c89 \uff0c \u771f \u4e0d \u9519 \u3002 \u81ed \u5473 \u6ca1 \u6709 \u90a3 \u4e48 \u5927 \uff0c \u53c8 \u5f88 \u8fa3 \u800c \u4e14 \u8fd8 \u6709 \u86cb \u767d \u68d2 \u3002 \u4e0d \u68c0 \u7d22 \u54c8 \u54c8 \uff0c \u6ca1 \u9519 \u3002 \u5982 \u679c \u4e00 \u4e0a \u6765 \u5c31 \u81ed \u5473 \u592a \u5927 \u7684 \u8bdd \u53ef \u80fd \u6211 \u5c31 \u653e \u5f03 \u4e86 \u4e0d \u68c0 \u7d22 \u55ef \u55ef \uff0c \u6211 \u770b \u597d \u591a \u90fd \u662f \u52a0 \u732a \u811a \uff0c \u70b8 \u86cb \uff0c \u9e2d \u638c \u3002 \u592a \u8bf1 \u60d1 \u4e86 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u5e73 \u51e1 \u7684 \u4e16 \u754c \u6cb3 \u5357 \u7701 \u6fee \u9633 \u5e02 \u8001 \u5e08 \u8ba9 \u5047 \u671f \u8bfb \u4e00 \u672c \u540d \u8457 \uff0c \u8be5 \u8bfb \u54ea \u672c \u597d \u5462 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u5e73 \u51e1 \u7684 \u4e16 \u754c \u6cb3 \u5357 \u7701 \u6fee \u9633 \u5e02 \u8001 \u5e08 \u8ba9 \u5047 \u671f \u8bfb \u4e00 \u672c \u540d \u8457 \uff0c \u8be5 \u8bfb \u54ea \u672c \u597d \u5462 \u3002 \u4e0d \u68c0 \u7d22 \u5f53 \u4ee3 \u7684 \u540d \u8457 \u4e5f \u6709 \u597d \u591a \u5440 \u3002 \u6211 \u6709 \u9009 \u62e9 \u56f0 \u96be \u75c7 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u5e73 \u51e1 \u7684 \u4e16 \u754c \u6cb3 \u5357 \u7701 \u6fee \u9633 \u5e02 \u8001 \u5e08 \u8ba9 \u5047 \u671f \u8bfb \u4e00 \u672c \u540d \u8457 \uff0c \u8be5 \u8bfb \u54ea \u672c \u597d \u5462 \u3002 \u4e0d \u68c0 \u7d22 \u5f53 \u4ee3 \u7684 \u540d \u8457 \u4e5f \u6709 \u597d \u591a \u5440 \u3002 \u6211 \u6709 \u9009 \u62e9 \u56f0 \u96be \u75c7 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u80fd \u7ed9 \u6211 \u5927 \u6982 \u8bf4 \u8bf4 \u8bb2 \u4e86 \u4ec0 \u4e48 \u5185 \u5bb9 \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u5e73 \u51e1 \u7684 \u4e16 \u754c \u6cb3 \u5357 \u7701 \u6fee \u9633 \u5e02 \u8001 \u5e08 \u8ba9 \u5047 \u671f \u8bfb \u4e00 \u672c \u540d \u8457 \uff0c \u8be5 \u8bfb \u54ea \u672c \u597d \u5462 \u3002 \u4e0d \u68c0 \u7d22 \u5f53 \u4ee3 \u7684 \u540d \u8457 \u4e5f \u6709 \u597d \u591a \u5440 \u3002 \u6211 \u6709 \u9009 \u62e9 \u56f0 \u96be \u75c7 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u80fd \u7ed9 \u6211 \u5927 \u6982 \u8bf4 \u8bf4 \u8bb2 \u4e86 \u4ec0 \u4e48 \u5185 \u5bb9 \u5417 \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u5f88 \u6709 \u610f \u601d \u554a \u3002 \u6211 \u5f97 \u8d76 \u7d27 \u8bfb \u4e00 \u8bfb \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6587 \u5316 \u540d \u8457 \u5e73 \u51e1 \u7684 \u4e16 \u754c \u6cb3 \u5357 \u7701 \u6fee \u9633 \u5e02 \u8001 \u5e08 \u8ba9 \u5047 \u671f \u8bfb \u4e00 \u672c \u540d \u8457 \uff0c \u8be5 \u8bfb \u54ea \u672c \u597d \u5462 \u3002 \u4e0d \u68c0 \u7d22 \u5f53 \u4ee3 \u7684 \u540d \u8457 \u4e5f \u6709 \u597d \u591a \u5440 \u3002 \u6211 \u6709 \u9009 \u62e9 \u56f0 \u96be \u75c7 \u3002 \u4e0d \u68c0 \u7d22 \u4f60 \u80fd \u7ed9 \u6211 \u5927 \u6982 \u8bf4 \u8bf4 \u8bb2 \u4e86 \u4ec0 \u4e48 \u5185 \u5bb9 \u5417 \uff1f \u68c0 \u7d22 \u542c \u8d77 \u6765 \u5f88 \u6709 \u610f \u601d \u554a \u3002 \u6211 \u5f97 \u8d76 \u7d27 \u8bfb \u4e00 \u8bfb \u3002 \u68c0 \u7d22 \u6211 \u5988 \u6bcf \u5929 \u53ea \u8ba9 \u6211 \u73a9 20 \u5206 \u949f \u624b \u673a \uff0c \u8bf4 \u662f \u6015 \u6211 \u8fd1 \u89c6 \u3002 \u6211 \u8fd8 \u662f \u8bfb \u7eb8 \u8d28 \u7248 \u7684 \u5427 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65e5 \u6f2b \u56db \u5ddd \u7701 \u5df4 \u4e2d \u5e02 \u52a8 \u6f2b \u662f \u4e0d \u662f \u65e5 \u672c \u7684 \u6700 \u706b \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65e5 \u6f2b \u56db \u5ddd \u7701 \u5df4 \u4e2d \u5e02 \u52a8 \u6f2b \u662f \u4e0d \u662f \u65e5 \u672c \u7684 \u6700 \u706b \uff1f \u68c0 \u7d22 \u4f60 \u6709 \u770b \u8fc7 \u54ea \u4e9b \u65e5 \u672c \u7684 \u52a8 \u6f2b \u5462 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65e5 \u6f2b \u56db \u5ddd \u7701 \u5df4 \u4e2d \u5e02 \u52a8 \u6f2b \u662f \u4e0d \u662f \u65e5 \u672c \u7684 \u6700 \u706b \uff1f \u68c0 \u7d22 \u4f60 \u6709 \u770b \u8fc7 \u54ea \u4e9b \u65e5 \u672c \u7684 \u52a8 \u6f2b \u5462 \uff1f \u68c0 \u7d22 \u6211 \u8bb0 \u5f97 \u6211 \u5c0f \u65f6 \u5019 \u770b \u8fc7 \u300a \u767e \u53d8 \u5c0f \u6a31 \u9b54 \u672f \u5361 \u300b \uff0c \u4f60 \u77e5 \u9053 \u8fd9 \u4e2a \u5417 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65e5 \u6f2b \u56db \u5ddd \u7701 \u5df4 \u4e2d \u5e02 \u52a8 \u6f2b \u662f \u4e0d \u662f \u65e5 \u672c \u7684 \u6700 \u706b \uff1f \u68c0 \u7d22 \u4f60 \u6709 \u770b \u8fc7 \u54ea \u4e9b \u65e5 \u672c \u7684 \u52a8 \u6f2b \u5462 \uff1f \u68c0 \u7d22 \u6211 \u8bb0 \u5f97 \u6211 \u5c0f \u65f6 \u5019 \u770b \u8fc7 \u300a \u767e \u53d8 \u5c0f \u6a31 \u9b54 \u672f \u5361 \u300b \uff0c \u4f60 \u77e5 \u9053 \u8fd9 \u4e2a \u5417 \uff1f \u68c0 \u7d22 \u540e \u6765 \u51fa \u4e86 \u7b2c \u4e8c \u90e8 \uff0c \u4f46 \u662f \u6211 \u6ca1 \u6709 \u518d \u770b \u4e86 \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65e5 \u6f2b \u56db \u5ddd \u7701 \u5df4 \u4e2d \u5e02 \u52a8 \u6f2b \u662f \u4e0d \u662f \u65e5 \u672c \u7684 \u6700 \u706b \uff1f \u68c0 \u7d22 \u4f60 \u6709 \u770b \u8fc7 \u54ea \u4e9b \u65e5 \u672c \u7684 \u52a8 \u6f2b \u5462 \uff1f \u68c0 \u7d22 \u6211 \u8bb0 \u5f97 \u6211 \u5c0f \u65f6 \u5019 \u770b \u8fc7 \u300a \u767e \u53d8 \u5c0f \u6a31 \u9b54 \u672f \u5361 \u300b \uff0c \u4f60 \u77e5 \u9053 \u8fd9 \u4e2a \u5417 \uff1f \u68c0 \u7d22 \u540e \u6765 \u51fa \u4e86 \u7b2c \u4e8c \u90e8 \uff0c \u4f46 \u662f \u6211 \u6ca1 \u6709 \u518d \u770b \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u56e0 \u4e3a \u6211 \u957f \u5927 \u4e86 \uff0c \u4e0d \u600e \u4e48 \u770b \u4e86 \uff0c \u89c9 \u5f97 \u90a3 \u4e9b \u8fd8 \u662f \u6709 \u4e9b \u5e7c \u7a1a \u3002 \u3002 \u68c0 \u7d22 \u3002', '\u6cdb \u5a31 \u4e50 \u52a8 \u6f2b \u65e5 \u6f2b \u56db \u5ddd \u7701 \u5df4 \u4e2d \u5e02 \u52a8 \u6f2b \u662f \u4e0d \u662f \u65e5 \u672c \u7684 \u6700 \u706b \uff1f \u68c0 \u7d22 \u4f60 \u6709 \u770b \u8fc7 \u54ea \u4e9b \u65e5 \u672c \u7684 \u52a8 \u6f2b \u5462 \uff1f \u68c0 \u7d22 \u6211 \u8bb0 \u5f97 \u6211 \u5c0f \u65f6 \u5019 \u770b \u8fc7 \u300a \u767e \u53d8 \u5c0f \u6a31 \u9b54 \u672f \u5361 \u300b \uff0c \u4f60 \u77e5 \u9053 \u8fd9 \u4e2a \u5417 \uff1f \u68c0 \u7d22 \u540e \u6765 \u51fa \u4e86 \u7b2c \u4e8c \u90e8 \uff0c \u4f46 \u662f \u6211 \u6ca1 \u6709 \u518d \u770b \u4e86 \u3002 \u4e0d \u68c0 \u7d22 \u56e0 \u4e3a \u6211 \u957f \u5927 \u4e86 \uff0c \u4e0d \u600e \u4e48 \u770b \u4e86 \uff0c \u89c9 \u5f97 \u90a3 \u4e9b \u8fd8 \u662f \u6709 \u4e9b \u5e7c \u7a1a \u3002 \u4e0d \u68c0 \u7d22 \u300a \u9b3c \u706d \u4e4b \u5203 \u300b \u662f \u4ec0 \u4e48 \uff1f \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5c6f \u95e8 \u533a \u6211 \u4e0a \u8f88 \u5b50 \u4e0d \u4f1a \u662f \u6c34 \u679c \u738b \u5427 \u4e0d \u7136 \u6211 \u600e \u4e48 \u90a3 \u4e48 \u559c \u6b22 \u5403 \u6c34 \u679c \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5c6f \u95e8 \u533a \u6211 \u4e0a \u8f88 \u5b50 \u4e0d \u4f1a \u662f \u6c34 \u679c \u738b \u5427 \u4e0d \u7136 \u6211 \u600e \u4e48 \u90a3 \u4e48 \u559c \u6b22 \u5403 \u6c34 \u679c \uff01 \u4e0d \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u5927 \u897f \u74dc \uff01 \u7279 \u522b \u662f \u90a3 \u79cd \u6c99 \u74dc \uff0c \u7b80 \u76f4 \u4e0d \u8981 \u592a \u597d \u5403 \uff01 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5c6f \u95e8 \u533a \u6211 \u4e0a \u8f88 \u5b50 \u4e0d \u4f1a \u662f \u6c34 \u679c \u738b \u5427 \u4e0d \u7136 \u6211 \u600e \u4e48 \u90a3 \u4e48 \u559c \u6b22 \u5403 \u6c34 \u679c \uff01 \u4e0d \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u5927 \u897f \u74dc \uff01 \u7279 \u522b \u662f \u90a3 \u79cd \u6c99 \u74dc \uff0c \u7b80 \u76f4 \u4e0d \u8981 \u592a \u597d \u5403 \uff01 \u68c0 \u7d22 \u8fd9 \u4e2a \u6211 \u5012 \u662f \u6ca1 \u53bb \u5173 \u6ce8 \u8036 \uff0c \u6211 \u5c31 \u5403 \u5b83 \u7684 \u679c \u8089 \u54c8 \u54c8 \u54c8 \u54c8 \u3002 \u68c0 \u7d22 \u3002', '\u8d2d \u7269 \u6c34 \u679c \u9999 \u6e2f \u7279 \u522b \u884c \u653f \u533a \u5c6f \u95e8 \u533a \u6211 \u4e0a \u8f88 \u5b50 \u4e0d \u4f1a \u662f \u6c34 \u679c \u738b \u5427 \u4e0d \u7136 \u6211 \u600e \u4e48 \u90a3 \u4e48 \u559c \u6b22 \u5403 \u6c34 \u679c \uff01 \u4e0d \u68c0 \u7d22 \u6211 \u6700 \u559c \u6b22 \u5927 \u897f \u74dc \uff01 \u7279 \u522b \u662f \u90a3 \u79cd \u6c99 \u74dc \uff0c 